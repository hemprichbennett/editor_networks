@article{ ISI:000247580800011,
Author = {Nilsson, Jan-Ake and Granbom, Martin and Raberg, Lars},
Title = {{Does the strength of an immune response reflect its energetic cost?}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
