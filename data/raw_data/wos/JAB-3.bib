@article{ ISI:000171167500010,
Author = {Blanco, G and Tella, JL and Potti, J and Baz, A},
Title = {{Feather mites on birds: costs of parasitism or conditional outcomes?}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{2001}},
Volume = {{32}},
Number = {{3}},
Pages = {{271-274}},
Month = {{SEP}},
Abstract = {{Feather mites (suborder Astigmata, superfamilies Analgoidea,
   Pterolichoidea and Freyaniidae) are among the commonest ectosymbionts of
   birds. Most researchers have assumed they are parasites, having negative
   effects on hosts. Here we present evidence that suggests that feather
   mites may not be parasites. We develop a framework for considering
   conditional outcomes in these interspecific associations, dealing with
   different kinds of relationships between symbionts. The non-parasitic
   status of feather mites is supported by a literature review as well as
   by preliminary data on mites' food. We illustrate symbiotic
   relationships with a graphical model showing different scenarios in
   which hosts' cost-benefit relations are determined by the interactions
   among their symbionts.}},
