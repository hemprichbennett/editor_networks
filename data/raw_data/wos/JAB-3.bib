@article{ ISI:000185539800002,
Author = {Miyazaki, M and Waas, JR},
Title = {{Acoustic properties of male advertisement and their impact on female
   responsiveness in little penguins Eudyptula minor}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{2003}},
Volume = {{34}},
Number = {{3}},
Pages = {{229-232}},
Month = {{SEP}},
Abstract = {{We examined the relationship between acoustic properties of male
   advertising calls, male body size and female responses in little
   penguins Euelyptula minor. Larger males produced calls of lower
   frequency. Playback experiments indicated that females were more likely
   to respond to low or medium pitched calls, than to high ones (although
   only 28\% of females responded vocally to playback). This may reflect
   the interest of unmated females in large to medium sized senders as
   potential mates, or indicate a stronger territorial response to large
   intruders.}},
