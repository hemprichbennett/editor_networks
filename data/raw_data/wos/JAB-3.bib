@article{ ISI:000247312100021,
Author = {Korsten, Peter and Limbourg, Tobias and Lessells, Catherine M. and
   Komdeur, Jan},
Title = {{Effectiveness of a commonly-used technique for experimentally reducing
   plumage UV reflectance}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{2007}},
Volume = {{38}},
Number = {{3}},
Pages = {{399-403}},
Month = {{MAY}},
Abstract = {{Ultraviolet (UV) plumage is thought to be sexually selected through
   intra-sexual competition, female choice and differential allocation.
   Experimental manipulations of plumage UV reflectance are essential to
   demonstrate that mate choice or intra-sexual competition are causally
   related to UV coloration. The most widely-used technique for
   manipulating UV reflectance in wild birds is the application of a
   Mixture of UV-absorbing chemicals and preen gland fat. However, although
   this UV reduction technique is commonly used, little is known about the
   persistence of the treatment and the termporal variation in UV
   reflectance that it causes. We manipulated the UV crown plumage of wild
   and captive blue tits Parus caeruleus, and took repeated
   photospectrometric measurements of both UV-reduced and control-treated
   individuals. Our results show that the UV reduction lasts for at least
   Five days and that the treatment has no negative effects on the survival
   of wild birds.}},
