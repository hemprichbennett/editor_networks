@article{ ISI:000169072500014,
Author = {Watanuki, Y and Mehlum, F and Takahashi, A},
Title = {{Water temperature sampling by foraging Brunnich's Guillemots with
   bird-borne data loggers}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{2001}},
Volume = {{32}},
Number = {{2}},
Pages = {{189-193}},
Month = {{JUN}},
Abstract = {{We describe the features of waters where seabirds were feeding by
   sampling vertical water temperature profiles with data loggers mounted
   on five Brunnich's Guillemots in Svalbard, Norway. The guillemots
   foraged in a cold water (-0.5-0.5 degreesC SST (sea surface
   temperature)) by making 1.8 dive bouts in short trips (32-257 min
   duration) as well as in moderate (0.5-2.0 degreesC SST) and warm waters
   (2.5-4.0 degreesC SST) by making 6.0 dive bouts during long trips
   (411-688 min duration). Judging from out bound flying time (15.7-24.4
   min), time between dive bouts (23.9-43.3 min) and water types, the birds
   probably fed in fjord or coastal waters during short hips and in both
   coastal and offshore waters during long trips. Water temperature and
   diving behaviour can be simultaneously recorded by small data loggers,
   which therefore will provide useful information on marine features and
   foraging activity of top predators.}},
