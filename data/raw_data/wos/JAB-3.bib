@article{ ISI:000237096100004,
Author = {Johnson, MD and Strong, AM and Sherry, TW},
Title = {{Migrants in tropical bird communities: the balanced breeding limitation
   hypothesis}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{2006}},
Volume = {{37}},
Number = {{3}},
Pages = {{229-237}},
Month = {{MAY}},
Abstract = {{Explanations for the ecological integration of migratory and
   non-migratory (resident) insectivorous birds in the tropics have been
   complicated by the paradox that arthropod abundances are low when bird
   abundances reach their annual peak. The breeding currency hypothesis and
   the nest predation hypothesis both account for this paradox by
   postulating that residents are held below the non-breeding season
   carrying capacity, which frees resources available for migratory
   insectivores. The breeding currency hypothesis suggests residents are
   limited by food suitable for nestlings, whereas the nest predation
   hypothesis emphasizes the primacy of high rates of nest predation.
   However, theoretical arguments suggest that food availability and
   predation risk interact strongly to limit breeding birds. We use
   graphical analyses to extend the breeding currency hypothesis to
   incorporate effects of nest predation. This yields a more synthetic and
   realistic model for the integration of migrant and resident insectivores
   in the tropics - the balanced breeding limitation hypothesis.}},
