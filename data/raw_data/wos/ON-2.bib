@article{ ISI:000256158100026,
Author = {Ferrari, Silvia and Sawicki, Zonia and Albrieu, Carlos and Loekemeyer,
   Nora and Gigli, Silvia and Bucher, Enrique H.},
Title = {{Management and conservation of shorebirds in Argentina: local
   experiences at four sites of the Western Hemisphere Shorebird Reserve
   Network (WHSRN)}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2008}},
Volume = {{19}},
Number = {{S}},
Pages = {{311-319}},
Note = {{8th Neotropical Ornithological Congress, Maturin, VENEZUELA, MAY 13-19,
   2007}},
Abstract = {{Argentina plays a role of hemispherical importance for numerous
   shorebirds, both Nearctic and Neotropical, which has been the basis for
   the inclusion of wetlands as part of the Western Hemisphere Shorebird
   Reserve Network. At present it includes four sites: Dulce river wetlands
   and Mar Chiquita lagoon in central Argentina (Cordoba), San Antonio bay
   (Rio Negro), Gallegos river estuary (Santa Cruz), and Atlantic Coast
   Reserve (Tierra del Fuego). A short description and the ornithological
   importance of each one are presented, together with the threats to their
   conservation, accomplished progress, and future challenges. In all of
   the sites environmental education actions, training, and research
   projects are carried on, with the intervention of governmental agencies
   and important contribution from universities and local and international
   NGOs. The gratest challenge is to make land uses compatible with the
   conservation objectives, for which the accomplishment of the managing
   plans, that are in different stages of development by means of community
   participation, is indispensable.}},
