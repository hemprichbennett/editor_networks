@article{ ISI:000261698800004,
Author = {Villa-Bonilla, Bernardino and Rojas-Soto, Octavio R. and Gabriela
   Colodner-Chamudis, Ana and Tejeda-Cruz, Cesar},
Title = {{Municipal-level inventories of the avifauna and its application to
   conservation: the case of Zacapoaxtla, Puebla, Mexico}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2008}},
Volume = {{19}},
Number = {{4}},
Pages = {{531-551}},
Abstract = {{Bird inventories at the municipal-level could improve the selection of
   priority areas in conservation. Based on an update of the avifauna of
   Zacapoaxtla Municipality, Puebla, Mexico, we used vegetation type and
   land use within the municipality to analyze bird species' richness,
   endemism, and species at risk. We also examined the general distribution
   patterns and described birds' seasonality. The highest species richness,
   and the highest numbers of endemic, threatened, and exclusive species
   were found in cloud forest, followed, in descending order, by pine
   forest, pine-oak forest, temporary agriculture and cultivated
   grasslands. Based on this information, we propose priority areas for
   bird preservation within the municipality and we discussed the role of
   inventories at this scale as a strategy for conservation.}},
