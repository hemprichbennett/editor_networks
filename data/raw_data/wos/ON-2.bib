@article{ ISI:000261698800007,
Author = {McCrary, Jeffrey K. and Young, Jr., David P.},
Title = {{NEW AND NOTEWORTHY OBSERVATIONS OF RAPTORS IN SOUTHWARD MIGRATION IN
   NICARAGUA}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2008}},
Volume = {{19}},
Number = {{4}},
Pages = {{573-580}},
Abstract = {{We present noteworthy sight records for 12 raptor species in southward
   migration in Nicaragua, including new country records for the
   Yellow-headed Caracara (Milvaga chimachima), and new records for it and
   eight other raptor species in migration. Additionally, we demonstrate a
   significant Black Vulture (Coragyps atratus) southward migration
   involving more than 200,000 individuals, and we document over 200
   Cooper's Hawks (Accipiter cooperi), in migration. We also report the
   migration of several non-raptor species which appear to utilize the same
   pathways during day time. These observations were made while monitoring
   southward raptor migration through central and southwestern Nicaragua
   from 26 August to 18 November 2006. We comment upon the hypothesis that
   range expansion may be accomplished through adoption of seasonal
   migrations. Accepted 3 September 2008.}},
