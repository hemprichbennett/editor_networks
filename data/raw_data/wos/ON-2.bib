@article{ ISI:000233958200004,
Author = {Sanchez, C},
Title = {{First description of the nest and eggs of the Slaty Finch (Haplospiza
   rustica) and observations on song and breeding behavior}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2005}},
Volume = {{16}},
Number = {{4}},
Pages = {{493-501}},
Abstract = {{The nest, eggs, and male song of the Slaty Finch (Haplospiza rustica)
   are described. Two nests were collected during a massive production of
   bamboo seeds of which the finches were feeding. The first nest contained
   four eggs, and the second three nestlings.. The eggs were-dull white
   with no markings. The nest was globular in shape, built mainly with
   mosses and liverworts. Three different song types and a ``flight
   song{''} were recorded in the population, and the song was compared with
   the closely related species.}},
