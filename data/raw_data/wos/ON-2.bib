@article{ ISI:000238110900004,
Author = {Estades, Cristian F. and Escobar, Martin A. H. and Tomasevic, Jorge A.
   and Vukasovic, Maria Angelica and Paez, Mauricio},
Title = {{Mist-nets versus point counts in the estimation of forest bird
   abundances in south-central Chile}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2006}},
Volume = {{17}},
Number = {{2}},
Pages = {{203-212}},
Abstract = {{We used variable-radius' point counts and mist-netting to estimate
   forest bird abundances in the Maule Region of South Central Chile.
   nest-netting detected a total of 25 species over 3 years and 3 seasons
   per year (Winter, Spring, and Summer) whereas point counts recorded 38
   species during the same time. In general, the relationship between
   capture rate and estimated density agreed with the prediction that
   canopy foragers and large and less mobile species tend to be
   underrepresented in mist-netting data. Point counts area much more
   cost-effective technique to assess bird populations in temperate
   Neotropical forests.}},
