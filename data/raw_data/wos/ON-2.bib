@article{ ISI:000256158100023,
Author = {Bertsch, Carolina and Barreto, Guillermo R.},
Title = {{Abundance and home range of the Yellow-knobbed Curassow (Crax
   daubentoni) in the Venezuelan Central Llanos}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2008}},
Volume = {{19}},
Number = {{S}},
Pages = {{287-293}},
Note = {{8th Neotropical Ornithological Congress, Maturin, VENEZUELA, MAY 13-19,
   2007}},
Abstract = {{The Yellow-knobbed curassow (Crax daubentoni) is one of most threatened
   cracid species to the north of the Orinoco river in Venezuela and
   Colombia. Despite this fact, its habitat requirements are poorly known
   and very few population abundance data are available. Here we show
   estimates of population abundance, home range and habitat use of
   Yellow-knobbed Curassows in a protected private ranch of central
   Venezuela. Firstly, we estimated abundance by means of transect surveys.
   Secondly, we studied curassows home range by using kernel probabilistic
   methods after radio tracking six individuals. Finally we overlapped home
   range contours on a vegetation map of the study area in order to assess
   habitat use by these birds. We found up to 100 birds per km(2) in the
   area. These densities may represent possible maximum values for an area
   with high quality habitat and no hunting. Home range was estimated to be
   150-200 ha depending on the habitat considered. Curassows used gallery
   and dry forests mainly. Nevertheless, these birds were also found in
   open areas, hedges and borders, as revealed by radio tracking. Curassows
   seem to use a heterogeneous landscape comprising forested areas but open
   areas, borders and hedges as well.}},
