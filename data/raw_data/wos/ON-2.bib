@article{ ISI:000249644600003,
Author = {McCormack, John E. and Castaneda-Guayasamin, Gabriela and Levandoski,
   Greg},
Title = {{Sierra Santa Rosa: An oasis of bird diversity in Arid northern Mexico}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2007}},
Volume = {{18}},
Number = {{3}},
Pages = {{369-377}},
Abstract = {{The Sierra Santa Rosa is part of a regional network of ``sky island{''}
   mountain ranges in northern Coahuila. From 2004 to 2006, we made three
   visits to a site in the Sierra, Santa Rosa located at 880 in elevation
   on the eastern slope of a low-lying parallel range. We report a diverse
   avifauna uncharacteristic for the region, including a new breeding
   record for the Yellow-green Vireo (Vireo flavoviridis) in Coahuila and
   major breeding range extensions for the 1,east Grebe (Tachybaptus
   dominicus), Tropical Parula (Parula pitiayumi), Rufous-capped Warbler
   (Basileuterus. rufifrons), Flame-colored Tanager (Piranga bidentata),
   and Olive Sparrow (Arremonops rufivirgatus). This site. extends many
   elements of tropical, Sierra Madre, and Gulf Coast bird communities
   hundreds of kilometers to the north and east. These results emphasize
   the need for more bird surveys in the remote sky islands of northern
   Coahuila.}},
