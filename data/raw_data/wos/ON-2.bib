@article{ ISI:000233958200009,
Author = {Trejo, A and Kun, M and Sahores, M and Seijas, S},
Title = {{Diet overlap and prey size of two owls in the forest-steppe ecotone of
   southern Argentina}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2005}},
Volume = {{16}},
Number = {{4}},
Pages = {{539-546}},
Abstract = {{We analyzed die diets of Barn Owls (Tyto alba) and Magellanic Horned
   Owls (Bubo magellanicus) in the ecotone between the western temperate
   Nothofagus forests,and the eastern and Patagonian steppe in northwestern
   Argentine Patagonia. We studied the degree of dietary similarity and
   compared prey size consumed by the two owl species. Field studies were
   conducted during two breeding seasons (2001-2003). We identified 1145
   prey from 638 Barn Owl pellets, and 910 prey from 380 Magellanic Horned
   Owls pellets. Both owls had diets dominated by sigmodontine rodents with
   variable proportions of other prey hares, birds and arthropods). Trophic
   overlap was very high (0.903). Mean vertebrate prey weight was 44.5 g
   for Barn Owls, and 47.0 g for Magellanic Horned Owls. In our study area,
   Magellanic Horned Owls consumed fewer large-sized prey (e.g.,
   lagomorphs) than in other regions, probably due to the high diversity
   and abundance of medium-sized sigmodontine rodents in the ecotone.}},
