@article{ ISI:000238110900002,
Author = {Dobbs, RC and Greeney, HF},
Title = {{Nesting and foraging ecology of the rufous-breasted flycatcher
   (Leptopogon rufipectus)}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2006}},
Volume = {{17}},
Number = {{2}},
Pages = {{173-181}},
Abstract = {{Seventeen Rufous-breasted Flycatcher (Leptopogon rufipectus) nests were
   active from March to May, and from May, and from September to November,
   in western Napo Province, Ecuador. Nests were semiglobular and pendant,
   composed primarily of moss, and located beneath stream banks. Eggs were
   white and unmarked; clutch size was two. Documented nestling periods
   were 21-23 days. Foraging birds searched for insects located on the
   undersides of live leaves from perches located primarily in the
   mid-story, and attacked insects by making upward sally-hovers and
   sally-strikes. Nesting and foraging ecology of Rufous-breasted
   Flycatchers appears generally similar to that which is known for other
   Leptopogon species.}},
