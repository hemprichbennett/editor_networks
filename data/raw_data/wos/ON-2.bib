@article{ ISI:000238110900007,
Author = {Hospitaleche, CA and Gasparini, G},
Title = {{Evaluation with systematic purposes of the cranial and mandibular
   characters in Spheniscidae.}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2006}},
Volume = {{17}},
Number = {{2}},
Pages = {{235-241}},
Abstract = {{Due to recent findings of a great number of cranial elements in the
   South American paleontological record, the possibility of using new
   techniques in the systematic of Spheniscidae is raised. To determine the
   diagnostic value of cranial and mandibular characters, multivariate
   analyses were performed using 11 quantitative variables, measured on 68
   adult skulls. Our analyses allowed the discrimination of all genera and
   most of the analyzed species from the set of considered variables.}},
