@article{ ISI:000208352700001,
Author = {Trejo, Ana and Figueroa R, Ricardo A. and Alvarado O, Sergio},
Title = {{Forest-specialist raptors of the temperate forests of southern South
   America: a review}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2006}},
Volume = {{14}},
Number = {{4}},
Pages = {{317-330}},
Month = {{DEC}},
Abstract = {{We review the published information on the raptor species present in the
   temperate forests of southern South America, focusing our efforts in the
   four forest-specialist species, that is, forest-depending species
   especially for nesting. We examined nine categories of ecological
   knowledge: distribution, habitat/habitat selection, movements,
   abundance, diet, breeding, behavior, parasites, and conservation status.
   Our objectives are: 1) generate a data base on raptors of the southern
   temperate forests; 2) detect gaps in the knowledge on raptors; and 3)
   establish research priorities on raptor ecology based on the amount of
   information referring to the above detailed categories.}},
