@article{ ISI:000258501200009,
Author = {Paranhos, Sandra Jammal and de Araujo, Carlos Barros and
   Marcondes-Machado, Luiz Octavio},
Title = {{Feeding behavior of the Yellow-chevroned Parakeet at the northeast of
   the State of Sao Paulo State, Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2007}},
Volume = {{15}},
Number = {{1}},
Pages = {{95-101}},
Month = {{MAR}},
Abstract = {{This research was carried out from March 1990 to December 1993. It was
   designed to obtain data on the feeding behavior of the Yellow-chevroned
   Parakeet (Brotogeris chiriri) at four distinct areas of northeast Sao
   Paulo State. The following aspects of its feeding biology have been
   addressed: the plant species used as food, the items (flower, fruit,
   pulp, seed) included in the diet, the methods used to obtain food, and
   the flock size variation. Brotogeris chiriri used 45 plant species as
   food resource. From these it used seeds from 27 species, fruit pulp from
   19 species and flowers from 16 species. There was a significant
   statistical variation on the methods used to gather food relative to the
   food item. The flock size of B. chiriri is highly variable (from one
   individual eating alone to about 45 individuals) on the same period of
   the year. The smallest average flock size was found during the breeding
   period of the species.}},
