@article{ ISI:000208352700004,
Author = {Azevedo, Marcos A. G. and Piacentini, Vitor de Queiroz and Ghizoni-,
   Jr., Ivo Rohling and Albuquerque, Jorge L. B. and Silva, Elsimar S. and
   Joenck, Cristian M. and de Mendonca-Lima, Andre and Zilio, Felipe},
Title = {{Biology of the Rufous-thighed Kite Harpagus diodon in the State of Santa
   Catarina, southern Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2006}},
Volume = {{14}},
Number = {{4}},
Pages = {{351-357}},
Month = {{DEC}},
Abstract = {{Biology of the Rufous-thighed Kite Harpagus diodon in the State of Santa
   Catarina, southern Brazil. The natural history of the Rufous-thighed
   kite is barely known, specially its reproduction, food and habitat
   requirements. We made opportunistic observations at several sites in the
   State of Santa Catarina between 1996 and 2006. These observations
   allowed us to gather data about the feeding behavior, food items,
   reproduction, seasonality and habitat use of the species. The kite is
   associated to woodlots. The species is primarily insectivorous, but it
   may prey upon vertebrates occasionally. We identified two distinct
   voices, which are described with a spectrographic analysis. The data
   reported here may contribute to the conservation of the species in Santa
   Catarina.}},
