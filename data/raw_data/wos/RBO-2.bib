@article{ ISI:000258501200015,
Author = {da Silva, Paulo Antonio},
Title = {{Seed predation by parakeets Brotogeris chiriri (Psittacidae) in Chorisia
   speciosa (Bombacaceae)}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2007}},
Volume = {{15}},
Number = {{1}},
Pages = {{127-129}},
Month = {{MAR}},
Abstract = {{Here I report the seed predation by Yellow-chevroned parakeet
   (Brotogeris chiriri) in the Floss-silk tree (Chorisia speciosa). The
   study was carried out in a reforestation in Ilha Solteira, west of the
   state of Sao Paulo, SE Brazil. In August and September 2003, two
   individuals of C. speciosa were monitored between 07.00 and 18.00 h,
   totaling 22 hours of direct observation. The parakeets destroyed 12.6\%
   of the seeds. The consumption of C. speciosa seeds by B. chiriri in
   reforestation suggests that seed predation by Psittacidae is an
   important phenomenon to understand the structure and dynamics of
   restored habitats.}},
