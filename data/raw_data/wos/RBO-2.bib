Title = {{Reproductive Behavior and characterization of breeding sites of the
   Guianan Cock-of-the-Rock (Rupicola rupicola) in Amazonas, Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2007}},
Volume = {{15}},
Number = {{1}},
Pages = {{81-84}},
Month = {{MAR}},
Abstract = {{We characterized the breeding sites and nesting behavior of the Guianan
   Cock-of-the-Rock (Rupicola rupicola) in two localities within Presidente
   Figueiredo, State of Amazonas, Brazil. Data were obtained from direct
   observations of nests between november/2002 and May/2003. The preferred
   nesting sites are cave, but dry, barren rock walls of more than 9 m
   height were also used. A total of 48 nests were monitored, 33 of them
   were actually used by breeding birds. Froth them 33 nestlings have
   hatched, eight of which were captured by illegal dealers and 25 fledged.}},
