@article{ ISI:000208352700019,
Author = {Roda, Sonia Aline},
Title = {{Diet of the Barn Owl (Tyto alba) in the Tapacura Ecological Station,
   Pernambuco, Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2006}},
Volume = {{14}},
Number = {{4}},
Pages = {{449-452}},
Month = {{DEC}},
Abstract = {{Diet of the Barn Owl (Tyto alba) in the Tapacura Ecological Station,
   Pernambuco, Brazil. Ninety two regurgitated pellets of Tyto alba were
   collected in the Tapacura Ecological Station. The bulk of the diet in
   terms of prey number was comprised by rodents (71.0\%), and secondarily
   by bats (17.2\%) and opossums (9.7\%). Birds and insects were of minor
   importance. In terms of estimated biomass consumption, rodents were also
   the staple of the diet (83.3\%). Rodents were more consumed in the
   beginning of the rainy season, corresponding to the period of
   reproduction and recruitment of these prey in the study area.}},
