@article{ ISI:000404877900006,
Author = {Nomi, Daisuke and Yuta, Teru and Koizumi, Itsuro},
Title = {{BREEDING BIOLOGY OF FOUR SYMPATRIC TITS IN NORTIIERN JAPAN}},
