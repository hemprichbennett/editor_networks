@article{ ISI:000419344300022,
Author = {Peers, Michael J. L. and Boutin, Stan},
Title = {{Northern Hawk Owl (Surnia ulula) Scavenges and Defends a Lynx (Lynx
   canadensis) Carcass in Winter}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2017}},
Volume = {{129}},
Number = {{4}},
Pages = {{875-878}},
Month = {{DEC}},
Abstract = {{Scavenging has been increasingly documented for difficult to observe
   species through advances in motion-triggered photography. Here, we
   document several instances of scavenging over multiple days by a
   Northern Hawk Owl (Surnia ulula) on a Canada lynx (Lynx canadensis)
   carcass. These observations are of particular interest because the owl
   visited the carcass 25 times, with the longest visit lasting >3 hrs. In
   addition, the owl routinely defended the carcass against Black-billed
   Magpies (Pica hudsonia). These observations suggest scavenging may
   represent a larger role in the diet of Northern Hawk Owls; further
   research is required in order to understand its importance for this and
   other raptor species. Received 29 September 2016. Accepted 24 March
   2017.}},
