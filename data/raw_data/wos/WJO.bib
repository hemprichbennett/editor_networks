@article{ ISI:000396646200022,
Author = {Powell, Luke L. and Jones, Kelsy L. and Carpenter, Jonathan H. and
   Tully, Jr., Thomas N.},
Title = {{Captive Hispaniolan Parrots (Amazona ventralis) can Discriminate Between
   Experimental Foods with Sodium Concentrations Found in Amazonian Mineral
   Licks}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2017}},
Volume = {{129}},
Number = {{1}},
Pages = {{181-185}},
Month = {{MAR}},
Abstract = {{Wild Amazonian parrots likely consume soil to supplement
   sodium-deficient diets. However, it remains uncertain whether parrots
   can detect sodium at the concentrations found in mineral licks. During
   3-8 day trials, we presented 20 captive Hispaniolan Amazon Parrots
   (Amazona ventralis) two foods with experimentally manipulated sodium
   concentrations. Parrots consumed more of the lower sodium food,
   confirming that they could distinguish between 891 ppm and 1,644 ppm
   available sodium-values well within the range of concentrations found in
   mineral licks in the wild. This study strongly suggests that wild
   parrots can use taste to detect salty soil horizons in the Amazon.}},
