@article{ ISI:000410546000019,
Author = {Rimmer, Christopher C. and Johnson, Patrick L. and Lloyd, John D.},
Title = {{Home Range Size and Nocturnal Roost Locations of Western Chat-Tanagers
   (Calyptophilus tertius)}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2017}},
Volume = {{129}},
Number = {{3}},
Pages = {{611-615}},
Month = {{SEP}},
Abstract = {{Western Chat-Tanagers (Calyptophilus tertius) are a rare and globally
   vulnerable songbird endemic to the island of Hispaniola. We conducted a
   radio-telemetry study of non-breeding Western Chat-Tanagers at two
   adjacent cloud forest sites in Sierra de Bahoruco in southwestern
   Dominican Republic during March and April of 2010. Male home ranges
   averaged 2.18 +/- 0.25 ha in size (n = 4) and were significantly larger
   than those of females (0.88 +/- 0.04 ha; n = 2). However, core use areas
   did not differ significantly between the sexes. We assumed that males
   and females with overlapping home ranges weremated pairs. At night, two
   putative pairs roosted an average distance apart of 15.8 +/- 3.5m (n = 7
   observations). Received}},
