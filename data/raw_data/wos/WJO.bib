Title = {{Potential distribution and climatic niche of seven species of
   Campylorhynchus (Aves, Troglodytidae): conservation implications for
   C-yucatanicus}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2018}},
Volume = {{130}},
Number = {{1}},
