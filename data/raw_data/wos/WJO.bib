@article{ ISI:000351460800020,
Author = {Gotanda, Kiyoko M. and Sharpe, Diana M. T. and De Leon, Luis F.},
Title = {{Galapagos Mockingbird (Mimus parvulus) Preys on an Invasive Mammal}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2015}},
Volume = {{127}},
Number = {{1}},
Pages = {{138-141}},
Month = {{MAR}},
Abstract = {{Galapagos Mockingbirds (Mimus parvulus) are opportunistic feeders that
   have been observed engaging in a variety of unusual predatory behaviors.
   Here, we report on a specific behavior that we observed: a Galapagos
   Mockingbird preying on an invasive mammal by repeatedly hitting it on
   the ground. We discuss the reasons that the mockingbird might have
   engaged in this behavior and the potential implications this could have
   for native biodiversity and conservation on the Galapagos Islands.}},
