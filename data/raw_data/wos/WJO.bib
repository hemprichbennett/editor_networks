@article{ ISI:000375242000016,
Author = {Titmus, Andrew J. and Arcilla, Nicole and Lepczyk, Christopher A.},
Title = {{Assessment of the Birds of Swains Island, American Samoa}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
