@article{ ISI:000393262800022,
Author = {Engelbrecht, Derek},
Title = {{A Case of Interspecific Nest Use in Larks (Alaudidae)}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2016}},
Volume = {{128}},
Number = {{4}},
Pages = {{894-898}},
Month = {{DEC}},
Abstract = {{Nest reuse is a common phenomenon in many passerine and non-passerine
   birds species. Although interspecific nest reuse has been reported in
   many African species, it is rare in passerines that nest in open,
   cup-shaped nests. Here, I report on interspecific nest reuse in larks
   (Alaudidae), specifically of a pair of Chestnut-backed Sparrow-larks
   Ereniopterix leucotis using a nest previously occupied by Pink-billed
   Larks Spizocorys conirostris. It is possible that the Chestnut-backed
   Sparrow-larks selected a nest site in an area where predation seemed low
   as there were other successful nests or nests with nestlings in the near
   vicinity. The interspecific nest reuse reported here suggests
   opportunistic occupation of a previously used nest while at the same
   time saving the pair some energy during a period of peak activity and
   high energetic demands.}},
