@article{ ISI:000419344300026,
Author = {Zenzal, Jr., Theodore J. and Lain, Emily J. and Sellers, Jr., J. Michael},
Title = {{An Indigo Bunting (Passerina cyanea) Transporting Snails During Spring
   Migration}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2017}},
Volume = {{129}},
Number = {{4}},
Pages = {{898-902}},
Month = {{DEC}},
Abstract = {{Organisms with limited motility may use animal transport as a mechanism
   for dispersal. Migratory birds can provide a vehicle to move small
   organisms great distances, which may allow them access to areas that are
   otherwise inaccessible. During normal mist netting operations at a
   spring migration banding station along the northern Gulf of Mexico coast
   in Louisiana, USA, we encountered an Indigo Bunting (Passerina cyanea)
   with numerous snails, possibly Galba cubensis, underneath its breast
   feathers. While encounters of songbirds carrying snails appear rare,
   long-distance migrating songbirds represent a possible mechanism to
   transport small snails great distances to expand ranges, colonize new
   areas, or maintain genetic continuity. Received 12 October 2016.
   Accepted 28 February 2017.}},
