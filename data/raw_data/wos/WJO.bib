@article{ ISI:000384566900005,
Author = {Battle, Kathryn E. and Foltz, Sarah L. and Moore, Ignacio T.},
Title = {{PREDICTORS OF FLIGHT BEHAVIOR IN RURAL AND URBAN SONGBIRDS}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
