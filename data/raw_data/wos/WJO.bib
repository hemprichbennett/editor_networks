@article{ ISI:000404877900008,
Author = {Cabrera, Maria B. and Montalti, Diego and Segura, Luciano N.},
Title = {{BREEDING PIIENOLOGY AND NEW HOST LIST OF TIIE BLACK-HEADED DUCK
   (HETERONETTA ATRICAPILLA) IN ARGENTINA}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2017}},
Volume = {{129}},
Number = {{2}},
Pages = {{311-316}},
Month = {{JUN}},
Abstract = {{The Black-headed Duck (Hetenmetta at capilla) is a unique interspecific
   brood parasite among birds where the host species provides parental care
   lilt incubation only. Despite this unique system, there are few studies
   on this species biology and the list of hosts remains incomplete. We
   present information about breeding phenology of the Black headed Duck
   and an updated list of hosts obtained front a comprehensive review of
   egg collections deposited in the most important natural science museums
   of Argentina. We include five new hosts to the list: Great Grebe
   (Poiliceps major), Maguari Stork (Ciconia maguaril, White-cheeked
   Pintail (Anus bahamensis), South American Stilt (llimantopus mexicanus),
   and Long-winged Harrier (Circus buffoni). We found that solitary nesting
   host species were more likely to be parasitized earlier during the
   breeding season than colonial nesting species. The Black-headed Duck has
   an extended breeding season in central Argentina (Aug-Feb) as it is able
   to use a variety of hosts that breed in different times during the
   breeding season. With this contribution, the new list rises to 23
   documented hosts of the Black-headed Duck belonging to eight different
   waterfowl orders.}},
