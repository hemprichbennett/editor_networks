@article{ ISI:000396646200021,
Author = {Skrade, Paul D. B. and Dinsmore, Stephen J. and Vleck, Carol M.},
Title = {{Testosterone and Prolactin Levels in Incubating Mountain Plovers
   (Charadrius montanus)}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2017}},
Volume = {{129}},
Number = {{1}},
Pages = {{176-181}},
Month = {{MAR}},
Abstract = {{The hormones prolactin (PRL) and testosterone (T) are linked to breeding
   behaviors such as territory defense, incubation, and brood-rearing in
   birds. The Mountain Plover (Charadrius montanus) is a shorebird with an
   uncommon parental care system in which males and females tend separate
   nests. We collected blood from incubating male (n = 38) and female (n =
   33) Mountain Plovers in Montana to determine how circulating plasma PRL
   and T varied by sex, day of incubation, and across the nesting season.
   PRL levels were similar in males and females. There was no relationship
   between day of incubation or Julian day and circulating PRL for either
   sex. T concentrations were higher in males than in females and tended to
   decrease across the incubation season for both sexes.}},
