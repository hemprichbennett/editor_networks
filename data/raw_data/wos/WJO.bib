@article{ ISI:000404877900004,
Author = {Zelt, Jessica and Deleon, Robert L. and Arab, Ali and Laurent, Kevin and
   Snodgrass, Joel W.},
Title = {{LONG-TERM TRENDS IN AVIAN MIGRATION TIMING FOR THE STATE OF NEW YORK}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
