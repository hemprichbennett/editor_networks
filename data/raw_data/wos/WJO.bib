@article{ ISI:000396646200017,
Author = {da Silva, Pericles Rocha and Marcos da Silva, Adriano and de Melol,
   Celine},
Title = {{Male Helmeted Manakins (Antilophia galeata) with More Colorful Crowns
   Have Better Body Conditions}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2017}},
Volume = {{129}},
Number = {{1}},
Pages = {{158-163}},
Month = {{MAR}},
Abstract = {{Bird plumage coloration is an important signal used in communication and
   may indicate individual fitness. We tested the hypothesis that
   coloration is related to body condition in male Helmeted Manakins
   (Antilophia galeata). Fourteen adult males were captured in a forest
   fragment in Brazilian Cerrado from November 2007 to June 2008. Crown
   feather coloration (brightness, hue, and saturation) was evaluated
   through spectrophotometry, and body condition was measured via the
   Relative Mass Index (RMI). Coloration characteristics were positively
   correlated with body condition, indicating that individuals with higher
   body conditions have brighter and more saturated feathers, and
   therefore, can use these traits to signal their good body condition.}},
