@article{ ISI:000357726800007,
Author = {Collins, Charles T.},
Title = {{FOOD HABITS AND RESOURCE PARTITIONING IN A GUILD OF NEOTROPICAL SWIFTS}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
