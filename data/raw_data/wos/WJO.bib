@article{ ISI:000384566900026,
Author = {Proett, Matt and Roberts, Shane B. and Messmer, Terry A.},
Title = {{Double Brooding Observed in a Columbian Sharp-tailed Grouse (Tympanachus
   phasianellus columbianus) in Idaho}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2016}},
Volume = {{128}},
Number = {{3}},
Pages = {{672-675}},
Month = {{SEP}},
Abstract = {{Double brooding is rare in Galliformes and has only been reported twice
   in grouse species (subfamily Tetraoninae). To our knowledge, a grouse
   species producing a second brood within a single breeding season that
   survived > 12 days has not been documented in the published literature.
   We report the case of a Columbian Sharp-tailed Grouse (Tympanachus
   phasianellus columbianus) in eastern Idaho (43 degrees 34' 28.3434'' N,
   111 degrees 360' 47.988'' W) that successfully re-nested after initial
   brood loss and had two chicks survive to 42 days post-hatch during the
   2015 breeding season. Early brood loss, in conjunction with favorable
   environmental conditions that contributed to early nest initiation and
   increased food availability, may have contributed to this female's
   ability to produce a successful second brood.}},
