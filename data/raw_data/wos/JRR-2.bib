@article{ ISI:000264293300002,
Author = {Ellis, David H.},
Title = {{ON THE UNIQUENESS OF COLOR PATTERNS IN RAPTOR FEATHERS}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2009}},
Volume = {{43}},
Number = {{1}},
Pages = {{11-26}},
Month = {{MAR}},
Abstract = {{For this study, I compared sequentially molted feathers for a few
   captive raptors from year to year and symmetrically matched feathers
   (left/right pairs) for many raptors to see if color patterns of
   sequential feather pairs were identical or if symmetrical pairs were
   mirror-image identical. Feather pairs were found to be identical only
   when without color pattern (e.g., the all-white rectrices of Bald Eagles
   {[}Haliaeetus leucocephalus]). Complex patterns were not closely
   matched, but some simple patterns were sometimes closely matched,
   although not identical. Previous claims that complex color patterns in
   feather pairs are fingerprint-identical (and therefore that molted
   feathers from wild raptors can be used to identify breeding adults from
   year to year with certainty) were found to untrue: each feather is
   unique. Although it is unwise to he certain of bird of origin using
   normal feathers, abnormal feathers can often be so used.}},
