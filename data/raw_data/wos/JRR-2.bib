@article{ ISI:000237905000002,
Author = {Linkhart, BD and Reynolds, RT},
Title = {{Lifetime reproduction of Flammulated Owls in Colorado}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2006}},
Volume = {{40}},
Number = {{1}},
Pages = {{29-37}},
Month = {{MAR}},
Abstract = {{We investigated lifetime reproductive,e success (LRS) of 22 male and 46
   female adult Flammulated Owls (Otus flammeolus) in Colorado from
   1981-2003. Sexual differences in LRS were at least partially
   attributable to incomplete breeding histories of females, due to females
   having a higher estimated emigration rate from the study area than
   males. The mean number of lifetime breeding attempts for males was 3.4
   +/- 0.6 (SE range = 1-12), and for females it was 1.9 +/- 0.3 (range =
   1-8). Adjusted emigration beyond the study area, the estimated mean
   number of lifetime breeding attempts was 3.6 for mates and 2.5 for
   fernales. The mean number Of lifetime successful breeding attempts
   (fledging at least one owlet) was similar to lifetime breeding attempts
   for each Sex, reflecting the fact that 84\% (85 of 101) of all breeding
   attempts were Successful. Breeding lifespan was correlated with lifetime
   reproduction for both sexes. Males tended nests that produced 6.9 +/-
   1.2 fledglings over 4.3 +/- 0.8 yr, while fernales produced 4.0 +/- 0.6
   fledglings over 2.0 +/- 0.3 yr; adjusted for emigration beyond the study
   area, inales tended nests that Produced 7.2 fledglings over 4.5 yr,
   while females produced 5.2 fledglings over 2.6 yr. Relatively few
   individuals of each sex produced most of the offspring, as 18\% of
   females and 24\% of 1 males produced 50\% of total owlets. Our finding
   that relatively few adults accounted for most. offspring appears to be
   associated with habitat quality in territories. Flammulated Owls tend to
   have it life-history strategy similar to larger raptors by having it
   relatively, low annual reproductive rate and a relatively long fifespan.}},
