@article{ ISI:000264293300003,
Author = {Glass, K. Andrew and Watts, Bryan D.},
Title = {{OSPREY DIET COMPOSITION AND QUALITY IN HIGH- AND LOW-SALINITY AREAS OF
   LOWER CHESAPEAKE BAY}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
