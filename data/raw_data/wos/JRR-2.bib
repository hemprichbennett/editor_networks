@article{ ISI:000233403500018,
Author = {DeStefano, S},
Title = {{A review of the status and distribution of Northern Goshawks in New
   England}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{3}},
Pages = {{342-350}},
Month = {{SEP}},
