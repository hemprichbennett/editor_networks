@article{ ISI:000220639200003,
Author = {Leyhe, JE and Ritchison, G},
Title = {{Perch sites and hunting behavior of Red-tailed Hawks (Buteo jamaicensis)}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{1}},
Pages = {{19-25}},
Month = {{MAR}},
Abstract = {{For sit-and-wait predators like Red-tailed Hawks (Buteo jamaicensis),
   perch sites are important components of hunting ranges. From October
   1999-July 2000, perches (N = 270) used by Red-tailed Hawks in central
   Kentucky were located and characterized. Perches used by Red-tailed
   Hawks were relatively high ((x) over bar = 12.3 m) in trees or oil
   poles. Such perches provide a large, relatively unobstructed field of
   view and likely increase the chances of detecting prey. Red-tailed Hawks
   spent more time on perches before attacking than before giving tip,
   perhaps waiting to attack prey after locating it to increase the chance
   of a successful attack. Characteristics of vegetation around perches
   used by hunting Red-tailed Hawks differed front those of areas around
   available (but apparently unused) sites during both the breeding and
   nonbreeding seasons. During the breeding season, areas used by
   Red-tailed Hawks had less forb cover, lower vegetation density, and
   shorter vegetation than available areas. During the nonbreeding season,
   areas used by Red-tailed Hawks were characterized by less bare ground,
   more grass cover. less shrub cover, fewer small trees, decreased
   vegetation density, and shorter vegetation than available areas. In
   general, Used areas provided less cover, which may increase prey
   vulnerability.}},
