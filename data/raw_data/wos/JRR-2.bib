@article{ ISI:000270099200004,
Author = {Arshad, Muhammad and Pedall, Inken and Gonzalez, Javier and Wink,
   Michael and Hatzofe, Ohad and Khan, Aleem Ahmed and Osborne, Tim},
Title = {{GENETIC VARIATION OF FOUR GYPS SPECIES (GYPS BENGALENSIS, G. AFRICANUS,
   G. INDICUS AND G. FULVUS) BASED ON MICROSATELLITE ANALYSIS}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
