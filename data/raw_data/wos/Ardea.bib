@article{ ISI:000272403600024,
Author = {Sunde, Peter and Odderskaer, Peter and Storgaard, Kurt},
Title = {{Flight distances of incubating Common Buzzards Buteo buteo are
   independent of human disturbance}},
Journal = {{ARDEA}},
Year = {{2009}},
Volume = {{97}},
Number = {{3}},
Pages = {{369-372}},
Month = {{FAL}},
Abstract = {{In a Danish landscape with relatively high (134/km(2)) but variable
   human density, 86\% of 213 incubating Common Buzzards Buteo buteo
   flushed from the nest at distances of 0-200 m in response to a person
   approaching on foot. Flight distances were not conditional on nest site
   parameters despite considerable variation in human disturbance
   variables, lending no support for the hypothesis that Common Buzzards
   are more tolerant of humans in disturbed locations.}},
