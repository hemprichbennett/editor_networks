@article{ ISI:000340807500003,
Author = {van Rhijn, Johan and Jukema, Joop and Piersma, Theunis},
Title = {{Diversity of nuptial plumages in male Ruffs Philomachus pugnax}},
Journal = {{ARDEA}},
