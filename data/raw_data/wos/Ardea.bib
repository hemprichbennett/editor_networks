@article{ ISI:000290827200013,
Author = {Onrubia, Alejandro and Munoz, Antonio-Roman and Arroyo, Gonzalo M. and
   Ramirez, Juan and de la Cruz, Andres and Barrios, Luis and Meyburg,
   Bernd-U. and Meyburg, Christiane and Langgemach, Torsten},
Title = {{Autumn migration of Lesser Spotted Eagle Aquila pomarina in the Strait
   of Gibraltar: accidental or regular?}},
Journal = {{ARDEA}},
Year = {{2011}},
Volume = {{99}},
Number = {{1}},
Pages = {{113-116}},
Month = {{SPR}},
Abstract = {{The Lesser Spotted Eagle (LSE) migrates to its winter quarters via the
   eastern Mediterranean corridor. During 1998 to 2009, 47 LSEs have been
   registered in the Strait of Gibraltar during a monitoring program of
   bird migration. All observations were made between 6 August to 12
   October (mean 16 September, median 12 September). Of thirteen
   age-identified LSEs, ten birds were non-juveniles. Six out of 86 LSEs
   fitted with a satellite transmitter at the western limit of its
   distribution followed the western route. Three of those birds reached
   Africa via the Strait of Gibraltar. The presence of LSE in our study
   area, albeit in small numbers, indicates the existence of a minor
   migration route across the Iberian Peninsula.}},
