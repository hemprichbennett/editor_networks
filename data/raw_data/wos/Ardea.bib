@article{ ISI:000255981800017,
Author = {Poisbleau, Maud and Desmonts, Diane and Fritz, Herve},
Title = {{Dominance relationships in Dark-bellied Brent Geese Branta bernicla
   bernicla at spring staging areas}},
Journal = {{ARDEA}},
Year = {{2008}},
Volume = {{96}},
Number = {{1}},
Pages = {{135-139}},
Month = {{SPR}},
Abstract = {{Spring staging is an important period influencing reproductive success
   for migratory birds. During this period, female geese must accumulate
   reserves for subsequent migration, egg laying and incubation. They may
   increase feeding opportunities by being guarded by dominant males
   protecting a large feeding area and providing feeding time, and/or
   having themselves a high dominance status. Using paired adults of
   Dark-bellied Brent Geese Branta bernicla, we tested if males had higher
   dominance scores than females, if larger individuals had higher
   dominance scores than smaller ones and if dominance ranks of paired
   partners were correlated. Our results supported the first and third
   predictions while, once controlled by sex, body size was not correlated
   with adult dominance status.}},
Publisher = {{NEDERLANDSE ORNITHOLOGISCHE UNIE}},
