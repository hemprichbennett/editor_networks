@article{ ISI:000077906200009,
Author = {Mayhew, P and Houston, D},
Title = {{Effects of winter and early spring grazing by Wigeon Anas penelope on
   their food supply}},
Journal = {{IBIS}},
Year = {{1999}},
Volume = {{141}},
Number = {{1}},
Pages = {{80-84}},
Month = {{JAN}},
Abstract = {{Wigeon Anas penelope selectively grazed small patches of grassland near
   water. Their grazing activity affected the vegetation on which they fed,
   resulting in a 52\% increase in leaf production over the winter and, at
   the end of winter, 4.75\% higher protein levels compared with plants
   which were ungrazed. These responses of the plants were caused by
   defoliation, which stimulated growth, and not a fertiliser effect from
   the birds' droppings. We suggest that repeated feeding on the same areas
   is a deliberate strategy used by Wigeon which causes an improvement in
   their dietary quality in late winter and early spring.}},
