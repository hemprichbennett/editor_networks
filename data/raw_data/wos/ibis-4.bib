@article{ ISI:000072941800001,
Author = {Horak, P and Lebreton, JD},
Title = {{Survival of adult Great Tits Parus major in relation to sex and habitat;
   a comparison of urban and rural populations}},
Journal = {{IBIS}},
Year = {{1998}},
Volume = {{140}},
Number = {{2}},
Pages = {{205-209}},
Month = {{APR}},
Abstract = {{Capture-recapture data were obtained for two populations of Great Tits
   Parus major in southeast Estonia, one rural, the other urban. These data
   were used to estimate and compare survival probabilities between sexes
   and populations using capture-recapture statistical models. As is
   generally assumed, urban birds survived better than rural ones. Females
   survived better than males, and the survival probabilities varied over
   time, The three sources of variation considered (sex, habitat and time)
   had additive effects on survival probability on a legit scale. The
   estimated annual survival probabilities varied from 0.19 to 0.70. The
   averages over time ranged from 0.26 to 0.47 and were generally lower
   than published estimates from other populations. The proportion of
   yearling breeders was higher among females than among males, contrary to
   what could have been deduced from survival patterns, and is possibly a
   consequence of complex recruitment mechanisms.}},
