@article{ ISI:000077906200016,
Author = {Ma, ZJ and Wang, ZJ and Tang, HX},
Title = {{Habitat use and selection by Red-crowned Crane Grus japonensis in winter
   in Yancheng Biosphere Reserve, China}},
Journal = {{IBIS}},
Year = {{1999}},
Volume = {{141}},
Number = {{1}},
Pages = {{135-139}},
Month = {{JAN}},
Abstract = {{We report the distribution and habitat selection by the Red-crowned
   Crane Grus japonensis in winter in the Yancheng Biosphere Reserve,
   China. Including original wetlands and artificial habitats, six types of
   habitat are used by the species: saltworks (salinas), fish ponds, reed
   beds, Wormwood Artemisia halodendron beaches, tidal grasslands and wheat
   fields. We compared the habitat availability with habitat use in each
   type. In winter, Red-crowned Crane preferentially used tidal grasslands
   and fish ponds. Saltworks, Wormwood beaches and wheat fields were
   avoided. The distribution of Red-crowned Crane in different types of
   habitat changed with the distribution of freshwater and with human
   activities.}},
