@article{ ISI:000076307500003,
Author = {Dhondt, AA and Kempenaers, B and Clobert, J},
Title = {{Sparrowhawk Accipiter nisus predation and Blue Tit Parus caeruleus adult
   annual survival rate}},
Journal = {{IBIS}},
Year = {{1998}},
Volume = {{140}},
Number = {{4}},
Pages = {{580-584}},
Month = {{OCT}},
Abstract = {{The adult annual survival rate of breeding Blue Tits Parus caeruleus
   decreased from 0.485 to 0.376 after a pair of Sparrowhawks Accipiter
   nisus started to breed in the study plot in 1990, whereas in two
   neighbouring plots, survival rates did not change. This decrease was
   likely caused by the Sparrowhawk predation mainly in spring, Although
   the size of the Blue Tit breeding population did not decrease, there
   remained fewer nonbreeding floaters, and as a result, replacement
   polygyny became more common.}},
