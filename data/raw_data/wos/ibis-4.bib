@article{ ISI:000175477900002,
Author = {Waldenstrom, J and Ottosson, U},
Title = {{Moult strategies in the common whitethroat Sylvia c. communis in
   northern Nigeria}},
Journal = {{IBIS}},
Year = {{2002}},
Volume = {{144}},
Number = {{1}},
Pages = {{E11-E18}},
Month = {{JAN}},
Abstract = {{Moult strategies in the Common Whitethroat Sylvia c. communis trapped in
   northern Nigeria in April 1999 are analysed. Differences in the extent
   of moult between age and sex classes are presented. The secondary moult
   of adult birds is considered to belong to a new pre-breeding moult or an
   arrested moult, beginning at the normal starting positions on the wing,
   and not as a continuation of a suspended post-breeding moult. Eccentric
   moult of primaries was found in nearly 19\% of the yearling birds, a
   pattern that is unusual for Western Palearctic passerines. Some
   individuals showed resemblance to the split moult pattern described for
   the Barred Warbler S. nisoria.}},
