@article{ ISI:000084564900005,
Author = {Ueta, M and Sato, F and Nakagawa, H and Mita, N},
Title = {{Migration routes and differences of migration schedule between adult and
   young Steller's Sea Eagles Haliaeetus pelagicus}},
Journal = {{IBIS}},
Year = {{2000}},
Volume = {{142}},
Number = {{1}},
Pages = {{35-39}},
Month = {{JAN}},
Abstract = {{The migration routes of Steller's Sea Eagles Haliaeetus pelagicus and
   differences in migration schedule between adult and young eagles were
   studied by satellite tracking. We tracked nine Steller's Sea Eagles
   successfully in two years. Eight of the eagles migrated north from
   Hokkaido to northern Sakhalin and then to nearby continental seaboards.
   The other eagle moved to Onecotan Island near the Kamchatka Peninsula in
   the Kuril Island chain. We obtained detailed tracking information from
   four adult and four young eagles. The period of migration was
   significantly shorter in adult eagles than in young ones, and adult
   eagles reached their summering sites earlier than young ones. The sea
   was still closed by ice in their summering sites when adult eagles
   arrived. Since Steller's Sea Eagles feed mainly on fish, the ice-covered
   sea would not seem to be a suitable feeding habitat for them. Adult
   eagles presumably return to their summering sites earlier than the young
   birds in order to breed, despite adverse feeding conditions.}},
