@article{ ISI:000084564900015,
Author = {Sasvari, L and Hegyi, Z},
Title = {{Sex-related local recruitment in colonial and solitary breeding European
   Tree Sparrows Passer montanus L.}},
Journal = {{IBIS}},
Year = {{2000}},
Volume = {{142}},
Number = {{1}},
Pages = {{119-122}},
Month = {{JAN}},
Abstract = {{European Tree Sparrows Passer montanus L. were studied with regard to
   their sex and the colonial or solitary nest spacing in which they were
   born and bred for the first time. Nestboxes were arranged in five plots
   in a suburban area of Budapest, Hungary. Each plot contained 25 boxes at
   50-m intervals ('solitary' breeding) and 25 boxes at 3-5-m intervals
   ('colonial' breeding). Brood number and hatching order of recruiting
   young, their mate choice and reproductive performance in their first
   breeding year were also recorded. More males than females recruited
   locally. The sexes differed with regard to their origin in three
   respects: (1) most female recruits were born in solitary nests, and most
   male recruits in colonial nests; (2) most recruits of both sexes were
   born into first broods but a higher proportion of recruits from second
   and third broods were females; (3) male recruits were most likely to
   have hatched first while female recruits were more likely to have
   hatched later. Recruits produced more fledglings if they were mated to
   birds which had already bred in a previous year.}},
