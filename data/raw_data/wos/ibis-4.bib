@article{ ISI:000071523200012,
Author = {Goss-Custard, JD and Ross, J and McGrorty, S and Durell, SEAL and
   Caldow, RWG and West, AD},
Title = {{Locally stable wintering numbers in the oystercatcher Haematopus
   ostralegus where carrying capacity has not been reached}},
Journal = {{IBIS}},
Year = {{1998}},
Volume = {{140}},
Number = {{1}},
Pages = {{104-112}},
Month = {{JAN}},
Abstract = {{Since 1976, the numbers of Oystercatchers Haematopus ostralegus
   wintering on the Exe estuary have fluctuated independently of the
   substantial increase over the same period in the numbers wintering in
   Britain as a whole, While this might imply that the carrying capacity of
   the Exe has been reached, closer examination suggests not, Over a sample
   of the years studied, Oystercatcher numbers on the main Mussel Mytilus
   edulis feeding areas increased even though the mussel bed quality
   remained unchanged, food abundance decreased and disturbance on some
   important beds increased, There was some redistribution of birds between
   mussel beds, apparently linked to the increased bird numbers and to a
   relative improvement in the food supply on two preferred mussel beds,
   The increase in Oystercatcher numbers on the main feeding areas over a
   period in which the feeding conditions did not improve suggests that
   carrying capacity was not reached on the mussel beds themselves and,
   therefore, on the estuary as a whole.}},
