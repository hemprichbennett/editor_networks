@article{ ISI:000398187700010,
Author = {Romero, Jose Maria and Redondo, Tomas},
Title = {{Kind to kin: weak interference competition among white stork Ciconia
   ciconia broodmates}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{2017}},
Volume = {{48}},
Number = {{3}},
Pages = {{417-430}},
Month = {{MAR}},
Abstract = {{Altricial nestlings in structured families show a diverse array of
   behavioural mechanisms to compete for food, ranging from signalling
   scrambles to aggressive interference. Rates of filial infanticide are
   moderately high in white storks. It has been hypothesized that this
   unusual behaviour is an adaptive parental response to the absence of
   efficient mechanisms of brood reduction (aggression or direct physical
   interference) by nestlings. To test this latter assumption, we analyzed
   video recordings of 41 complete feeding episodes at 32 broods during the
   first half of the nestling period, when nestlings complete 90\% of
   growth and chick mortality and size asymmetries are highest. Parents
   delivered food to all nestlings simultaneously by regurgitating on the
   nest floor. No direct (bill to bill) feeding was recorded. Senior
   nestlings were never observed to limit their junior nestlings from
   eating food, either by aggression or physical interference. Experimental
   feeding tests revealed that heavier nestlings handled prey items more
   efficiently and ate food at a higher speed. The high degree of tolerance
   shown by senior nestlings is unusual among birds with similar ecological
   and phylogenetic affinities, such as herons. Tolerance by seniors cannot
   be easily explained by absence of parental favouritism or proximate
   factors known to affect the occurrence of sibling aggression in other
   species (rate of food transfer, brood size, hatching asynchrony or
   length of nestling period).}},
