@article{ ISI:000351356000008,
Author = {Davis, Amy J. and Phillips, Michael L. and Doherty, Jr., Paul F.},
Title = {{Survival of Gunnison sage-grouse Centrocercus minimus in Colorado, USA}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
