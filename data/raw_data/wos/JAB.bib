@article{ ISI:000339485600003,
Author = {DeGregorio, Brett A. and Chiavacci, Scott J. and Weatherhead, Patrick J.
   and Willson, John D. and Benson, Thomas J. and Sperry, Jinelle H.},
Title = {{Snake predation on North American bird nests: culprits, patterns and
   future directions}},
