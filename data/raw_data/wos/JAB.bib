@article{ ISI:000386128000012,
Author = {Grinde, Alexis R. and Niemi, Gerald J.},
Title = {{A synthesis of species interactions, metacommunities, and the
   conservation of avian diversity in hemiboreal and boreal forests}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
