@article{ ISI:000341706400002,
Author = {Moreno-Rueda, Gregorio and Campos, Francisco and Gutierrez-Corchero,
   Francisco and Hernandez, M. -Angeles},
Title = {{Costs of rearing and sex-ratio variation in southern grey shrike Lanius
