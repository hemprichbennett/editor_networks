@article{ ISI:000329866900002,
Author = {Lehikoinen, Aleksi and Green, Martin and Husby, Magne and Kalas, John
   Atle and Lindstroem, Ake},
Title = {{Common montane birds are declining in northern Europe}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
