@article{ ISI:000373014800012,
Author = {Beziers, Paul and Roulin, Alexandre},
Title = {{Double brooding and offspring desertion in the barn owl Tyto alba}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
