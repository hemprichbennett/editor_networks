@article{ ISI:000268022300001,
Author = {Murn, Campbell and Hunt, Samuel},
Title = {{An assessment of two methods used to release red kites (Milvus milvus)}},
Journal = {{AVIAN BIOLOGY RESEARCH}},
Year = {{2008}},
Volume = {{1}},
Number = {{2}},
Pages = {{53-57}},
Abstract = {{Between 2003 and 2005, we released 12 red kites (Milvus milvus) to the
   wild in Hampshire, England. Four kites were captive-bred and released as
   fledglings in artificial nests ('hacking'). The remaining birds were
   mature and released from a large aviary. Interaction with electricity
   power-lines killed two of the captive-bred birds three weeks
   post-release and a third captive-bred kite died as a result of head
   injuries six months post-release. One mature kite died 10 days
   post-release. We suggest that the different release methods of the two
   groups amplified the behavioural variation between individuals and
   exposed them to different risk factors. We concluded that releasing
   mature flight-fit kites from aviaries is likely to be a superior method
   to hacking pre-fledged kites in artificial nests. The flight skills of
   the mature kites, developed prior to release, enabled them to avoid
   potentially lethal interactions with power-lines and aggressive
   inter-specific encounters. Modifying or adapting release methods to
   incorporate behavioural variation between individuals within a release
   population should be a consideration for reintroduction practitioners,
   particularly where release numbers are small.}},
Publisher = {{SCIENCE REVIEWS 2000 LTD}},
Address = {{PO BOX 314, ST ALBANS AL1 4ZG, HERTS, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Murn, C (Reprint Author), Hawk Conservancy Trust, Andover SP11 8DY, Hants, England.
   Murn, Campbell; Hunt, Samuel, Hawk Conservancy Trust, Andover SP11 8DY, Hants, England.}},
DOI = {{10.3184/175815508X360452}},
ISSN = {{1758-1559}},
Keywords = {{red kite; Milvus; hacking; release; mortality; behavioural variation}},
Keywords-Plus = {{REINTRODUCTION PROGRAMS; TRANSLOCATION; POPULATION; SUCCESS; FALCONS;
