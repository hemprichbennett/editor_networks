@article{ ISI:000084900400014,
Author = {Rangen, SA and Clark, RG and Hobson, KA},
Title = {{Visual and olfactory attributes of artificial nests}},
Journal = {{AUK}},
Year = {{2000}},
Volume = {{117}},
