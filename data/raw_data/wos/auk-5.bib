@article{ ISI:000084900400013,
Author = {Hobson, KA and Brua, RB and Hohman, WL and Wassenaar, LI},
Title = {{Low frequency of ``double molt{''} of remiges in Ruddy Ducks revealed by
   stable isotopes: Implications for tracking migratory waterfowl}},
Journal = {{AUK}},
Year = {{2000}},
Volume = {{117}},
Number = {{1}},
