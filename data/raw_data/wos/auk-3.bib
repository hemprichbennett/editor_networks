@article{ ISI:000224756500004,
Author = {Whitney, BM and Oren, DC and Brumfield, RT},
Title = {{A new species of Thamnophilus antshrike (Aves : Thamnophilidae) from the
   Serra do Divisor, Acre, Brazil}},
Journal = {{AUK}},
Year = {{2004}},
Volume = {{121}},
Number = {{4}},
Pages = {{1031-1039}},
Month = {{OCT}},
Abstract = {{We describe a new species of Thamnophilus antshrike-T. divisorius (Acre
   Antshrike) -from the Serra do Divisor of Acre, Brazil. The new species
   is known from a single small ridge (though we expect that it is present
   on other, unexplored ridges in the Acre Arch uplands), and is common in
   the interior of shrubby woodlands on poor soil physiognomically similar
   to white-sand habitats in the Guianas and Amazonia. Thaninophilus
   divisorius is sister to T. insignis (Streak-backed Antshrike), a narrow
   endemic of pantepui, and forms a well-supported clade with that species
   and T. amazonicus (Amazonian Antshrike). Thamnophilus divisorius occurs
   in a remote region within a national park in Brazil; despite its limited
   range, we are optimistic about its future survival. Received 16 May
   2003, accepted 24 June 2004.}},
