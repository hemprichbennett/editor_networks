@article{ ISI:000229711300016,
Author = {Withey, JC and Marzluff, JM},
Title = {{Dispersal by juvenile American Crows (Corvus brachyrhynchos) influences
   population dynamics across a gradient of urbanization}},
Journal = {{AUK}},
Year = {{2005}},
Volume = {{122}},
