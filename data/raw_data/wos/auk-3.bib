@article{ ISI:000229005600022,
Author = {Rice, NH},
Title = {{Phylogenetic relationships of antpitta genera (Passeriformes :
   Formicariidae)}},
Journal = {{AUK}},
Year = {{2005}},
Volume = {{122}},
Number = {{2}},
Pages = {{673-683}},
Month = {{APR}},
Abstract = {{Phylogenetic relationships among the antpitta genera were studied using
   DNA sequence data from the mitochondrial genome. The clade representing
   the traditional ``antpitta{''} genera (Grallaria, Grallaricula,
   Hylopezus, Myrmothera, and Pittasoma) was found to be paraphyletic,
   owing to the sister relationship of Pittasoma and Conopophaga. In a
   previously unreported relationship, Pittasoma was strongly supported as
   the sister genus to Conopophaga (Conopophagidae). The remaining antpitta
   genera form a fully resolved and well-supported monophyletic lineage
   with two major subclades. The first clade consists of the genus
   Grallaria and supports the subgenera identified by Lowery and O'Neill
   (1969). The second antpitta clade has Hylopezus as the sister genus to
   Myrmothera, with Grallaricula as their sister genus. The results here
   allow for a new interpretation of the morphological characters used in
   previous studies.}},
