@article{ ISI:000284497900021,
Author = {Davenport, John and O'Halloran, John and Hannah, Fiona and McLauchlin,
   Orla and Smiddy, Pat},
Title = {{Comparison of Plumages of White-throated Dipper Cinclus cinclus and
   Blackbird Turdus merula}},
Journal = {{WATERBIRDS}},
Year = {{2009}},
Volume = {{32}},
Number = {{1}},
Pages = {{169-178}},
Month = {{MAR}},
Abstract = {{This study compares the plumage of a single Eurasi In White throated
   Dipper Cinclus cinclus with that of three Common (European) Blackbirds 1
   urdus merula (all road casualties) As expected the Dipper had fat mote
   down and contour feathers (3 322) than the Blackbirds (mean 1 704) with
   Dipper down and contour feathers being significantly shorter than those
   of Blackbirds However contrary to published information the plumage of
   dippers is not exceptionally dense the mean follicle density of the
   Dipper was 59 2 follicles cm 2 that of the Blackbirds 58 6-61 7
   follicles cm(2) Instead the enhanced insulation of diving Dippers stems
   from a) much more extensive plumage (the apterylae between feather
   tracts are fully feathered whilst naked in Blackbirds) b) substantial
   down cover of head (absent in Blackbirds) c) extensive down coy er of
   wings Much of the higher number of feathers in the Dipper is made up of
   contour feathers rather than down/semiplume feathers (the ratio between
   Dipper con tour and down/semiplume feathers 3 35 I was similar to that
   of Blackbirds 355 1) indicating an insulative e function for contour
   feathers as well as down Ultrastructural investigation showed that
   Dipper breast and back down was substantially finer (more complex) than
   in Blackbirds but leg down and contour feathers of the two species were
   indistinguishable Received 18 March 2008, accepted 11 September 2008}},
