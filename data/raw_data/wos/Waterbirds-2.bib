@article{ ISI:000255666200012,
Author = {Eilertsen, Karin and Barrett, Robert T. and Pedersen, Torstein},
Title = {{Diet, growth and early survival of Atlantic Puffin (Fratercula arctica)
   chicks in North Norway}},
Journal = {{WATERBIRDS}},
Year = {{2008}},
Volume = {{31}},
Number = {{1}},
Pages = {{107-114}},
Month = {{MAR}},
Abstract = {{Atlantic Puffin (Fratercula arctica) chick growth, diet, feeding rate
   and early survival and adult body condition were quantified on Hornoya,
   North Norway in 2002 and 2003 after a long-term change in diet from
   large Capelin (Mallolus villosus) and Sand Lance (Ammodytes spp.) in the
   earl), 1980s to smaller, poorer quality gadids, Sand Lance and Herring
   (Clupea harengus). The results were compared with data collected in
   1980-1982, when feeding conditions were considered very favourable.
   Chick growth was similar in both periods. The breeding adults
   compensated for reduced food quality by increasing the feeding
   frequency, such that the chicks received the same amount of fish (g
   d(-1) and kj d(-1)) or even more in 2002-2003. This increase in activity
   did not, however, result in a decline in adult body mass, suggesting
   that prey were abundant close to the colony and that the parents did not
   Suffer from the apparent increased feeding effort.}},
