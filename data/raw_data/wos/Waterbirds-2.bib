@article{ ISI:000252725600014,
Author = {Kushlan, James A.},
Title = {{Sympatric foraging of Little Egrets and Snowy Egrets in Barbados, West
   Indies}},
Journal = {{WATERBIRDS}},
Year = {{2007}},
Volume = {{30}},
Number = {{4}},
Pages = {{609-612}},
Month = {{DEC}},
Abstract = {{The New World Snowy Egret (Egretta thula) and Old World Little Egret
   (Egretta garzetta) are ecologically similar and historically allopatric
   species both of which recently colonized Barbados, West Indies, as
   breeding species. Examination of their foraging behavior in their zone
   of recent sympatry suggests that they choose the same (very limited)
   foraging sites but differ in foraging behavior. The larger Little Egret
   was more active and diverse in its foraging, fed more in upright
   posture, chose open, unvegetated sites more frequently, and was dominant
   over the Snowy Egret. The conservation situation in Barbados for these
   two species, and their thus-far unique sympatry, is uncertain as both
   feeding and nesting sites are far from secure.}},
