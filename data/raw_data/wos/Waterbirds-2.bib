@article{ ISI:000246093100020,
Author = {Konter, Andre},
Title = {{Response of Great Crested Grebes Podiceps cristatus to storm damage of
   nests}},
Journal = {{WATERBIRDS}},
Year = {{2007}},
Volume = {{30}},
Number = {{1}},
Pages = {{140-143}},
Month = {{MAR}},
Abstract = {{Great Crested Grebes (Podiceps cristatus), especially when nesting in
   large colonies, are known to suffer huge egg losses during storms.
   However, if the devastation of nests is not complete, partial or entire
   clutches can still be saved. The effects of high waves and strong wind
   can be countered to some extent by continuous and accelerated building.
   Even eggs floating in the water inside the remains of destroyed
   platforms may be recuperated by rebuilding the nest around and pushing
   the eggs with the beak ever higher on the weed. Due to the high
   resistance of eggs to cooling, they may even hatch after having passed
   several hours in cold water. In addition, grebes are capable to resume
   or continue laying quickly to replace lost eggs.}},
