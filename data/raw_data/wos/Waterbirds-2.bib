@article{ ISI:000261043500004,
Author = {Monticelli, David and Ramos, Jaime A. and Guerreiro-Milheiras, Sergio A.
   and Doucet, Jean-Louis},
Title = {{Adult Survival of Tropical Roseate Terns Breeding on Aride Island,
   Seychelles, Western Indian Ocean}},
Journal = {{WATERBIRDS}},
Year = {{2008}},
Volume = {{31}},
Number = {{3}},
Pages = {{330-337}},
Month = {{SEP}},
Note = {{31st Annual Meeting of the Waterbird-Society, Barcelona, SPAIN, NOV  
   02, 2007}},
Organization = {{Waterbird Soc}},
Abstract = {{Survival of tropical Roseate Terns (Sterna dougallii) from the western
   Indian Ocean was modeled using a capture-mark-recapture dataset of 102
   breeding adults ringed and resighted on Aride Island, Seychelles, from
   2002 to 2007. The effect of covariated reflecting oceanographic
   conditions and resighting effort was also examined during the modeling.
   A time-invariant survival rate was best supported by our data, with
   annual adult survival estimated at 0.807 +/- 0.033 (SE). Resighting
   probability was found to be influenced by sea-surface temperature (SST)
   anomalics, as expressed by the mean Indian Ocean Dipole mode index
   recorded during the nonbreeding period. Years of higher SST in the
   western part of the Indian Ocean in September-April (8 months)
   corresponded to a lower probability of resighting adults at the colony
   during the breeding season (May-August), which may be related to a
   tendency for some adults at the colony during the breeding season
   (May-August), which may be related to a tendency for some adults to
   refrain from breeding in less favourable years. Comparing our results
   with temperate studies, Roseate Terns breeding on Aride were found to
   exhibit similar survival estimates. Consequently, this study does not
   support the hypothesis that tropical Roseated Terns may counterbalance
   their lower fecundity (clutch size and breeding success) compared to
   their temperature and northern-hemisphere counterparts (Europe, North
   America, Caribbean) by relatively higher survival rates.}},
