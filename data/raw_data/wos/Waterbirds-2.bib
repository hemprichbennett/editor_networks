@article{ ISI:000284498100013,
Author = {Spendelow, Jeffrey A. and Mostello, Carolyn S. and Nisbet, Ian C. T. and
   Hall, C. Scott and Welch, Linda},
Title = {{Interregional Breeding Dispersal of Adult Roseate Terns}},
Journal = {{WATERBIRDS}},
Year = {{2010}},
Volume = {{33}},
Number = {{2}},
Pages = {{242-245}},
Month = {{JUN}},
Abstract = {{Long-distance breeding dispersal is infrequent among seabirds and has
   rarely been quantified. Six adult Roseate Terns (Sterna dougallii) that
   had bred at colony sites in Buzzards Bay, Massachusetts, USA from
   20042006 switched regions and moved 200-400 km to two colony sites in
   Maine between 2005 and 2007. Two of the emigrants presumably had nested
   for at least eight years in Massachusetts before moving to Maine,
   demonstrating that even long-time philopatric residents of one region
   may switch to another region. Received 6 October 2009, accepted 25
   February 2010.}},
