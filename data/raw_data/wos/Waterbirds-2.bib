@article{ ISI:000253831200006,
Author = {Viverette, Catherine B. and Garman, Greg C. and McIninch, Stephen P. and
   Markham, A. Catherine and Watts, Bryan D. and Macko, Stephen A.},
Title = {{Finfish-waterbird trophic interactions in tidal freshwater tributaries
   of the Chesapeake Bay}},
Journal = {{WATERBIRDS}},
Year = {{2007}},
Volume = {{30}},
Number = {{1}},
