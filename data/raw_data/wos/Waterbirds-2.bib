@article{ ISI:000284498000006,
Author = {Bond, Alexander L. and McClelland, Gregory T. W. and Jones, Ian L. and
   Lavers, Jennifer L. and Kyser, T. Kurt},
Title = {{Stable Isotopes Confirm Community Patterns in Foraging Among Hawaiian
   Procellariiformes}},
