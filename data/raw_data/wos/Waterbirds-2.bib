@article{ ISI:000255666200016,
Author = {Amat, Juan A. and Varo, Nico},
Title = {{Grit ingestion and size-related consumption of tubers by Graylag Geese}},
Journal = {{WATERBIRDS}},
Year = {{2008}},
Volume = {{31}},
Number = {{1}},
Pages = {{133-137}},
Month = {{MAR}},
Abstract = {{In herbivorous birds the processing rate of food is constrained by
   gizzard capacity. To enhance digestive processes, many species ingest
   grit to grind the food. Grit ingestion, however, may further limit the
   capacity of the gizzard. Graylag Geese (Anser anser) wintering in SW
   Spain fed mainly on Alkali Bulrush (Scirpus maritimus) tubers, showing a
   preference for small tubers. This preference may be due to a faster
   disintegration of small tubers than larger ones inside the gizzard. As
   larger tubers are likely coarser than smaller tubers, more grit would be
   necessary to process larger tubers. However, the ingestion of more grit
   to grind large tubers would be at the expense of ingesting additional
   tubers because of gizzard capacity limitations. Under these
   circumstances, there may be an inverse relationship between tuber size
   and amount of grit ingested to optimize food ingestion. Indeed, we found
   such a relationship. Grit facilitated the disintegration of tubers. This
   suggests that relying on some amount of grit to facilitate the grinding
   of food should outweigh the loss of gizzard capacity to the amount of
   food ingested.}},
Publisher = {{WATERBIRD SOC}},
Address = {{NATL MUSEUM NATURAL HISTORY SMITHSONIAN INST, WASHINGTON, DC 20560 USA}},
