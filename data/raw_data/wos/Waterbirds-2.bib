@article{ ISI:000263006600018,
Author = {Hicklin, Peter W. and Barrow, William R.},
Title = {{Wintering Harlequin Ducks on the Wolves Archipelago, Bay of Fundy}},
Journal = {{WATERBIRDS}},
Year = {{2008}},
Volume = {{31}},
Pages = {{130-132}},
Abstract = {{Between November 1994 and November 1995, regular boat surveys for
   Harlequin Ducks (Histrionicus histrionicus) were conducted on The Wolves
   archipelago in New Brunswick, Bay of Fundy. The birds arrived in early
   November and departed by mid-May, reaching a peak of 38 birds in March.
   Sex ratios tended to be close to unity, consistent with observations in
   Maine in the mid-1990s. Juvenile age ratios were noticeably higher than
   those observed at other wintering sites, suggesting that the juvenile
   birds may prefer The Wolves archipelago as a wintering site. Overall,
   these surveys confirm the importance of The Wolves archipelago as a
   non-breeding area for this species.}},
