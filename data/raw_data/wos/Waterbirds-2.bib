@article{ ISI:000250549900017,
Author = {Balkiz, Oezge and Danol, Stephanie and Barbraud, Christophe and Tekin,
   Serpil and Oezesmi, Uycar and Duendar, Munis and Bechet, Arnaud},
Title = {{Sexing Greater Flamingo chicks from feather bulb DNA}},
Journal = {{WATERBIRDS}},
Year = {{2007}},
Volume = {{30}},
Number = {{3}},
Pages = {{450-453}},
Month = {{SEP}},
Abstract = {{Adult Greater Flamingos (Phoenicopterus roseus) are sexually dimorphic,
   with males being on average larger and heavier than females. However,
   there is no practical way to sex the chicks by their morphology. Here we
   describe a method relying on quick and easy DNA extraction from
   feathers. A PCR test employing primers to amplify introns whose lengths
   usually differ between the CHD-W and the CHD-Z genes allow sex
   discrimination. This method is thus a fast, accurate and inexpensive
   protocol to sex flamingo chicks from feathers bulbs sampled in the
   field.}},
