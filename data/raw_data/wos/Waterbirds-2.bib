@article{ ISI:000258481800002,
Author = {Kojadinovic, Jessica and Richard, Pierre and Le Corre, Matthieu and
   Cosson, Richard P. and Bustamante, Paco},
Title = {{Effects of lipid extraction on delta C-13 and delta N-15 values in
   seabird muscle, liver and feathers}},
Journal = {{WATERBIRDS}},
Year = {{2008}},
Volume = {{31}},
Number = {{2}},
Pages = {{169-178}},
Month = {{JUN}},
Abstract = {{We determined whether lipid extraction of seabird muscle, liver and
   feathers is required for delta C-13 and delta N-15 analysis, based on a
   study of Barau's Petrels (Pterodroma baraui) and White-tailed
   Tropicbirds (Phaethon lepturus). Samples were analyzed for stable
   isotopes before and after lipid removal. Lipid-free muscle and liver
   were significantly enriched in C-13 compared to those containing lipids
   (0.52 parts per thousand and 0.61 parts per thousand mean differences in
   delta C-13 respectively) implying that lipids should be extracted from
   these tissues to avoid effects of uncontrolled differential lipid
   content. However lipid extraction also slightly increased muscle delta
   N-15 values in tropicbirds. Researchers should thus run samples
   separately for delta C-13 and delta N-15 analysis, extracting lipids
   only in aliquots in which delta C-13 is measured, or apply arithmetic
   delta C-15 normalization methods on non lipid-extracted samples.
   Predictive models developed from our data, inferring delta C-13 values
   of lipid-extracted material from delta N-13 values and C:N ratios of non
   lipid-extracted material are presented and compared to five
   normalization methods taken from literature. Our models showed the best
   prediction efficiencies followed by that developed by McConnaughey and
   McRoy (1979). In feathers, no difference in stable isotopic measurements
   were noted between aliquots treated with sodium hydroxide or 2:1
   chloroform:methanol. Both protocols seem suitable for preparing feather
   samples for isotopic analysis.}},
