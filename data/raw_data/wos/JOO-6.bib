@article{ ISI:000077213800006,
Author = {Copete, JL and Bigas, D and Marine, R and Martinez-Vilalta, A},
Title = {{Frequency of complete moult in adult and juvenile Great Reed Warblers
   (Acrocephalus arundinaceus) in Spain}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{1998}},
Volume = {{139}},
Number = {{4}},
Pages = {{421-424}},
Month = {{OCT}},
Abstract = {{We examined a large data set of adult and juvenile Great Reed Warblers
   (Acrocephalus arundinaceus), caught over five years, to study the
   occurrence of complete moult in a Spanish population. A total of 27
   adults and 5 juveniles were in active moult The proportion of birds
   moulting each year varied between 0.03 and 0.19. Nearly half the adults
   were finishing moult. At least one bird was certainly of local origin,
   rather than on passage, and four more were moulting when retrapped in
   subsequent years, suggesting that they too were resident breeders in the
   area. Some other birds were probably resident also, because they were in
   moult when caught at the beginning of August. Two of the five juveniles
   finished their moult in the area. In conclusion, our data seem to show
   that complete moult before departure to the wintering quarters is a
   regular process undertaken by some of the breeding population of Great
   Reed Warblers in north-east Spain.}},
