@article{ ISI:000223079300010,
Author = {Korner-Nievergelt, F and Leisler, B},
Title = {{Morphological convergence in conifer-dwelling passerines}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2004}},
Volume = {{145}},
Number = {{3}},
Pages = {{245-255}},
Month = {{JUL}},
Abstract = {{We tested for morphological convergence in conifer specialists among 88
   passerines belonging to seven different phylogenetic lineages by
   discriminant factor analysis. We found a parallel trend among the seven
   lineages in body mass and digital pad morphology, whereas no such trends
   existed for the feeding and flight apparatus. Compared to the control
   species, the conifer specialists have smaller body masses and higher
   digital pads with maximal widths lying more distally within each of the
   seven lineages. These traits are interpreted as adaptations to dwelling
   among coniferous needles.}},
