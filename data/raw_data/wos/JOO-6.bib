@article{ ISI:000230719700013,
Author = {Amat, JA},
Title = {{Does the expression of a male plumage trait in female Kentish plovers
   (Charadrius alexandrinus) signal individual quality?}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2005}},
Volume = {{146}},
Number = {{3}},
Pages = {{287-290}},
Month = {{JUL}},
Abstract = {{A small percentage (4.5\%) of females Kentish plovers (Charadrius
   alexandrinus) observed breeding in southern Spain expressed a plumage
   trait characteristic of males: the forecrown (frontal) black bar. The
   expression of this male trait may be age-related, and reversible, i.e.,
   within individuals it may be present in some years but not in other
   years. Females with frontal bars were not in better condition and nor
   did they initiate breeding earlier than in years when they lacked
   frontal bars. Nevertheless, when females had frontal bars they laid
   larger eggs. I suggest that the frontal bar on females could be an
   epiphenomenon resulting from hormonal imbalances of females as they age.}},
