@article{ ISI:000082272900007,
Author = {Niecke, M and Heid, M and Kruger, A},
Title = {{Correlations between melanin pigmentation and element concentration in
   feathers of White-tailed Eagles (Haliaeetus albicilla)}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{1999}},
Volume = {{140}},
Number = {{3}},
Pages = {{355-362}},
Month = {{JUL}},
Abstract = {{The element concentration of moult feathers of White-tailed Eagles was
   investigated. Using the 2- MeV Hamburg proton microprobe we tried to
   differentiate between elements incorporated into the feather via the
   food chain and those which are deposited externally onto the feather
   vane. Regarding incorporated elements, special attention has been given
   to a possible correlation between element concentration and feather
   pigmentation. Concerning the elements detected in this work (S, K, Ca,
   Ti, Mn, Fe, Cu, Zn, Hg, Pb), calcium, manganese and zinc show a
   considerable enhancement within the pigmented feather as compared with
   pigment-free feather sections. On the other hand, no differences were
   found in concentrations for sulfur, titanium, iron, copper, mercury and
   lead. Our findings therefore imply a special enrichment of Ca, Mn and Zn
   within melanin, the source of the feather's pigmentation. The possible
   role of these elements with regard to melanin formation is discussed.}},
