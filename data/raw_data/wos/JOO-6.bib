@article{ ISI:000230719700007,
Author = {Hewson, CM and Fuller, RJ and Day, C},
Title = {{An investigation of habitat occupancy by the nightingale Luscinia
   megarhynchos with respect to population change at the edge of its range
   in England}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2005}},
Volume = {{146}},
Number = {{3}},
Pages = {{244-248}},
Month = {{JUL}},
Abstract = {{The nightingale Luscinia megarhynchos has undergone population decline
   and range contraction at the north-western limit of its distribution in
   England during the last 25 years. We examine patterns of habitat
   occupancy and habitat availability across sites with a range of
   population histories to see whether habitat loss is a plausible
   explanation for these declines. The number of singing males in 1999
   correlated with area of primary nightingale habitat in the East Midlands
   (where the species has declined), but not in East Anglia (where the
   population has been stable). Change in population size between 1980 and
   1999 and current habitat availability were weakly correlated in the East
   Midlands but not in East Anglia. These results are consistent with
   habitat loss having contributed to the decline of the nightingale in the
   East Midlands, but suggest that other, wider-scale, factors may be at
   least partially responsible for determining the abundance of the species
   within England as a whole.}},
