@article{ ISI:000291035900006,
Author = {Cibois, Alice and Thibault, Jean-Claude and Raust, Philippe and Pasquet,
   Eric},
Title = {{Systematics of the reed-warblers of the Tuamotu Archipelago, eastern
   Polynesia}},
