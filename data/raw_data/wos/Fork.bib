@article{ ISI:000207780700004,
Author = {Li, Yong Ding and Kasorndorkbua, Chaiyan},
Title = {{The status of the Himalayan Griffon Gyps himalayensis in South-East Asia}},
Journal = {{FORKTAIL}},
Year = {{2008}},
Number = {{24}},
Pages = {{57-62}},
Month = {{AUG}},
Abstract = {{The Himalayan Griffon Gyps himalayensis, a large scavenging raptor
   previously known to be resident to the Sino-Himalayas and Central Asia,
   is increasingly being recorded throughout South-East Asia It now been
   recorded in six South-East Asian countries and is represented by a total
   of over 30 documented records, mostly of immature birds The causes for
   the Increase in sightings are unknown, but we speculate that climate
   change, deforestation and hunting, coupled with natural patterns of
   post-fledging dispersal and navigational inexperience may be
   contributing to this change}},
