@article{ ISI:000285983000002,
Author = {Timmins, R. J. and Ostrowski, Stephane and Mostafawi, Naqeebullah and
   Noori, Hafizullah and Rajabi, Ali Madad and Svensson, Lars and Olsson,
   Urban and Poole, Colin M.},
Title = {{New information on the Large-billed Reed Warbler Acrocephalus orinus,
   including its song and breeding habitat in north-eastern Afghanistan}},
Journal = {{FORKTAIL}},
Year = {{2010}},
Number = {{26}},
Pages = {{9-23}},
Month = {{AUG}},
Abstract = {{We report in detail on the first well documented, probable breeding
   location of the Large-billed Reed Warbler Acrocephalus orinus, found in
   north-east Afghanistan, give a description of its song and summarise its
   identification criteria using new information from live birds. Fifteen
   birds were captured and measured in the presumed breeding season, and
   later their identity was confirmed using DNA markers. In one of the
   localities visited many birds were singing. We also describe the habitat
   and assess some conservation issues.}},
Publisher = {{ORIENTAL BIRD CLUB}},
Address = {{P. O. BOX 324, BEDFORD, ENGLAND}},
Type = {{Article}},
Language = {{English}},
