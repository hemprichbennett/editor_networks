@article{ ISI:000285983000004,
Author = {Brickle, Nick W. and Eaton, James A. and Rheindt, Frank E.},
Title = {{A rapid bird survey of the Menyapa mountains, East Kalimantan, Indonesia}},
Journal = {{FORKTAIL}},
Year = {{2010}},
Number = {{26}},
Pages = {{31-41}},
Month = {{AUG}},
Abstract = {{We report on a rapid bird survey in the Menyapa Mountains of East
   Kalimantan Province, Indonesia. Six days were spent at altitudes of
   between 150 m and 1,600 m during November 2007. A total of 184 species
   were recorded including one new species for Kalimantan, one new for
   Indonesia, and several range extensions, highlighting the interest and
   importance of the Menyapa region. The status, distribution and taxonomy
   of several montane species are discussed in general, including species
   both detected and undetected by this survey.}},
