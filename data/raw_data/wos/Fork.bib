@article{ ISI:000327110900018,
Author = {Buatip, Somsak and Karntanut, Wanchamai and Swennen, Cornelis},
Title = {{Nesting period and breeding success of the Little Egret Egretta garzetta
   in Pattani province, Thailand}},
Journal = {{FORKTAIL}},
Year = {{2013}},
Number = {{29}},
Pages = {{120-123}},
Month = {{SEP}},
Abstract = {{Nesting of Little Egret Egretta garzetta was studied between October
   2008 and September 2009 in a colony near Pattani, southern Thailand,
   where the species is a recent colonist. Nesting was bimodal over a
   12-month observation period.The first nesting period started in December
   in the middle of the rainy season (November-December).The second period
   started in March during the dry season (February-April). In the second
   period, nesting began in an area not occupied during the first period
   but gradually expanded into areas used in the first period. Egg and
   chick losses were high; the mean number of chicks that reached two weeks
   of age was 1.0 +/- 1.2 (n = 467 nests), based on nests that had
   contained at least one egg. Considerable heterogeneity of clutch size
   and nest success was apparent between different locations within the
   colony. The main predator appeared to be the Malayan Water Monitor
   Varanus salvator.}},
