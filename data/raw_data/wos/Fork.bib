@article{ ISI:000295444700003,
Author = {Duckworth, J. W. and Round, P. D. and Tizard, R. J.},
Title = {{The Yellow-throated Fulvetta Alcippe cinerea in Indochina}},
Journal = {{FORKTAIL}},
Year = {{2011}},
Number = {{27}},
Pages = {{11-14}},
Month = {{AUG}},
Abstract = {{While the Yellow-throated Fulvetta Alcippe cinerea is rather common in
   Kachin state in northern Myanmar, in South-East Asia it is otherwise
   known with certainty only from a highly disjunct cluster of four
   localities in rugged mountains on the international border between Laos
   and Vietnam: Phou Kabo, Ban Muang-Ngat and Phou San (all in the
   Xiangkhouang highlands), Laos, and Pu Mat Nature Reserve (northern
   Annamite mountains), Vietnam. There is no obvious biogeographic
   explanation of this distributional pattern, which is most unlikely to be
   an artefact of fieldwork bias. Improved conservation status of forest
   within the range of the species is desirable.}},
