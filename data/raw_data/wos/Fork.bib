@article{ ISI:000404813600005,
Author = {Dewi, Arellea R. and Purnomo, Nurrohman E. and Ahmadi, Reza A. and
   Purwianshari, I. and Ashshidiqi, Muhamad H.},
Title = {{Field records of Zappey's Flycatcher Cyanoptila cumatilis on Java and
   Sumatra, with notes on the distribution and status of Blue-and-white
   Flycatcher C. cyanomelana in Indonesia}},
Journal = {{FORKTAIL}},
Year = {{2016}},
Number = {{32}},
Pages = {{36-40}},
Abstract = {{Zappey's Flycatcher Cyanoptila cumatilis was a long-forgotten taxon that
   is now recognised as a distinct species from Blue-and-white Flycatcher
   C. cyanomelana. Its distribution, especially during the non-breeding
   season; is poorly known. In Indonesia it was known from two records from
   Sumatra and a single record from Java. Observations from Halimun Salak
   National Park in February 2014 and Cibodas Botanical Gardens in March
   2015 revealed new records of Zappey's Flycatcher from mainland Java.
   Published information and observations by Indonesian birdwatchers
   confirm the presence of the two races of Blue-and-white Flycatcher
   (cyanomelana and intermedia) on Java and suggest that it is a much
   commoner migrant than previously believed.}},
