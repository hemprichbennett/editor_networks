@article{ ISI:000226239300011,
Author = {Clark, WS and Peter, HM},
Title = {{Basic II and Basic III plumages of Rough-legged Hawks}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2005}},
Volume = {{76}},
Number = {{1}},
Pages = {{83-89}},
Month = {{WIN}},
Abstract = {{Adult Rough-legged Hawks (Buteo lagopus) are depicted and described in
   bird guides and handbooks as sexually dimorphic in plumage. We describe
   for the first time a Basic II plumage for this species, determined by
   incomplete flight feather molt, in which both sexes have the characters
   of adult females, and Basic III plumages that are essentially adult by
   sex but show pale primary patches on the upper wings.}},
