@article{ ISI:000090032100001,
Author = {Dougill, SJ and Johnson, L and Banko, PC and Goltz, DM and Wiley, MR and
   Semones, JD},
Title = {{Consequences of antenna design in telemetry studies of small passerines.}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2000}},
Volume = {{71}},
Number = {{3}},
Pages = {{385-388}},
Month = {{SUM}},
Abstract = {{Entanglement and mortality of Palila (Loxioides bailleui), an endangered
   Hawaiian honeycreeper, occurred when birds were radio-tagged with
   transmitters equipped with a long, limp, solder-tipped antenna. Birds
   were found suspended in trees by their transmitter antenna on eight
   occasions. Although these birds eventually freed themselves or were
   freed by us, at least one bird died afterwards. For radio telemetry
   studies of small passerine species we recommend avoiding transmitters
   equipped with an antenna that is bulbous at the tip, >16 cm in length,
   limp, and shiny.}},
