@article{ ISI:000236520300007,
Author = {Dunn, PO and Whittingham, LA},
Title = {{Radio-tracking of female Tree Swallows prior to egg-laying}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2005}},
Volume = {{76}},
Number = {{3}},
Pages = {{259-263}},
Month = {{SUM}},
Abstract = {{We radio-tracked eight female Tree Swallows (Tachycineta bicolor) prior
   to egg-laying in 2000 and 2001. Six females made movements > 2 kin to
   evening roosting sires. Females often roosted farther away than the
   nearest available roost site, they used different roosts on different
   nights, and during the day they foraged Lip to 10 kin from their nest
   site. Thus, female swallows move over a large area (103 km(2)) prior to
   egg-laying. These widespread movements appear to be unusual among
   swallows, and they may allow females to survey a large number of
   extra-pair males.}},
