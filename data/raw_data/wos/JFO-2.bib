@article{ ISI:000220930400013,
Author = {Linkhart, BD and Reynolds, RT},
Title = {{Longevity of Flammulated Owls: additional records and comparisons to
   other North American strigiforms}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2004}},
Volume = {{75}},
Number = {{2}},
Pages = {{192-195}},
Month = {{SPR}},
Abstract = {{A male Flammulated Owl (Otus flammeolus), banded as a breeding adult
   (age unknown) on 8 July 1988 in central Colorado, was last recaptured on
   8 July 2001. On the same study area, a female Flammulated Owl was banded
   as a breeding adult (age unknown) on 7 July 1988 and was last recaptured
   on 22 June 1995. These data establish longevity at a minimum 14 yrs for
   males and 8 yrs for females. The data suggest that Flammulated Owls may
   be relatively long-lived, and support evidence that this species has a
   life history strategy similar to larger raprors.}},
