@article{ ISI:000226239300012,
Author = {Londei, T},
Title = {{The conspicuous juvenile bill coloration in the Piapiac Ptilostomus
   afer: an infant signal from aggressive helpers?}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2005}},
Volume = {{76}},
Number = {{1}},
Pages = {{90-93}},
Month = {{WIN}},
Abstract = {{The Piapiac, a little-known corvid, may be the African counterpart of
   the American jays, and its behavior supports a hypothesized link between
   conspicuous bill coloration Of juveniles and sociality of the species.
   During field observations in Cameroon, juvenile Piapiacs in mixed groups
   displayed their brightly pink bills by gaping at adults as though
   begging for food but often when no food could be obtained. They were
   more aggressive and vocal than adults against presumed enemies' and
   situations that aroused the group seemed to elicit gaping. The
   conspicuous bill coloration of juvenile Piapiacs matches their
   conspicuous behavior in mixed groups, and may act as a persisting infant
   signal, useful to block aggressive reactions by adults and facilitate
   acceptance of juveniles as helpers.}},
