@article{ ISI:000220930400014,
Author = {Klaassen, M and Baarspul, T and Dekkers, T and van Tienen, P},
Title = {{The relationship between carbon stable isotope ratios of hatchling down
   and egg yolk in Black-headed Gulls}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2004}},
Volume = {{75}},
Number = {{2}},
Pages = {{196-199}},
Month = {{SPR}},
Abstract = {{We reconstructed the nutrient source for egg synthesis by sampling
   Black-headed Gull (Larus ridibundus) eggs for yolk, analyzing their
   carbon stable isotope ratio, and comparing that to hatchling down. Most
   of the variation in carbon stable isotope ratio was explained by,
   differences between nests, the within-nest variation being explained by
   laying order. These data indicate significant differences in diet choice
   between individual females and changes in food choice or body-store
   dynamics during egg laying. Among-egg variation in the carbon stable
   isotope ratios of the yolk were closely reflected in the hatchling down,
   on average down being enriched by 3.1parts per thousand delta(13)C
   relative to yolk. Our findings support the contention that pre-laying
   female diets can be evaluated from hatchling down.}},
Publisher = {{ASSOC FIELD ORNITHOLOGISTS}},
Address = {{GEORGIA SOUTHERN UNIV, DEPT BIOLOGY, STATESBORO, GA 30460-8042 USA}},
