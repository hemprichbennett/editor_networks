@article{ ISI:000072309000007,
Author = {Dean, TF and Delany, MF and Chapman, EW and Vickery, PD},
Title = {{Longevity and site fidelity of Florida Grasshopper Sparrows}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1998}},
Volume = {{69}},
Number = {{1}},
Pages = {{51-54}},
Month = {{WIN}},
Abstract = {{Three adult male Florida Grasshopper Sparrows recaptured during a
   banding study on the Avon Park Air Force Range, Highlands Count):
   Florida extend the longevity record for Grasshopper Sparrows beyond
   published records to at least 6.6 yr. Recaptures were less than or equal
   to 320 m from original banding locations, which is consistent with the
   supposition that Florida Grasshopper Sparrows are sedentary. This
   population of the endangered Florida subspecies appears to be
   longer-lived than Grasshopper Sparrow populations in other parts of the
   species' range and may have different demographic patterns. Long-term
   studies are needed to confirm this.}},
