@article{ ISI:000166325300019,
Author = {Wang, XH and Trost, CH},
Title = {{Trapping territorial Black-billed Magpies}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2000}},
Volume = {{71}},
Number = {{4}},
Pages = {{730-735}},
Month = {{FAL}},
Abstract = {{Trapping territorial Black-billed Magpies (Pica Pica) is challenging
   because magpies are extremely wary birds. We describe three successful
   techniques to capture territorial magpies during different periods of
   the breeding cycle. Funneled walk-in traps were more effective early in
   the breeding season (late February to late March) when snow covered the
   ground. A bal-chatri trap with an adult female magpie decoy captured
   43\% of targeted birds when female magpies were fertile and incubating.
   A mist-net with a mounted Great Horned Owl (Bubo virginianus) decoy in
   front of the net captured 52\% of targeted birds when adults were
   feeding nestlings.}},
Publisher = {{ASSOC FIELD ORNITHOLOGISTS}},
Address = {{GEORGIA SOUTHERN UNIV, DEPT BIOLOGY, STATESBORO, GA 30460-8042 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Wang, XH (Reprint Author), Univ Illinois, Dept Food Sci \& Human Nutr, 259 ERML,1201 W Gregory, Urbana, IL 61801 USA.
