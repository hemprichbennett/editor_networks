@article{ ISI:000080066900004,
Author = {Garthe, S and Huppop, O},
Title = {{Effect of ship speed on seabird counts in areas supporting commercial
   fisheries}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1999}},
Volume = {{70}},
Number = {{1}},
Pages = {{28-32}},
Month = {{WIN}},
Abstract = {{Ship speed may have an important effect on the results of seabird
   surveys. We counted seabirds on a 20.5 X 0.3 km transect in the Kattegat
   that was sailed nine rimes with the RV ``Heincke.{''} Ship speed
   alternated between 9 knots and 5 knots. The numbers of the most common
   species, the Lesser Black-backed Gull (Larusfuscus), were significantly
   higher when the ship was sailing at lower speed similar to that of
   commercial fishing vessels when trawling in the area. It is postulated
   that scavenging species (e.g., gulls) are attracted to the low speed of
   vessels whereas non-scavengers (e.g., auks) are not.}},
