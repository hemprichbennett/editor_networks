@article{ ISI:000170048700004,
Author = {Rimmer, CC and Tietz, JR},
Title = {{An adult male Blackpoll Warbler in female-like plumage}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2001}},
Volume = {{72}},
Number = {{3}},
Pages = {{365-368}},
Month = {{SUM}},
Abstract = {{We describe a breeding male Blackpoll Warbler (Dendroica striata) in
   aberrant female-like plumage captured on Mt. Mansfield, Vermont, in
   1998. The bird had been banded the previous summer in typical
   alternate-male plumage. This appears to represent the first
   documentation of a wild adult male bird in female-like alternate
   plumage. We suspect that the abnormal plumage reported here was due to
   hormonal influences on pigmentation. The bird's between-year change in
   appearance appeared to have no effect on its reproductive behavior.}},
