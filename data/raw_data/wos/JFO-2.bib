@article{ ISI:000080066900016,
Author = {Shackford, JS and Leslie, DM and Harden, WD},
Title = {{Range-wide use of cultivated fields by Mountain Plovers during the
   breeding season}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1999}},
Volume = {{70}},
Number = {{1}},
Pages = {{114-120}},
Month = {{WIN}},
Abstract = {{During six summers from 1986 through 1995, we searched cultivated fields
   in Montana, Wyoming, Nebraska, Colorado, Kansas, Oklahoma, New Mexico,
   and Texas for Mountain Plovers (Charadrius montanus) during the breeding
   and premigratory seasons. We observed 2208 Mountain Plovers on 416
   cultivated fields, 96.9\% of which were observed in seven counties in
   four states. We located 52 nests on cultivated fields: 50\% on fallow or
   bare fields (greater than or equal to 95\% bare), 23.1\% on wheat
   fields, 1.9\% on both wheat and forb stubble fields, and the remainder
   on fields of mile (13.5\%), forbs (7.7\%), and corn (3.8\%). Of 46 nests
   found on cultivated fields, 31 failed; 70.9\% were destroyed by farm
   machinery, and known losses to predation were only 3.3\%. Mountain
   Plovers nested and appeared to rear young (30 chicks observed) on
   cultivated fields in southern latitudes, particularly on fields used for
   winter wheat. Cultivated fields will play an important role in the
   conservation of the Mountain Plover, as it is considered for threatened
   or endangered status, but comparative research is needed to clarify that
   role relative re use of native habitat.}},
