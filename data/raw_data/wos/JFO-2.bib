@article{ ISI:000167072200015,
Author = {Bull, EL},
Title = {{Survivorship of Pileated Woodpeckers in northeastern Oregon}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2001}},
Volume = {{72}},
Number = {{1}},
Pages = {{131-135}},
Month = {{WIN}},
Abstract = {{Knowledge of the survival of the Pileated Woodpecker (Dryocopus
   pileatus) is essential in managing viable populations of the species. In
   eight study areas in northeastern Oregon, survivorship of adult Pileated
   Woodpeckers was 0.60 after 6 mo, 0.47 after 12 mo, and 0.35 after 18 mo.
   Of three juveniles radio-tagged in late summer or fall, two survived to
   breed the next year. Of 13 juveniles radio-tagged as nestlings, 23-54\%
   survived 3.5 mo. In these populations adult mortality exceeded
   recruitment of young into the breeding population. Survival may have
   been influenced by transmitter attachments, habitat quality, or annual
   variability in predation.}},
