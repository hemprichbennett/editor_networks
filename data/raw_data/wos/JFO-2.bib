@article{ ISI:000072309000005,
Author = {Kratter, AW},
Title = {{The nests of two bamboo specialists: Celeus spectabilis and Cercomacra
   manu}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1998}},
Volume = {{69}},
Number = {{1}},
Pages = {{37-44}},
Month = {{WIN}},
Abstract = {{The nests of Celeus spectabilis (Rufous-headed Woodpecker) and
   Cercomacra manu (Manu Antbird) are described. These are the first
   published descriptions of the nests of these species, both of which are
   restricted to thickets of spiny bamboo (Guadua weberbaueri) in
   southwestern Amazonia. The nest of the woodpecker, a hole placed 2.8 m
   high in a soft-wooded dead tree, was similar to the nests of other
   species in the genus. The nest was in an area of almost pure bamboo. The
   nest of the antbird, also in a dense bamboo thicker, was a pensile pouch
   suspended by the nest rim. The nest was 3.5 m high and attached to small
   bamboo branches. The previously defined groups in the genus (the
   tyrannina group and the nigricans group) may differ in nest structure,
   although comparative material is scant.}},
