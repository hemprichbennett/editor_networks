@article{ ISI:000081985700008,
Author = {Mawhinney, K and Diamond, T},
Title = {{Sex determination of Great Black-backed Gulls using morphometric
   characters}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1999}},
Volume = {{70}},
Number = {{2}},
Pages = {{206-210}},
Month = {{SPR}},
Abstract = {{We evaluated sexual size dimorphism of Great Black-backed Gulls (Larus
   marinus) from a breeding colony in the Bay of Fundy and provide a
   reliable method for predicting the sex of measured individuals. Males
   were significantly larger than females in all body measurements. A
   predictive function using only three linear measurements (head length,
   bill depth and wing length) was more accurate than any single linear
   measure and correctly sexed 99-100\% of the individuals.}},
