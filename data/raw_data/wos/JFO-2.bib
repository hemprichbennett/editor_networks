@article{ ISI:000167072200005,
Author = {Robinson, WD and Robinson, TR},
Title = {{Observations of predation events at bird nests in central Panama}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2001}},
Volume = {{72}},
Number = {{1}},
Pages = {{43-48}},
Month = {{WIN}},
Abstract = {{We describe 14 nest predation events witnessed in the Republic of Panama
   during studies of avian nesting success. Eight predations were by birds,
   including two species of toucan and one forest-falcon. Four predations
   were by two species of snake, one was by monkeys, and one near-predation
   was by arm) ants (Eciton burchelli). In three cases, a fraction of the
   nest contents were removed by the predator on one visit before it
   returned later to consume the remaining items. Our observations,
   although limited to diurnal encounters, indicate that Neotropical birds
   are susceptible to a diverse array of potential predators. Detailed
   investigations using cameras are needed to quantify the potential
   importance of each predator species and to identify the occurrence and
   importance of nocturnal predation.}},
