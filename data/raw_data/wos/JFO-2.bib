@article{ ISI:000172449700002,
Author = {Alonso, JA and Martin, CA and Alonso, JC and Morales, MB and Lane, SJ},
Title = {{Seasonal movements of male Great Bustards in central Spain}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2001}},
Volume = {{72}},
Number = {{4}},
Pages = {{504-508}},
Month = {{FAL}},
Abstract = {{A sample of 22 radio-tagged male Great Bustards from central Iberian
   populations was studied from 1996 to 1998. All birds undertook seasonal
   movements to postbreeding areas located up to 167 km away from their
   leks. Departures from leks occurred between May and July, with most
   birds moving northeast to summering areas. Approximately 47\% of the
   males spent only the summer away from their leks, returning to leks
   between September and December. Other males overwintered in areas
   located to the southeast, returning to lek sites between January and
   March.}},
