@article{ ISI:000081985700002,
Author = {Plice, L and Balgooyen, TG},
Title = {{A remotely operated trap for American Kestrels using nestboxes}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1999}},
Volume = {{70}},
Number = {{2}},
Pages = {{158-162}},
Month = {{SPR}},
Abstract = {{American Kestrels (Falco sparverius) can be difficult to catch with
   standard trapping methods. We describe the use of a commercially
   available radio controller as a remotely operated trap for placement in
   a nestbox and relate trap utility to kestrel breeding behaviors. The
   ease of operation and immediate response of the remote device increase
   trapping success of breeding adults during feeding of the young. The
   design is scaleable for other species using nestboxes.}},
