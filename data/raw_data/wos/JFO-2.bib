@article{ ISI:000081985700014,
Author = {Folk, MJ and Schmidt, JA and Nesbitt, SA},
Title = {{A trough-blind for capturing cranes}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1999}},
Volume = {{70}},
Number = {{2}},
Pages = {{251-256}},
Month = {{SPR}},
Abstract = {{A specialized blind resembling a feeding trough for cattle was developed
   to capture cranes. Cranes were attracted to bait placed strategically
   over hand-holes in the sides of the blind. Twenty-one cranes (2 Florida
   Sandhill Cranes {[}Grus canadensis pratensis] and 19 Whooping Cranes
   {[}G. americana]) were captured in 21 attempts. The trough blind was
   inexpensive (US \$100 for materials) and provided a safe and efficient
   technique for capturing individual cranes.}},
