@article{ ISI:000083070100005,
Author = {Houston, CS},
Title = {{Dispersal of Great Horned Owls banded in Saskatchewan and Alberta}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1999}},
Volume = {{70}},
Number = {{3}},
Pages = {{343-350}},
Month = {{SUM}},
Abstract = {{From 557 recoveries of nestling Great Horned Owls banded in Saskatchewan
   and 152 banded in Alberta, 176 from Saskatchewan and 51 from Alberta
   were recovered in an exact month >12 mo post-banding. The mean dispersal
   distance was 149 km (Saskatchewan, 154 km and Alberta, 131 km). For the
   five months, April-August, considered to best reflect natal dispersal,
   the mean dispersal was 75 km; separate median distances for each of
   these months ranged from 39 to 50 km, equivalent to the width of ten owl
   territories in average habitat.
   I compared calendar years of peak snowshoe hare numbers, 1958-1960,
   1967-1970, and 1978-1981, with intervening years of low hare density,
   and with 1985-1994 when no hare cycle was apparent. At times of low hare
   density, owls of all ages travel great distances in fall and winter in a
   remarkably consistent southeast direction. Median dispersal distances
   were 43 km in high hare years, 56.5 km in low hare years and 72 km in
   noncyclic years. But in high hare years the only three owls to travel
   >100 km went northwest or northeast, opposite to the direction in low
   hare years.}},
