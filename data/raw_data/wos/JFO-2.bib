@article{ ISI:000175362000012,
Author = {Redondo, AJ and Arias-de-Reyna, L},
Title = {{A photographic method for measuring eggs}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2002}},
Volume = {{73}},
Number = {{2}},
Pages = {{197-198}},
Month = {{SPR}},
Abstract = {{We describe a system for photographing the longitudinal and transverse
   planes of bird eggs that improves accuracy of measurements of egg size
   and increases the speed of data collection. The proposed system consists
   of a flat support, with a few small holes where eggs are lodged. The
