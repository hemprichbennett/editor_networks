@article{ ISI:000072309000001,
Author = {Woodworth, BL and Faaborg, J and Arendt, WJ},
Title = {{Breeding and natal dispersal in the Puerto Rican Vireo}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1998}},
Volume = {{69}},
Number = {{1}},
Pages = {{1-7}},
Month = {{WIN}},
Abstract = {{Information on dispersal is critical for understanding the population
   dynamics of birds. We estimated breeding and natal dispersal in two
   studies of a population of;he Puerto Rican Vireo (Vireo latimeri) that
   is in danger of local extirpation due to low reproductive success. From
   7.1-29\% of adult males and 12.5-25\% of adult females changed
   territories between breeding seasons. The median breeding dispersal
   distance of 12 birds (7 males, 2 females, and 3 of unknown sex) was 370
   m (range 110-560 m). Four fledglings dispersed a median distance of 490
   m from their natal nests to breeding territories (range 300-2030 m). The
   implications of these data for population trends of the vireo in Guanica
   Forest are discussed.}},
