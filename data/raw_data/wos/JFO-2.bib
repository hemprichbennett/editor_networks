@article{ ISI:000080066900012,
Author = {Blem, CR and Blem, LB and Berlinghoff, LS},
Title = {{Old nests in Prothonotary Warbler Nest boxes: Effects on reproductive
   performance}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1999}},
Volume = {{70}},
Number = {{1}},
Pages = {{95-100}},
Month = {{WIN}},
Abstract = {{We compared reproductive performance of Prothonotary Warblers
   (Protonotaria critrea) nesting in cleaned nest boxes with that of
   warblers using boxes in which old nests were allowed to remain. In the
   present study, warblers built 207 nests in 300 boxes over a single
   breeding season. Mean clutch size was 5.1 +/- 0.7 (SE) eggs in 118 early
   nests (Prothonotary Warblers are double-brooded) and 3.8 +/- 0.7 eggs in
   89 late nests. Presence of an old nest from the previous year did not
   significantly affect (1) probability of a nest box being chosen for
   nesting, (2) date of nest initiation, (3) clutch sizes of early or late
   nests, or (4) mortality rates of nestlings. Brown-headed Cowbirds
   (Molothrus ater) parasitized few nests (4\%) and showed no preference
   for clean boxes vs those containing old nests. Nest-dwelling
   ectoparasites such as feather mites and bird lice were relatively rare
   and their numbers did not differ between warbler nests constructed in
   boxes containing old nests and those in empty nest boxes.}},
