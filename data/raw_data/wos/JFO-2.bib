@article{ ISI:000226239300003,
Author = {Boland, CRJ and Phillips, RM},
Title = {{A small, lightweight, and inexpensive ``burrowscope{''} for viewing nest
   contents of tunnel-nesting birds}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2005}},
Volume = {{76}},
Number = {{1}},
Pages = {{21-26}},
Month = {{WIN}},
Abstract = {{Traditional methods of examining the nest contents of burrow-nesting
   birds have often been destructive, expensive, and/or limited to
   large-bodied species. Here we present details of a small, efficient, and
   inexpensive ``burrowscope{''} designed for examining the nest tunnels of
   very small birds. The burrowscope consists of a tiny (16 min X 22 mm)
   video camera and infrared light-emitting diodes, Mounted on the end of
   flexible conduit (20-min diameter) that houses a live video feed to a
   small, hand-held monitor display. In total, the burrowscope is only 22
   turn wide at its widest point, weighs approximately 900 g, and was built
   for less than US \$400. The burrowscope was tested in the field and
   could be Used at 99.8\% of Rainbow Bee-eater (Merops ornatus) nests and
   100\% of Spotted Pardalote (Pardalotuspunctatus) nests (one of the
   world's smallest burrow-nesting birds). The accuracy of the burrowscope
   was evaluated by excavating a nest access hole at a sub-sample of
   Rainbow Bee-eater nests. The burrowscope enabled precise determination
   Of Clutch size at 30 of 30 nests, and brood size at 125 of 127 nests.
   The equipment Could be applied to other organisms, including some
   invertebrates, reptiles, amphibians, and burrow-nesting mammals.}},
