@article{ ISI:000236520900009,
Author = {Colwell, MA and Millett, CB and Meyer, JJ and Hall, JN and Hurley, SJ
   and McAllister, SE and Transou, AN and LeValley, RR},
Title = {{Snowy Plover reproductive success in beach and river habitats}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2005}},
Volume = {{76}},
Number = {{4}},
Pages = {{373-382}},
Month = {{FAL}},
Abstract = {{Poor reproductive Success has contributed to the decline and low
   population size of the federally listed Western Snowy Plover (Charadrius
   alexandrinus nivosus), especially where it breeds on coastal beaches
   used by humans for recreation. Front 2001-2004, we compared reproductive
   Success of color-marked plovers breeding oil ocean beaches with chose on
   gravel bars of the lower Eel River in coastal northern California, one
   of six recovery units as identified by the species' recovery plan. In
   three of four years, more plovers (54-64\%) nested in river than beach
   habitats, but this pattern was reversed in the last year of the Study
   when 62\% of plovers used beaches. Each year, a higher proportion Of
   Clutches hatched and more chicks fledged from river than beach habitats,
   producing a disproportionate number of yearlings recruited into the
   local population From the river. On average, river-liesting males rended
   significantly fewer eggs, hatched similar numbers of chicks, and fledged
   significantly more young compared with males breeding oil beaches.
   Corvids were more prevalent in river habitats in two of four years, but
   beaches consistently had significantly greater human activity. These
   habitat differences in reproductive Success exist despite efforts to
   manage predators (e.g., exclosures around nests) and humans (e.g.,
   signs, Fencing, and vehicle restrictions) on beaches and almost no
   management of river habitats.}},
