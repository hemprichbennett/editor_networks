@article{ ISI:000177409100006,
Author = {Bowman, J and Wallace, MC and Ballard, WB and Brunjes, JH and Miller, MS
   and Hellman, JM},
Title = {{Evaluation of two techniques for attaching radio transmitters to turkey
   poults}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2002}},
Volume = {{73}},
Number = {{3}},
Pages = {{276-280}},
Month = {{SUM}},
Abstract = {{We conducted a pen study to evaluate two methods of attaching radio
   transmitters to Wild Turkey (Meleagris gallopavo) poults. Transmitters
   (1.8 g) were attached interscapularly to 2-d-old turkeys, either with
   glue or as implants. Attachment method had no detectable effect on the
   growth or survival of poults. Transmitter retention differed between the
   methods. Mean retention times were 27.6 d for glued backpacks and 30.5 d
   for interscapular implants. Ease of implementation and behavioral
   differences of birds immediately after application suggest that glued
   backpacks are preferred in situations where a retention time of 29 d is
   adequate.}},
