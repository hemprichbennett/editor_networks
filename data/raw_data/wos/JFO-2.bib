@article{ ISI:000225141100012,
Author = {Randler, C},
Title = {{Vigilance of Mallards in the presence of Greylag Geese}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2004}},
Volume = {{75}},
Number = {{4}},
Pages = {{404-408}},
Month = {{FAL}},
Abstract = {{I examined vigilance behaviour of Mallards (Anas platyrhynchos) feeding
   in flocks with Greylag Geese (Anser anser) on stubble-fields in northern
   Germany. I measured scans/min and total scan duration, and I calculated
   mean scan duration. There was an inverse correlation between group size
   and scanning for Mallards, but not when geese and Mallards were
   considered as a combined flock. Mallards did not benefit from the
   presence of geese in terms of reduced vigilance, and they seemed to
   adjust their vigilance according only to Mallard flock sizes. Vigilance
   was higher in the presence of geese and may have been directed towards
   geese as potential competitors. No differences in vigilance between male
   and female Mallards were found. In August, when males were in eclipse
   plumage, they showed reduced vigilance.}},
