@article{ ISI:000077748900002,
Author = {Brua, RB},
Title = {{Negative effects of patagial tags on Ruddy Ducks}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1998}},
Volume = {{69}},
Number = {{4}},
Pages = {{530-535}},
Month = {{FAL}},
Abstract = {{Patagial tags have been used in studies to identify waterfowl
   individually. I evaluated the influence of patagial tags on the behavior
   of male and female Ruddy Ducks (Oxyura jamaicensis) in Manitoba, Canada.
   Patagial-tagged male Ruddy Ducks decreased their courtship rate and
   increased the amount of time spent sleeping and preening in comparison
   to males with leg bands only and unmarked males. Compared with unmarked
   females, patagial-tagged female Ruddy Ducks increased the amount of time
   devoted to preening. I suggest that patagial tags not be used on Ruddy
   Ducks or other diving ducks.}},
