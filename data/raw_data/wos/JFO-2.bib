@article{ ISI:000072309000011,
Author = {Singleton, DR and Harper, RG},
Title = {{Bacteria in old House Wren nests}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1998}},
Volume = {{69}},
Number = {{1}},
Pages = {{71-74}},
Month = {{WIN}},
Abstract = {{Little attention has been paid to the presence of bacteria that inhabit
   old bird nests, some of which may be harmful to nestlings, adults, or
   both. In this study we documented the presence of three bacteria genera
   (Pseudomonas, Bacillus, and Staphylococcus) in old House Wren
   (Troglodytes aedon) nests. These bacteria are not usually pathogenic.
   However, we also documented the presence of Salmonella, a known
   pathogen. We hypothesize that one potential consequence of the removal
   of old nest material by male House Wrens and other cavity-nesting
   species before constructing a new nest is reduction of populations of
   potentially harmful bacteria in the nest cavity.}},
