@article{ ISI:000083070100010,
Author = {Benson, J and Suryan, RM},
Title = {{A leg-noose for capturing adult Kittiwakes at the nest}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1999}},
Volume = {{70}},
Number = {{3}},
Pages = {{393-399}},
Month = {{SUM}},
Abstract = {{We developed a leg-noose for capturing adult Black-legged Kittiwakes
   (Rissa tridactyla), a cliff-nesting, colonial seabird. The capture
   device consisted of an adjustable wire base secured to the rim of the
   nest. The base is simple to construct, and we describe three design
   options. The base held open a circular noose that was used to snare a
   kittiwake around the tarsometatarsi. A spool of line attached to the
   noose permitted the capturer to move away from the colony, encouraging
   the birds' return. Using this device, we captured 75 kittiwakes in 1996
   and 1997. In particular, the leg-noose proved invaluable in the safe
   capture and recapture of specific individuals for our study that could
   not be captured by noose-pole. This leg-noose concept is versatile and
   could be adapted for capture of other nesting avian species.}},
