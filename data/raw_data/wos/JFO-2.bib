@article{ ISI:000077748900003,
Author = {Buckley, NJ},
Title = {{Fading of numbers from patagial tags: A potential problem for long-term
   studies of vultures}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1998}},
Volume = {{69}},
Number = {{4}},
Pages = {{536-539}},
Month = {{FAL}},
Abstract = {{New World vultures cannot be banded with conventional leg bands. As a
   result, commercially available cattle eartags are commonly used to mark
   individuals in field studies. Although there is no evidence of numbers
   fading from these tags when they are used to mark cattle, I recorded at
   least four cases (3.8\% of marked birds, 25\% of resighted birds) in
   which tags became partially or completely illegible less than 3 yr after
   being used to mark Turkey Vultures (Cathartes aura). Because tag fading
   could adversely affect long-term studies of vultures, workers should be
   aware of this potential problem so that appropriate precautions can be
   taken.}},
