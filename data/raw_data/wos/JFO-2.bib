@article{ ISI:000090032100004,
Author = {Alworth, T and Scheiber, IBR},
Title = {{Nest building in House Wrens (Troglodytes aedon): A reexamination of
   male and female roles}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2000}},
Volume = {{71}},
Number = {{3}},
Pages = {{409-414}},
Month = {{SUM}},
Abstract = {{Nest building by House Wrens (Troglodytes aedon) has been described as
   occurring in two stages. Males arrive on territories before females and
   begin the first stage by building a stick foundation in a suitable
   cavity. In the second stage, females build a soft cup-like structure
   into the stick base. However, descriptive data from over 300 h of
   observation of House Wren nest construction in New York demonstrate that
   female House Wrens actually carry significantly more sticks to nest
   cavities than do males. Because females perform a behavior previously
   attributed to males, these data refocus the question of the function of
   sticks in House Wren nests.}},
