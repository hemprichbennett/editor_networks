@article{ ISI:000179816000008,
Author = {Penteriani, V and Gallardo, M and Cazassus, H},
Title = {{Conspecific density biases passive auditory surveys}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2002}},
Volume = {{73}},
Number = {{4}},
Pages = {{387-391}},
Month = {{FAL}},
Abstract = {{Surveys of vocalizing individuals are useful in detecting widely
   dispersed species that are hard to locate visually. We determined
   whether conspecific density affects spontaneous vocalizations of
   territorial Eagle Owls (Bubo bubo) and thus creates a bias in passive
   auditory surveys. We mainly listened to dusk call displays of 17 males
   breeding in two areas of high and low density from the beginning of call
   activity in October to the start of egg-laying in February, We detected
   differences in call behavior between males breeding in high density vs.
   low density. The nearest neighbor distance between breeding males
   strongly and positively affected call duration. Low density caused low
   detectability of male Eagle Owls, and later timing of the first call
   after sunset. Failing to incorporate conspecific density and nearest
   neighbor distances in surveys of territorial species may reduce the
   accuracy of population estimates.}},
