@article{ ISI:000083721300003,
Author = {McLaren, AA and Cadman, MD},
Title = {{Can novice volunteers provide credible data for bird surveys requiring
   song identification?}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1999}},
Volume = {{70}},
Number = {{4}},
Pages = {{481-490}},
Month = {{FAL}},
Abstract = {{We tested whether interested volunteers with low to moderate skill
   levels could be trained to identify and count a subset of forest birds,
   by song or call, well enough to provide credible data. Paired novice and
   experienced observers performed five simultaneous, unlimited distance
   10-min point counts of 12 (1995) or 13 (1996) target species. We found
   no differences in either counts of individual species or in the suite of
   species present between experienced-experienced pairs and
   experienced-novice pairs. Nevertheless, novices tended to count fewer
   birds of most target species than experienced observers. These
   differences were significant for Red-eyed Vireo, Rose-breasted Grosbeak
   and Scarlet Tanager in 1995 but only for Rose-breasted Grosbeak in 1996.
   Of the nine novices who participated in both years, all improved their
   pre-training ability to identify the target species (from a tape)
   between 1995 and 1996. The counts of Red-eyed Vireos (the only species
   with a sample size large enough to test) were significantly lower than
   the counts of the experienced observers in 1995 bur not in 1996. We
   conclude that trained and tested novice observers can provide credible
   data for a carefully selected subset of species.}},
