@article{ ISI:000179373700006,
Author = {Xirouchakis, S and Nikolakakis, M},
Title = {{Conservation implications of the temporal and spatial distribution of
   Bearded Vulture Gypaetus barbatus in Crete}},
Journal = {{BIRD CONSERVATION INTERNATIONAL}},
Year = {{2002}},
Volume = {{12}},
Number = {{3}},
Pages = {{269-280}},
Month = {{SEP}},
Abstract = {{We carried out a four-year (1995-1999) survey on the foraging and
   breeding distribution of Bearded Vulture Gypaetus barbatus on the island
   of Crete. The age classes of all birds sighted were systematically
   recorded and data on seasonal distribution patterns were collected; 262
   observations were made covering an area of about 4,000 km(2), at
   altitudes ranging from 200 In to 2,450 m a.s.1 (mean 1,100 m). Adult
   birds were distributed evenly throughout the island while young birds
   avoided active territories and were mostly sighted at the periphery of
   the high mountains. The species depended almost entirely on domestic
   livestock herded into and away from the mountains seasonally, with birds
   foraging in the uplands (> 1,200 m a.s.1.) from early May to late
   October and frequenting midland pastures (600-1,200 m a.s.1.) for the
   rest of the year. The mean altitude of nesting sites was 750 m (range =
   280-1,450 m), with 80\% of nests positioned below the 1,000 m contour.
   Bearded Vultures are more vulnerable to human-induced mortality within
   their breeding territories and on midland pastures in winter than in the
   upland areas in summer.}},
