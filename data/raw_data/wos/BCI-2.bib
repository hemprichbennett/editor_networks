@article{ ISI:000222307600003,
Author = {Boertmann, D and Lyngs, P and Merkel, FR and Mosbech, A},
Title = {{The significance of Southwest Greenland as winter quarters for seabirds}},
Journal = {{BIRD CONSERVATION INTERNATIONAL}},
Year = {{2004}},
Volume = {{14}},
Number = {{2}},
Pages = {{87-112}},
Month = {{JUN}},
Abstract = {{The coastal and offshore waters of Southwest Greenland are
   internationally important winter quarters for seabirds. We crudely
   estimate a minimum Of 3.5 million seabirds using the region in winter,
   mainly from Arctic Canada, Greenland and Svalbard, with smaller numbers
   also from Alaska, Iceland, mainland Norway and Russia. The most numerous
   species are Common Eider Somateria mollissima, King Eider S.
   spectabilis, Brunnich's Guillemot Uria lomvia and Little Auk Alle alle.
   The most immediate threat to the seabirds in Southwest Greenland is
   hunting, and current levels of usage of the Greenland breeding
   populations of Brunnich's Guillemot and Common Eider are considered
