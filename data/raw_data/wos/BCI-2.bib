@article{ ISI:000170345800003,
Author = {Fellowes, JR and Fang, Z and Shing, LK and Hau, BCH and Lau, MWN and
   Lam, VWY and Young, L and Hafner, H},
Title = {{Status update on White-eared Night Heron Gorsachius magnificus in South
   China}},
Journal = {{BIRD CONSERVATION INTERNATIONAL}},
Year = {{2001}},
Volume = {{11}},
Number = {{2}},
Pages = {{101-111}},
Month = {{JUN}},
Abstract = {{White-eared Night Heron Gorsachius magnificus is probably the most
   threatened heron species in the world, and the highest priority for
   heron species conservation. From 1990 to 1998, there were sightings from
   only six localities in the wild. There are none in captivity. In 1998 a
   caged bird was found in a wildlife market in the city of Nanning,
   Guangxi Zhuang Autonomous Region, People's Republic of China. This
   finding prompted a 12-month survey in 1998-1999 of both markets and
   potential habitats in Guangxi. Several captured birds provided direct
   evidence of the existence of small populations in Guangxi and Guangdong
   Provinces. The respective habitats were surveyed in spring 2000, with
   emphasis on observations at dusk. The species was seen at two locations.
   Although some of the captured birds came from highly degraded habitat,
   the best sites seemed to be in areas near extensive primary forests,
   with streams, rice fields and marshes. The information obtained will be
   used to compile a detailed Action Plan designed to prevent the
   extinction of the species.}},
