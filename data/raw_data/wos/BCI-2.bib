@article{ ISI:000082189800007,
Author = {Clayton, KM and Schmutz, JK},
Title = {{Is the decline of Burrowing Owls Speotyto cunicularia in prairie Canada
   linked to changes in Great Plains ecosystems?}},
Journal = {{BIRD CONSERVATION INTERNATIONAL}},
Year = {{1999}},
Volume = {{9}},
Number = {{2}},
Pages = {{163-185}},
Month = {{JUN}},
Abstract = {{We examined population and ecosystem patterns hoping to inform
   conservation strategies for Burrowing Owls Speotyto cunicularia in
   Canada. The owls chose moderately to heavily grazed grasslands for
   nesting and roosting, and avoided cultivated fields. Where grassland
   patches were isolated in 90\% cultivation, owls dispersed later, for
   shorter distances and less often. Mortality rate during the 5-month
   study was high (adult ratio 0.45, juveniles 0.55), which may contribute
   to local declines. Additional mortality may occur on migration and
   during winter. We extrapolate from local effects (loss of grassland
   habitat with burrows for roosting, and similar to 2\% ``incidental{''}
   predation) to suggest that such changes detrimental to the owls occur
   throughout the central Great Plains ecosystem which the owls occupy
   year-round. The changes include ecosystem processes such as plant
   succession, owl dispersal and predation. These changes are likely to be
   irreversible on a scale that would be required to help this owl. The
   species may face extinction in Canada, at the northern limit of its
   range.}},
Publisher = {{CAMBRIDGE UNIV PRESS}},
Address = {{40 WEST 20TH STREET, NEW YORK, NY 10011-4211 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Clayton, KM (Reprint Author), Powder River Eagle Studies, POB 2411, Gillette, WY 82717 USA.
   Univ Saskatchewan, Dept Biol, Saskatoon, SK S7N 5E2, Canada.}},
DOI = {{10.1017/S0959270900002288}},
ISSN = {{0959-2709}},
Keywords-Plus = {{HABITAT FRAGMENTATION; SWAINSONS HAWKS; BASIN DESERT; VEGETATION;
   DISPERSAL; PREDATION; CONSERVATION; DIVERSITY; SURVIVAL; NESTS}},
