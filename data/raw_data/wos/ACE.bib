@article{ ISI:000438899000001,
Author = {Farrell, Patrick D. and Baasch, David M. and Farnsworth, Jason M. and
   Smith, Chadwin B.},
Title = {{Interior Least Tern and Piping Plover nest and brood survival at
   managed, off-channel sites along the central Platte River, Nebraska, USA
   2001-2015}},
Journal = {{AVIAN CONSERVATION AND ECOLOGY}},
