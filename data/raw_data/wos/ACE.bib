@article{ ISI:000315376200001,
Author = {Desrochers, Andre and Tardif, Jacinthe and Mazerolle, Marc J.},
Title = {{Use of Large Clear-Cuts by Wilson's Warbler in an Eastern Canadian
   Boreal Forest}},
