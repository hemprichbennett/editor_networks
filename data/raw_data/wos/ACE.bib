@article{ ISI:000208195200004,
Author = {Dunn, Erica H. and Hobson, Keith A. and Wassenaar, Len I. and Hussell,
   David J. T. and Allen, Martha L.},
Title = {{Identification of Summer Origins of Songbirds Migrating through Southern
   Canada in Autumn}},
