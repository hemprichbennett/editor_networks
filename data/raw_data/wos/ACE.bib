@article{ ISI:000357622700002,
Author = {Toms, Judith D. and Villard, Marc-Andre},
Title = {{Threshold detection: matching statistical methodology to ecological
   questions and conservation planning objectives}},
Journal = {{AVIAN CONSERVATION AND ECOLOGY}},
Year = {{2015}},
Volume = {{10}},
Number = {{1}},
Month = {{JUN}},
Abstract = {{Two types of ecological thresholds are now being widely used to develop
   conservation targets: breakpoint-based thresholds represent tipping
   points where system properties change dramatically, whereas
   classification thresholds identify groups of data points with
   contrasting properties. Both breakpoint-based and classification
   thresholds are useful tools in evidence-based conservation. However, it
   is critical that the type of threshold to be estimated corresponds with
   the question of interest and that appropriate statistical procedures are
   used to determine its location. On the basis of their statistical
   properties, we recommend using piecewise regression methods to identify
   breakpoint-based thresholds and discriminant analysis or classification
   and regression trees to identify classification thresholds.}},
