@article{ ISI:000208195400004,
Author = {Rock, Jennifer C. and Leonard, Marty L. and Boyne, Andrew W.},
Title = {{Foraging Habitat and Chick Diets of Roseate Tern, Sterna dougallii,
   Breeding on Country Island, Nova Scotia}},
Journal = {{AVIAN CONSERVATION AND ECOLOGY}},
