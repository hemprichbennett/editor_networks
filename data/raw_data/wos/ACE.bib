@article{ ISI:000208195600003,
Author = {DeVink, Jean-Michel A. and Clark, Robert G. and Slattery, Stuart M. and
   Scheuhammer, Anton M.},
Title = {{Cross-Seasonal Association Between Winter Trophic Status and Breeding
   Ground Selenium Levels in Boreal White-Winged Scoters}},
Journal = {{AVIAN CONSERVATION AND ECOLOGY}},
Year = {{2008}},
Volume = {{3}},
Number = {{1}},
Month = {{JUN}},
Abstract = {{The effect of cross-seasonal interactions on reproduction and fitness in
   migratory species is of increasing interest to ecologists, particularly
   because of the conservation implications of habitat change. Variation in
   contaminant levels that can affect reproduction in migratory species may
   reflect differing exposure across seasons. We examined the relationship
   between concentrations of liver selenium, a known teratogen, and winter
   trophic level in breeding White-winged Scoters (Melanitta fusca) using
   claw delta N-15 values as an index of winter trophic level. Claw delta
   N-15 was a significant predictor of variation in breeding ground
   selenium levels (r = 0.32), and liver selenium increased by
   approximately 12 +/- 5 SE mu g.g(-1) with one trophic level increase in
   delta N-15 (Delta 3 parts per thousand). This relationship demonstrates
   that contaminant exposure from wintering or staging areas can result in
   higher levels in birds on breeding grounds, where some contaminants are
   more likely to have impacts.}},
