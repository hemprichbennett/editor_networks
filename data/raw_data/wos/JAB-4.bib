@article{ ISI:A1994PY99700012,
Author = {RAMOS, JA},
Title = {{FERN FROND FEEDING BY THE AZORES BULLFINCH}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1994}},
Volume = {{25}},
Number = {{4}},
Pages = {{344-347}},
Month = {{NOV}},
Abstract = {{Fern feeding is rare amongst birds.  I describe the fern frond feeding
   behaviour of the Azores Bullfinch Pyrrhula murina.  This bird takes fern
   fronds when other foods, seeds and sporangia, are scarce and is highly
   selective within and between species.  It is suggested that variations
   in chemistry within fern species, involving nutrients and secondary
   compounds, may cause this behaviour.}},
