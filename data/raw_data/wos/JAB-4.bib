@article{ ISI:000166802000012,
Author = {Bradley, JS and Cox, JM and Nicholson, LW and Wooller, RD and Hamer, KC
   and Hill, JK},
Title = {{Parental influence upon the provisioning schedules of nestling
   Short-tailed Shearwaters Puffinus tenuirostris}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{2000}},
Volume = {{31}},
Number = {{4}},
Pages = {{522-526}},
Month = {{DEC}},
Abstract = {{The single young of shearwaters grows slowly and is fed at long, often
   highly variable, intervals. We monitored the growth patterns of nestling
   Short-tailed Shearwaters Puffinus tenuirostris exchanged between nests
   in order to assess whether the patterns of parental visits or the
   demands of their young determined the rate of food provisioning in this
   species. These cross-fostering trials indicate a strong parental
   influence upon the rate of food delivery, but did not exclude the
   possibility of a minor influence from chicks.}},
