@article{ ISI:A1997XJ15200011,
Author = {Hoglund, J and Stohr, S},
Title = {{A non-lekking population of black grouse Tetrao tetrix}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1997}},
Volume = {{28}},
Number = {{2}},
Pages = {{184-187}},
Month = {{JUN}},
Abstract = {{In a two-year study of an area in central Sweden a population of Black
   Grouse Tetrao tetrix was found in which displaying males were not
   clustered on leks. The study area which covered approximately 44 km(2)
   contained 19 and 30-35 displaying males in 1995 and 1996, respectively.
   In one of the years two males were seen together on a lek situated on a
   sphagnum bog next to a lake. All others were separated by at least 50 m
   at any given time. The mean nearest neighbour distance between males
   (excluding the two lekking ones) was 1.1 and 0.6 km in the two Sears,
   respectively, The spacing of the displaying males described here, most
   closely resembles that of the non-lekking North American Spruce Grouse
   Dendragopus obscurus. Possible causes (population density, habitat
   structure, age structure and a genetic difference) of the described
   intra-specific variation in spacing (and hence mating system) of the
   Black Grouse are discussed. It is concluded that a low population
   density and shortage of suitable display sites are the most relevant
   factors in explaining why this particular population does not assemble
   on leks.}},
