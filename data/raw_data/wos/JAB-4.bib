@article{ ISI:A1997XJ15200012,
Author = {Winkel, W and Hudde, H},
Title = {{Long-term trends in reproductive traits of tits (Parus major,
   P-caeruleus) and pied flycatchers Ficedula hypoleuca}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1997}},
Volume = {{28}},
Number = {{2}},
Pages = {{187-190}},
Month = {{JUN}},
Abstract = {{Between 1970 and 1995, Great and Blue Tits in northern Germany started
   breeding progressively earlier. Also Pied Flycatchers showed a
   significant trend towards earlier breeding, and in this species clutch
   size and number of fledged young increased significantly from 1970 to
   1995. In all three species the fluctuations in annual mean hatching
   dates were significantly correlated with the mean average spring
   temperatures. The recorded time trends in breeding correspond with
   increasing March and April temperatures from 1970 to 1995.}},
