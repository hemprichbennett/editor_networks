@article{ ISI:A1996VQ62800013,
Author = {Hillgarth, N},
Title = {{Ectoparasite transfer during mating in ring-necked pheasants Phasianus
   colchicus}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1996}},
Volume = {{27}},
Number = {{3}},
Pages = {{260-262}},
Month = {{SEP}},
Abstract = {{There are three models of parasite-mediated sexual selection by female
   choice. Females may get (1) indirect, genetic gains; (2) direct,
   parental care gains and (3) direct contagion avoidance gains. Little is
   known about frequency and mechanisms of parasite transfer during mating.
   In an experiment, 20 male pheasants had lice removed. One week later
   sticky tape was placed on their legs and they were then allowed to mate
   with Mallophaga-infested females. A significant number of males were
   infested with lice within two seconds. These results support the
   contagion avoidance hypothesis, while not discounting other models.}},
