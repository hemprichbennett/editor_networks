@article{ ISI:000088488200006,
Author = {Fessl, B and Hoi, H},
Title = {{Song complexity and song structure in the Moustached Warbler
   Acrocephalus melanopogon}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{2000}},
Volume = {{31}},
Number = {{2}},
Pages = {{144-150}},
Month = {{JUN}},
Abstract = {{We examined different song parameters leading to a complex song
   configuration, and song output (production), and their relation in male
   Moustached Warblers and discuss them with reference to female choice.
   With more than 120 different syllables per two minutes of song, male
   Moustached Warblers can be regarded as one of the most complex singers
   within the genus Acrocephalus. We found significant differences between
   males for almost all song parameters investigated (repertoire size,
   switching and repetition rate, song speed and strophe length) but not
   for time spent singing per 20 minutes. This individual variation
   probably is an important cue for female mate choice. Male Moustached
   Warblers achieve song complexity mainly via frequent syllable switching.
   Song complexity seems to be consistent throughout the song and our
   results suggest that females could assess the overall song complexity of
   a singer by examining even very short song bouts. Furthermore, a high
   proportion of motifs - clusters of syllables with the same sequence -
   occurs within the song of each male which would also Facilitate
   assessment of song complexity by females. However, repertoire size and
   strophe length were positively related, thus it remains to be
   investigated which song feature, if any, females actually use.}},
Publisher = {{MUNKSGAARD INT PUBL LTD}},
Address = {{35 NORRE SOGADE, PO BOX 2148, DK-1016 COPENHAGEN, DENMARK}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Fessl, B (Reprint Author), Inst Comparat Ethol, Savoyenstr 1A, A-1160 Vienna, Austria.
   Inst Comparat Ethol, A-1160 Vienna, Austria.}},
DOI = {{10.1034/j.1600-048X.2000.310206.x}},
ISSN = {{0908-8857}},
Keywords-Plus = {{GREAT REED WARBLER; RED-WINGED BLACKBIRDS; GENUS ACROCEPHALUS; PIED
