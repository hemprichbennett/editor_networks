@article{ ISI:A1996UR57200005,
Author = {Chilton, G and Lein, MR},
Title = {{Song repertoires of puget sound White-crowned Sparrows Zonotrichia
   leucophrys pugetensis}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1996}},
Volume = {{27}},
Number = {{1}},
Pages = {{31-40}},
Month = {{MAR}},
Abstract = {{Of 233 adult male White-crowned Sparrows Zonotrichia leucophrys
   pugetensis recorded in British Columbia and Washington, 25 (10.7\%) were
   found to have repertoires of two or three song types. This proportion is
   higher than any reported previously for this species. Within
   repertoires, most song type differences were minor, but some songs
   differed in theme and dialect type. Individuals sang few renditions of
   one song type before switching to the other. We believe that the songs
   in an individual's repertoire have the same meaning, and that multi-song
   repertoires provide no selective advantage over a single song type in
   this species.}},
