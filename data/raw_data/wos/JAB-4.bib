@article{ ISI:A1996UR57200013,
Author = {Cullen, NJ and Heinsohn, R and Cockburn, A},
Title = {{Food supplementation induces provisioning of young in cooperatively
   breeding White-winged Choughs}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1996}},
Volume = {{27}},
Number = {{1}},
Pages = {{92-94}},
Month = {{MAR}},
Abstract = {{Cooperatively breeding White-winged Choughs have an extremely long
   period of post fledgling care, they will provision young for up to eight
   months. During winter, however, provisioning ceases despite evidence
   that young continue to have difficulty finding food themselves. A
   provisioning experiment was designed to determine whether food
   availability played a role in the termination of care. Groups of
   White-winged Choughs were observed before, during, and after food was
   supplied in both small and large amounts. The supply of large amounts of
   food switched on provisioning behaviour in both breeders and helpers,
   and begging behaviour in juveniles. Helping behaviour is therefore
   influenced by the costs of supplying food.}},
