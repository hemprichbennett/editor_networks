@article{ ISI:A1995RJ42700012,
Author = {WILSON, RP},
Title = {{A DEVICE FOR MEASURING SEABIRD ACTIVITY AT SEA}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1995}},
Volume = {{26}},
Number = {{2}},
Pages = {{172-175}},
Month = {{JUN}},
Abstract = {{A miniature foot-attached water temperature logger for deployment on
   large seabirds is described. This unit was tested on 5 breeding
   Wandering Albatrosses and indicated flight or resting on water
   behaviours with a resolution to within a few seconds. Device-fitted
   birds were also equipped with satellite transmitters for determination
   of position. Combination of data on water temperature from the logger
   together with position from the transmitter enabled us to examine sea
   surface temperatures in the albatross foraging areas.}},
