@article{ ISI:A1996VQ62800001,
Author = {Sandberg, R and Gudmundsson, GA},
Title = {{Orientation cage experiments with Dunlins during autumn migration in
   Iceland}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1996}},
Volume = {{27}},
Number = {{3}},
Pages = {{183-188}},
Month = {{SEP}},
Abstract = {{Migratory orientation of Dunlins Calidris alpina at sunset was studied
   by using orientation cages, during autumn migration in SW Iceland. The
   aim of the experiments was to examine whether orientation funnels can be
   used to record the directional behaviour of shorebirds. The results
   clearly demonstrate that Dunlins are capable of expressing their
   preferred migratory direction in funnels. Unmanipulated Dunlins showed a
   well-oriented mean direction towards geographical SSE which is in
   excellent agreement with available ringing recovery data on Dunlins
   linked to Iceland. This opens up the possibility to perform carefully
   controlled experiments to investigate the orientation and navigation
   mechanisms of migrating waders.}},
