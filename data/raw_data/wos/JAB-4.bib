@article{ ISI:000074304200014,
Author = {Olsen, P and Barry, S and Baker, GB and Mooney, N and Cam, G and Cam, A},
Title = {{Assortative mating in falcons: do big females pair with big males?}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1998}},
Volume = {{29}},
Number = {{2}},
Pages = {{197-200}},
Month = {{JUN}},
Abstract = {{Several raptors have been shown to mate assortatively, most commonly
   according to age, less often in relation to various linear measurements.
   This paper demonstrates positive assortative mating for winglength in
   three falcons: the Peregrine Falcon Falco peregrinus, Brown Falcon F.
   berigora and Nankeen Kestrel F. cenchroides. Because females showed
   greater variation in winglength than did males, larger females formed
   more dimorphic pairs than smaller females. The results suggest a
   possible link between assortative mating and dimorphism.}},
