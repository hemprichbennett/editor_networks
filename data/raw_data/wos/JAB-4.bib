@article{ ISI:000082977500014,
Author = {Koivula, M and Viitala, J},
Title = {{Rough-legged Buzzards use vole scent marks to assess hunting areas}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1999}},
Volume = {{30}},
Number = {{3}},
Pages = {{329-332}},
Month = {{SEP}},
Abstract = {{zeVoles scent mark their territories with urine and faeces that are
   visible in ultraviolet (UV) light. Rough-legged Buzzards Buteo lagopus
   feed mainly on votes. We performed a field experiment in northern
   Finland to test whether buzzards are attracted to UV-visible vole scent
   marks. The buzzards hunted more often on plots with vole scent marks
   than on control plots. Our results indicate that Rough-Legged Buzzards
   may be able to use vote scent marks as a cue when searching for
   profitable hunting and breeding areas.}},
