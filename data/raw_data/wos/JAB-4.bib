@article{ ISI:000080855100012,
Author = {Wilson, RP and Kierspel, MAM and Scolaro, JA and Laurenti, S and Upton,
   J and Gallelli, H and Frere, E and Gandini, P},
Title = {{To think of swim: does it really cost penguins more to waddle?}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1999}},
Volume = {{30}},
Number = {{2}},
Pages = {{221-224}},
Month = {{JUN}},
Abstract = {{We looked at the routes taken by Magellanic Penguins up the beach while
   moving between the sea and the colony at a breeding site in San Julian,
   Argentina. Birds swam parallel to the shore for a period before leaving
   the water to cross the beach hut trajectories over the beach were not
   perpendicular to the water's edge but at an angle of 39 degrees. We
   examined the premise that birds might be optimizing for a trade off
   between time or energy by adopting this procedure and found that birds
   can gain little or no time by walking obliquely but can, under
   particular circumstances, save energy. These circumstances require that
   the previously-calcutated costs of transport for walking have been
   over-estimated by a factor of two.}},
