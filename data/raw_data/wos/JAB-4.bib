@article{ ISI:A1996VF15500010,
Author = {Slotow, R},
Title = {{Black coucal Centropus grillii egg volume predicts their polyandrous
   mating system}},
Journal = {{JOURNAL OF AVIAN BIOLOGY}},
Year = {{1996}},
Volume = {{27}},
Number = {{2}},
Pages = {{171-173}},
Month = {{JUN}},
Abstract = {{Polyandrous females in the Charadriiformes lay relatively small eggs,
   which is hypothesized to allow increased egg production. I examined the
   generality of this hypothesis by testing the prediction that the
   classical polyandrous Black Coucal C. grillii (Cuculiformes) will have
   relatively smaller eggs than its monogamous congenerics. Indeed,
   C.grillii did have smaller eggs than monogamous Centropus spp. In
   addition, Centropus spp, that lay larger eggs invest more in incubation
   and feeding of offspring, with C.sinensis (females both incubate eggs
   and feed young) having significantly larger eggs than predicted by body
   size alone.}},
