@article{ ISI:000440670000010,
Author = {Sun, Yanfeng and Cui, Peng and Li, Juyong and Li, Mo and Wu, Yuefeng and
   Li, Dongming},
Title = {{The Common Koel Eudynamys scolopaceus in northern China, new
   distributional information, and a brief review of range extensions of
   cuckoos in China}},
Journal = {{ORNITHOLOGICAL SCIENCE}},
Year = {{2018}},
Volume = {{17}},
Number = {{2}},
Pages = {{217-221}},
Month = {{JUL}},
Abstract = {{Climate change-mediated range shifts have been well illustrated for a
   range of avian species. Here, we report on a new distribution site of
   the Common Koel Eudynamys scolopaceus in Qinglong Manchu Autonomous
   County, Hebei Province, China. This site lies about 700 km beyond the
   previously traditional northern limit of the species' range. Based on
   publications and birding records of Cuckoos in China, we found that 8 of
   17 species had extended their known distribution ranges northward or
   westward since 1976.}},
