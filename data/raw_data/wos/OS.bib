@article{ ISI:000395099700012,
Author = {Matsui, Shin and Kasahara, Satoe and Kato, Takahiro and Izumi, Hiroe and
   Morimoto, Gen and Ueda, Keisuke and Mikami, Osamu K.},
Title = {{Badge size of male Eurasian Tree Sparrows Passer montanus correlates
   with hematocrit during the breeding season}},
Journal = {{ORNITHOLOGICAL SCIENCE}},
Year = {{2017}},
Volume = {{16}},
Number = {{1}},
Pages = {{87-91}},
Month = {{JAN}},
Abstract = {{We investigated whether the badge size of the Eurasian Tree Sparrow
   Passer montanus is correlated with morphological traits and hematocrit
   during the breeding season in order to elucidate the function of badge
   size as a potential signal of body size and physiological condition. We
   found a positive correlation in males between badge size and hematocrit,
   but not in females. As individuals with larger hematocrit values can
   transport more oxygen to their tissues or organs, male badge size may be
   linked with oxygen transport ability. We suggest that male badge size
   may be a signal of physiological status.}},
