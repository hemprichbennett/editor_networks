
@article{ ISI:000440670000001,
Author = {Hasegawa, Masaru},
Title = {{Sexual selection mechanisms for male plumage ornaments in Japanese Barn
   Swallows}},
Journal = {{ORNITHOLOGICAL SCIENCE}},
