@article{ ISI:000324668400008,
Author = {Shirai, Masaki and Niizuma, Yasuaki and Tsuchiya, Kenji and Yamamoto,
   Maki and Oka, Nariko},
Title = {{Sexual size dimorphism in Streaked Shearwaters Calonectris leucomelas}},
Journal = {{ORNITHOLOGICAL SCIENCE}},
Year = {{2013}},
Volume = {{12}},
Number = {{1}},
Pages = {{57-62}},
Month = {{JUN}},
Abstract = {{We collected fresh Streaked Shearwater Calonectris leucomelas carcasses
   and describe sex differences in external measurements following sex
   identification using reproductive organs. Males had significantly deeper
   bills, longer bills, longer heads, longer tarsi, greater wingspans and
   greater wing areas than females, but overall wing and tail lengths and
   aspect ratios did not differ between sexes. Streaked Shearwaters showed
   a greater degree of sexual dimorphism in bill size than other Puffinus
   species. Comparing the wing loadings of male and female Streaked
   Shearwaters, the mass increase required for females to have the same
   wing loading as males is 0.036 kg. Females could feed to increase their
   body mass during a foraging trip to have similar wing loading to males.}},
