@article{ ISI:000346347700017,
Author = {Bray, Timothy C. and Hockey, Phil A. R.},
Title = {{Population genetics and demographic inferences in a recovering
   shorebird, the African Black Oystercatcher Haematopus moquini}},
Journal = {{IBIS}},
Year = {{2015}},
Volume = {{157}},
Number = {{1}},
Pages = {{171-176}},
Month = {{JAN}},
Abstract = {{The extensive literature on the African Black Oystercatcher is a
   testament to what is now a conservation success story. Here we provide
   the first genetic insight into the population dynamics of this
   recovering shorebird and an assessment of genetic variation within the
   species using microsatellite markers. Although behavioural studies
   suggest strong natal philopatry, we found a single genetic cluster
   across all of the locations sampled and a significant signal of
   isolation by distance suggesting some geographical structuring. The
   microsatellite markers used in this study are useful at a population
   level, and the limited genetic variation detected is likely to be due to
   a low historical population size.}},
