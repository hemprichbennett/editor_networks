@article{ ISI:000320550900005,
Author = {Mira, Sara and Arnaud-Haond, Sophie and Palma, Luis and Cancela, Maria
   Leonor and Beja, Pedro},
Title = {{Large-scale population genetic structure in Bonelli's Eagle Aquila
   fasciata}},
