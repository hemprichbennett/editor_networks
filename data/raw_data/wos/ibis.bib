@article{ ISI:000324544200018,
Author = {van Wijk, Rien E. and Schaub, Michael and Tolkmitt, Dirk and Becker,
   Detlef and Hahn, Steffen},
Title = {{Short-distance migration of Wrynecks Jynx torquilla from Central
   European populations}},
Journal = {{IBIS}},
Year = {{2013}},
Volume = {{155}},
Number = {{4}},
Pages = {{886-890}},
Month = {{OCT}},
Abstract = {{European Wrynecks Jynx torquilla torquilla have generally been
   considered to be long-distance Palaearctic-African migrants that spend
   the non-breeding season in Sahelian Africa, where they have been
   reported regularly. Results from tracking individual birds showed that
   Wrynecks from two Central European populations migrated only relatively
   short distances to the Iberian Peninsula and northwestern Africa (c.
   1500km and 3000km, respectively), compared with a minimum distance of
   about 4500km to Sahelian Africa. Additionally, differences in wing
   lengths of populations from Central and Northern Europe support the idea
   of leap-frog migration, populations from Northern Europe being
   long-distance migrants with a non-breeding distribution in Sahelian
   Africa.}},
Publisher = {{WILEY-BLACKWELL}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
