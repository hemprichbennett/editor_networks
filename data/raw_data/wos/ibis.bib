@article{ ISI:000337568100015,
Author = {Cross, Adam D. P. and Hentati-Sundberg, Jonas and Osterblom, Henrik and
   McGill, Rona A. R. and Furness, Robert W.},
Title = {{Isotopic analysis of island House Martins Delichon urbica indicates
   marine provenance of nutrients}},
Journal = {{IBIS}},
Year = {{2014}},
Volume = {{156}},
Number = {{3}},
Pages = {{676-681}},
Month = {{JUL}},
Abstract = {{The presence of one of the largest colonies of House Martins in Europe
   on the small island of Stora Karlso, Sweden, led us to investigate the
   source of their food by analysis of stable isotopes of carbon and
   nitrogen. Carbon isotopic values of House Martin nestlings were the same
   as those of Common Guillemot Uria aalge nestlings fed on marine fish,
   but differed from local Collared Flycatcher Ficedula albicollis
   nestlings fed on woodland insects. We infer that these House Martins fed
   their chicks almost exclusively on insects that had used nutrients
   derived from seabirds, indicating a dependence on the presence of a
   large seabird colony. We suggest by extension that some populations of
   island passerines of high conservation importance may also be dependent
   on nutrient subsidies from seabird colonies.}},
