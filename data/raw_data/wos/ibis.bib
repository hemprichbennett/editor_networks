@article{ ISI:000316326600003,
Author = {Miles, Will T. S. and Parsons, Matt and Close, Andrew J. and Luxmoore,
   Richard and Furness, Robert W.},
Title = {{Predator-avoidance behaviour in a nocturnal petrel exposed to a novel
   predator}},
