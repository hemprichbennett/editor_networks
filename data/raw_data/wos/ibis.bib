@article{ ISI:000361007700018,
Author = {Ruiz-De-Castaneda, Rafael and Burtt, Jr., Edward H. and
   Gonzalez-Braojos, Sonia and Moreno, Juan},
Title = {{Bacterial degradability of white patches on primary feathers is
   associated with breeding date and parental effort in a migratory bird}},
Journal = {{IBIS}},
Year = {{2015}},
Volume = {{157}},
Number = {{4}},
Pages = {{871-876}},
Month = {{OCT}},
Abstract = {{We compared the invitro bacterial degradability of the white patch and
   melanized area of primary feathers of breeding male and female Pied
   Flycatchers Ficedula hypo-leuca and related this variation to laying
   date, brood size and brood mass. Bacterial degradability of male and
   female white feather patches, but not of melanized areas, was positively
   correlated with laying date. Male Pied Flycatchers showed a positive
   correlation between bacterial degradability of the white patch, though
   not of the melanized patch, and brood size and brood mass. Feather
   degradability appears to be negatively related to individual quality and
   positively related to reproductive effort.}},
