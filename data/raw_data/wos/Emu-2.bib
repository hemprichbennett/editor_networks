@article{ ISI:000173151300005,
Author = {Boles, WE},
Title = {{A new emu (Dromaiinae) from the Late Oligocene Etadunna Formation}},
Journal = {{EMU}},
Year = {{2001}},
Volume = {{101}},
Number = {{4}},
Pages = {{317-321}},
Abstract = {{A new emu (Emuarius guljaruba, sp. nov.) is described from the Late
   Oligocene Etadunna Formation (Ngama Local Fauna), based on a complete
   tarsometatarsus. While exhibiting evidence of cursorial abilities
   advanced over those of cassowaries (Casuarius), this taxon was not as
   cursorially adapted as the living Emu (Dromaius novaehollandiae). This
   taxon is provisionally referred to the genus Emuarius, although a
   definite generic assignment cannot be made.}},
