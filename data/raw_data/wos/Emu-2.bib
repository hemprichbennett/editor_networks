@article{ ISI:A1996WM41900003,
Author = {Mawson, PR and Massam, MC},
Title = {{Red-capped Parrot Purpureicephalus spurius: Moult, age and sex
   determination}},
Journal = {{EMU}},
Year = {{1996}},
Volume = {{96}},
Number = {{4}},
Pages = {{240-244}},
Month = {{DEC}},
Abstract = {{Descriptions of the plumage and soft parts, the timing and duration of
   moult and some morphometrics of wild-caught and captive Red-capped
   Parrots Purpureicephalus spurius were recorded so that age and sex could
   be determined. Plumage characteristics of adults and immatures were very
   different while those of males and females within age classes were very
   similar. Presence or absence of a wing-stripe allowed some, but not all,
   birds within age classes to be sexed. The sequence of flight feather
   moult was clearly defined and the period of body moult continued
   throughout the year in all age classes.}},
