@article{ ISI:A1997YF95600007,
Author = {Nicholls, DG and Murray, MD and Butcher, E and Moors, P},
Title = {{Weather systems determine the non-breeding distribution of Wandering
   Albatrosses over southern oceans}},
Journal = {{EMU}},
Year = {{1997}},
Volume = {{97}},
Number = {{3}},
Pages = {{240-244}},
Month = {{SEP}},
Abstract = {{The tracked flights of 14 non-breeding Wandering Albatrosses captured in
   Australia and New Zealand provided 3946 locations in the Tasman Sea,
   around southern Australia, and across the Indian and Pacific Oceans.
   Nearly all of the locations (98.7\%) were between 30 degrees S and 50
   degrees S, a zone where circumpolar weather systems enable albatrosses
   to fly in any direction and thus exploit fully this southern
   environment, It is also a zone where currently only the limited
   Exclusive Economic Zones offer a potential for enforceable legislative
   protection of this endangered species from the hazards of international
   longline fisheries.}},
