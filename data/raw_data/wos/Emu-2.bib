@article{ ISI:A1997YF95600006,
Author = {Brothers, NP and Reid, TA and Gales, RP},
Title = {{At-sea distribution of Shy Albatrosses Diomedea cauta cauta derived from
   records of band recoveries and colour-marked birds}},
Journal = {{EMU}},
Year = {{1997}},
Volume = {{97}},
Number = {{3}},
Pages = {{231-239}},
Month = {{SEP}},
Abstract = {{The distribution of Shy Albatrosses Diomedea cauta cauta from Australian
   colonies was studied using band recoveries and colour-marked birds.
   Adults from the colonies on Albatross Island and the Mewstone were found
   to be relatively sedentary. Birds were found to travel much more widely
   in their first five years, with birds from Albatross Island recovered
   from Western Australia, and birds from the Mewstone recovered from South
   Africa. The difference in dispersion between birds from two close
   colonies and the overlap of Shy Albatrosses at-sea with areas of
   longline fishing has important differential implications for the
   conservation of this population.}},
