@article{ ISI:A1997YF95600002,
Author = {Waugh, SM and Sagar, PM and Paull, D},
Title = {{Laying bates, breeding success and annual breeding of southern Royal
   Albatrosses Diomedea epomophora epomophora at Campbell island during
   1964-69}},
Journal = {{EMU}},
Year = {{1997}},
Volume = {{97}},
Number = {{3}},
Pages = {{194-199}},
Month = {{SEP}},
Abstract = {{Aspects of the breeding biology of Southern Royal Albatrosses Diomedea
   epomophora epomophora were studied at Campbell Island during four
   breeding seasons, 1964 to 1969. Laying and hatching dates for D, e.
   epomophora on Campbell Island were 17-22 d later than those reported for
   Northern Royal Albatrosses D. e. sanfordi at Taiaroa Head. Although
   recapture rates were low most birds followed a biennial breeding pattern
   typical of the great albatrosses. Notable exceptions were two pairs
   which bred in consecutive years after raising chicks to fledging in the
   first year. Breeding success approximated long-term averages found for
   Wandering Albatrosses D. exulans. D. e. epomophora differed from D.
   exulans in having higher rates of chick failure when overall breeding
   success was low. Incubation period was approximately similar to that
   reported for D. e. sanfordi and mate fidelity was high, as with most
   petrel species.}},
Publisher = {{ROYAL AUSTRALASIAN ORNITHOL UN}},
Address = {{21 GLADSTONE ST, MOONEE PONDS VICTORIA 3039, AUSTRALIA}},
