@article{ ISI:000169376500002,
Author = {Maller, CJ and Jones, DN},
Title = {{Vocal behaviour of the Common Keel, Eudynamys scolopacea, and
   implications for mating systems}},
Journal = {{EMU}},
Year = {{2001}},
Volume = {{101}},
Number = {{2}},
Pages = {{105-112}},
Abstract = {{The Common Keel, Eudynamys scolopacea, is a migratory cuckoo that occurs
   along the eastern coast of Australia. Its most conspicuous feature is
   the loud and persistent calling of males heard throughout the breeding
   season. Although the commonest vocalisation of the Keel, the Cooee call,
   is well known, other vocalisations have not been described in detail.
   Here we report on six vocalisations produced by adult birds, including
   duetting. The rate of calling of male Keels was investigated on two
   different scales: monthly over the breeding season, and hourly during
   the day and night. Calling peaked both in the early and late months of
   the breeding season, possibly associated with the availability of host
   nests. During the day, calling rates were significantly higher at dawn
   and dusk than at all other times of the day. Data collected on trees
   inhabited by Keels showed that calling males favoured trees with a
   particularly wide canopy and dense foliage; most often, these were
   species of Ficus. We speculate that the Keel shares with other cuckoos a
   basic breeding pattern based on male dominance interactions that lead to
   polygyny but that the discovery of duetting strongly suggests the
   possibility of short-term pair-bonding within this mating system.}},
