@article{ ISI:000185866900012,
Author = {Reid, T and Park, P},
Title = {{Continuing decline of Eastern Curlew, Numenius madagascariensis, in
   Tasmania}},
Journal = {{EMU}},
Year = {{2003}},
Volume = {{103}},
Number = {{3}},
Pages = {{279-283}},
Abstract = {{Regular counts of Eastern Curlew, Numenius madagascariensis, in Tasmania
   indicate that they have declined by over 65\% in Tasmania since the
   1950s. The declines have been noted at many of the sites regularly used
   in Tasmania, and there is no indication of the birds moving to areas
   interstate. The reasons for the decline are not known; however, they may
   include habitat destruction and disturbance in Tasmania or on the
   migration routes, or hunting pressure in their breeding grounds or
   migration routes. There is an urgent need to protect the species'
   roosting and foraging grounds in Tasmania and throughout its migration
   routes.}},
