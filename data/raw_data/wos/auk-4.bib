@article{ ISI:000221824600022,
Author = {Villard, P and Cuisin, J},
Title = {{How do woodpeckers extract grubs with their tongues? A study of the
   Guadeloupe Woodpecker (Melanerpes herminieri) in the French West Indies}},
Journal = {{AUK}},
Year = {{2004}},
Volume = {{121}},
Number = {{2}},
Pages = {{509-514}},
Month = {{APR}},
Abstract = {{We held an adult Guadeloupe Woodpecker (Melanerpes herminieri) in
   captivity for two weeks and documented its tongue movements as it caught
   grubs placed into holes that had been excavated out of a log. We provide
   evidence that the Guadeloupe Woodpecker does not spear grubs with its
   tongue but instead grabs them with the tongue's horny tip, which is
   barbed and coated with saliva, and pulls them out of the holes.}},
