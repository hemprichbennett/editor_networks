@article{ ISI:000169466700031,
Author = {Delestrade, A},
Title = {{Sexual size dimorphism and positive assortative mating in Alpine Choughs
   (Pyrrhocorax graculus)}},
Journal = {{AUK}},
Year = {{2001}},
Volume = {{118}},
Number = {{2}},
Pages = {{553-556}},
Month = {{APR}},
Abstract = {{The degree of sexual size dimorphism in a number of different
   morphological characters was examined in a social corvid, the Alpine
   Chough, using measurements taken on 178 males and 144 females. A small
   amount of size dimorphism appeared in all morphological characters, and
   weight was the most dimorphic character. To identify ii Alpine Choughs
   mate assortatively, measurements of mates were compared in 76 pairs. A
   positive assortative mating was found on tarsus length, and a small
   positive trend is suggested between body condition of partners, but that
   needs to be confirmed with a larger sample size.}},
