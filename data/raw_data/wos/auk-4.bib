@article{ ISI:000221824600025,
Author = {Puglisi, L and Adamo, C},
Title = {{Discrimination of individual voices in male Great Bitterns (Botaurus
   stellaris) in Italy}},
Journal = {{AUK}},
Year = {{2004}},
Volume = {{121}},
Number = {{2}},
Pages = {{541-547}},
Month = {{APR}},
Abstract = {{Recorded vocalizations from 18 known Great Bitterns (Botaurus stellaris)
   were analyzed, both quantitatively and qualitatively, to determine the
   amount of intra- and inter-individual variation. All parameters examined
   showed significant variation among males, and vocalizations of the same
   male were not stable over time. Variability within males over time makes
   re-identification of individuals on the basis of vocalizations
   difficult. The high degree of uncertainty in recognizing individuals
   limits the usefulness of vocalizations for censusing of bitterns.}},
