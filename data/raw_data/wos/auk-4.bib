@article{ ISI:000175709400020,
Author = {Roulin, A},
Title = {{Offspring desertion by double-brooded female Barn owls (Tyto alba)}},
Journal = {{AUK}},
Year = {{2002}},
Volume = {{119}},
Number = {{2}},
Pages = {{515-519}},
Month = {{APR}},
Abstract = {{In double-brooded species, the sex that contributes less to breeding
   success may desert offspring before the end of the rearing period to
   start a new breeding attempt with another partner. I evaluated that
   prediction in the Barn Owl (Tyto alba), a species in which the male
   feeds the brood on average twice as often as the female. Among birds
   that produced a second brood, 46\% of females and 4\% of males deserted
   their offspring before completion of parental duties to remate with
   another partner at a distance of 1 to 10 km. Offspring desertion did not
   appear to incur a reproductive cost to the deserter, because deserting
   females at the first nest produced a similar number of offspring as
   nondeserting ones.}},
