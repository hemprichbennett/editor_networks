@article{ ISI:000186338400013,
Author = {Mack, AL and Jones, J},
Title = {{Low-frequency vocalizations by cassowaries (Casuarius spp.)}},
Journal = {{AUK}},
Year = {{2003}},
Volume = {{120}},
Number = {{4}},
Pages = {{1062-1068}},
Month = {{OCT}},
Abstract = {{Although some birds can detect wavelengths in the infrasound range,
   there has been little evidence that birds produce very low frequencies.
   We made nine recordings of a captive Dwarf Cassowary (Casuarius
   bennetti) and one recording of a wild Southern Cassowary (C. casuarius)
   near Crater Mountain, Papua New Guinea. Both species produced sounds
   near the floor of the human hearing range in their pulsed booming notes:
   down to 32 Hz for C. casuarius and 23 Hz in C. bennetti. Recordings of
   C. bennetti indicate four levels of harmonics with the 23 Hz fundamental
   frequency. Such low frequencies are probably ideal for communication
   among widely dispersed, solitary cassowaries in dense rainforest. The
   discovery of very low-frequency communication by cassowaries creates new
   possibilities for studying those extremely secretive birds and for
   learning more about the evolution of avian vocalizations. Received 17
   July 2002, accepted 14 June 2003.}},
