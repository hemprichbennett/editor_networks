@article{ ISI:000171105000018,
Author = {Ardia, DR and Bildstein, KL},
Title = {{Sex-related differences in habitat use in wintering American Kestrels}},
Journal = {{AUK}},
Year = {{2001}},
Volume = {{118}},
Number = {{3}},
Pages = {{746-750}},
Month = {{JUL}},
Abstract = {{We investigated sex-related differences in habitat use in wintering
   American Kestrels (Falco sparverius) at two scales: within a 10 rn
   radius and within a 100 in radius of perch sites. Female kestrels used
   areas containing a higher percentage of short vegetation (<0.25 m high)
   suitable for foraging than did males at both scales (100 in radius
   females 80\%, males 69\%; 10 rn radius females 80\%, males 73\%). At
   both scales, females had more pasture (a high-quality foraging
   substrate) available than did males; areas within a 100 m radius of male
   perch sites contained more woodlot than did female perch sites. Logistic
   regression models indicated greater overlap between male and female
   habitat use on a 10 rn radius scale than on a 100 m radius scale,
   suggesting that males may preferentially select smaller areas devoid of
   woody vegetation relative to what is available within 100 in radius of
   perch sites. Our results suggest that males may be constrained to winter
   in areas with lower overall foraging opportunities and possibly higher
   predation risk than areas used by females. Our work supports the
   hypothesis that males and female kestrels prefer open areas as wintering
   habitat.}},
