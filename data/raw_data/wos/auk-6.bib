@article{ ISI:A1994NM64100017,
Author = {HANEY, JC and LEE, DS},
Title = {{AIR-SEA HEAT-FLUX, OCEAN WIND FIELDS, AND OFFSHORE DISPERSAL OF GULLS}},
Journal = {{AUK}},
