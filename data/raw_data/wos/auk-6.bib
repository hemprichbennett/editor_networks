@article{ ISI:A1994RX85600013,
Author = {MIYAMOTO, MM and ALLARD, MW and MORENO, JA},
Title = {{CONSERVATION GENETICS OF THE PLAIN PIGEON (COLUMBA-INORNATA) IN
   PUERTO-RICO}},
Journal = {{AUK}},
Year = {{1994}},
Volume = {{111}},
Number = {{4}},
Pages = {{910-916}},
Month = {{OCT}},
Abstract = {{Over the last five to eight years, the Puerto Rico Department of Natural
   Resources, U.S. Fish and Wildlife Service, and University of Puerto Rico
   have maintained a breeding and recovery program for the Plain Pigeon
   (Columba inornata), which is declining throughout its range. To assist
   this ongoing effort, variability in nuclear DNA and mitochondrial DNA
   (in its control region) was quantified for the 20 surviving founders of
   this program by DNA fingerprinting and polymerase-chain-reaction/dideoxy
   sequencing, respectively. Compared to unrelated individuals of other
   birds, the DNA data indicate that a low level of genetic variation
   exists among the founders, among their captive-bred descendants, and in
   their source population. This conclusion is evaluated against the recent
   history of the species in Puerto Rico and then in terms of its
   importance to the ongoing recovery program.}},
