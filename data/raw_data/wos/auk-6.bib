@article{ ISI:A1995TX68300003,
Author = {Keast, A and Pearce, L and Saunders, S},
Title = {{How convergent is the American Redstart (Setophaga ruticilla, Parulinae)
   with flycatchers (Tyrannidae) in morphology and feeding behavior?}},
Journal = {{AUK}},
Year = {{1995}},
Volume = {{112}},
Number = {{2}},
Pages = {{310-325}},
Month = {{APR}},
Abstract = {{Possible convergence between the unique aerial-feeding American Redstart
   (Setophaga ruticilla) and the tyrannid flycatchers was considered using
   skeleton, external morphology, substrate use, and locomotory feeding
   movements. The redstart is a typical paruline in the broader functional
   units of forelimb, hindlimb, and body axis, as well as in proportionate
   lengths of the main axial elements of both the forelimb and hindlimb. It
   retains and utilizes the advantageous features of the wood-warblers,
   including the capacity to hop rapidly through the vegetation. To this it
   has added the long and broad bill, and long rictal bristes (adaptations
   for aerial feeding) of the tyrannids. The wing shape remains
   wood-warblerlike, but it is convergent with the flycatchers in its low
   wing loading. The tail is long, as in the flycatchers, for better aerial
   control, but is spread and, apparently, used to flush insects from
   vegetation, as in the specialized Australo-Papuan fantails (Rhipidura).
