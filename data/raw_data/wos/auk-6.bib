@article{ ISI:A1993MC01700007,
Author = {OSORIOBERISTAIN, M and DRUMMOND, H},
Title = {{NATAL DISPERSAL AND DEFERRED BREEDING IN THE BLUE-FOOTED BOOBY}},
Journal = {{AUK}},
Year = {{1993}},
Volume = {{110}},
Number = {{2}},
Pages = {{234-239}},
Month = {{APR}},
Abstract = {{Male Blue-footed Boobies (Sula nebouxii) first nested at an age of two
   to six years (xBAR = 4.7), and females a little earlier at one to six
   years (xBAR = 3.6). Both sexes nested selectively close to the natal
   site in their first reproduction (males 24.1 m, females 28.3 m) and
   similarly close to the natal site in their second reproduction, despite
   displacement from first to second site. First sites of both sexes were
   similarly close to the natal site and the parents' current site, and
   parents dispersed only 26.2 m between hatching and first reproduction of
   their offspring. Throughout their lives, both sexes may tend to nest
   close to the natal site and, hence, close to kin. Nonetheless, given
   high nest density in the colony (0.0356 nests/m2) and high variance in
   natal dispersal (males, SD = 33.1 m; females, SD = 35.2 m), inbreeding
   and social interactions with kin are unlikely to result. Overall, these
   results do not support the hypothesis that philopatry in some marine
   birds and other animals functions to achieve an optimal balance between
   inbreeding and outbreeding.}},
