@article{ ISI:000187405600012,
Author = {Hay, JM and Evans, PR and Ward, RM and Hamer, KC},
Title = {{Poor nutritional condition as a consequence of high dominance status in
   the Coal Tit Parus ater}},
Journal = {{IBIS}},
Year = {{2004}},
Volume = {{146}},
Number = {{1}},
Pages = {{103-107}},
Month = {{JAN}},
Abstract = {{The costs and benefits of dominance status have been investigated in the
   past and it has generally been reported that subdominant birds are at a
   nutritional disadvantage owing to their low dominance status. The
   nutritional condition of birds during winter can be important in
   determining their likelihood of survival. This is particularly so in
   small passerines that are sensitive to severe weather conditions.
   Ptilochronology is an accurate method used to produce a long-term
   estimate of body condition spanning the number of days that it takes to
   grow a new feather. Ptilochronology was used during this study to
   estimate the nutritional condition of Coal Tits Parus ater during one
   winter and how condition was affected by dominance status. Dominant Coal
