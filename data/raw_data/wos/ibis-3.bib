@article{ ISI:000225188900021,
Author = {Aebischer, NJ and Ewald, JA},
Title = {{Managing the UK Grey Partridge Perdix perdix recovery: population
   change, reproduction, habitat and shooting}},
Journal = {{IBIS}},
Year = {{2004}},
Volume = {{146}},
