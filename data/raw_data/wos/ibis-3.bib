@article{ ISI:000234739700005,
Author = {Krams, I and Krama, T and Igaune, K},
Title = {{Mobbing behaviour: reciprocity-based co-operation in breeding Pied
   Flycatchers Ficedula hypoleuca}},
Journal = {{IBIS}},
Year = {{2006}},
Volume = {{148}},
Number = {{1}},
Pages = {{50-54}},
Month = {{JAN}},
Abstract = {{Recent non-experimental evidence suggests that reciprocal altruism may
   be more common in nature than was previously thought. Here we present
   experimental evidence for mobbing behaviour as reciprocal altruism in
   breeding Pied Flycatchers Ficedula hypoleuca, a small migratory
   passerine. Pied Flycatchers attended mobs initiated by their
   co-operating neighbours. However, they did not join in mobbing initiated
   by their conspecific neighbours when not assisted in the test an hour
   before. The results suggest that birds followed a `tit-for-tat-like'
   strategy and that responses of neighbouring Flycatchers may be related
   to reciprocal altruism.}},
