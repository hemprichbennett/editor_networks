@article{ ISI:000243474600009,
Author = {Kraaijeveld, Ken and Ming, Ma and Komdeur, Jan and Mulder, Raoul A.},
Title = {{Offspring sex ratios in relation to mutual ornamentation and extra-pair
   paternity in the Black Swan Cygnus atratus}},
Journal = {{IBIS}},
Year = {{2007}},
Volume = {{149}},
Number = {{1}},
Pages = {{79-85}},
Month = {{JAN}},
Abstract = {{In sexually dichromatic birds, females may adaptively adjust the sex
   ratio of their offspring prior to hatching in relation to male
   ornamentation, for example, by producing more sons when paired to a
   highly attractive partner. However, to our knowledge no studies have
   investigated offspring sex ratio modification in species in which both
   sexes are ornamented, and it is unknown whether such a process would be
   adaptive. Here we examine variation in offspring sex ratio in the
   mutually ornamented Black Swan Cygnus atratus. Brood sex ratio was not
   related to the degree of ornament elaboration in either parent, or to
   extra-pair paternity. We suggest that parental attractiveness may not be
   inherited in a sex-linked manner, or may be largely non-heritable. Thus,
   females may not benefit from biasing the sex ratio of their offspring in
   relation to parental attractiveness.}},
