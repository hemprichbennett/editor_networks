@article{ ISI:000250011800004,
Author = {Edwards, Darryl B. and Forbes, Mark R.},
Title = {{Absence of protandry in the spring migration of a population of Song
   Sparrows Melospiza melodia}},
Journal = {{IBIS}},
Year = {{2007}},
Volume = {{149}},
Number = {{4}},
Pages = {{715-720}},
Month = {{OCT}},
Abstract = {{Early male arrival at breeding sites, or protandry, is thought to have
   evolved from intrasexual competition among males for access to mates or
   breeding resources. Males of polygynous species tend to be larger than
   females and have exaggerated secondary sexual traits. Additionally, such
   species show a high degree of protandry, suggesting that timing of
   arrival is sexually selected. Species showing limited sexual dimorphism
   and showing sexual monochromatism may be expected to show limited early
   male arrival. However, there are very few studies of migration timing of
   the sexes in such species because individuals cannot be readily
   identified to sex in the hand. In this study, we genetically sexed birds
   and found no evidence for early male arrival, for a population of
   migratory Song Sparrows Melospiza melodia. For our study population,
   males and females display limited sexual size dimorphism and are
   sexually monochromatic which is characteristic of the species. Fat
   scores for males were inversely associated with timing of arrival,
   whereas for females, larger-winged birds arrived sooner - suggesting
   that early migration timing may be selected for in both sexes.}},
