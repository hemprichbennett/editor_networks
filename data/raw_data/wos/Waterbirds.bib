@article{ ISI:000395960800007,
Author = {Craig, David P. and Larson, Keith},
Title = {{Migratory Connectivity of North American Caspian Tern (Hydroprogne
   caspia) Populations}},
Journal = {{WATERBIRDS}},
Year = {{2017}},
Volume = {{40}},
Number = {{1}},
Pages = {{58-62}},
Month = {{MAR}},
Abstract = {{Migratory connectivity of Caspian Terns (Hydroprogne caspia) was
   investigated using individuals marked in North America between 1922 and
   2015. The results support recent genetic work describing three breeding
   regions (Pacific, Great Lakes, and Central Canada). Further., our
   results show strong migratory connectivity of Pacific breeders to
   wintering regions in central and western Mexico, and connectivity of
   Great Lakes breeders to wintering regions in the Gulf of Mexico and
   Caribbean. This integrated picture of demographic connectivity
   throughout North America should help in planning more effective
   management of the species.}},
