@article{ ISI:000372377000001,
Author = {Gonzalez, Alieny and Jimenez, Ariam and Mugica, Lourdes and Acosta,
   Martin and Garcia-Lau, Ianela and Castro, Rodolfo and Lopez, Manuel and
