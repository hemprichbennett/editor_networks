@article{ ISI:000356642700012,
Author = {Ceron, Gerardo},
Title = {{Torrent Ducks (Merganetta armata) Diving and Feeding in Hot Springs}},
Journal = {{WATERBIRDS}},
Year = {{2015}},
Volume = {{38}},
Number = {{2}},
Pages = {{214-219}},
Month = {{JUN}},
Abstract = {{Opportunistic behavior in birds can enhance the ability of a species to
   expand their distribution. Currently, there is no documentation of any
   bird diving and feeding in hot springs with temperatures greater than 30
   degrees C. A pair of Torrent Ducks (Merganetta armata) feeding in a hot
   spring with temperatures between 36-50 degrees C is described. The diet
   and food availability in this hot spring was studied in a Torrent Duck
   territory. While the sector of stream with hot waters represents only
   13\% of the territory of the Torrent Duck pair, prey collected there
   represented 30\% of their diet. This is an extreme example of how this
   highly specialized species can also use a wide variety of environments.}},
