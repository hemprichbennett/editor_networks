@article{ ISI:000396646100012,
Author = {Heath, Susan A. and Dahlgren, Sabrina and Simon, Dana and Brooks, Daniel
   M.},
Title = {{Monofilament Fishing Line as a Threat to American Oystercatchers
   (Haematopus palliatus) on the Texas Coast, USA}},
Journal = {{WATERBIRDS}},
Year = {{2017}},
Volume = {{40}},
Number = {{1}},
Pages = {{123-126}},
Month = {{FEB}},
Abstract = {{Over the course of a 6-year study of American Oystercatchers (Haematopus
   palliatus), five cases of entanglement in monofilament fishing line were
   encountered. Two cases resulted in confirmed death, one case resulted in
   probable death, and two cases resulted in survival but required human
   intervention. Approximately 2-4\% of the study population was
   represented in these events, which is cause for alarm as monofilament
   fishing line is a hazard both to American Oystercatchers and other
   species. This report emphasizes the need for careful responsibility by
   recreational users of fishing tackle, educational programs to stress
   this need, and signage to reinforce this message. Received 16 June 2016,
   accepted 11 July 2016.}},
