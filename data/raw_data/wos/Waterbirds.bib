@article{ ISI:000361930800012,
Author = {Founier, Auriel M. V. and Shieldcastle, Mark C. and Kashmer, Tom and
   Mylecraine, Kristin A.},
Title = {{Comparison of Arrival Dates of Rail Migration in the Southwest Lake Erie
   Marshes, Ohio, USA}},
Journal = {{WATERBIRDS}},
Year = {{2015}},
Volume = {{38}},
Number = {{3}},
Pages = {{312-314}},
Month = {{SEP}},
Abstract = {{Several studies have documented the arrival time of spring migration of
   Virginia Rails (Rallus limicola), King Rails (R. elegans), and Soras
   (Porzana carolina) on the southwestern shore of Lake Erie, though not in
   recent decades, and most of this information is based on anecdotal
   records. These three species were captured in wetlands on Ottawa
   National Wildlife Refuge in Ottawa and Lucas Counties, Ohio, USA, from
   2004 to 2009. Virginia Rails and Soras were documented arriving in
   northern Ohio earlier than previous research, but not older anecdotal
   records King Rails were within the bounds of all previous records. Using
   traps with playback may have allowed us to detect these species at
   earlier dates than previous research. Documenting current spring
   migration arrival timing of these three secretive marsh bird species is
   important for future monitoring, research and wetland management.}},
