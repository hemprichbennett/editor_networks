@article{ ISI:000339132400013,
Author = {Levengood, Jeffrey M. and Smith, Randolph V. and Gay, David A. and
   Davis, Mark A. and Stafford, Joshua D.},
Title = {{Mercury in Migrating Shorebirds in the Illinois River Valley}},
Journal = {{WATERBIRDS}},
Year = {{2014}},
Volume = {{37}},
Number = {{2}},
Pages = {{225-229}},
Month = {{JUN}},
Abstract = {{Hepatic mercury concentrations of Pectoral Sandpiper (Calidris
   melanotos) and Killdeer (Charadrius vociferus collected from the central
   Illinois River Valley, Illinois, during early fall migration in 2007
   were examined. Mean and median mercury concentrations were significantly
   greater in the livers of Pectoral Sandpipers (0.30 and 0.28 mu g/g wet
   weight, respectively) than in Killdeer (0.07 and 0.06 mu g/g wet weight,
   respectively). Differences in foraging ecology between these species
   likely account for the observed differences in tissue mercury
   concentrations. In general, mercury concentrations were low in these
   species relative to other shorebirds and can serve as baseline for
   future monitoring.}},
