@article{ ISI:000379882900025,
Author = {Robertson, Gregory J. and Fife, Danielle T. and Mallory, Mark L. and
   Calvert, Anna M.},
Title = {{Survival of Large Gulls Breeding in Eastern Newfoundland, Canada}},
