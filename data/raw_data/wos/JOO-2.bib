@article{ ISI:000337792500029,
Author = {Hartmann, Stefanie A. and Schaefer, H. Martin and Segelbacher, Gernot},
Title = {{Development of 12 microsatellite loci for the endangered Pale-headed
   Brushfinch (Atlapetes pallidiceps) and their cross-amplification in two
   co-occurring Brushfinches}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2014}},
Volume = {{155}},
Number = {{3}},
Pages = {{835-839}},
Month = {{JUL}},
Abstract = {{Using 454 pyro-sequencing, we established a microsatellite dataset of 12
   polymorphic markers for the endangered Pale-headed Brushfinch (Atlapetes
   pallidiceps). Within 24 individuals, mean number of alleles ranged from
   2 to 13, observed heterozygosity from 0.17 to 1.0, expected
   heterozygosity from 0.16 to 0.91, and polymorphic information content
   from 0.15 to 0.88. Additionally, we tested our marker set for
   cross-amplification in two related brushfinch species. Whereas all 12
   markers amplified and were polymorphic in A. latinuchus, only 5 of the
   11 markers amplified variable loci in Buarremon torquatus. This panel of
   microsatellites will allow a comparison between the genetic diversity of
   the threatened Atlapetes pallidiceps and two co-occurring non-endangered
   brushfinch species.}},
