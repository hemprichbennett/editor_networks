@article{ ISI:000323929100029,
Author = {Bauch, Christina and Wellbrock, Arndt H. J. and Nagel, Rolf and Rozman,
   Jan and Witte, Klaudia},
Title = {{``Bug-eggs{''} for Common Swifts and other small birds:
   minimally-invasive and stress-free blood sampling during incubation}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2013}},
Volume = {{154}},
Number = {{2}},
Pages = {{581-585}},
Month = {{APR}},
Abstract = {{The use of haematophagous bugs (Triatominae) for minimally-invasive
   blood sampling has increasingly gained interest. We developed a small
   bug-containing dummy egg (size: 25 x 19 {[}mm]) for stress-free blood
   sampling during incubation in Common Swifts (Apus apus) and potentially
   even smaller bird species. Our design expands on an application of a
   method previously used successfully on larger birds. In our study, 40 \%
   of blood-sampling attempts were successful. Success was highest in the
   early breeding season, higher at noon than later in the day and
   unaffected by nest infestation with ectoparasitic louse-flies
   (Crataerina pallida). We recommend this method for blood-sampling birds
   without trapping during the sensitive period of incubation and encourage
   its application in small bird species.}},
