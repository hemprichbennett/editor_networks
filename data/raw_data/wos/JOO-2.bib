@article{ ISI:000308823100025,
Author = {Thorup, Kasper and Ortvad, Troels Eske and Holland, Richard A. and
   Rabol, Jorgen and Kristensen, Mikkel Willemoes and Wikelski, Martin},
Title = {{Orientation of vagrant birds on the Faroe Islands in the Atlantic Ocean}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{153}},
Number = {{4}},
Pages = {{1261-1265}},
Month = {{OCT}},
Abstract = {{Migratory birds are frequently found far outside their normal range but
   the phenomenon is poorly understood. We used radio telemetry to track
   individual migratory flights of several species of songbirds on the
   Faroe Islands, far west of their normal migration route. Birds with
   expected easterly and south-easterly migration direction departed
   westwards out over the Atlantic Ocean, indicating that these birds are
   actively flying in the ``wrong{''} direction and that their occurrence
   is not caused by wind drift. This is in contrast to the apparently
   normal south-westerly to easterly departure directions in birds expected
   to migrate south or southwest.}},
