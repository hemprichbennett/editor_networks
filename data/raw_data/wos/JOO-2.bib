@article{ ISI:000309538300020,
Author = {Conn, Paul B. and Cooch, Evan G. and Caley, Peter},
Title = {{Accounting for detection probability when estimating force-of-infection
   from animal encounter data}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{152}},
Number = {{2}},
Pages = {{S511-S520}},
