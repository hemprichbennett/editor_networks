@article{ ISI:000305232700039,
Author = {Wellbrock, Arndt H. J. and Bauch, Christina and Rozman, Jan and Witte,
   Klaudia},
Title = {{Buccal swabs as a reliable source of DNA for sexing young and adult
   Common Swifts (Apus apus)}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{153}},
Number = {{3}},
Pages = {{991-994}},
Month = {{JUL}},
Abstract = {{We investigated the reliability of buccal swabs for molecular sex
   determination in very young nestlings (0-14 days old) and adults of the
   Common Swift (Apus apus) compared to the results from blood samples of
   the same individuals. Sex determination based on buccal swabs matched
   the result of sex determination based on blood samples in 46 out of 47
   nestlings (98 \%) and in all 10 adults (100 \%). Therefore, we consider
   that buccal swab sampling is a reliable noninvasive method to obtain DNA
   for sex determination in swifts. We recommend buccal swabbing as an
   alternative to blood sampling in future genetic studies in birds.}},
