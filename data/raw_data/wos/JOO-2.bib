@article{ ISI:000310548700024,
Author = {Tsutsui, Kazuyoshi and Haraguchi, Shogo and Inoue, Kazuhiko and
   Miyabara, Hitomi and Suzuki, Saori and Ubuka, Takayoshi},
Title = {{Control of circadian activity of birds by the interaction of melatonin
   with 7 alpha-hydroxypregnenolone, a newly discovered neurosteroid
   stimulating locomotion}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{153}},
Number = {{1}},
Pages = {{S235-S243}},
Month = {{AUG}},
Note = {{25th International Ornithological Congress (IOC), Campos do Jordao,
   BRAZIL, AUG 22-28, 2010}},
Abstract = {{Melatonin regulates diurnal locomotor rhythms in birds as well as in
   other vertebrates, but the molecular mechanism by which melatonin
   regulates locomotor activity is poorly understood. Here, we summarize
   new findings showing that 7 alpha-hydroxypregnenolone, a previously
   undescribed avian neurosteroid, mediates melatonin action on diurnal
   locomotor rhythms in birds. Recently, 7 alpha-hydroxypregnenolone was
   identified as a novel avian neurosteroid in Japanese quail Coturnix
   japonica brain. It was found that 7 alpha-hydroxypregnenolone acutely
   stimulates quail locomotor activity. Subsequently, it was clarified that
   diurnal changes in 7 alpha-hydroxypregnenolone synthesis occur in
   parallel with changes in locomotor activity in quail. Finally, it was
   demonstrated that melatonin depresses the synthesis of 7
   alpha-hydroxypregnenolone, thus providing a mechanism through which the
   nocturnal increase in melatonin regulates diurnal changes in locomotor
   activity. This review highlights a novel molecular mechanism controlling
   circadian activity of birds by the interaction of melatonin with 7
   alpha-hydroxypregnenolone, a newly discovered neurosteroid stimulating
   locomotion.}},
