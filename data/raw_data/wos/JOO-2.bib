@article{ ISI:000310548700003,
Author = {Macedo, Regina H. and Manica, Lilian and Dias, Raphael I.},
Title = {{Conspicuous sexual signals in a socially monogamous passerine: the case
   of neotropical Blue-black Grassquits}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{153}},
Number = {{1}},
Pages = {{S15-S22}},
