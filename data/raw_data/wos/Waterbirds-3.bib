@article{ ISI:000174440400009,
Author = {Bustnes, JO and Erikstad, KE and Bjorn, TH},
Title = {{Body condition and brood abandonment in common eiders breeding in the
   high Arctic}},
Journal = {{WATERBIRDS}},
Year = {{2002}},
Volume = {{25}},
Number = {{1}},
Pages = {{63-66}},
Month = {{MAR}},
Abstract = {{Post-hatch brood amalgamation is common in many waterfowl species and
   especially prevalent in the Common Eider (Somateria mollissima). Studies
   in the temperate region have shown that brood abandonment is influenced
   both by female body condition and brood size. In this study we tested
   how these two factors influenced the probability of brood desertion in
   ciders breeding under harsh conditions in the high Arctic (78degreesN).
   We found that females in poor body condition were less likely, to care
   for their young, but there was no effects of brood size. We speculate
   that the differences between southern and northern populations of ciders
   may be due to different environmental conditions. Received 10 October
   2001. accepted 16 November 2001.}},
