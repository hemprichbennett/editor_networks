@article{ ISI:000172510400021,
Author = {Campos, F and Lekuona, JM},
Title = {{Are rice fields a suitable foraging habitat for Purple Herons during the
   breeding season?}},
Journal = {{WATERBIRDS}},
Year = {{2001}},
Volume = {{24}},
Number = {{3}},
Pages = {{450-452}},
Month = {{DEC}},
Abstract = {{Adult Purple Herons (Ardea purpurea) were observed while foraging along
   rivers and rice fields during the breeding season. Proportion of
   foraging time was higher in rice fields than on rivers and the
   relationship between biomass ingested in relation to foraging time was
   greater on rivers. The success of feeding attempts was similar in both
   habitats. Most Purple Herons preferred to forage along rivers but rice
   fields may be a valuable option.}},
