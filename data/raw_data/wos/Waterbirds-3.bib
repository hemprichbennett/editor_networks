@article{ ISI:000166519000024,
Author = {Butler, RW and Vennesland, RG},
Title = {{Integrating climate change and predation risk with wading bird
   conservation research in North America}},
Journal = {{WATERBIRDS}},
Year = {{2000}},
Volume = {{23}},
Number = {{3}},
Pages = {{535-540}},
Abstract = {{In the next century, wading birds will have to deal with the cumulative
   impact of climate change, human population growth, and the recovery of
   avian predators. Conservation areas, established under former climate
   conditions and when there were fewer people and predators, might be
   inadequate to maintain wading bird populations in the future. We assess
   the potential effect of climate change and increased predation, and
   provide hypotheses as a framework for future work in these areas.}},
