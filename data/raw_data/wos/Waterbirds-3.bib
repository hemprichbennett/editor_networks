@article{ ISI:000083508100017,
Author = {Tsipoura, N and Burger, J},
Title = {{Shorebirds and surf clams: An unusual interaction}},
Journal = {{WATERBIRDS}},
Year = {{1999}},
Volume = {{22}},
Number = {{1}},
Pages = {{140-141}},
Abstract = {{After a severe storm in 1996 along the Atlantic Ocean share of New
   Jersey that washed up large numbers of surf clams (Spisula solidissima)
   onto the bench, we found 39 Dunlin (Calidris alpina) with bills caught
   between the two valves of the clams. There was a flock of about 3,000
   dunlin actively feeding on the clams along the 6.5 km of beach censused.
   This interaction of surf clams with shorebirds has not been described
   before, but the clams may be an important food source as well as a
   mortality factor for these birds.}},
Publisher = {{WATERBIRD SOC}},
Address = {{NATL MUSEUM NATURAL HISTORY SMITHSONIAN INST, WASHINGTON, DC 20560 USA}},
