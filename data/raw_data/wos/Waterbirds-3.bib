@article{ ISI:000174440400006,
Author = {Tobin, ME and King, DT and Dorr, BS and Werner, SJ and Reinhold, DS},
Title = {{Effect of roost harassment on Cormorant movements and roosting in the
   delta region of Mississippi}},
Journal = {{WATERBIRDS}},
Year = {{2002}},
Volume = {{25}},
Number = {{1}},
Pages = {{44-51}},
Month = {{MAR}},
Abstract = {{Double-crested Cormorant (Phalacvocorax auritis) predation on Channel
   Catfish (Ictalurus punctatus) at commercial farms has been estimated to
   cost the Mississippi aquaculture industry approximately \$5 million
   annually to replace consumed fingerlings. In 1997. catfish producers
   assumed responsibility for the dispersal of corininants in flight roosts
   in Life eastern if (interior) delta region of Mississippi, where catfish
   tariffs are concentrated. We documented movements of 50 cormorants
   marked with radio transmitters in the delta region from january through
   March 1997. We obtained 161 post-capture day locations and 176
   post-capture flight roost locations. Cormorants that were harassed at
   their night roost flew farther to their next day's location than birds
   that were not harassed thee previous flight. Of the cormorants for which
   we had more than one flight roost location, only, 11\% of cormorants
   that were harassed returned to the same roost within 48 hours. compared
   with an 81\% return of cormorants to a previonsly un-harassed night
   roost. Moreover, cormorants in the eastern portion of the delta (where
   all harassment was conducted) changed night roosts more frequently than
   cormorants in the western (non-harassed) delta. Since cormorants ill our
   study foraged relatively close to their flight roosts and only 11\% of
   the birds that we observed roosting in the western delta traveled to the
   eastern delta to forage the following clav. coordinated and intensive
   dispersal of cormorants from the interior delta may, temporarily, limit
   cormorant impacts to Mississippi aquaculture. Received 10 July 2001,
   accepted 17,September 2001.}},
