@article{ ISI:000180160100039,
Author = {Lumsden, HG},
Title = {{Laying and incubation behavior of captive Trumpeter Swans}},
Journal = {{WATERBIRDS}},
Year = {{2002}},
Volume = {{25}},
Number = {{1}},
Pages = {{293-295}},
Note = {{4th International Swan Symposium/18th Trumpeter-Swan-Society Conference,
   WARRENTON, VIRGINIA, FEB 13-18, 2001}},
Organization = {{Trumpeteer Swan Soc}},
Abstract = {{Trumpeter Swans (Cygnus buccinator) do not develop a brood patch as do
   most other species of waterfowl. Observations of three pairs of captive
   breeding Trumpeter Swans, which were uncharacteristically
   non-aggressive, showed that they incubated with their feet on top of the
   eggs. In addition, two instances of egg-laying were witnessed at close
   range and are described here. First view of the egg in the cloaca to
   final emergence took just over two minutes. Trumpeter Swans lay very
   small eggs in proportion to their body size, and their large feet can
   theoretically cover 5.7 eggs. It is Suggested that Trumpeter Swans are
   able to regulate blood flow to their feet and provide effective
   incubation.}},
