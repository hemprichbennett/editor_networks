@article{ ISI:000172510400019,
Author = {Mori, Y},
Title = {{Individual diving behavior, food availability and chick growth rates in
   Chinstrap Penguins}},
Journal = {{WATERBIRDS}},
Year = {{2001}},
Volume = {{24}},
Number = {{3}},
Pages = {{443-445}},
Month = {{DEC}},
Abstract = {{The relationship between foraging effort and chick growth rate was
   investigated in two Chinstrap Penguins (Pygoscelis antartica), fitted
   with time-depth recorders and whose diving behavior differed from each
   other. Foraging effort did not differ between the individuals, but the
   growth rates of their chicks differed. Although these birds bred at the
   same colony and at the same time, food available to the parents differed
   because of the depth at which they fed and this possibly was reflected
   in the growth of the chicks. Food availability depends on the abundance
   of potential Food and also on the feeding behavior of individuals.}},
