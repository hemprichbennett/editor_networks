@article{ ISI:000224562700013,
Author = {Bolduc, F and Afton, AD},
Title = {{Relationships between wintering waterbirds and invertebrates, sediments
   and hydrology of coastal marsh ponds}},
Journal = {{WATERBIRDS}},
Year = {{2004}},
Volume = {{27}},
Number = {{3}},
Pages = {{333-341}},
Month = {{SEP}},
Abstract = {{We studied relationships among sediment variables (carbon content, C:N,
   hardness, oxygen penetration, silt-clay fraction), hydrologic variables
   (dissolved oxygen, salinity, temperature, transparency, water depth),
   sizes and biomass of common invertebrate classes, and densities of 15
   common waterbird species in ponds of impounded freshwater, oligohaline,
   mesohaline, and unimpounded mesohaline marshes during winters 1997-98 to
   1999-2000 on Rockefeller State Wildlife Refuge, Louisiana, USA.
   Canonical correspondence analysis and forward selection was used to
   analyze the above variables. Water depth and oxygen penetration were the
   variables that best segregated habitat characteristics that resulted in
   maximum densities of common waterbird species. Most common waterbird
   species were associated with specific marsh types, except Green-winged
   Teal (Anas crecca) and Northern Shoveler (Anas clypeala). We concluded
   that hydrologic manipulation of marsh ponds is the best way to manage
   habitats for these birds, if the hydrology can be controlled adequately.}},
