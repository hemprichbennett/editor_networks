@article{ ISI:000180160100016,
Author = {Vilina, YA and Cofre, HL and Silva-Garcia, C and Garcia, MD and
   Perez-Friedenthal, C},
Title = {{Effects of El Nino on abundance and breeding of Black-necked Swans on El
   Yali wetland in Chile}},
Journal = {{WATERBIRDS}},
Year = {{2002}},
Volume = {{25}},
Number = {{1}},
Pages = {{123-127}},
Note = {{4th International Swan Symposium/18th Trumpeter-Swan-Society Conference,
   WARRENTON, VA, FEB 13-18, 2001}},
Organization = {{Trumpeteer Swan Soc}},
Abstract = {{The El Nino Southern Oscillation (ENSO) phenomenon has frequently been
   associated with negative effects on bird populations. Here we analyze
   the effect of increased rainfall associated with an El Nino event on
   Black-necked Swans (Cygnus melancoryphus) at El Yali National Reserve,
   an important coastal wetland of the Mediterranean region of Chile. The
   Black-necked Swan is a regular winter visitor to the El Yali wetland
   but, during and after El Nino years, it increases markedly in number,
   stays longer (throughout the summer) and breeds. During and after El
   Nino years, El Yali has more swans and higher breeding productivity than
   at other known breeding sites in central Chile. Consequently, after El
   Nino events, the El Yali wetland may be a source from which swans
   colonize other wetlands in the Mediterranean and Austral regions of
   Chile.}},
