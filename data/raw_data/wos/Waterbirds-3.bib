@article{ ISI:000179535700015,
Author = {Nuechterlein, GL and Buitron, D},
Title = {{Nocturnal egg neglect and prolonged incubation in the Red-necked Grebe}},
Journal = {{WATERBIRDS}},
Year = {{2002}},
Volume = {{25}},
Number = {{4}},
Pages = {{485-491}},
Month = {{DEC}},
Abstract = {{Miniaturized temperature loggers placed in Red-necked Grebe (Podiceps
   grisegina) nests revealed that 16 of the 32 monitored pairs abandoned
   their nests for extended periods lasting 3-9 h during at least two of
   the three nights that they were monitored. When incubating pairs left
   their nests unattended, temperatures of the thermistored egg usually
   dropped precipitously to below 30degreesC within 15-25 min. Duration of
   a pair's incubation period was positively associated with the number of
   minutes that the thermistored egg was below 30degreesC in our 3-day
   samples, yet nesting success remained high. Embryos appeared to be
   tolerant of this cooling; 26 of the 32 monitored nests hatched young,
   and there was no detectable increase in the failure of eggs to hatch for
   pairs neglecting their clutches more than 3 h per night. We suggest
   Red-necked Grebes preemptively leave their nests at night to avoid
   predation on their eggs or themselves.}},
