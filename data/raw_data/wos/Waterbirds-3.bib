@article{ ISI:000180160100022,
Author = {Collins, R},
Title = {{Sex differences in the movements and mortality of Mute Swans}},
Journal = {{WATERBIRDS}},
Year = {{2002}},
Volume = {{25}},
Number = {{1}},
Pages = {{157-161}},
Note = {{4th International Swan Symposium/18th Trumpeter-Swan-Society Conference,
   WARRENTON, VA, FEB 13-18, 2001}},
Organization = {{Trumpeteer Swan Soc}},
Abstract = {{Mute Swan (Cygnus olor) movements were examined in relation to sex.
   Movement patterns differed between the sexes, with females making longer
   movements, on average during the study, than mates (46.3 km compared to
   40.1 km). Females also moved further from their place of banding than
   males (46.9 knit compared to 39.8 kin). Although males appeared to
   undertake more frequent movements, it is likely that the frequency of
   movements by females was under-estimated since females moved further and
   a higher percentage of females than males went missing at all ages.
   Collisions with fixed obstacles accounted for at least 30\% of all
   recorded deaths and 57\% of mortality where the cause of death was
   known.}},
