@article{ ISI:000176179900017,
Author = {Ochoa-Acuna, H and Montevecchi, WA},
Title = {{Basal metabolic rate of adult Leach's Storm-petrels during incubation}},
Journal = {{WATERBIRDS}},
Year = {{2002}},
Volume = {{25}},
Number = {{2}},
Pages = {{249-252}},
Month = {{JUN}},
Abstract = {{We evaluated the basal metabolic rate (BMR), thermal conductance (C-m),
   and lower critical temperature (T-lc) of Leach's Storm-petrel
   (Oceanodroma leucorhoa) using open-flow respirometry. Measurements were
   taken from six adult collected at their nests at Great Island, Witless
   Bay Ecological Reserve, Ne Newfoundland, Canada. In order to establish
   T, and C,,,, we determined rates of oxygen consumption after exposing
   birds to different ambient temperatures (10degreesC, 20degreesC,
   26degreesC, and 30degreesC). Storm-petrels had a minimal C-m of 0.122
   +/- 0.019 ml O-2/g.degreesC.h and T-lc was 24degreesC. The basal rate of
   oxygen consumption was 1.85 +/- 0.24 ml O-2/.hg. Incubating Leach's
   Storm-petrels had a basal rate of metabolism lower than previously
   reported for this species. Possible explanations for this discrepancy
   include the use of different techniques to measure metabolic rates, and
   the physiological state of the birds being measured. In this respect,
   our results Coupled with those from Previous Studies, suggest decreased
   metabolic rates in incubating Procellariformes compared to those of
   non-reproducing birds measured at the same temperature. This decline in
   metabolism is possibly related to metabolic depression due to fasting.}},
