@article{ ISI:000180160100032,
Author = {Babcock, CA and Fowler, AC and Ely, CR},
Title = {{Nesting ecology of Tundra Swans on the coastal Yukon-Kuskokwim Delta,
   Alaska}},
Journal = {{WATERBIRDS}},
Year = {{2002}},
Volume = {{25}},
Number = {{1}},
