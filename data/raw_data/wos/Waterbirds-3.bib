@article{ ISI:000083508100019,
Author = {Huppop, O},
Title = {{Rubber bands as a simple tool to detect foraging ranges of gulls}},
Journal = {{WATERBIRDS}},
Year = {{1999}},
Volume = {{22}},
Number = {{1}},
Pages = {{145-147}},
Abstract = {{Rubber pieces of different shapes were baked in dough and spread at four
   localities around a mixed breeding colony of Mew (Larus canus) and
   Herring (L. argentatus) gulls on the lower River Elbe, northern Germany.
   Of 100 rubber pieces, 23 were detected later in four Mew Gull pellets
   found in the colony The method is recommended as a means of studying the
   foraging ranges and potential feeding localities of gulls when trapping
   and marking of the birds is not possible (e.g. before incubation) or is
   likely to after their natural behavior.}},
