@article{ ISI:000177616500007,
Author = {Boertmann, D and Mosbech, A},
Title = {{Molting Harlequin Ducks in Greenland}},
Journal = {{WATERBIRDS}},
Year = {{2002}},
Volume = {{25}},
Number = {{3}},
Pages = {{326-332}},
Month = {{SEP}},
Abstract = {{An aerial survey of post-breeding molting Harlequin Ducks (Histrionicus
   histrionicus) was carried out in late July 1999 along the southern half
   of the West Greenland coast. About 3,500 birds were located, which at
   this time of the year almost exclusively are males. Based on this
   observation a total of 5,000-10,000 Harlequin Duck males were estimated
   to molt in West Greenland. About 98\% of the surveyed birds were found
   in the southern half of the study area. This distribution pattern may be
   linked to the presence of Atlantic Canadian males, which probably
   account for an appreciable proportion of the molting males in Greenland.}},
