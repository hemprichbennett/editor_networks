@article{ ISI:000304351100005,
Author = {Buij, R.},
Title = {{Pallid Harrier Circus macrourus bird hunting behaviour and capture
   success in northern Cameroon}},
Journal = {{OSTRICH}},
Year = {{2012}},
Volume = {{83}},
Number = {{1}},
Pages = {{27-32}},
Abstract = {{Detailed observations were made of Pallid Harrier Circus macrourus
   behaviour when hunting birds, the bird prey species and bird capture
   success in northern Cameroon. Four hunting strategies are described to
   capture birds: fast contour flight, overt approach with rapid
   acceleration, covert ambush, and stoop from flight. Capture success of
   149 bird capture attempts by 47 Pallid Harriers was 18.8\%, but it
   differed with hunting strategy used. Covert ambush and overt approach
   strategies were more successful compared to fast contour flight.
   Red-billed Quelea was the most commonly targeted and captured bird prey.}},
