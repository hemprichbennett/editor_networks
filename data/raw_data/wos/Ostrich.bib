Title = {{Ecomorphology and foraging height in the Malagasy endemic Velvet Asity
   (Philepitta castanea)}},
Journal = {{OSTRICH}},
Year = {{2007}},
Volume = {{78}},
Number = {{2}},
Pages = {{305-307}},
