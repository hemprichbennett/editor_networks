@article{ ISI:000329161200003,
Author = {Makhado, Azwianewi B. and Crawford, Robert J. M. and Waller, Lauren J.
   and Underhill, Les G.},
Title = {{An assessment of the impact of predation by Cape fur seals Arctocephalus
   pusillus pusillus on seabirds at Dyer Island, South Africa}},
Journal = {{OSTRICH}},
Year = {{2013}},
Volume = {{84}},
Number = {{3}},
Pages = {{191-198}},
Abstract = {{At Dyer Island, South Africa, observations of predation of seabirds by
   Cape Fur Seals Arctocephalus pusillus pusillus were made during 2004 and
   2006/07. It was estimated that seals killed about 7\% of adult African
   Penguins Spheniscus demersus annually. This may have contributed to the
   penguin colony not increasing in spite of food becoming more available
   in its vicinity during an eastward shift of epipelagic fish resources
   off South Africa at the start of the twenty-first century. Adult
   penguins were most susceptible to mortality in the breeding season as
   they returned to feed chicks in the evening. Seals also killed
   substantial numbers (3-9\%) of Cape Cormorant Phalacrocorax capensis
   fledglings as they left the island, mostly in the morning. The losses of
   Cape Cormorant fledglings accounted for about 95\% of the overall
   mortality of seabirds attributable to seals. Mortality inflicted by
   seals on adults of four species of cormorant was negligible and no
   predation by seals on gulls and terns was observed.}},
