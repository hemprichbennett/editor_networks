@article{ ISI:000248669100003,
Author = {Louette, Michel and Urban, Emil K.},
Title = {{Overview of the eleven Pan-African Ornithological Congresses}},
Journal = {{OSTRICH}},
Year = {{2007}},
Volume = {{78}},
Number = {{2}},
Pages = {{XIII-XXIV}},
Month = {{JUN}},
Note = {{11th Pan-African Ornitological Congress, Assoc Amis Oiseaux, Djerba,
   TUNISIA, NOV 20-25, 2004}},
Organization = {{Arnaud Bechet Tour Valat; Assoc Amis Oiseaux}},
Abstract = {{This paper gives an overview of all Pan-African Ornithological
   Congresses (PAOCs) held through to 2004. Comments are made on topics
   covered in each Congress in terms of science, conservation and stimulus
   for ornithology in Africa. Comments are also made on how the PAOCs have
   become truly Pan-African, with Congresses held in all major regions of
   Africa including anglophone and francophone nations. Other topics
   covered include: the nation of origin of participants, including
   indigenous Africans (0\% in 1957 in the first Congress to more than 60\%
   in 2004); organisations involved (the South African Ornithological
   Society in the beginning and later the International Council for Bird
   Preservation/BirdLife International and the African Bird Club); and
   important events such as the formation and election of the PAOC
   Committee, the preparation and approval of the PAOC constitution,
   including elections of officers beginning in 1988, and the continued
   validity and use of the PAOC constitution. Among the topics covered in
   the Congresses, conservation was especially important, starting with
   PAOC 1, and it became increasingly so in subsequent Congresses.}},
