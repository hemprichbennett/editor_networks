@article{ ISI:000354571500021,
Author = {Ryan, Peter G. and Dilley, Ben J. and Jones, Christopher and Bond,
   Alexander L.},
Title = {{Blue Petrels Halobaena caerulea discovered breeding on Gough Island}},
Journal = {{OSTRICH}},
Year = {{2015}},
Volume = {{86}},
Number = {{1-2, SI}},
Pages = {{193-194}},
Month = {{MAY}},
Abstract = {{The Blue Petrel Halobaena caerulea breeds at five subantarctic island
   groups and at islands off southern Chile in a narrow latitudinal band
   from 47 degrees to 56 degrees S on either side of the Antarctic Polar
   Front. We found a colony on Gough Island (40 degrees S, 10 degrees W),
   central South Atlantic Ocean, more than 700 km north of its known
   breeding range. Breeding appears to take place later than at colonies
   farther south. Although the colony is in a fairly frequently visited
   part of the island, it might have been overlooked rather than
   representing a recent range extension.}},
