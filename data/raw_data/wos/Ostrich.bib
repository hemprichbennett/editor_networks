@article{ ISI:000387783000013,
Author = {Oswald, Krista N.},
Title = {{Observed instances of alarm calling in the Cape Rockjumper}},
Journal = {{OSTRICH}},
Year = {{2016}},
Volume = {{87}},
Number = {{3}},
Pages = {{285-288}},
Abstract = {{Here I give an account of alarm calls recorded during behavioural
   observations on Cape Rockjumpers (CRJs) and the potential predators
   observed nearby. I found CRJs did not produce alarm calls in the
   presence of Boomslang (a previously known predator of CRJ nestlings),
   whereas they did produce alarm calls in the presence of Rock Kestrels,
   which was unexpected as previous literature suggested Rock Kestrels do
   not prey upon CRJ. However, during field observations I witnessed a Rock
   Kestrel attack an adult CRJ, suggesting that Rock Kestrels may
   opportunistically prey upon CRJs. Further studies on site-and
   temporal-specificity in CRJs may lead to additional understanding of the
   flexibility of alarm calling in group-living species.}},
