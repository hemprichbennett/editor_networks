@article{ ISI:000299427600007,
Author = {Hanane, Saad},
Title = {{Breeding ecology of Kentish Plovers Charadrius alexandrinus in rocky and
   sandy habitats of north-west Morocco (North Africa)}},
Journal = {{OSTRICH}},
Year = {{2011}},
Volume = {{82}},
Number = {{3}},
Pages = {{217-223}},
Month = {{DEC}},
Abstract = {{On the north-west coast of Morocco, egg laying by Kentish Plovers
   Charadrius alexandrinus occurred from late March to early June, with
   peaks in early April and mid May. Nests in rocky habitat were
   significantly closer to each other than were those in sandy habitat.
   Higher human disturbance was evident in sandy habitat, whereas tidal
   flooding had an important impact on nests in rocky habitat. The Moroccan
   Kentish Plover population has similar breeding parameters and faces
   comparable threats to those known from populations on the northern
   border of the Mediterranean Sea and at other localities in southern
   Europe.}},
