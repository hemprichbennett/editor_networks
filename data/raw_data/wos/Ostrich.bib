@article{ ISI:000326502100012,
Author = {Ryan, Peter G. and Rose, Barrie and Carter, Michael and Clarke, Rohan H.},
Title = {{A review of Jouanin's Petrel records in the eastern Indian Ocean}},
Journal = {{OSTRICH}},
Year = {{2013}},
Volume = {{84}},
Number = {{2}},
Pages = {{161-164}},
Abstract = {{Jouanin's Petrel Bulweria fallax mainly occurs in the western Indian
   Ocean. Prior to our study there were only two records from east of 82
   degrees E. We show that small numbers of Jouanin's Petrels are regular
   visitors to the eastern Indian Ocean, occurring to 15 degrees N 90
   degrees E in the Bay of Bengal and 15 degrees S 123 degrees E off
   northern Australia. Although seasonal coverage is limited, they have
   been recorded east of 80 degrees E from April-July and October-December,
   but not in February-March. Most sightings are from oceanic waters at
   least 20 km offshore where depths exceed 500 m.}},
