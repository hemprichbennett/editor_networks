@article{ ISI:000268744900004,
Author = {Hampton, Shannon L. and Ryan, Peter G. and Underhill, Les G.},
Title = {{The effect of flipper banding on the breeding success of African
   Penguins Spheniscus demersus at Boulders Beach, South Africa}},
Journal = {{OSTRICH}},
Year = {{2009}},
Volume = {{80}},
Number = {{2}},
Pages = {{77-80}},
Month = {{JUN}},
Abstract = {{Concerns have been expressed over the impact of flipper banding on the
   survival and reproductive performance of penguins. This study compared
   the breeding success of banded and unbanded African Penguins Spheniscus
   demersus at Boulders Beach, South Africa, in 2006. It was based on 100
   nests; 50 had at least one banded adult and 50 had no banded adults.
   Besides flipper banding, other variables considered were extent of
   vegetation cover, proximity to tourist path, burrow depth, and either
   sand or clay substratum. There were no significant differences in
   breeding success between the different nest types or between banded and
   unbanded penguins, suggesting that once an individual reaches the level
   of fitness required to breed, banding does not affect the outcome of
   breeding by African Penguins.}},
Publisher = {{NATL INQUIRY SERVICES CENTRE PTY LTD}},
Address = {{19 WORCESTER STREET, PO BOX 377, GRAHAMSTOWN 6140, SOUTH AFRICA}},
