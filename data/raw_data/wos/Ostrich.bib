@article{ ISI:000329161200009,
Author = {Engelbrecht, Derek},
Title = {{Filial cannibalism in the Chestnut-backed Sparrow-lark Eremopterix
   leucotis}},
Journal = {{OSTRICH}},
Year = {{2013}},
Volume = {{84}},
Number = {{3}},
Pages = {{227-229}},
Abstract = {{Cannibalism is a rare event in passerines, especially filial
   cannibalism. Here I report on two instances of filial cannibalism
   observed in the Chestnut-backed Sparrow-lark Eremopterix leucotis in the
   Limpopo province, South Africa. In both instances it was only the
   females that engaged in filial cannibalism. It is suggested that a dead
   nestling is viewed as a waste product that must be moved from the nest
   as part of nest sanitation, either by carrying it from the nest or by
   consuming it. These events represent not only the first record of
   cannibalism in the Alaudidae, but also only the second record of a lark
   eating another vertebrate.}},
