@article{ ISI:000367457200014,
Author = {Bignert, Anders and Helander, Bjorn O.},
Title = {{Monitoring of contaminants and their effects on the common Guillemot and
   the White-tailed sea eagle}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2015}},
