@article{ ISI:000404236700020,
Author = {Martens, Johanne M. and Woog, Friederike},
Title = {{Nest cavity characteristics, reproductive output and population trend of
   naturalised Amazon parrots in Germany}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
