@article{ ISI:000367457200015,
Author = {Strazds, Maris and Bauer, Hans-Guenther and Vaeli, Uelo and Kukare,
   Agnese and Bartkevics, Vadims},
Title = {{Recent impact of DDT contamination on Black Stork eggs}},
