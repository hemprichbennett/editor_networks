@article{ ISI:000367457200032,
Author = {McKechnie, Andrew E. and Noakes, Matthew J. and Smit, Ben},
Title = {{Global patterns of seasonal acclimatization in avian resting metabolic
   rates}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2015}},
Volume = {{156}},
Number = {{1}},
Pages = {{S367-S376}},
