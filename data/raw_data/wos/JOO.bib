@article{ ISI:000382943900015,
Author = {Corbel, Helene and Legros, Ariane and Haussy, Claudy and Jacquin, Lisa
   and Gasparini, Julien and Karimi, Battle and Frantz, Adrien},
Title = {{Stress response varies with plumage colour and local habitat in feral
   pigeons}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
