@article{ ISI:000356447100024,
Author = {Matson, Kevin D. and Versteegh, Maaike A. and van der Velde, Marco and
   Tieleman, B. Irene},
Title = {{Effects of immune supplementation and immune challenge on bacterial
   assemblages in the avian cloaca}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2015}},
Volume = {{156}},
Number = {{3}},
Pages = {{805-810}},
Month = {{JUL}},
Abstract = {{Relationships between avian physiology and bacterial assemblages in the
   cloaca are poorly understood. We used molecular techniques to analyze
   cloacal swabs from pigeons that were subjected to two immunological
   manipulations: lysozyme supplementation and endotoxin challenge. From
   the swabs, we derived ecological indices of evenness, richness, and
   diversity of bacterial assemblages. Challenge led to changes in evenness
   that depended on supplementation. When analyzing these changes, we
   neutralized the effects of a possible statistical artifact by including
   the starting values as a covariate. Repeatability calculations suggested
   that swabbing reliably captured the evenness but not the richness or
   diversity of bacterial assemblages in the cloaca.}},
