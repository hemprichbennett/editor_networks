@article{ ISI:000437001800019,
Author = {Theuerkauf, Jorn and Gula, Roman},
Title = {{Indirect evidence for body size reduction in a flightless island bird
   after human colonisation}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2018}},
Volume = {{159}},
Number = {{3}},
Pages = {{823-826}},
Month = {{JUL}},
Abstract = {{Rhynochetos orarius has been described from Holocene fossils as the
   sister species of the smaller extant Kagu Rhynochetos jubatus, a bird
   endemic to New Caledonia. However, we argue that there has never been
   evidence justifying the description of R. orarius. Additionally, for
   biogeographical reasons it seems unlikely that two Kagu species would
   have evolved in New Caledonia. We therefore synonymise R. orarius and R.
   jubatus and postulate that Holocene Kagu were larger than today probably
   because historic hunting by humans targeted larger birds in richer
   habitat.}},
