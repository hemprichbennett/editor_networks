@article{ ISI:000351069300021,
Author = {Harnos, A. and Nora, A. and Kovacs, Sz and Lang, Zs and Csoergo, T.},
Title = {{Increasing protandry in the spring migration of the Pied Flycatcher
   (Ficedula hypoleuca) in Central Europe}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2015}},
Volume = {{156}},
Number = {{2}},
Pages = {{543-546}},
Month = {{APR}},
Abstract = {{Sexual selection theory suggests that the degree of protandry in spring
   migration should increase when earlier males benefit from better mating
   opportunities. Western European studies failed to show differences in
   sex-specific arrival dates for the Pied Flycatcher. However, different
   climatic conditions and other constraints may affect its phenology in
   continental Europe. Here, we present evidence that sex-related
   phenological changes have occurred over the past 25 years (between 1989
   and 2013) in Central Europe (Ocsa Bird Ringing Station, Hungary). The
   spring arrival of male flycatchers shifted to earlier dates whereas
   female arrival showed no change, implying an increasing degree of
   protandry.}},
