@article{ ISI:000356447100025,
Author = {Gladalski, Michal and Skwarska, Joanna and Kalinski, Adam and Banbura,
   Miroslawa and Markowski, Marcin and Wawrzyniak, Jaroslaw and Zielinski,
   Piotr and Banbura, Jerzy},
Title = {{Patterns of year-to-year variation in haemoglobin and glucose
   concentrations in the blood of nestling Pied Flycatchers Ficedula
   hypoleuca}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2015}},
Volume = {{156}},
Number = {{3}},
Pages = {{811-817}},
Month = {{JUL}},
Abstract = {{Physiological tools can be used to identify the sources and consequences
   of stressors on animals. Understanding the influences of variation in
   habitat quality and anthropogenic disturbance on organism condition and
   health may improve future management and conservation. We present
   results concerning variation in haemoglobin and glucose concentrations
   in the blood of about 14-day-old nestling Pied Flycatchers Ficedula
   hypoleuca in central Poland over a 4-year period, 2011-2014, in a
   deciduous forest. The most important findings of the study are: (1) the
   concentration of haemoglobin and glucose of the nestlings from the same
   brood tended to be consistently similar, with much variation occurring
   among broods; (2) repeatability of haemoglobin concentration was higher
   than repeatability of glucose concentration; (3) mean levels of
   haemoglobin and glucose varied among years; (4) haemoglobin and glucose
   concentrations were negatively correlated; and (5) there was a positive
   relationship between haemoglobin levels and breeding success.}},
