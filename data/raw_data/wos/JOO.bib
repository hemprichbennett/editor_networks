@article{ ISI:000347703700031,
Author = {Charter, Motti and Leshem, Yossi and Izhaki, Ido and Roulin, Alexandre},
Title = {{Pheomelanin-based colouration is correlated with indices of flying
   strategies in the Barn Owl}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2015}},
Volume = {{156}},
Number = {{1}},
Pages = {{309-312}},
Month = {{JAN}},
Abstract = {{Resource polymorphism refers to individuals from the same population
   foraging in alternative habitats or on alternative food. Food
   specialization can be associated with adaptations such as colour
   polymorphism, with pale and dark colours conferring differential
   camouflage in different habitats. Pale and dark-reddish pheomelanic Barn
   Owls (Tyto alba) forage on different prey species in closed and open
   habitats, respectively. We show here that darker-reddish owls have
   heavier stomach content when found dead, and their 5th secondary wing
