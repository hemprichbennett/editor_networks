@article{ ISI:000367457200031,
Author = {Paeckert, Martin and Martens, Jochen and Sun, Yue-Hua and Tietze, Dieter
   Thomas},
Title = {{Evolutionary history of passerine birds (Aves: Passeriformes) from the
   Qinghai-Tibetan plateau: from a pre-Quarternary perspective to an
   integrative biodiversity assessment}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2015}},
Volume = {{156}},
Number = {{1}},
Pages = {{S355-S365}},
