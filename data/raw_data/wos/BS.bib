@article{ ISI:000326713400005,
Author = {Curado, Guillermo and Figueroa, Enrique and Sanchez, Marta I. and
   Castillo, Jesus M.},
Title = {{Avian communities in Spartina maritima restored and non-restored salt
   marshes}},
Journal = {{BIRD STUDY}},
Year = {{2013}},
