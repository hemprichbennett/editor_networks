@article{ ISI:000335850000015,
Author = {Pagani-Nunez, Emilio and Fregenal, Javier and Hernandez-Gomez, Sergio
   and Dominguez-Santaella, Miguel},
Title = {{Wintering location and moult patterns of juvenile Common Chiffchaffs
   Phylloscopus collybita}},
Journal = {{BIRD STUDY}},
Year = {{2014}},
Volume = {{61}},
Number = {{2}},
Pages = {{270-276}},
Month = {{APR 3}},
Abstract = {{Capsule The pattern of moult of juvenile Common Chiffchaffs Phylloscopus
   collybita wintering in two distant localities of the Iberian Peninsula,
   Malaga (south) and Barcelona (north) differed. Individuals wintering in
   the northern locality moulted more contour than flight feathers, and
   vice versa, while sexes did not differ; individuals moulting more
   contour feathers arrived later and individuals moulting more flight
   feathers arrived earlier. Taken together, our results suggest that the
   pattern of moult of juvenile Common Chiffchaffs may depend on the
   location to which they migrate, in addition to the geographic origin and
   the time of breeding.}},
