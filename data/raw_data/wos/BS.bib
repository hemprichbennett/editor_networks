@article{ ISI:000327175800009,
Author = {Duijns, Sjoerd and Hidayati, Nur Annis and Piersma, Theunis},
Title = {{Bar-tailed Godwits Limosa l. lapponica eat polychaete worms wherever
   they winter in Europe}},
Journal = {{BIRD STUDY}},
