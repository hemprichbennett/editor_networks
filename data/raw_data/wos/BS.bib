@article{ ISI:000310614000013,
Author = {Muniz-Lopez, Ruth and Liminana, Ruben and Cortes, Gonzalo D. and Urios,
   Vicente},
Title = {{Movements of Harpy Eagles Harpia harpyja during their first two years
   after hatching}},
Journal = {{BIRD STUDY}},
Year = {{2012}},
Volume = {{59}},
Number = {{4}},
Pages = {{509-514}},
Abstract = {{Capsule Two juvenile Harpy Eagles were tagged at their nests with
   satellite transmitters. During the first two years after hatching both
   birds moved very short distances, with a maximum recorded distance of
   1.3 km from the nest. During the study period, neither bird started
   juvenile dispersal, but while one of the birds showed a significant
   overall increase in mean monthly distance from nest with age, the other
   bird did not. Protecting territories of adult Harpy Eagles may enhance
