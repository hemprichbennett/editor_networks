@article{ ISI:000343708000014,
Author = {Peleg, Ori and Charter, Motti and Leshem, Yossi and Izhaki, Ido and
   Roulin, Alexandre},
Title = {{Conditional association between melanism and personality in Israeli Barn
   Owls}},
Journal = {{BIRD STUDY}},
Year = {{2014}},
Volume = {{61}},
Number = {{4}},
Pages = {{572-577}},
Month = {{OCT 2}},
Abstract = {{Capsule Boldness defines the extent to which animals are willing to take
   risks in the presence of a predator. Late, but not early, in the
   breeding season, Israeli nestling Barn Owls displaying larger black
   feather spots were more docile, feigned death longer and had a lower
   breathing rate when handled than smaller-spotted nestlings.
   Larger-spotted breeding females were less docile if heavy but more more
   docile if light. The covariation between personality (boldness vs.
   timid) and melanin-based colouration is therefore conditional on
   environmental factors.}},
Publisher = {{TAYLOR \& FRANCIS LTD}},
Address = {{4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND}},
