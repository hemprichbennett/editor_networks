@article{ ISI:000326713500014,
Author = {Mellone, Ugo and De La Puente, Javier and Lopez-Lopez, Pascual and
   Liminana, Ruben and Bermejo, Ana and Urios, Vicente},
Title = {{Migration routes and wintering areas of Booted Eagles Aquila pennata
   breeding in Spain}},
Journal = {{BIRD STUDY}},
Year = {{2013}},
Volume = {{60}},
Number = {{3}},
Pages = {{409-413}},
Month = {{AUG 1}},
Abstract = {{CapsuleFive Booted Eagles breeding in Spain were tracked by GPS during
   migration. Autumn routes were generally more eastern than spring routes,
   showing a typical loop migration. Birds covered on average ca. 200
   km/day, and only one individual used a long-term stopover site (for up
   to 4 weeks). All but one used a single wintering area, located in
   Sub-Saharan Africa, at 2800-3500 km from their nests. Eagles were forced
   to stop migration at the Strait of Gibraltar for up to 6 days.}},
