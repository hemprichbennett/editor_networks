@article{ ISI:000327175800010,
Author = {Shitikov, Dmitry A. and Dubkova, Elena V. and Makarova, Tatiana V.},
Title = {{The demography of Yellow Wagtails Motacilla flava on abandoned fields in
   northern European Russia}},
