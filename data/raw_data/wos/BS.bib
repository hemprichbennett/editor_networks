@article{ ISI:000327175800016,
Author = {Lopez-Lopez, Pascual and Zuberogoitia, Inigo and Alcantara, Manuel and
   Antonio Gil, Juan},
Title = {{Philopatry, natal dispersal, first settlement and age of first breeding
   of bearded vultures Gypaetus barbatus in central Pyrenees}},
Journal = {{BIRD STUDY}},
Year = {{2013}},
Volume = {{60}},
Number = {{4}},
Pages = {{555-560}},
Month = {{NOV 1}},
Abstract = {{Capsule Male bearded vultures tended to disperse closer to natal
   territories than females with a preferred eastward natal dispersal
   direction. In general, birds settled for the first time in breeding
   territories at an average age of 7.6 years, first breeding attempt
   occurred at 10.0 years and first successful breeding at 10.4 years.
   Measures aimed at favouring the settlement of new pairs in the edge of
   their current distribution area would allow natural range expansion of
   this endangered species.}},
