@article{ ISI:000294215300007,
Author = {Weiser, Emily L. and Powell, Abby N.},
Title = {{Evaluating gull diets: a comparison of conventional methods and stable
   isotope analysis}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
