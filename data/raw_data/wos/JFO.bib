@article{ ISI:000307965300002,
Author = {Redmond, Lucas J. and Murphy, Michael T.},
Title = {{Using complementary approaches to estimate survival of juvenile and
   adult Eastern Kingbirds}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
