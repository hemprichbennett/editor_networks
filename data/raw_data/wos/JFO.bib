@article{ ISI:000307965300003,
Author = {Brand, Kimberly B. and Bowman, Reed},
Title = {{Body mass variation in breeding Florida Scrub-Jays}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2012}},
Volume = {{83}},
