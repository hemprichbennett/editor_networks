@article{ ISI:000300765600001,
Author = {Gil-Weir, Karine C. and Grant, William E. and Slack, R. Douglas and
   Wang, Hsiao-Hsuan and Fujiwara, Masami},
Title = {{Demography and population trends of Whooping Cranes}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
