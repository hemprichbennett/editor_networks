@article{ ISI:000311413800010,
Author = {Horton, Kyle G. and Morris, Sara R.},
Title = {{Estimating mass change of migrant songbirds during stopover: comparison
   of three different methods}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
