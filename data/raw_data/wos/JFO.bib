@article{ ISI:000263601800004,
Author = {McConnell, Scott and O'Connell, Timothy J. and Leslie, Jr., David M. and
   Shackford, John S.},
Title = {{Mountain Plovers in Oklahoma: distribution, abundance, and habitat use}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
