@article{ ISI:000315329000003,
Author = {Luna-Maira, Laura and Alarcon-Nieto, Gustavo and Haugaasen, Torbjorn and
   Brooks, Daniel M.},
Title = {{Habitat use and ecology of Wattled Curassows on islands in the lower
   Caqueta River, Colombia}},
