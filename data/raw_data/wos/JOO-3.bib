@article{ ISI:000270283700004,
Author = {Puebla-Olivares, Fernando and Margarita Figueroa-Esquivel, Elsa},
Title = {{Sexual dimorphism in Ivory-billed Woodcreepers (Xiphorhynchus
   flavigaster) in Mexico}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2009}},
Volume = {{150}},
Number = {{4}},
Pages = {{755-760}},
Month = {{OCT}},
Abstract = {{We analyzed a sample of 173 specimens of different subspecies within the
   distribution of the Ivory-billed Woodcreeper (Xiphorhynchus flavigaster)
   in Mexico. Morphological variables were analyzed separately for males,
   females, juveniles, and adults. The results showed that males were
   larger in wing chord, tail length, body mass, and primary 8 length than
   females. Discriminant function analysis yielded correct classification
   as males or females in 95\% of samples analyzed (92.3\% of males and
   100\% of females). Additionally, we identified morphological variables
   which presented the highest correlation to obtain linear regression
   functions for estimating one or more missing variables in specimens. The
