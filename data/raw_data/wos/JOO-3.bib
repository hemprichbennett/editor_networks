@article{ ISI:000285783900025,
Author = {Maurer, Golo and Cassey, Phillip},
Title = {{Evaluation of a glossmeter for studying the surface appearance of avian
   eggs}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2011}},
Volume = {{152}},
Number = {{1}},
Pages = {{209-212}},
Month = {{JAN}},
Abstract = {{Gloss is a striking characteristic of avian eggshells that may affect
   both egg recognition and clutch predation. Previously, gloss has proved
   difficult to quantify objectively. Newly developed handheld glossmeters
   seem to offer a solution. We evaluated the use of a handheld glossmeter
   for a range of different eggshell surfaces, shapes and sizes. In all
   cases, their measurements were strongly affected by egg size and seem
   currently suitable only for very large eggs; the size of Rhea sp. eggs
   or larger. A glossmeter may, however, still prove useful to quantify
   other structures in comparative ornithology, with flat surfaces such as
   feathers or nest material.}},
