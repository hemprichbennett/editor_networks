@article{ ISI:000259440000021,
Author = {Ludwig, Sonja C. and Becker, Peter H.},
Title = {{Within-season divorce in Common Terns Sterna hirundo in a year of heavy
   predation}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2008}},
Volume = {{149}},
Number = {{4}},
Pages = {{655-658}},
Month = {{OCT}},
Abstract = {{Divorce between seasons is a common phenomenon in seabirds, whereas
   within-season mate change is rarely observed. We report four cases of
   within-season mate change in renesting Common Terns Sterna hirundo
   during a year of heavy predation by a Long-eared Owl Asio otus, when we
   found more deserted clutches, lower hatching success as well as more
   disappeared chicks compared to other years. Within-season nest-site
   fidelity also decreased under predation pressure.}},
