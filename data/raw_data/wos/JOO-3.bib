Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Furness, RW (Reprint Author), Univ Glasgow, Inst Biomed \& Life Sci, Graham Kerr Bldg, Glasgow G12 8QQ, Lanark, Scotland.
   Univ Glasgow, Inst Biomed \& Life Sci, Glasgow G12 8QQ, Lanark, Scotland.}},
DOI = {{10.1007/s10336-007-0152-2}},
