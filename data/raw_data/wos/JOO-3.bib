@article{ ISI:000264043500007,
Author = {Yohannes, Elizabeth and Biebach, Herbert and Nikolaus, Gerhard and
   Pearson, David J.},
Title = {{Passerine migration strategies and body mass variation along geographic
   sectors across East Africa, the Middle East and the Arabian Peninsula}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2009}},
Volume = {{150}},
Number = {{2}},
Pages = {{369-381}},
Month = {{APR}},
Abstract = {{Using the body mass estimates of 12 long-distance migrating Palearctic
   passerine species monitored at successive sites across the Eastern
   Africa flyway, we tested whether birds modulate their body mass
   according to specific seasonal demands across different geographic
   sectors. We compared body mass estimates across latitudinal distances
   and geographic sectors in Europe, the desert, Northeast Africa and East
   Africa. Our results show that, depending on the species and season
   considered, the average body mass increase or decrease is variable at
   and among different geographic sectors. By comparing the variation in
   body mass between different ecological sectors, we were able to show
   when and where migrants accumulate their migratory fuel reserves during
   migration.}},
