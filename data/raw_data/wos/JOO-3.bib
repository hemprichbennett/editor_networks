@article{ ISI:000249730900026,
Author = {Maddocks, Tracy A. and Geiser, Fritz},
Title = {{Heterothermy in an Australian passerine, the Dusky Woodswallow (Artamus
   cyanopterus)}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2007}},
Volume = {{148}},
Number = {{4}},
Pages = {{571-577}},
Month = {{OCT}},
Abstract = {{Information regarding passerine heterothermy and torpor is scant,
   although many species are small and must cope with a fluctuating food
   supply and presumably would benefit from energy savings afforded by
   torpor. We studied whether insectivorous Dusky Woodswallows ( Artamus
   cyanopterus; similar to 35 g) enter spontaneous torpor ( food ad
   libitum) when held outdoors as a pair in autumn/winter. Woodswallows
   displayed pronounced and regular daily fluctuations in body temperature
   ( T-b) over the entire study period. The mean Tb ranged from similar to
   39 degrees C to 40 degrees C ( photophase, day time) and similar to 33
   degrees C to 36 degrees C ( scotophase, night time). However, on 88\% of
   bird nights, nocturnal Tb minima fell to < 35 degrees C. The lowest T-b
   observed in air was 29.2 degrees C. However, when a bird fell into water
   its T-b dropped further to similar to 22 degrees C; this T-b was
   regulated for several hours and the bird survived. Our observations
   suggest that heterothermy is a normal part of the daily thermal regime
   for woodswallows to minimise energy expenditure. Spontaneous nocturnal
   torpor in captive woodswallows suggests that torpor in the wild may be
   more pronounced than recorded here because free-living birds are likely
   challenged by both low food availability and adverse weather.}},
