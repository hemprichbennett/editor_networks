@article{ ISI:000255199000026,
Author = {Nichols, James D. and Runge, Michael C. and Johnson, Fred A. and
   Williams, Byron K.},
Title = {{Adaptive harvest management of North American waterfowl populations: a
   brief history and future prospects}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2007}},
Volume = {{148}},
Number = {{2}},
Pages = {{S343-S349}},
Month = {{DEC}},
Abstract = {{Since 1995, the US Fish and Wildlife Service has used an adaptive
   approach to the management of sport harvest of mid-continent Mallard
   ducks (Anas platyrhynchos) in North America. This approach differs from
   many current approaches to conservation and management in requiring
   close collaboration between managers and scientists. Key elements of
   this process are objectives, alternative management actions, models
   permitting prediction of system responses, and a monitoring program. The
   iterative process produces optimal management decisions and leads to
   reduction in uncertainty about response of populations to management.
   This general approach to management has a number of desirable features
   and is recommended for use in many other programs of management and
   conservation.}},
