
@article{ ISI:000288395800023,
Author = {Legault, Andrew and Chartendrault, Vivien and Theuerkauf, Joern and
   Rouys, Sophie and Barre, Nicolas},
Title = {{Large-scale habitat selection by parrots in New Caledonia}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
