@article{ ISI:000085002200007,
Author = {Brouse, P},
Title = {{Twenty year change in the raptor community in northern Utah during the
   nonbreeding season}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{1999}},
Volume = {{33}},
Number = {{4}},
Pages = {{317-322}},
Month = {{DEC}},
Abstract = {{Winter roadside surveys were conducted to count wintering raptors in
   Cache Valley Utah. The study design allowed comparisons in distribution
   and species composition over the past 20 yr. Even though an increase in
   abundance was observed for American Kestrels (Falco sparverius), Merlins
   (F: columba,ius), Bald Eagles (Haliaeetus leucocephalus) and Ferruginous
   Hawks (Buteo regalis), an overall decline was evident. Raptor numbers
   for the winter of 1997-98 were 61\% of those reported for the winters of
   1976-79. Despite changes in abundance and a change in geographic
   distribution, the temporal distribution remained the same. Raptor
   numbers were highest in irrigated pastures, followed closely by dry
   cropland. Numbers increased slightly from December-January were lowest
   during February and rose steadily again through April.}},
