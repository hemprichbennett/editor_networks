@article{ ISI:A1994PL68300004,
Author = {MOLLER, AP},
Title = {{FACTS AND ARTIFACTS IN NEST-BOX STUDIES - IMPLICATIONS FOR STUDIES OF
   BIRDS OF PREY}},
