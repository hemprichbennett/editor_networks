@article{ ISI:000173424900015,
Author = {Arrowood, PC and Finley, CA and Thompson, BC},
Title = {{Analyses of Burrowing Owl populations in New Mexico}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2001}},
Volume = {{35}},
