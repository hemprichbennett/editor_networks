@article{ ISI:A1994NL00700001,
Author = {WIGGERS, EP and KRITZ, KJ},
Title = {{PRODUCTIVITY AND NESTING CHRONOLOGY OF THE COOPERS-HAWK AND
   SHARP-SHINNED HAWK IN MISSOURI}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{1}},
Pages = {{1-3}},
Month = {{MAR}},
Abstract = {{Sharp-shinned hawk (Accipiter striatus) and Cooper's hawk (A. cooperii)
   nests were studied in Missouri during 1985 and 1986 to determine
   productivity and nesting chronology. The nesting season for
   Sharp-shinned hawks extended from May to August, with most young
   fledging by August. The Cooper's hawk nesting season extended from May
   to july. Mean dutch size and number-of young reaching banding age
   (greater-than-or-equal-to 14 days) per successful nest for Sharp-shinned
   hawks were 4.5 (N = 8) and 3.2 (N = 5), respectively. Mean clutch size
   and number of young per successful nest for Cooper's hawk were 3.8 (N =
   12) and 3.5 (N = 20). No differences were detected (P
   greater-than-or-equal-to 0.73) in clutch size, brood size, or number of
   young per successful nest for Cooper's hawk in pine (Pinus spp.) versus
   oak-hickory (Quercus spp.-Carya spp.) hardwood habitats.}},
