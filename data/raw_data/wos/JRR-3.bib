ISSN = {{0892-1016}},
EISSN = {{2162-4569}},
Keywords = {{CANADIAN ARCTIC; FALCO-RUSTICOLUS; GYRFALCON; REPRODUCTION; TREE NESTING}},
Research-Areas = {{Zoology}},
