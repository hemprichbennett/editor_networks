@article{ ISI:A1997XE87500010,
Author = {Travaini, A and Donazar, JA and Ceballos, O and Rodriguez, A and
   Hiraldo, F and Delibes, M},
Title = {{Food habits of Common Barn-Owls along an elevational gradient in Andean
   Argentine Patagonia}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{1}},
Pages = {{59-64}},
Month = {{MAR}},
Abstract = {{We evaluated the diet of Common Barn-owls (Tyto alba) along an
   elevational gradient in Argentine Patagonia. Small mammals (mainly
   rodents) were the main prey accounting for 93.2\% of total prey items.
   Consumption of rodents appeared to be dependent on their availability.
   Sizes of mammalian prey were variable but most ranged from 10-100 g in
   body mass. We concluded that the diet of these barn owls could be used
   as an index of cricetid rodent populations along the gradient.}},
