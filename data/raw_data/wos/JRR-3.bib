@article{ ISI:A1994PL68300009,
Author = {KOGA, K and SHIRAISHI, S},
Title = {{PARENT-OFFSPRING RELATIONS DURING THE POSTFLEDGING DEPENDENCY PERIOD IN
   THE BLACK KITE (MILVUS-MIGRANS) IN JAPAN}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{3}},
Pages = {{171-177}},
Month = {{SEP}},
Abstract = {{The transition to independence of the juvenile black kite (Milvus
   migrans) was investigated in four families near the tip of Nagasaki
   Peninsula in Japan, where the kite is resident.  The juveniles fledged
   at 58-63 d of age, and they became independent at 46.8 d on average
   after fledging.  The frequency of begging and approaches by the juvenile
   toward the parents did not change through the post-fledging dependency
   period.  With respect to parental effort, the frequency of parental
   approaches toward the juvenile and the parental feeding frequency
   decreased with juvenile age.  Furthermore, the parents in the two
   families studied in detail avoided juvenile approaches, and the female
   parent of one family dived her offspring two d before juvenile
   independence.  Thus, the parents decreased the parental care over time. 
   This suggests that decreases in parental effort are related to the
   juvenile independence of the resident black kite.}},
