@article{ ISI:000086414300002,
Author = {Bretagnolle, V and Ghestemme, T and Thiollay, JM and Attie, C},
Title = {{Distribution, population size and habitat use of the Reunion Marsh
   Harrier, Circus m. maillardi}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2000}},
Volume = {{34}},
Number = {{1}},
Pages = {{8-17}},
Month = {{MAR}},
Abstract = {{On Reunion Island, Indian Ocean, the only surviving resident raptor is
   the endemic Reunion Marsh Harrier, Circus m, maillardi. It has been
   considered conspecific with the Madagascar Marsh Harrier (C. m.
   macrosceles) but a reanalysis of the classification of both is needed. A
   survey was carried out,ut on the island in 1997-98 and the breeding
   population was found to be no larger than 100 pairs, mostly concentrated
   in forested areas at mid-elevations (300-700 m) and down to sea lever,
   but rarely above 1200 m. The distribution of pairs was clumped with as
   many as 8 pairs within a 10 km(2) patch, and mostly concentrated in low
   degraded native woodlands on steep slopes. Foraging habitats were more
   diversified and were widely distributed from coastal wetlands to
   cultivated fields, forest and upper montane vegetation. Breeding was
   apparently not synchronized among pairs but egg laying occurred
   primarily from December-March. The Reunion Marsh Harrier must be
   considered as Threatened, although its distribution Iras apparently not
   declined between 1976-98.}},
