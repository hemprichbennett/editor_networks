@article{ ISI:000087970900005,
Author = {Sun, YH and Wang, Y and Lee, CF},
Title = {{Habitat selection by Tawny Fish-Owls (Ketupa flavipes) in Taiwan}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2000}},
Volume = {{34}},
Number = {{2}},
Pages = {{102-107}},
Month = {{JUN}},
Abstract = {{Four radio-tagged Tawny Fish-Owls (Ketupa flavipes) were studied to
   determine their choice of roosting and foraging/resting habitats in
   Taiwan. A total of 51 roost sites were Located in two territories
   measuring 6.7 and 5.7 km in length. Owls selected old-growth forests
   more than expected according to availability and tended to roost uphill
   during late spring and fall. They foraged along streams more frequently
   than expected (P < 0.05) and along creeks less than expected (P < 0.05)
   with use of small creeks and fish farms increasing after streams
   flooded.}},
