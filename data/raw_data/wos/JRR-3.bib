@article{ ISI:000086414300005,
Author = {Koenen, MT and Koenen, SG and Yanez, N},
Title = {{An evaluation of the Andean Condor population in northern Ecuador}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2000}},
Volume = {{34}},
Number = {{1}},
Pages = {{33-36}},
Month = {{MAR}},
Abstract = {{We evaluated the population structure of Andean Condors ( Vultur
   gryphus) in the Cotacachi-Cayapas and Cayambe-Coca Ecological Reserves,
   Ecuador. We conducted 1298 hr of fieldwork and made 496 condor
   observations. Age class and sex could be determined in 127 and 48
   observations, respectively. The population consisted of 1:1 female to
   male ratio yet only 20\% of our observations were of juveniles and
   subadults. The apparent skewed population structure suggested that the
   population may be declining.}},
