@article{ ISI:000074718600013,
Author = {Foster, GW and Morrison, JL and Hartless, CS and Forrester, DJ},
Title = {{Haemoproteus tinnunculi in Crested Caracaras (Caracara plancus
   audubonii) from southcentral Florida}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{2}},
Pages = {{159-162}},
Month = {{JUN}},
Abstract = {{From 1994-96, 223 Crested Caracaras (Caracara plancus audubonii) from
   eight counties in southcentral Florida were examined for blood
   parasites. Haemoproteus tinnunculi was the only hematozoan seen. The
   overall prevalence was 28.2\%. Prevalence was higher for adults (50.0\%)
   than for nestlings (20.4\%, P = 0.0001). There were no differences in
   prevalence of infection between years (P = 0.8899). The only significant
   interaction was between age and sampling month (P = 0.0027). The adult
   birds had a constant prevalence from February-May, while nestlings had
   an increasing prevalence over the same time period. This is the first
   report of Haemoproteus tinnunculi in Crested Caracaras.}},
