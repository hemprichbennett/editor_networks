@article{ ISI:A1996VL82100004,
Author = {Cerasoli, M and Penteriani, V},
Title = {{Nest-site and aerial meeting point selection by common buzzards (Buteo
   buteo) in central Italy}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{1996}},
Volume = {{30}},
Number = {{3}},
Pages = {{130-135}},
Month = {{SEP}},
Abstract = {{A nesting population of common buzzards (Buteo buteo) was studied in a
   mountainous area of central Italy from 1988-92. Nesting density averaged
   19.74 pairs/100 km(2), and the average minimum distance between pairs
   was 1.4 km (SD = 0.432). Mean number of young fledged/successful nest
   was 1.78 for all years combined. Of nests examined, a significantly (P =
   0.001) larger proportion were on slopes facing to the northeast
   (73.3\%), most were on the mid-portions of slopes (60\%), and were built
   at the intersections between tree branches and tree trunks (86.6\%).
   Other factors including elevation, the angle between tree trunks and
   branches, tree height, tree crown volume, the distance of nests from a
   forest edge, the distance of the nest from areas of timber harvesting,
   and the average trunk spacing were also important variables in terms of
   nest placement. The distance of aerial meeting sites (areas where a
   group of at least three buzzards regularly soared, tumbled together, and
   chased each other) from neighboring nest sites and maximum slope were
   also important factors in the choice of these gathering points.}},
