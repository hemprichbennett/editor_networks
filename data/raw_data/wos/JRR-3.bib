@article{ ISI:000073563100002,
Author = {Bildstein, KL and Schelsky, W and Zalles, J and Ellis, S},
Title = {{Conservation status of tropical raptors}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{1998}},
