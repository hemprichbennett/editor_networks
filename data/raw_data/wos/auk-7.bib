@article{ ISI:A1992HU28300006,
Author = {POMEROY, H and HEPPNER, F},
Title = {{STRUCTURE OF TURNING IN AIRBORNE ROCK DOVE (COLUMBA-LIVIA) FLOCKS}},
Journal = {{AUK}},
Year = {{1992}},
Volume = {{109}},
Number = {{2}},
Pages = {{256-267}},
Month = {{APR}},
Abstract = {{We describe a nonstereo, three-dimensional photographic technique to
   study the turning movements of flocks of semidomestic Rock Doves
   (Columba livia). The method permits sequential examination of an
   individual's position and flight path. The birds in flocks we studied
   with this technique did not maintain fixed positions. Birds continually
   repositioned themselves during a turn. Such repositioning of individuals
   may be more significant in the predator-evasion function of cluster
   flocks than for aerodynamics.}},
