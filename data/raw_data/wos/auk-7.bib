@article{ ISI:A1992JR93300019,
Author = {LARSEN, T and MOLDSVOR, J},
Title = {{ANTIPREDATOR BEHAVIOR AND BREEDING ASSOCIATIONS OF BAR-TAILED GODWITS
   AND WHIMBRELS}},
Journal = {{AUK}},
Year = {{1992}},
Volume = {{109}},
Number = {{3}},
Pages = {{601-608}},
Month = {{JUL}},
Abstract = {{Breeding associations between Whimbrels (Numenius phaeopus) and
   Bar-tailed Godwits (Limosa lapponica) were studied in Finnmark, northern
   Norway. Bar-tailed Godwit nests were distributed closer than expected by
   chance to Whimbrel nests. Timing of egg laying showed that the godwit,
   not the Whimbrel, chose to nest close to the other species. Whimbrels
   defended their nests aggressively against predators, while Bar-tailed
   Godwits did not. Godwits nested within the aggressive response distance
   of nesting Whimbrels. We suggest that Bar-tailed Godwits seek nest
   protection by breeding under the ``protective umbrella{''} of the
   Whimbrel.}},
