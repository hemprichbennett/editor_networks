@article{ ISI:A1992HQ21200006,
Author = {LOVE, J and DEININGER, P},
Title = {{CHARACTERIZATION AND PHYLOGENETIC SIGNIFICANCE OF A REPETITIVE
   DNA-SEQUENCE FROM WHOOPING-CRANES (GRUS-AMERICANA)}},
Journal = {{AUK}},
Year = {{1992}},
Volume = {{109}},
Number = {{1}},
Pages = {{73-79}},
Month = {{JAN}},
Abstract = {{We surveyed a Whooping Crane (Grus americana) genomic library enriched
   for repetitive clones, and isolated a clone whose insert hybridized
   stringently to a repeated-DNA family in the genomes of Whooping Cranes,
   but not Sandhill Cranes (G. canadensis).  This tandem sequence, repeated
   approximately 500 times in the Whooping Crane genome, displays
   taxon-specific properties suggesting that the Common Crane (G. grus) is
   the Whooping Crane's nearest living relative.  Low-stringency
   hybridizations with this repeat produced conserved patterns in all
   cranes except crowned-cranes (Balearica), which indicates an early
   divergence of the crowned-cranes and the remaining cranes.  Sequence and
   DNA-hybridization analyses imply that this repeat is a satellite
   sequence of similar complexity and organization to the primate alphoid
   DNA-sequence family, which also has chromosome and species specificity.}},
