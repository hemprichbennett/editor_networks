@article{ ISI:000297981600010,
Author = {Ceia, Ricardo S. and Ramos, Jaime A. and Heleno, Ruben H. and Hilton,
   Geoff M. and Marques, Tiago A.},
Title = {{Status assessment of the Critically Endangered Azores Bullfinch Pyrrhula
   murina}},
