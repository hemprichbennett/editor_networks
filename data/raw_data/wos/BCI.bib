Title = {{T Novel foraging by wintering Siberian Cranes Leucogeranus leucogeranus
   at China's Poyang Lake indicates broader changes in the ecosystem and
   raises new challenges for a critically endangered species}},
Journal = {{BIRD CONSERVATION INTERNATIONAL}},
Year = {{2017}},
