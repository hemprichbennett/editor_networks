@article{ ISI:000245825500008,
Author = {He Fen-Qi and Fellowes, John R. and Chan, Bosco P. L. and Lau, Michael
   W. N. and Lin Jian-Sheng and Shing, Lee Kwok},
Title = {{An update on the distribution of the `endangered' white-eared Night
   Heron Gorsachius magnificus in China}},
Journal = {{BIRD CONSERVATION INTERNATIONAL}},
Year = {{2007}},
Volume = {{17}},
Number = {{1}},
Pages = {{93-101}},
Month = {{MAR}},
Abstract = {{The literature on the past distribution and status of the White-eared
   Night Heron in China is reviewed, and updated based on recent field
   investigations and reports from Hubei, Jiangxi, Guangxi, Guangdong and
   Hainan. Recent records reveal the species to be more widespread than
   previously thought, but there remain many uncertainties about the area
   of occupancy and connectivity between known populations. The rate of
   discovery of new sites suggests the species may be under-recorded, but a
   number of recent sites have been lost, and no truly secure populations
   are known. There thus remains an urgent need for better information from
   throughout the species' range, especially from historical sites, as well
   as increased protection.}},
