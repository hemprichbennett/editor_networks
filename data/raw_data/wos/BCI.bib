ISSN = {{0959-2709}},
Keywords-Plus = {{PROTECTED AREAS; POPULATION DECLINE; NATIONAL-PARK; LOCAL PEOPLE;
   ATTITUDES; COMMUNITY; RESERVE; INCENTIVES; TANZANIA}},
