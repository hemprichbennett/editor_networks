@article{ ISI:000308828000007,
Author = {Laura Agueero, Maria and Garcia Borboroglu, Pablo and Esler, Daniel},
Title = {{Distribution and abundance of Chubut Steamerducks: an endemic species to
   Central Patagonia, Argentina}},
Journal = {{BIRD CONSERVATION INTERNATIONAL}},
