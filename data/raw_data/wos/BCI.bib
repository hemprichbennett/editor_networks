@article{ ISI:000316202600001,
Author = {Angelov, Ivaylo and Hashim, Ibrahim and Oppel, Steffen},
Title = {{Persistent electrocution mortality of Egyptian Vultures Neophron
   percnopterus over 28 years in East Africa}},
Journal = {{BIRD CONSERVATION INTERNATIONAL}},
Year = {{2013}},
Volume = {{23}},
Number = {{1}},
Pages = {{1-6}},
Month = {{MAR}},
Abstract = {{Electrocution on poorly designed power poles is increasingly shown to
   pose a threat for the populations of many large raptors. Here we
   document that a power line in Sudan continues to cause mortality of
   Egyptian Vultures Neophron percnopterus, a problem that was first
   identified in 1984. We suggest that this power line may have caused the
   death of sufficient Egyptian Vultures to partially explain population
   declines in the Middle East, from where the electrocuted birds may
   originate. This report highlights the urgent need to plan and retro-fit
   power lines in Africa with non-lethal support structures.}},
