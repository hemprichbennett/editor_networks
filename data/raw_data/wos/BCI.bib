@article{ ISI:000306208300009,
Author = {Boesing, Andrea Larissa and Dos Anjos, Luiz},
Title = {{The Azure Jay may reproduce in plantations of Araucaria angustifolia in
   southern Brazil}},
Journal = {{BIRD CONSERVATION INTERNATIONAL}},
Year = {{2012}},
Volume = {{22}},
Number = {{2}},
Pages = {{205-212}},
Month = {{JUN}},
Abstract = {{We undertook field observations of the Azure Jay Cyanocorax caeruleus in
   Araucaria plantations and a secondary forest in southern Brazil from
   2007 to 2010. During the breeding season, the territory and the flock
   sizes in Araucaria plantations (31.6-42.2 ha; 2-3 individuals) seemed to
   be smaller than in the secondary forest (64.5-72.6 ha; 5-6 individuals).
   One active nest was monitored in October November 2009 and indicated
   cooperative breeding by the Azure Jay, as for the majority of American
   jays. The fact that reproduction was documented in Araucaria plantations
   may be positive for the conservation of this species, since natural
   areas of mixed rainforest are becoming increasingly scarce.}},
