@article{ ISI:000259453100003,
Author = {Croxall, John P.},
Title = {{The role of science and advocacy in the conservation of Southern Ocean
   albatrosses at sea}},
Journal = {{BIRD CONSERVATION INTERNATIONAL}},
Year = {{2008}},
Volume = {{18}},
Number = {{1}},
Pages = {{S13-S29}},
Month = {{SEP}},
Abstract = {{Mortality of albatrosses (and petrels) as bycatch in longline fisheries
   is one of the most important and pervasive sources of mortality for many
   species and is often closely linked to observed population declines. In
   the area of the Southern Ocean managed by the Commission for the
   Conservation of Antarctic Marine Living Resources (CCAMLR), which
   includes the waters around South Georgia, Prince Edward Islands, Iles
   Crozet and Kerguelen (the most important sub-Antarctic breeding sites
   for many albatross - and petrel - species), such bycatch was reduced to
   negligible levels (in demographic terms) over the list decade. The
   process by which this was achieved, in terms of data, policy and
   practice, together with -,in assessment of the main drivers and
   obstacles, is described. The extent to which the CCAMLR example is a
   model for other Regional Fisheries Management Organisations (RFMOs) (and
   even for states with jurisdiction in relevant coastal waters) is
   assessed. Some current actions and priorities for further action in
   relation to seabird bycatch are summarised.}},
