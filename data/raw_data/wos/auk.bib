@article{ ISI:000405095500018,
Author = {Martin, Kathy and Wilson, Scott and MacDonald, Elizabeth C. and
   Camfield, Alaine F. and Martin, Michaela and Trefry, Sarah A.},
Title = {{Effects of severe weather on reproduction for sympatric songbirds in an
