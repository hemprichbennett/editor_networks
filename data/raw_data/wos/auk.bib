@article{ ISI:000379663400003,
Author = {Dzielski, Sarah A. and Van Doren, Benjamin M. and Hruska, Jack P. and
   Hite, Justin M.},
Title = {{Reproductive biology of the Sapayoa (Sapayoa aenigma), the ``Old World
   suboscine{''} of the New World}},
Journal = {{AUK}},
Year = {{2016}},
Volume = {{133}},
