@article{ ISI:000317947800007,
Author = {Brzezinski, Marcin and Jedlikowski, Jan and Zmihorski, Michal},
Title = {{The effect of nest site on the nesting success of the Coot Fulica atra}},
Journal = {{ORNIS FENNICA}},
Year = {{2013}},
Volume = {{90}},
Number = {{1}},
Pages = {{57-64}},
Abstract = {{The nesting success of Coots (Fulica atra) breeding at lakes and small
   inland water bodies in the Mazurian Lakeland, Poland, was estimated in
   2008 and 2009. Mean nesting success, indicated by hatching, was 75\%; it
   was highest at lakes in the vicinity of built-up areas (86\%), moderate
   at small inland water bodies (69\%) and lowest at lakes far from human
   settlements (44\%). Nesting success improved with decreasing distance to
   open water. Our results suggest that small inland water bodies and
   built-up areas along lake banks may serve as Coot refuges, acting as
   safeguards against predation, especially by the American Mink.}},
