@article{ ISI:000232719100001,
Author = {Park, CR and Lee, WS and Hino, T},
Title = {{Temporal changes in foraging niche among breeding tits (Paridae) in a
   Korean temperate deciduous forest}},
Journal = {{ORNIS FENNICA}},
Year = {{2005}},
Volume = {{82}},
Number = {{3}},
Pages = {{81-88}},
Abstract = {{The temporal change of foraging niche among breeding tits (Great Tit
   Parus major, Varied Tit P. varius, Marsh Tit P. palustris and Coal Tit
   P. ater) was studied in a Korean temperate deciduous forest from
   December 1998 to June 1999. We studied foraging niches in terms of
   foraging location, foraging maneuver and prey size, and measured
   morphological characters of tits. Great Tit had the largest body length
   among tits. There was significant difference in foraging locations among
   months and across species. Foraging maneuvers and prey sizes did not
   significantly differ among months, but showed inter-specific
   differences. There was a negative correlation between body weight and
   the use of the hanging maneuver among tits. This study suggests that
   closely related tits can coexist due to inter-specific differences in
   their foraging niche and because foraging locations change as the season
   advances.}},
