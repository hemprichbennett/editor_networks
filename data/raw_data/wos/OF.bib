@article{ ISI:000241528400004,
Author = {Leito, Aivar and Truu, Jaak and Roosaluste, Elle and Sepp, Kalev and
   Poder, Indrek},
Title = {{Long-term dynamics of breeding birds in broad-leaved deciduous forest on
   Hanikatsi Island in the West-Estonian archipelago}},
Journal = {{ORNIS FENNICA}},
Year = {{2006}},
Volume = {{83}},
Number = {{3}},
Pages = {{124-130}},
Abstract = {{The temporal dynamics (1974-2004) of the breeding bird community in
   broad-leaved deciduous forest on Hanikatsi Island on the eastern side of
   the Baltic Sea was studied. Altogether 33 bird species of seven orders
   were recorded. The year-to-year variation in the number of species (CV =
   10.99\%) and in the total numbers (CV = 15.17\%) were relatively small.
   No significant trends in the number of bird species and in their
   abundances were detected, though an abrupt change in community structure
   took place at the beginning of the 1990s. No significant relationships
   between abundance of single bird species were detected, except for
   Turdus merula and Turdus philomelos.}},
