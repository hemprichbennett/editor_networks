@article{ ISI:000252689000005,
Author = {Johansen, Hilde Marie and Selas, Vidar and Fagerland, Kristine and
   Johnsen, Jon Trygve and Sveen, Bjorn-Arild and Tapia, Luis and Steen,
   Ronny},
Title = {{Goshawk diet during the nestling period in farmland and forest-dominated
   areas in southern Norway}},
Journal = {{ORNIS FENNICA}},
Year = {{2007}},
Volume = {{84}},
Number = {{4}},
Pages = {{181-188}},
Abstract = {{During the past decades, goshawk (Accipiter gentilis) populations have
   declined in torest-dominated landscapes in Norway. To reveal the
   importance of different prey species in forest versus farmland
   landscapes, we video recorded 146 prey deliveries at four nests in a
   farmland-dominated area and three nests in a forest-dominated area in
   south-eastern Norway in June 2005. Thrushes and corvids were the
   dominating prey in both areas. We found higher diet diversity in the
   forest-dominated landscape, but we also found high dietary overlap,
   presumably due to the dominance of thrushes in both areas. Prey
   composition of forest versus farmland species differed significantly
   between the areas, mainly due to a higher proportion of corvids and a
   lower proportion of grouse in the farmland-dominated area. Goshawk diet
   thus reflects the habitats within the nesting territory.}},
