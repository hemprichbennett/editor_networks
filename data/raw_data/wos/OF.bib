@article{ ISI:000322854800003,
Author = {Jakubas, Dariusz and Manikowska-Slepowronska, Brygida},
Title = {{Response of Grey Herons (Ardea cinerea) to human disturbance in a
   suburban village in Poland}},
Journal = {{ORNIS FENNICA}},
Year = {{2013}},
Volume = {{90}},
Number = {{2}},
Pages = {{86-93}},
Abstract = {{Human-caused disturbances affect, e.g., the spatial distribution and
   breeding performance of most organisms, including birds. Here, we
   assessed the response of Grey Herons (Ardea cinerea) to frequent human
   disturbance in a large heron colony in Poland during 2009-2012. The
   colony covered an area of 2.5 ha and had annually 174-220 occupied
   nests. We found that egg losses increased and nest occupancy decreased
   with an increase in the area covered by buildings nearby. The distances
   to the nearest building or road were not significantly related to egg
   losses.}},
