@article{ ISI:000250841100001,
Author = {Selas, Vidar and Tveiten, Reidar and Aanonsen, Ole Martin},
Title = {{Diet of Common Buzzards (Buteo buteo) in southern Norway determined from
   prey remains and video recordings}},
Journal = {{ORNIS FENNICA}},
Year = {{2007}},
Volume = {{84}},
Number = {{3}},
Pages = {{97-104}},
Abstract = {{We examined the diet of six breeding Common Buzzard (Buteo buteo) pairs
   in southern Norway, by analysing pellets and prey remains collected
   around and in nests, and by video recording prey delivery at the nests.
   Mammals, birds and reptiles were the major prey groups. Amphibians were
   underestimated when identified from pellets and prey remains compared to
   video recording, while birds > 120 g were overestimated. Selection of
   avian prey was studied by comparing the proportions of different weight
   groups of birds among prey with their proportions in the bird community,
   as estimated by point counts around each nest. Common Buzzards
   selectively preyed upon medium-sized birds and neglected many of the
   numerous small passerines.}},
