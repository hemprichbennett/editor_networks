@article{ ISI:A1994PZ91400016,
Author = {WILSON, WH},
Title = {{THE DISTRIBUTION OF WINTERING BIRDS IN CENTRAL MAINE - THE INTERACTIVE
   EFFECTS OF LANDSCAPE AND BIRD FEEDERS}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1994}},
Volume = {{65}},
Number = {{4}},
Pages = {{512-519}},
Month = {{FAL}},
Abstract = {{The abundances of birds at 50 stops along a 43-km route were determined
   by 10 censuses in the winter of 1999-1993 in central Maine. Each stop
   was classified as one of the following: Agricultural, Edge, Deciduous
   Forest or Coniferous Forest. Habitat preferences for 16 Iandbird species
   were documented. For five of these species, Edge stops with feeders were
   preferred over Edge stops without feeders. When stations with feeders
   were removed from the habitat preference analysis, four of these five
   species showed significant changes in their habitat preferences. The
   data underscore the necessity of knowing the location of supplemental
   food in any field ornithological study.}},
