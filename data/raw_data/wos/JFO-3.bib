@article{ ISI:A1995RL13400014,
Author = {PIETZ, PJ and BRANDT, DA and KRAPU, GL and BUHL, DA},
Title = {{MODIFIED TRANSMITTER ATTACHMENT METHOD FOR ADULT DUCKS}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1995}},
Volume = {{66}},
Number = {{3}},
Pages = {{408-417}},
Month = {{SUM}},
Abstract = {{The value of radio telemetry for waterfowl research depends on the
   availability of suitable methods of attaching transmitters. Tn previous
   studies, external transmitters attached to adult Mallards (Anas
   platyrhynchos) with sutures and glue did not stay on birds reliably. In
   an attempt to improve transmitter retention, a method of attachment was
   tested in which 4-g transmitters were attached mid-dorsallvy with
   sutures and with a stainless steel anchor-shaped wire inserted
   subcutaneously (anchor transmitters). Field tests indicated that all of
   26 female Mallards and 63 of 65 female Gadwalls (Anas strepera) retained
   their anchor transmitters during 4369 bird-days of monitoring during
   nesting and brood rearing. Survival races of females with anchor
   transmitters compared favorably with those reported from other studies.
   In this study, females with and without anchor transmitters did not
   differ with respect to survival rates of their ducklings. The anchor
   transmitter may be suitable for a variety of field studies on numerous
   species.}},
