@article{ ISI:000075830900005,
Author = {Kelly, JF},
Title = {{Latitudinal variation in sex ratios of Belted Kingfishers}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1998}},
Volume = {{69}},
Number = {{3}},
Pages = {{386-390}},
Month = {{SUM}},
Abstract = {{Data from the Bird Banding: Laboratory and Christmas Bird Counts
   indicated that northern populations of Belted Kingfishers (Ceryle
   alcyon) were composed of proportionately more males than southern
   populations during fall and winter. The pattern of male bias in northern
   populations was also evident in fall/winter census data from an
   intensive study in Colorado, USA. Bird Banding Laboratory data indicated
   that the fall/winter trend in sex ratio was not maintained during
   spring/summer.}},
