@article{ ISI:A1993KR84600007,
Author = {SERVAT, G},
Title = {{A NEW METHOD OF PREPARATION TO IDENTIFY ARTHROPODS FROM STOMACH CONTENTS
   OF BIRDS}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1993}},
Volume = {{64}},
Number = {{1}},
Pages = {{49-54}},
Month = {{WIN}},
Abstract = {{A micropaleontology method modified for the study of fragments of
   arthropods in stomach contents of birds is described. Fragments are
   sorted then mounted with gum tragacanth onto numbered grids of
   micropaleontological slides. Specimens on these slides are easily
   compared to museum reference collections of arthropods for
   identification. Reference collections of fragments with data fully
   computerized are proposed as vouchers for resultant analyses thereby
   aiding additional dietary investigations by subsequent workers. This
   method can be adapted readily for similar studies in non-avian
   vertebrates.}},
