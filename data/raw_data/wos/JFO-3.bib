@article{ ISI:A1991GY13700008,
Author = {HOUSTON, CS and FOX, GA and CRAWFORD, RD},
Title = {{UNHATCHED EGGS IN SWAINSONS HAWK NESTS}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1991}},
Volume = {{62}},
Number = {{4}},
Pages = {{479-485}},
Month = {{FAL}},
Abstract = {{Unhatched eggs were recorded during banding visits to 987 nests of the
   Swainson's Hawk in western Saskatchewan between 1972 and 1988. In 104 of
   these nests there were 129 unhatched eggs, representing 10.5\% of the
   nests and 6.3\% of the eggs. Twenty-six of the unhatched eggs (20\%)
   contained visible embryos. Although this hawk migrates through Central
   and South America, where DDT is still widely used, the organochlorine
   residue and PCB content of unhatched eggs collected from nests in the
   Canadian Prairies in 1984 and 1985 were low. The percentage of unhatched
   eggs was highest in 1985, the peak year for grasshopper abundance and
   use of insecticides for grasshopper control.}},
