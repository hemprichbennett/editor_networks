@article{ ISI:A1997WX04800008,
Author = {Herkert, JR},
Title = {{Population trends of the Henslow's sparrow in relation to the
   conservation reserve program in Illinois, 1975-1995}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1997}},
Volume = {{68}},
Number = {{2}},
Pages = {{235-244}},
Month = {{SPR}},
Abstract = {{Data from Illinois' Spring Bird Count was used to estimate long-term
   population trends of Henslow's Sparrows in Illinois and to examine if
   the Conservation Reserve Program has affected these trends. Spring Bird
   Count data suggest that Henslow's Sparrow populations in Illinois have
   declined significantly over the last 21 yr, with an estimated average
   rate of decline of 7.1\% per year between 1975-1995. These data
   corroborate analyses of other long-term data sets and provide additional
   support for the general impression that populations of this species have
   declined in many parts of its range. Analyses of the potential benefits
   of the Conservation Reserve Program for Henslow's Sparrows revealed that
   recent population trends (1987-1995) in counties with high enrollment in
   this program were significantly greater than trends in counties with
   little Conservation Reserve Program enrollment. Although these data
   suggest that the Conservation Reserve Program may have benefitted
   Henslow's Sparrows in Illinois, this benefit has been insufficient to
   offset long-term declines due to other factors. Other conservation
   actions, beyond those associated with efforts aimed at reauthorizing and
   improving the Conservation Reserve Program, will likely be needed to
   achieve adequate protection for this species.}},
