@article{ ISI:A1993KR84600003,
Author = {TIERSCH, TR and MUMME, RL},
Title = {{AN EVALUATION OF THE USE OF FLOW-CYTOMETRY TO IDENTIFY SEX IN THE
   FLORIDA SCRUB JAY}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1993}},
Volume = {{64}},
Number = {{1}},
Pages = {{18-26}},
Month = {{WIN}},
Abstract = {{Two-parameter flow cytometry was used to measure sex differences in
   nuclear DNA content in the Florida Scrub jay (Aphelocoma c.
   coerulescens), a sexually monomorphic, cooperative breeder. Blood
   samples were obtained from 39 individuals, comprising 14 known males, 14
   known females and 11 birds of unknown sex. Although significant
   among-session variation complicated the analysis, the standard estimate
   of genome size for known males (3.159 +/- 0.019 pg DNA per nucleus) was
   significantly larger than that of known females (3.095 +/- 0.019 pg),
   and the distributions of genome size for the two sexes did not overlap.
   Sex of most birds of unknown sex could thereby be assigned
   unambiguously. The mean value for genome size was 3.129 +/- 0.036 pg
   (SD), and intraspecific variation was 4.63\% of the mean (range
   3.195-3.045 pg). Analysis of feather pulp samples, complicated by high
   levels of debris and variability in the fluorescence distributions, was
   improved by use of electronic bitmap gating.  Although flow cytometric
   techniques will probably not allow identification of sex in all cases,
   flow cytometry is nonetheless a potentially useful method for
   determining the sex of live birds.}},
