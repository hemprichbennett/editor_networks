@article{ ISI:A1996TR66200018,
Author = {Reid, JA and Horn, RB and Forsman, ED},
Title = {{A method for replacing tail-mounted radio transmitters on birds}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
