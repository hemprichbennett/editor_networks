@article{ ISI:A1993LQ76900009,
Author = {HAYS, H and PARKES, KC},
Title = {{ERYTHRISTIC EGGS IN THE COMMON TERN}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1993}},
Volume = {{64}},
Number = {{3}},
Pages = {{341-345}},
Month = {{SUM}},
Abstract = {{Erythrism, an abnormally reddish color, is known in the eggs of a few
   Laridae and Stercorariidae. It apparently results from the absence of
   one of the normal pigments and possibly an overdose of another.
   Erythrism in eggs of the Common Tern (Sterna hirundo) is exceedingly
   rare. Data from the long-term study of the large tern colony on Great
   Gull Island, New York, provide accurate information about relative
   frequency of the phenomenon and demonstrate its continuity in two
   females with changing mates.}},
