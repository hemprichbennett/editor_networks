@article{ ISI:A1991GH32200005,
Author = {HIRALDO, F and DELIBES, M and DONAZAR, JA},
Title = {{COMPARISON OF DIETS OF TURKEY VULTURES IN 3 REGIONS OF NORTHERN MEXICO}},
