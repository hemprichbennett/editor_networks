@article{ ISI:000075830900016,
Author = {Plentovich, SM and Holler, NR and Hill, GE},
Title = {{Site fidelity of wintering Henslow's Sparrows}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1998}},
Volume = {{69}},
Number = {{3}},
Pages = {{486-490}},
Month = {{SUM}},
Abstract = {{Little is known about the winter biology of many grassland bird species
   or about how alteration of winter habitat might be affecting
   populations. Therefore, we examined between-year and within-year winter
   site fidelity of the Henslow's Sparrow (Ammodramus henslowii) in Baldwin
   County, Alabama. In 1995, we banded 22 Henslow's Sparrows. In 1996, we
   banded 30 Henslow's Sparrows and recaptured 13 individuals a total of 20
   times. All birds were recaptured in the site where they were originally
   banded. No movement between sites was detected. No birds banded in 1995
   were recaptured in 1996. Our data suggest that Henslow's Sparrows tend
   to be site faithful within a winter, but not fr om one year to the next.}},
