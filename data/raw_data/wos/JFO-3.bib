@article{ ISI:A1995QH36400014,
Author = {RENKEN, RB and SMITH, JW},
Title = {{ANNUAL ADULT SURVIVAL OF INTERIOR LEAST TERNS}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1995}},
Volume = {{66}},
Number = {{1}},
Pages = {{112-116}},
Month = {{WIN}},
Abstract = {{During 1987-1990, 194 adult Least Terns (Sterna antillarum) were
   color-banded to estimate the adult annual survival rare for the
   population in the lower Mississippi River valley. A total of 103
   resightings were compiled and 40\% (78) of the 194 banded adults was
   resighted at least once during 1988-1992. Program JOLLY was used to
   calculate an adult annual survival rate. Model B, which assumes a
   constant survival rate and time-specific capture probabilities, was the
   most appropriate model for the data. The estimated adult annual survival
   rate was 85\% (SE = 0.057; 95\% CI = 0.73-0.95).}},
