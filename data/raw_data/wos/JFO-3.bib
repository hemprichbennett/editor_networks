@article{ ISI:A1996TR66200001,
Author = {Croll, DA and Jansen, JK and Goebel, ME and Boveng, PL and Bengtson, JL},
Title = {{Foraging behavior and reproductive success in chinstrap penguins: The
   effects of transmitter attachment}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1996}},
Volume = {{67}},
Number = {{1}},
Pages = {{1-9}},
Month = {{WIN}},
Abstract = {{The effects of radio transmitter attachment on foraging trip duration
   and reproductive success of breeding Chinstrap Penguins (Pygoscelis
   antarctica) were studied on Seal Island, South Shetland Islands,
   Antarctica. Attachment of transmitters having a cross-sectional area
   0.9\% that of the penguin had no significant effect on foraging trip
   duration, nest visit duration or the number of foraging trips per nest
   per day. Adults equipped with transmitters had significantly lower chick
   survival rates than control animals, however. This difference resulted
   from a higher nest failure rare of transmitter-equipped adults. Most of
   these failures occurred in the early part of the guard period. The
   average number of chicks raised to the creche stage in successful nests
   was similar for control and transmitter-equipped animals. Thus, chick
   mortality occurred as a result of failure of the entire nest (i.e.,
   abandonment by the adults) rather than lowered chick survival in
   individual nests. Nests at which both members were equipped with
   transmitters failed at a higher rate than nests at which one member was
   equipped with a transmitter. The transmitters used in this study may be
   considered a maximum size to be used for foraging trip duration
   experiments of medium-sized penguins.}},
