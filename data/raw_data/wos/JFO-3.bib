@article{ ISI:A1997WX04800010,
Author = {Klute, DS and Robel, RJ and Kemp, KE},
Title = {{Seed availability in grazed pastures and conservation reserve program
   fields during winter in Kansas}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1997}},
Volume = {{68}},
Number = {{2}},
Pages = {{253-258}},
Month = {{SPR}},
Abstract = {{Studies have documented the importance of Conservation Reserve Program
   (CRP) fields to breeding birds, but few have examined them as food
   sources for wintering birds. We compared the biomass of seeds in CRP
   fields to that in grazed native grass pastures in northeastern Kansas
   during two winters. Log transformed total seed biomass was significantly
   lower in grazed pastures than in CRP fields during the first winter but
   not the second. Total seed biomass in CRP fields was highly variable,
   and decreased between November and February. Seeds that were typically
   abundant in CRP fields are important food items of wintering grassland
   birds. In conclusion, CRP fields are superior to grazed native grass
   pastures in northeastern Kansas as winter foraging habitat for birds.}},
