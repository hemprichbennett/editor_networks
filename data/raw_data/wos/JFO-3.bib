@article{ ISI:A1992HE68400009,
Author = {FERNS, PN},
Title = {{THERMOREGULATORY BEHAVIOR OF ROCK DOVES ROOSTING IN THE NEGEV DESERT}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1992}},
Volume = {{63}},
Number = {{1}},
Pages = {{57-65}},
Month = {{WIN}},
Abstract = {{Free-living Rock Doves (Columba livia) were observed roosting throughout
   the day on ledges in a canyon in the Negev Desert. Potential
   thermoregulatory behaviors were recorded, including movement between
   sites in order to remain in the shade, the adoption of postures
   minimizing energy expenditure (and enhancing convective cooling),
   feather erection consistent with the cutaneous evaporation of water (and
   enhancing convective cooling) and elevated rates of blinking. These
   observations are consistent with recent laboratory studies on the heat
   balance of Rock Doves.}},
