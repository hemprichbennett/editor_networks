@article{ ISI:A1993KR84600013,
Author = {HITCHCOCK, RR and BALCOMB, R and KENDALL, RJ},
Title = {{MIGRATION CHRONOLOGY OF AMERICAN WIGEON IN WASHINGTON, OREGON AND
   CALIFORNIA}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1993}},
Volume = {{64}},
Number = {{1}},
Pages = {{96-101}},
Month = {{WIN}},
Abstract = {{Census data were compiled on American Wigeon (Anas americana) from 25
   National Wildlife Refuges for the years 1976-1985 to determine migration
   chronology in Washington, Oregon and California. Arrival of wigeon on
   wintering grounds in Washington begins in August, and in Oregon and
   California in September and October. Peak numbers of wigeon typically
   occur November-December in Washington and Oregon while populations in
   California peak in December-February. Migration to northern breeding
   areas is prominent in March and April. Wigeon are for the most part
   absent from California, Oregon and most of Washington, from May to
   September each year.}},
