@article{ ISI:A1993MZ23800015,
Author = {BLANCO, P and GOOSSEN, JP and ALONSO, HG and SIROIS, J},
Title = {{OCCURRENCES OF THE PIPING PLOVER IN CUBA}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1993}},
Volume = {{64}},
Number = {{4}},
Pages = {{520-526}},
Month = {{FAL}},
Abstract = {{Piping Plovers (Charadrius melodus) have been seen in Cuba as early as
   the 1800s. There are at least 14 known records of the Piping Plover in
   the Cuban archipelago for the period 1965-1992. The number of
   individuals seen ranged from 1 to 25 with most sightings made on Cuba's
   northern keys in the Sabana and Camaguey archipelagos. A cursory
   evaluation of Cuba's insular habitats, through limited surveys and
   examination of coastal relief and vegetation maps, suggests that Cuba is
   marginally suitable for wintering Piping Plovers. Habitats in some of
   the northern keys may be more suitable. Additional surveys are required
   to confirm this evaluation. Currently, the status of the Piping Plover
   in Cuba appears to be that of a rare winter visitor.}},
