@article{ ISI:A1996VA95400004,
Author = {Witham, JW and Kimball, AJ},
Title = {{Use of a geographic information system to facilitate analysis of
   spot-mapping data}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1996}},
Volume = {{67}},
Number = {{3}},
Pages = {{367-375}},
Month = {{SUM}},
Abstract = {{Spot mapping is widely used in bird research, but the time required to
   compile species composite maps and the limited utility of paper maps are
   a drawback to its use. A Geographic Information System (GIS) was used to
   enter and analyze spot-mapping data. A new program (BIRDMAP) was written
   using the programming language of PC ARC/INFO to digitize data from
   field maps. Use of BIRDMAP greatly reduced the time required to produce
   species composite maps. Specific applications and benefits of using a
   GIS for analysis of spot-mapping data are presented.}},
