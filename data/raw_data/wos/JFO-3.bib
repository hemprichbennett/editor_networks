@article{ ISI:A1995QH36400009,
Author = {WIEDENFELD, DA and WIEDENFELD, MG},
Title = {{LARGE KILL OF NEOTROPICAL MIGRANTS BY TORNADO AND STORM IN LOUISIANA,
   APRIL-1993}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1995}},
Volume = {{66}},
Number = {{1}},
Pages = {{70-80}},
Month = {{WIN}},
Abstract = {{A tornado and storm on 8 Apr. 1993 at Grand Isle, Louisiana, killed an
   estimated 40,000 birds of 45 species. The ten species killed in greatest
   numbers accounted for about half of the birds killed. Species of concern
   because of their already declining numbers included the Wood Thrush
   (Hylocichla mustelina) and Cerulean (Dendroica cerulea) and Swainson's
   Warblers (Limnothlypis swainsonii). This kill is the largest one
   recorded on the Gulf of Mexico coast. It may have been so severe because
   the storm occurred at the time of day when the greatest number of birds
   was arriving at the coast after having migrated all night northward
   across the Gulf.}},
