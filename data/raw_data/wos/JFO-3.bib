@article{ ISI:A1994PZ91400003,
Author = {TATE, DP},
Title = {{OBSERVATIONS ON NESTING-BEHAVIOR OF THE COMMON POTOO IN VENEZUELA}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1994}},
Volume = {{65}},
Number = {{4}},
Pages = {{447-452}},
Month = {{FAL}},
Abstract = {{Reproductive behavior of potoos (Nyctibiidae) is not well known; this
   paper reports nesting behavior of a pair of Common Potoos (Nyctibius
   griseus) during the rainy season in central Venezuela. The nest site was
   observed from 31 May 1991 to 20 Aug. 1991. The single nestling uas first
   seen 8 July, and was brooded by a parent until 18 July. It then roosted
   alone at the nest until its departure on 11 August, at an estimated age
   of 49 d. Adult potoos displayed three types of nest defense: remaining
   motionless (relying on camouflage), cautious movement to conceal the
   nestling, and employing an active, vocal distraction display at night.}},
