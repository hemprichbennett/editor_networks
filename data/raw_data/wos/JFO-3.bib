@article{ ISI:A1995QH36400015,
Author = {CASTELLANOS, A and ORTEGARUBIO, A},
Title = {{ARTIFICIAL NESTING SITES AND OSPREYS AT OJO-DE-LIEBRE AND GUERRERO-NEGRO
   LAGOONS, BAJA-CALIFORNIA-SUR, MEXICO}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1995}},
Volume = {{66}},
Number = {{1}},
Pages = {{117-127}},
Month = {{WIN}},
Abstract = {{Historically the non-migratory Osprey (Pandion haliaetus) population at
   the Ojo de Liebre and Guerrero Negro Lagoons, in Baja California Sur,
   Mexico, was confined to three small islands. In response to the addition
   of channel markers, electrical and utility poles and many human-made
   structures in and around the Lagoons during the 1950s, the Ospreys began
   nesting on suitable artificial structures. Nesting on artificial
   structures increased Osprey reproductive success, which caused an
   increase in Osprey numbers, and the structures themselves influenced
   nesting distributions. Now, nearly 50\% of the Ospreys in the area nest
   on artificial structures in and around both Lagoons.}},
