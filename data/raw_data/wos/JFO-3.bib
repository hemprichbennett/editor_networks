@article{ ISI:A1991GY13700010,
Author = {LOTT, DF},
Title = {{BRONZY SUNBIRDS TOLERATE INTRUSION ON FORAGING TERRITORIES BY FEMALE
   GOLDEN-WINGED SUNBIRDS THAT PERFORM BEGGING DISPLAY}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1991}},
Volume = {{62}},
Number = {{4}},
Pages = {{492-496}},
Month = {{FAL}},
Abstract = {{Female Golden-winged Sunbirds (Nectarinia reichenowi) are subordinate to
   both sexes of Bronzy Sunbirds (N. kilimensis). Nevertheless, they were
   often able to avoid being evicted from foraging territories of Bronzy
   Sunbirds by using a wing-quivering, tail-spread display strongly
   evocative of food-begging displays. The female Golden-winged Sunbirds
   appeared to be exploiting communication that functions in the
   reproduction of Bronzy Sunbirds.}},
