@article{ ISI:A1992HQ27900002,
Author = {MARTELLA, MB and NAVARRO, JL},
Title = {{CAPTURING AND MARKING GREATER RHEAS}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1992}},
Volume = {{63}},
Number = {{2}},
Pages = {{117-120}},
Month = {{SPR}},
Abstract = {{In 1989-1990, 14 Greater Rheas (Rhea americana) were neck-collared at
   Cordoba province, Argentina.  Birds were stupefied during the night by a
   light-beam and then caught using `boleadoras.'  The marking device was a
   numbered adjustable cattle-type PVC legband.  This capture and banding
   technique proved to be efficient and comparatively inexpensive, and
   could be effective both for other ratites and some large mammals with
   long legs.}},
