@article{ ISI:A1995QV96900017,
Author = {LINDSEY, GD and WILSON, KA and HERRMANN, C},
Title = {{COLOR-CHANGE IN HUGHESS CELLULOID LEG BANDS}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1995}},
Volume = {{66}},
Number = {{2}},
Pages = {{289-295}},
Month = {{SPR}},
Abstract = {{Color change was recorded in 10 colors for 237 Hughes celluloid bands
   attached to the tarsi of 84 birds for up to 5 yr and for 45 celluloid
   bands exposed to natural sunlight on Mauna I;ea, Hawaii. Colors of four
   solid-colored bands (mauve, light green, yellow and black) and two
   split-colored bands (orange/dark green and dark blue/white) attached to
   free-flying birds were still recognizable greater than or equal to 3 yr.
   Within 2-3 yr, light blue, orange and red solid-colored bands and a
   red/white split-colored band had faded and could be confused with other
   colors. For studies less than or equal to 2 yr, most Hughes celluloid
   bands retained enough color to be recognized under field conditions. For
   long-term studies, biologists should select only those colors that can
   be recognized for the duration of the study or use another type of color
   band that is more resistant to fading or discoloration.}},
