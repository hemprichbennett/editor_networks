
@article{ ISI:000077748900010,
Author = {Lubjuhn, T and Brun, J and Winkel, W and Muth, S},
Title = {{Effects of blood sampling in Great Tits}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1998}},
Volume = {{69}},
Number = {{4}},
Pages = {{595-602}},
Month = {{FAL}},
Abstract = {{Because of legal requirements in different countries as well as moral
   and ethical considerations, the effects of behavioral studies on animal
   welfare should be quantified. In this study, the effects of brood
   sampling on juvenile and adult Great Tits (Poecile major) were examined.
   Direct injuries were looked for as a possible short-term effect. We also
   quantified survival until fledging of nestlings that were bled versus
   those that were not bled as a possible medium-term effect, and survival
   until the next breeding season as well as the reproductive success in
   the year following the treatment were investigated as possible long-term
   effects. We found no effects of blood sampling in each of these
   instances.}},
