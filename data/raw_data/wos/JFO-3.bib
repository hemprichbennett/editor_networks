@article{ ISI:A1995QV96900002,
Author = {COLWELL, MA and GERSTENBERG, RH and WILLIAMS, OE and DODD, MG},
Title = {{4 MARBLED GODWITS EXCEED THE NORTH-AMERICAN LONGEVITY RECORD FOR
   SCOLOPACIDS}},
