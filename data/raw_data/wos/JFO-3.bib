@article{ ISI:A1996VA95400002,
Author = {Craig, AJFK and Hulley, PE},
Title = {{Supplementary head molt in Cape White-eyes: A consequence of nectar
   feeding?}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1996}},
Volume = {{67}},
Number = {{3}},
Pages = {{358-359}},
Month = {{SUM}},
Abstract = {{Cape White-eyes (Zosterops pallidus) frequently feed on nectar during
   early winter. This feeding can leave feathers on the forehead matted
   with nectar and pollen. At least some individuals then shed these
   feathers, at a time when the annual complete molt has already been
   terminated. It is suggested that such localized replacement of fouled
   plumage may occur regularly both, in while-eyes and in other
   short-billed nectar-feeders.}},
