@article{ ISI:A1992JZ88000001,
Author = {BROWN, BT},
Title = {{NESTING CHRONOLOGY, DENSITY AND HABITAT USE OF BLACK-CHINNED
   HUMMINGBIRDS ALONG THE COLORADO RIVER, ARIZONA}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1992}},
Volume = {{63}},
Number = {{4}},
Pages = {{393-400}},
Month = {{FAL}},
Abstract = {{Black-chinned Hummingbird (Archilochus alexandri) nests (n = 119) were
   located by means of systematic searches of two different riparian
   habitats along the Colorado River in Grand Canyon, Arizona, from April
   through July 1982-1987. Most nests (94\%) were located in the introduced
   riparian shrub tamarisk (Tamarix ramosissima). Active nests were present
   from 22 March through 10 July, with greater-than-or-equal-to 80\% of all
   nests active during the peak of nesting in May. Black-chinned
   Hummingbird nests were located only in tamarisk-dominated habitats,
   where nesting density ranged from 0 to 3.5 active nests/ha, and no nests
   were found in native habitats dominated by honey mesquite (Prosopis
   glandulosa). Nests were found only in riparian habitat patches that were
   greater-than-or-equal-to 0.5 ha in size, exhibited
   greater-than-or-equal-to 69\% vegetative cover and had a mean canopy
   height of greater-than-or-equal-to 1.4 m. Maintenance of this
   Black-chinned Hummingbird population depends on the continued existence
   of tamarisk-dominated habitat with these minimal patch-size
   characteristics.}},
