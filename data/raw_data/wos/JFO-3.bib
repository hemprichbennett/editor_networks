@article{ ISI:A1997WX04800013,
Author = {Purcell, KL},
Title = {{Use of fiberscope for examining cavity nests}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1997}},
Volume = {{68}},
Number = {{2}},
Pages = {{283-286}},
Month = {{SPR}},
Abstract = {{A system is described that uses a fiberscope to view nests in cavities
   to provide detailed information on eggs and nestlings. The flexible
   probe can be inserted around bends, and the tip articulates to allow
   viewing of the entire cavity and nest A light guide bundle furnishes
   light to enable viewing of dark cavities and optical fibers transmit the
   image from the lens to the eyepiece. Using the fiberscope, we were able
   to get accurate egg counts and to see details of the nestlings that
   indicated age and development, including feather growth and whether the
   eyes were open. Nest checks were also quicker than using the standard
   light and mirror apparatus. Drawbacks include cost and, possibly,
   durabilty.}},
