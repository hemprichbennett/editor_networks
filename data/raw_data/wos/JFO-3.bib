@article{ ISI:A1992JJ48600017,
Author = {FERRER, M and DELECOURT, C},
Title = {{SEX IDENTIFICATION IN THE SPANISH IMPERIAL EAGLE}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1992}},
Volume = {{63}},
Number = {{3}},
Pages = {{359-364}},
Month = {{SUM}},
Abstract = {{An external morphometric criterion for sex discrimination in the Spanish
   Imperial Eagle, Aquila adalberti, was investigated. Seven measurements
   were taken on 38 museum skins. Stepwise Discriminant Analysis selected
   forearm length, which classified correctly 94.7\% of the birds. Tarsus
   length was also a useful criterion for sexing eagles. Repeatability,
   accuracy for sexing eagles in different conditions (alive, museum skins
   and skeleton), and age were tested using tarsus and forearm length.
   Forearm length was proposed as the best measurement for sexing Spanish
   Imperial Eagles and perhaps other dimorphic raptors.}},
