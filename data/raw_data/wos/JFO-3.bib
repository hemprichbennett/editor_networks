@article{ ISI:A1996VA95400012,
Author = {Whitcomb, SD and OConnell, AF and Servello, FA},
Title = {{Productivity of the spruce grouse at the southeastern limit of its range}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1996}},
Volume = {{67}},
Number = {{3}},
Pages = {{422-427}},
Month = {{SUM}},
Abstract = {{We measured productivity of the Spruce Grouse (Dendragapus canadensis
   canadensis) in patchy black spruce (Picea mariana) habitat along the
   southeastern limit of its range in mid-coastal Maine. We captured grouse
   and attached necklace-mounted radio transmitters to hens prior to
   nesting. Of 19 females monitored, only 26\% raised chicks to the late
   brood-rearing period. Predation was high on hens (37\%) and five were
   killed before hatching eggs. Six (55\%) entire broods were lost and only
   30\% of chicks survived to late summer. Production (No. of
   chicks/female), an index of productivity was <1 and lower in Maine and
   Minnesota study areas in black spruce than areas dominated by jack pine
   (Pinus banksiana) or a mixture of jack pine and spruce with dense
   undergrowth. Where Spruce Grouse breed in patchy black spruce
   communities, immigration from neighboring populations or inter-patch
   movement by local individuals may be required to maintain viable
   populations.}},
