@article{ ISI:A1991GH32200004,
Author = {CASTRO, G and WUNDER, BA and KNOPF, FL},
Title = {{TEMPERATURE-DEPENDENT LOSS OF MASS BY SHOREBIRDS FOLLOWING CAPTURE}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1991}},
Volume = {{62}},
Number = {{3}},
Pages = {{314-318}},
Month = {{SUM}},
Abstract = {{It is well established that birds lose mass following capture.  In this
   paper the effects of bird species and temperature on mass loss are
   examined, and it is shown that mass loss after capture is independent of
   bird species, but strongly influenced by temperature, especially above a
   threshold of about 30 C.  Data on body mass must be collected
   immediately after capture to minimize this potential bias, especially on
   hot days.}},
