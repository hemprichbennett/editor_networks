@article{ ISI:A1996TR66200013,
Author = {Pravosudov, VV and Pravosudova, EV and Zimireva, EY},
Title = {{The diet of nestling Eurasian nuthatches}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1996}},
Volume = {{67}},
Number = {{1}},
Pages = {{114-118}},
Month = {{WIN}},
Abstract = {{The diet of nestling Eurasian Nuthatches (Sitta europaea) was studied in
   the Magadan region of northeastern Siberia in 1957-1990. The number of
   prey items in a load and mass of a load did not vary significantly among
   years and bloods. Mass of a load was positively related to the number of
   food items in a load. Plecoptera, Lepidoptera, Hymenoptera and
   Arachnoidea comprised the majority of nestling food both in frequency
   (76.6\%) and in mass (83.2\%). Almost all food types varied among years,
   but only a few prey types varied significantly. Arachnoidea were
   represented in nestling diet most evenly among all pears of study
   Individual variation in food types among broods was low with only one or
   two prey types varying significantly during each pear. The seeds of the
   Japanese stone pine found in the diet of nestling nuthatches suggest
   that nuthatches could have been using caches made at least seven mo
   previously.}},
