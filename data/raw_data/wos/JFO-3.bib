@article{ ISI:A1993MZ23800009,
Author = {BARRENTINE, CD and MCCLURE, HE},
Title = {{WHITE-CROWNED SPARROW SHIFTS WINTER HOME-RANGE NORTH}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1993}},
Volume = {{64}},
Number = {{4}},
Pages = {{470-471}},
Month = {{FAL}},
Abstract = {{A White-crowned Sparrow (Zonotrichia leucophrys gambelii) was recaptured
   over-wintering 145 km NW of where it was banded as an immature, 2 yr
   earlier. It is believed that this is the first time that a latitudinal
   shift in winter home range has been documented for this species. It is
   likely that fixation to a winter home range may be reversible, at least
   occasionally, in some White-crowned Sparrows.}},
