@article{ ISI:A1992HQ27900001,
Author = {MILLS, ED and ROGERS, DT},
Title = {{RATIOS OF NEOTROPICAL MIGRANT AND NEOTROPICAL RESIDENT BIRDS IN WINTER
   IN A CITRUS PLANTATION IN CENTRAL BELIZE}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1992}},
Volume = {{63}},
Number = {{2}},
Pages = {{109-116}},
Month = {{SPR}},
Abstract = {{Overwintering migrants composed 61.3\% of the individuals and 40.3\% of
   the species mist-netted in a large citrus plantation in central Belize
   during the winters of 1986-1987 and 1987-1988.  The percentage of
   migrants ranged from 50.0 to 80.6\% of the individuals within five
   individual orange and grapefruit groves.  The most frequently captured
   migrant species were Black-and-white Warbler (Mniotilta varia) and
   Magnolia Warbler (Dendroica magnolia), which together composed 36\% of
   the 509 neotropical migrants of 25 species captured during more than
   3300 net hours.  The Rufous-tailed Hummingbird (Amazilia tzacatl) was
   the most common of the 37 species of neotropical residents that were
