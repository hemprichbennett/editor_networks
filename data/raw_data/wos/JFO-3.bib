@article{ ISI:A1993KZ07500001,
Author = {HARMATA, AR and STAHLECKER, DW},
Title = {{FIDELITY OF MIGRANT BALD EAGLES TO WINTERING GROUNDS IN SOUTHERN
   COLORADO AND NORTHERN NEW-MEXICO}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1993}},
Volume = {{64}},
Number = {{2}},
Pages = {{129-134}},
Month = {{SPR}},
Abstract = {{Ten of 36 Bald Eagles (Haliaeetus leucocephalus) color-marked in a
   Colorado wintering area were found there during subsequent winters,
   whereas only one was known to winter elsewhere. Four adult eagles
   returned to the same winter ranges on which they had been radio-tracked
   during the winter of their capture, one color-marked adult was seen near
   its capture site for six succeeding winters, and a banded adult was
   found dead 3 km from its winter capture site 10 yr later. Percent of
   marked adults observed in subsequent years in the same wintering area
   was equivalent to that of immatures, but mean number of years adults
   were seen post-marking was greater, indicating traditional use of
   wintering grounds by adults. Breeding grounds of some adults from a
   southern Colorado wintering population were at least 1900 km north in
   cast-central Saskatchewan and western Manitoba, Canada.}},
Publisher = {{ASSOC FIELD ORNITHOLOGISTS}},
Address = {{BELOIT COLLEGE, DEPT BIOLOGY, 700 COLLEGE ST, BELOIT, WI 53511}},
