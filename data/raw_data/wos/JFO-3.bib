@article{ ISI:A1996TR66200011,
Author = {Soulliere, GJ and Rusch, TP},
Title = {{Nesting characteristics of Hooded Mergansers, Wood Ducks, European
   Starlings and Tree Swallows in Wisconsin}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1996}},
Volume = {{67}},
Number = {{1}},
Pages = {{100-104}},
Month = {{WIN}},
Abstract = {{Nest initiation dates and typical characteristics were recorded and
   summarized from 300 nests of four species of cavity-nesting birds in
   central Wisconsin. Hooded Mergansers (Mergus cucullatus) were the
   earliest nesters, followed by Wood Ducks (Air sponsa), European
   Starlings (Sturnus vulgaris) and Tree Swallows (Tachycineta bicolor).
   Egg laying began on 25 March, 13 April, 23 April and 12 May,
   respectively Modal nest initiation dates were 10 April for Hooded
   Mergansers, 30 April for Wood Ducks, 1 May for Starlings and 24 May for
   Tree Swallows. Composition of nest material and egg-shell color and
   thickness can be used to distinguish nest sites of these species before
   and after hatch.}},
