@article{ ISI:A1995RL13400001,
Author = {KAISER, GW and DEROCHER, AE and CRAWFORD, S and GILL, MJ and MANLEY, IA},
Title = {{A CAPTURE TECHNIQUE FOR MARBLED MURRELETS IN COASTAL INLETS}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1995}},
Volume = {{66}},
Number = {{3}},
Pages = {{321-333}},
Month = {{SUM}},
Abstract = {{Capturing Marbled Murrelets (Brachyramphus marmoratus) will facilitate
   demographic and telemetry studies needed to guide conservation and
   management programs. During the breeding season, an array of three mist
   nets floating on light rafts was used to catch 223 murrelets as they
   flew through narrow coastal channels. There was only one capture
   mortality. The mean capture times were 5 min before sunrise and 22 min
   after sunset. The mean capture rate was 2.5 birds/h (SE = 0.2) with a
   high of 8.8 birds/h on the evening of 25 Jun. 1994 in Theodosia Inlet,
   British Columbia.}},
