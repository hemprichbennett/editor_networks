@article{ ISI:A1991FH41100008,
Author = {JEWELL, SD and BANCROFT, GT},
Title = {{EFFECTS OF NEST-TRAPPING ON NESTING SUCCESS OF EGRETTA HERONS}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{1991}},
Volume = {{62}},
Number = {{1}},
Pages = {{78-82}},
Month = {{WIN}},
Abstract = {{A box trap was designed and used to capture 30 adult Egretta herons at
   active nests.  The trap was placed over a nest with eggs or nestlings
   and the door was manually closed by pulling a string when the bird had
   settled to incubate or brood.  The trap worked well for catching
   breeding adults.  Fifty-seven percent of the nests remained active after
   trapping.  The probability of the nest remaining active was greatest
   when birds were trapped in the morning at nests with nestlings at least
   4 d old.  Nest failures were most frequent when the trap was set between
   1000-1600 hours EDT on eggs or on chicks less than 4 d old.}},
