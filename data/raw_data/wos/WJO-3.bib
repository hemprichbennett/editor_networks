@article{ ISI:000259050800022,
Author = {Rimmer, Christopher C. and Woolaver, Lance G. and Nichols, Rina K. and
   Fernandez, Eladio M. and Latta, Steven C. and Garrido, Esteban},
Title = {{First description of nests and eggs of two Hispaniolan endemic species:
   Western Chat-tanager (Calyptophilus tertius) and Hispaniolan
   Highland-tanager (Xenoligea montana)}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2008}},
Volume = {{120}},
Number = {{1}},
Pages = {{190-195}},
Month = {{MAR}},
Abstract = {{We present the first nest descriptions for two Hispaniolan endemic
   songbirds. the Western Chat-tanager (Calyptophilus tertius) and
   Hispaniolan Highland-tanager (Xenoligea montana) from a montane
   broadleaf forest site in the Sierra de Bahoruco of the Dominican
   Republic. Single Western Chat-tanager nests were found On 17 May 7002
   and 9 June 2004. Both were coarsely-built, partially-domed, bulky
   structures 1.0-1.5 m above ground. One nest was, freshly-depreciated
   when found, whereas the second contained two eggs which hatched on 19
   and 20 June. The nestlings were depredated on 25 June. A Hispaniolan
   Highland-tanager nest found on 27 June 2004 fledged a single chick the
   following day. This nest, in a vine tangle 2.5 m above,round, was an
   open cup structure composed of moss, small herbaceous stems, leaf
   fragments. lichens, and other plant fibers. We describe the eggs of both
   species. the nestlings of Western Chat-tanager, and the juvenal plumage
   of Hispaniolan Highland-tanager. We believe that depreciation by
   introduced feral cats (Felis domesticus) and rats (Rattus spp.) is a
   serious problem in these montane forests.}},
