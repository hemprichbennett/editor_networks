@article{ ISI:000247126300007,
Author = {Lindell, Catherine A. and Riffell, Samuel K. and Kaiser, Sara A. and
   Battin, Andrea L. and Smith, Michelle L. and Sisk, Thomas D.},
Title = {{Edge responses of tropical and temperate birds}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2007}},
Volume = {{119}},
Number = {{2}},
Pages = {{205-220}},
Month = {{JUN}},
Abstract = {{Tropical birds may differ from temperate birds in their sensitivity to
   forest edges. We provide predictions about the proportions of tropical
   and temperate species that should avoid or exploit edges, and
   relationships between natural-history characters and edge responses. We
   conducted exploratory meta-analyses from 11 studies using 287 records of
   220 neotropical and temperate species' responses to edges to address our
   predictions. A higher proportion of neotropical species were
   edge-avoiders compared with temperate species and a higher proportion of
   temperate species were edge-exploiters compared with neotropical
   species. Edge-avoiding responses were positively associated with being
   an insectivore for neotropical birds, and with being of small body mass
   and a latitudinal migrant for temperate birds. Temperate edge-exploiters
   were less likely to be insectivores and migrants than temperate birds
   that were not edge-exploiters. A greater proportion of neotropical birds
   than temperate birds may be at risk from forest fragmentation if
   edge-avoidance is a reasonable indicator of an inability to adapt to
   land-cover change. Future progress in our understanding of forest bird
   responses to edges is dependent upon greater standardization of methods
   and designing studies in the context of recent theoretical developments.}},
