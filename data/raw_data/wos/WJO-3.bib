@article{ ISI:000259171400025,
Author = {Wang, Qiang and Li, Feng},
Title = {{Nest raising by Red-crowned Cranes in response to human-mediated
   flooding at Zhalong Nature Reserve, China}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2008}},
Volume = {{120}},
Number = {{3}},
Pages = {{610-613}},
Month = {{SEP}},
Abstract = {{Water was released from a reservoir in May 2005 to restore the wetland
   of international importance at Zhalong, China. As,I result. the water
   level on the floodplain rose rapidly. A pair of Red-crowned Cranes (Grus
   japonensis) was observed raisin,, their nest to avoid submersion and
   loss of eggs. The nest and two eggs were tended by both adults. but the
   eggs did not hatch. Hatching failure may have resulted from low egg
   temperatures due to addition of wet and new nesting materials, and
   reduction of incubation time during the flooding event. Reservoir
   discharge should avoid the breeding period of waterbirds and discharge
   rates should be reduced.}},
