@article{ ISI:000249466400029,
Author = {Lebbin, Daniel J. and Harvey, Michael G. and Lenz, Timothy C. and
   Andersen, Michael J. and Ellis, Jesse M.},
Title = {{Nocturnal migrants foraging at night by artificial light}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2007}},
Volume = {{119}},
Number = {{3}},
Pages = {{506-508}},
Month = {{SEP}},
Abstract = {{Artificial lights can have detrimental effects on nocturnal migrant
   birds and other wildlife, yet some species of typically diurnal
   insectivorous birds are capable of foraging at night under artificial
   illumination. Here, we report observations of at least 15 wood-warbler
   species (Parulidae), one tyrant-flycatcher (Tyrannidae), and one mimid
   (Mimidae) foraging at night in areas illuminated by powerful artificial
   lights. To our knowledge, our observations represent the first report of
   a mixed-species flock of birds foraging on insects attracted to
   artificial lights or within foliage illuminated by artificial lights at
   night.}},
