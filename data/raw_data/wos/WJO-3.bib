@article{ ISI:000269682500014,
Author = {Engilis, Jr., Andrew and Kelt, Douglas A.},
Title = {{FORAGING BEHAVIOR OF TUFTED TIT-TYRANTS (ANAIRETES PARULUS) IN SEMIARID
   NORTHCENTRAL CHILE}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2009}},
Volume = {{121}},
Number = {{3}},
Pages = {{585-592}},
Month = {{SEP}},
Abstract = {{We studied foraging behavior of Tufted Tit-Tyrants (Anairetes parulus)
   in Matorral (shrubland) habitat of northcentral Chile. This species is a
   generalist insectivore feeding in most shrubs of Matorral habitat at our
   study site, although they favored three of the dominant plant species.
   Their foraging is typical of.;mail tyrannid flycatchers, using rapid
   perch gleans coupled with hover gleans and supplemented by flycatching.
   They use relatively long search periods (35 see) followed by rapid
   gleans, which is typical for small tyrannids. Their active foraging (3.1
   +/- 1.8 prey attacks/min) coupled with a longer search time
   distinguishes them from parids or regulids of the Holarctic with which
   they often are compared. They generally forage singly or in pairs and
   aggressively defend what appears to be foraging territories in winter
   and summer. Densities of Tufted Tit-Tyrants at our study site were
   higher than reported in other studies from Chile and Argentina,
   presumably reflecting resource availability.}},
