@article{ ISI:000247126300012,
Author = {Duckworth, J. W. and Evans, T. D.},
Title = {{First records of White-browed Crake (Porzana cinerea) for Laos and its
   current range in southeast Asia}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2007}},
Volume = {{119}},
Number = {{2}},
Pages = {{253-258}},
Month = {{JUN}},
Abstract = {{All historical records of White-browed Crake (Porzana cinerea) in
   mainland Southeast Asia were south of the Isthmus of Kra, suggesting a
   Sundaic distribution. The first records from continental Thailand,
   Cambodia, Vietnam, and Laos revise the known resident distribution north
   almost to China. Information is inadequate to assess whether the species
   was historically overlooked across this huge area, or has genuinely
   expanded its range. Several factors suggest the latter, a pattern shown
   by no other bird species, and surprising given the pressing threats
   faced by wetlands and rallids in Southeast Asia.}},
