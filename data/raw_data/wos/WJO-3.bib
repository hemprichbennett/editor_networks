@article{ ISI:000276803600024,
Author = {Biasiolli, Traynor G.},
Title = {{Depredation of Black-throated Blue Warbler Nestlings by an Introduced
   Slug (Arionidae)}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2009}},
Volume = {{121}},
Number = {{2}},
Pages = {{422-423}},
Month = {{JUN}},
Abstract = {{Black-throated Bloc Warblers (Dendroica caerulescens) are subject to
   nest predation by a variety of avian and mammalian species. I present
   evidence that slugs (Gastropoda: Mollusca) can also function as nest
   predators. On two occasions. slugs were observed feeding on 6-7 day-old
   nestling Black-throated Blue Warblers at Hubbard Brook Experimental
   Forest. New Hampshire, USA. This is apparently the first report
   documenting that slugs can function as avian nest predators. Received 22
   February 2008. Accepted 25 December 2008.}},
