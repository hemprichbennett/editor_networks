@article{ ISI:000259171400003,
Author = {Brown, David R. and Sherry, Thomas W.},
Title = {{Solitary winter roosting of Ovenbirds in core foraging area}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
