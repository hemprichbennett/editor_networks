@article{ ISI:000237132500016,
Author = {Riehl, C},
Title = {{Widespread cannibalism by fledglings in a nesting colony of
   Black-crowned Night-Herons}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2006}},
Volume = {{118}},
Number = {{1}},
Pages = {{101-104}},
Month = {{MAR}},
Abstract = {{I studied the diet and foraging behavior of fledgling Black-crowned
   Night-Herons (Nycticorax nycticorax) in a mixed-species nesting colony
   of Black-crowned Night-Herons and Snowy Egrets (Egretta thula) in New
   Orleans, Louisiana. In I of 5 years, cannibalism of nestlings that had
   fallen or climbed out of nests was common, accounting for 66 of 94
   (70.2\%) prey items taken by fledglings. Juveniles took younger
   conspecifics by both predation and scavenging. Isolated incidents of
   cannibalism among Black-crowned Night-Herons have been reported
   previously, but not on a colony-wide scale.}},
