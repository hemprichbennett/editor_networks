@article{ ISI:000249466400018,
Author = {Wingelmaier, Katja and Winkler, Hans and Nemeth, Erwin},
Title = {{Mating behavior of reed buntings (Emberiza schoeniclus) in captivity}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2007}},
Volume = {{119}},
Number = {{3}},
Pages = {{463-466}},
Month = {{SEP}},
Abstract = {{We studied sexual pair behavior and cuckoldry of nine female and five
   male Reed Buntings (Emberiza schoeniclus) in a large outdoor aviary.
   Three males established small territories and paired with females. We
   observed 23 copulation attempts with identified partners during a period
   of approximately 6 weeks, 10 between social mates, 12 between impaired
   females and paired males (extrapair for males), and one between a paired
   female and a paired male which was not the social mate (extrapair for
   both). Both males and females initiated copulation attempts which, in
   most cases, were preceded by precopulatory displays. No forced
   copulations were observed and females appeared to have an active role in
   mating behavior.}},
