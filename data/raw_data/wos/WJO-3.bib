@article{ ISI:000269682500019,
Author = {Olson, Storrs L.},
Title = {{Additional Data on the Occurrence of the Plantaris Muscle in the
   Hawaiian Finches (Carduelinae: Drepanidini)}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2009}},
Volume = {{121}},
Number = {{3}},
Pages = {{621-623}},
Month = {{SEP}},
Abstract = {{New data are presented on the phylogenetically informative plantaris
   muscle in the Drepanidini. The primitive condition (presence)
   corroborates the basal placement of the creepers of the genera
   Oreomystis and Paroreomyza. The derived condition of the plantaris
   (absence) was found in the Hawaii Creeper (Loxops mana), previously
   considered by some to be congeneric with Oreomystis, and agrees with
   other evidence placing that species in Loxops. The Hawaii Akepa (L.
   coccineus), Anianiau (L. parvus), and Kauai Amakihi (L. stejnegeri) also
   lack the plantaris. This condition still appears to define a
   monophyletic group within the Drepanidini.}},
