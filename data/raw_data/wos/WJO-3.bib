@article{ ISI:000272963900031,
Author = {Gavin, Michael C. and Solomon, Jennifer N.},
Title = {{Active and Passive Bait-fishing by Black-crowned Night Herons}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2009}},
Volume = {{121}},
Number = {{4}},
Pages = {{844-845}},
Month = {{DEC}},
Abstract = {{Previous research reports bait-fishing in seven different species of
   ardeids. Our observations of Black-crowned Night Herons (Nycticorax
   nycticorax) on the island of Oahu, Hawaii, USA are the first to detail
   both passive and active bait-fishing in the same species. These
   observations provide support for the hypothesis that passive
   bait-fishing is a precursor to development of active bait-fishing
   methods in wading birds. Received 19 March 2009. Accepted 9 July 2009.}},
