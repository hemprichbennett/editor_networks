@article{ ISI:000245113700022,
Author = {Kleiber, Danika and Turner, Jenne and Budden, Amber E. and Dickinson,
   Janis L.},
Title = {{Interspecific egg-dumping by a Violet-green Swallow in an active Western
   Bluebird nest}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2007}},
Volume = {{119}},
Number = {{1}},
Pages = {{126-128}},
Month = {{MAR}},
Abstract = {{We observed a Violet-green Swallow (Tachycineta thalassina) laying an
   egg in an active Western Bluebird (Sialia mexicana) nest. The Western
   Bluebird male and not the female, was aggressive to the Violet-green
   Swallow but the swallow remained to lay the egg. This is the first
   documented incidence of which we are aware involving altricial
   interspecific egg-laying during the nestling phase. We suggest the
   timing of this event was more consistent with incidental egg deposition,
   or egg-dumping, than brood parasitism or nest usurpation.}},
