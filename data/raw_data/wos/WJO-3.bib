@article{ ISI:000272963900030,
Author = {Wittenberg, Sara Ress and Smith, Kimberly G.},
Title = {{Lack of Differential Migration in Juvenile Merlins and Northern Harriers
   During Fall Migration in the Florida Keys}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2009}},
Volume = {{121}},
Number = {{4}},
Pages = {{841-843}},
Month = {{DEC}},
Abstract = {{We used hydrogen stable isotopes in feathers collected from 1998 through
   2003 to examine if there was differential migration of juvenile Merlins
   (Falco columbarius) and Northern Harriers (Circus cyaneus) through the
   Florida Keys, USA in fall. We examined differences by gender,
   population, and intra-season variation for 120 Merlins and 60 harriers.
   We found no difference in delta D-f values among years, and between
   genders, and dates of capture for either species. Received 30 October
   2008. Accepted 15 April 2009.}},
