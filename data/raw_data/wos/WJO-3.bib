@article{ ISI:000243309800011,
Author = {Castillo-Guerrero, Jose Alfredo and Mellink, Eric},
Title = {{Maximum diving depth in fledging Blue-Footed Boobies: skill development
   and transition to independence}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2006}},
Volume = {{118}},
Number = {{4}},
Pages = {{527-531}},
Month = {{DEC}},
Abstract = {{We evaluated maximum diving depth and time spent at the nest of fledging
   Blue-footed Boohies (Sula nebouxii) at Isla El Rancho, Sinaloa, in the
   Gulf of California, Mexico. Within three consecutive 10-day
   post-fledging intervals, maximum diving depth was highly variable, but
   was not affected by sex, weight, or body condition. During the first
   days of post-fledging flight, maximum diving depth increased rapidly. By
   the second week after first flight, the plunge-dives of juveniles were
   almost as deep as those of adults. Parental care and attachment to the
   nest lasted several additional weeks (up to 40 days after first flight).
   Although their diving capacity rapidly reached a level similar to that
   of the adults, it appeared that juvenile boobies took much longer in
   acquiring other foraging skills.}},
