@article{ ISI:000269682500024,
Author = {Wong, Mark M. L. and Fedy, Brad C. and Wilson, Scott and Martin, Kathy
   M.},
Title = {{Adoption in Rock and White-tailed Ptarmigan}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2009}},
Volume = {{121}},
Number = {{3}},
Pages = {{638-641}},
Month = {{SEP}},
Abstract = {{Reports of adoption in birds are widespread, but few studies report
   rates of adoption or possible mechanisms for this phenomenon,
   particularly in the Order Galliformes. We report incidents of adoption
   in Rock Ptarmigan (Lagopus muta) and White-tailed Ptarmigan (L. leucura)
   from two sites in western Canada. Adoption rates for White-tailed
   Ptarmigan on Vancouver Island, British Columbia, and the Ruby Ranges,
   Yukon Territory were 13\% (n = 16 broods) and 4\% (n = 27),
   respectively, while rates for Rock Ptarmigan were 14\% (n = 29) in the
   Ruby Ranges. Low brood densities may result in lower rates of adoption
   for ptarmigan.}},
