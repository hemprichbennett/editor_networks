@article{ ISI:000238436300015,
Author = {Graber, AE and Davis, CA and Leslie, DM},
Title = {{Golden-cheeked Warbler males participate in nest-site selection}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2006}},
Volume = {{118}},
Number = {{2}},
Pages = {{247-251}},
Month = {{JUN}},
Abstract = {{Nest-site selection behaviors have rarely been described for songbirds.
   Furthermore, male involvement in nest-site selection is generally
   assumed to be minimal among most species, especially those predominantly
   exhibiting female nest building. This assumption has held true for the
   federally endangered Golden-cheeked Warbler (Dendroica chrysoparia), a
   breeding resident of central Texas. We observed Golden-cheeked Warbler
   males and females searching for nest sites together on three separate
   occasions, 2001-2003. Although rare, such observations add to our
   knowledge of the life history of songbirds.}},
