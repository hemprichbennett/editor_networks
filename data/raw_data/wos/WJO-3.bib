@article{ ISI:000237132500020,
Author = {Rivers, JW and Kuehn, MJ},
Title = {{Predation of Eared Grebe by Great Blue Heron}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2006}},
Volume = {{118}},
Number = {{1}},
Pages = {{112-113}},
Month = {{MAR}},
Abstract = {{Great Blue Herons (Ardea herodias) typically prey upon fish and other
   aquatic organisms, and they occasionally take small mammals and birds.
   We observed a Great Blue Heron attack, kill, and attempt to consume an
   Eared Grebe (Podiceps nigricollis'). The heron was unable to swallow the
   grebe. and it abandoned the carcass after approximately 30 min. An
   examination of the carcass showed that the grebe lacked obvious physical
   deformities. Our observation, coupled with a similar one nearby,
   indicates that Great Blue Herons attack and kill birds larger than
   reported previously.}},
