@article{ ISI:000269682500002,
Author = {Hartman, Patricia J. and Maehr, David S. and Larkin, Jeffery L.},
Title = {{HABITAT SELECTION BY CERULEAN WARBLERS IN EASTERN KENTUCKY}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2009}},
Volume = {{121}},
Number = {{3}},
Pages = {{469-475}},
Month = {{SEP}},
Abstract = {{We examined habitat characteristics associated with Cerulean Warblers
   (Dendroica cerulea) in breeding areas on the Cumberland Plateau in
   eastern Kentucky. Cerulean Warbler distribution was examined in 2004
   within a large forest stand and no differences were found between
   Occupied and unoccupied areas. We examined differences between preferred
   song perches and random points in 2005, and identified three potentially
   important predictors of Cerulean Warbler habitat: large diameter trees,
   east-facing aspect, and high shrub cover. Cerulean Warbler habitat
   selection on the Cumberland Plateau appears to be similar to other
   well-studied portions of the breeding range. Conservation and management
   efforts directed toward establishing and protecting large tracts of
   mature forest, especially those on cast-facing slopes, should be a
   reasonable approach to managing Cerulean Warbler populations in this
   area.}},
