@article{ ISI:000240843000014,
Author = {Buidin, Christophe and Rochepault, Yann and Savard, Michel and Savard,
   Jean-Pierre L.},
Title = {{Breeding range extension of the Northern Saw-whet Owl in Quebec}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2006}},
Volume = {{118}},
Number = {{3}},
Pages = {{411-413}},
Month = {{SEP}},
Abstract = {{Although the breeding range of the Northern Saw-whet Owl (Aegolius
   acadicus) is restricted to North America, the northern limits of its
   range are still unclear. In Quebec, the most northerly confirmed
   breeding records had come from the Saguenay area (Chicoutimi; 48 degrees
