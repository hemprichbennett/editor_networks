@article{ ISI:000297566400005,
Author = {Campedelli, Tommaso and Florenzano, Guido Tellini and Londi, Guglielmo
   and Cutini, Simonetta and Fornasari, Lorenzo},
Title = {{EFFECTIVENESS OF THE ITALIAN NATIONAL PROTECTED AREAS SYSTEM IN
   CONSERVATION OF FARMLAND BIRDS: A GAP ANALYSIS}},
