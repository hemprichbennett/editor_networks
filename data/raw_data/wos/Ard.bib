@article{ ISI:000299446500012,
Author = {Ciach, Michal},
Title = {{HABITAT-RELATED DIFFERENCES IN EGG SIZE IN THE SPUR-WINGED LAPWING
   VANELLUS SPINOSUS}},
Journal = {{ARDEOLA}},
Year = {{2011}},
Volume = {{58}},
Number = {{2}},
Pages = {{335-341}},
Month = {{DEC}},
Abstract = {{Egg size of the spur-winged lapwing Vanellus spinosus was studied in the
   Nile Delta, Egypt. Nests were located in three habitat types: on fish
   pond dykes, on dry fish pond bottoms, and in urban areas. Although
   clutch size did not differ between these habitat types, eggs from nests
   located on pond dykes were significantly smaller than those from nests
   located on pond bottoms and in urban areas. Differences in egg sizes
   between habitats may be related to food supply, parental quality or
   predation pressure.}},
