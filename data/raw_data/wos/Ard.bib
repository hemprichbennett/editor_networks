@article{ ISI:000395100800010,
Author = {Gil-Velasco, Marcel and Rouco, Miguel and Ferrer, Joan and
   Garcia-Tarrason, Manuel and Javier Garcia-Vargas, Francisco and
   Gutierrez, Antonio and Hevia, Ricardo and Lopez, Ferran and
   Lopez-Velasco, Daniel and Olle, Alex and Rodriguez, Guillermo and
   Sagardia, Juan and Antonio Salazar, Jose},
