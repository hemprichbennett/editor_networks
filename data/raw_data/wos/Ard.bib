@article{ ISI:000260441400003,
Author = {Gil-Tena, Assu and Torras, Olga and Saura, Santiago},
Title = {{RELATIONSHIPS BETWEEN FOREST LANDSCAPE STRUCTURE AND AVIAN SPECIES
   RICHNESS IN NE SPAIN}},
Journal = {{ARDEOLA}},
Year = {{2008}},
