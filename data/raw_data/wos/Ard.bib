Author = {Aguirre, Jose I. and Arana, Pilar and Teresa Antonio, Maria},
Title = {{TESTING EFFECTIVENESS OF DISCRIMINANT FUNCTIONS TO SEX DIFFERENT
   POPULATIONS OF MEDITERRANEAN YELLOW-LEGGED GULLS LARUS MICHAHELLIS
   MICHAHELLIS}},
