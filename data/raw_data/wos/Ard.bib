@article{ ISI:000291577700004,
Author = {D'Amico, Veronica L. and Barbosa, Andres},
Title = {{FLOCKING BEHAVIOUR DOES NOT FAVOUR HIGH CHEWING LICE LOAD IN SHOREBIRDS}},
