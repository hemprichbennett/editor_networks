@article{ ISI:000291577700012,
Author = {Cabeza Arroyo, Antonio and de la Cruz Solis, Carlos},
Title = {{INDIVIDUAL AND SEASONAL VARIABILITY IN TERRITORY SIZE OF THREE ADULT
