@article{ ISI:000437825000008,
Author = {Luis Telleria, Jose},
Title = {{OLD COUNTS SUGGEST THE COLLAPSE OF TWO RED-BACKED SHRIKE LANIUS COLLURIO
   POPULATIONS}},
