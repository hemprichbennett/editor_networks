@article{ ISI:000384573300003,
Author = {Rodriguez, Samuel and Barba, Emilio},
Title = {{EFFECTS OF COOL NEST MICROCLIMATES ON NESTLING DEVELOPMENT: AN
   EXPERIMENTAL STUDY WITH MEDITERRANEAN GREAT TITS PARUS MAJOR}},
Journal = {{ARDEOLA}},
Year = {{2016}},
Volume = {{63}},
Number = {{2}},
Pages = {{251-260}},
Month = {{SEP}},
Abstract = {{Suboptimal temperatures may exert negative effects on altricial
   nestlings, especially during their first days of life, when
   thermoregulation is not yet fully developed. We experimentally lowered
   nest temperatures by a mean of 4.5 degrees C during early development of
   great tit Parus major nestlings from the Mediterranean region. The
   thermal treatment only affected nestling size, as cooled nestlings had
   smaller tarsi by day 15 than controls. Female brooding constancy
   remained unaltered and female body condition was not negatively
   affected, so females did not incur additional energetic costs to
   ameliorate thermal conditions for nestlings. In conclusion, we found
   that colder nest microclimates may impair nestling growth, which may
   have negative consequences on future survival.}},
