@article{ ISI:000232856900003,
Author = {Le Cuziat, J and Vidal, E and Roche, P and Lacroix, F},
Title = {{Human activities affect the potential distribution of the Houbara
   Bustard Chlamydotis undulata undulata}},
Journal = {{ARDEOLA}},
Year = {{2005}},
