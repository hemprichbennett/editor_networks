@article{ ISI:000286503400016,
Author = {Perez-Granados, Cristian},
Title = {{DIET OF ADULT LESSER KESTRELS FALCO NAUMANNI DURING THE BREEDING SEASON
   IN CENTRAL SPAIN}},
Journal = {{ARDEOLA}},
Year = {{2010}},
Volume = {{57}},
Number = {{2}},
Pages = {{443-448}},
Month = {{DEC}},
Abstract = {{Pellet analyses were used to study dietary variation in the lesser
   kestrel during spring 2009 at a colony in central Spain. Coleoptera
   (92\%) were preferred in early spring and Orthoptera (22\%) in May and
   June. No seasonal changes in the abundance of other prey items were
   detected. Small mammals were the principal prey (50\%), in biomass
   terms, during the breeding season. Comparisons with previous studies
   show that lesser kestrel diet varies between colonies, chiefly according
   to the surrounding land use. It may also vary between and within years
   at the same colony according to changes in availability of different
   types of prey.}},
