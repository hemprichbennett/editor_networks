@article{ ISI:000329767600007,
Author = {Ramo, Cristina and Aguilera, Eduardo and Figuerola, Jordi and Manez,
   Manuel and Green, Andy J.},
Title = {{LONG-TERM POPULATION TRENDS OF COLONIAL WADING BIRDS BREEDING IN DONANA
   (SW SPAIN) IN RELATION TO ENVIRONMENTAL AND ANTHROPOGENIC FACTORS}},
Journal = {{ARDEOLA}},
Year = {{2013}},
Volume = {{60}},
