@article{ ISI:000274137100004,
Author = {Carrillo, Jose and Gonzalez-Davila, Enrique},
Title = {{LATITUDINAL VARIATION IN BREEDING PARAMETERS OF THE COMMON KESTREL FALCO
   TINNUNCULUS}},
