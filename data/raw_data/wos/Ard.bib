@article{ ISI:000232856900011,
Author = {Pons, P and Bas, JM},
Title = {{Open-habitat birds in recently burned areas: The role of the fire extent
   and species' habitat breadth}},
Journal = {{ARDEOLA}},
Year = {{2005}},
Volume = {{52}},
Number = {{1}},
