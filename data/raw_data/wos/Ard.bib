@article{ ISI:000286503400010,
Author = {Zehtindjiev, Pavel and Ilieva, Mihaela and Bogdanova, Maria I.},
Title = {{TEMPORAL DYNAMICS OF PASSERINE BIRD MIGRATION IN THE EASTERN PART OF THE
   BALKAN PENINSULA}},
