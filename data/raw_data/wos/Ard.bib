@article{ ISI:000189257300011,
Author = {Hernandez, M and Campos, F and Arizaga, J and Alonso, D},
Title = {{Migration of the bluethroat Luscinia svecica in the Iberian Peninsula}},
Journal = {{ARDEOLA}},
Year = {{2003}},
Volume = {{50}},
Number = {{2}},
Pages = {{259-263}},
Month = {{DEC}},
Abstract = {{We analysed 384 recoveries of Bluethroats (Luscinia svecica) ringed
   inside and outside the Iberian Peninsula and recovered inside or outside
   it. In the postbreeding season, Bluethroats enter the Iberian Peninsula
   by using two ways: one through the East and the other through the West
   of Pyrenees. Adult Bluethroats were more abundant than yearlings in
   December and January, and males were larger than females (excepting
   February-March). These data suggest that diferential migration could be
   proposed for this species.}},
