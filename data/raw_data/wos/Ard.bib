@article{ ISI:000297566400011,
Author = {Garcia-Barcelona, Salvador and Baez, Jose C. and Luz Marquez, Ana and
   Estrada, Alba and Real, Raimundo and Macias, David},
Title = {{DIFFERENTIAL AGE-RELATED PHENOLOGY IN LESSER BLACK-BACKED GULL LARUS
   FUSCUS WINTERING IN THE MALAGA AREA}},
Journal = {{ARDEOLA}},
Year = {{2010}},
Volume = {{57}},
Number = {{SI}},
Pages = {{129-134}},
Month = {{DEC}},
Abstract = {{We aimed to identify the existence of age-classes groups that shared
   similar seasonal patterns in migration movements for the lesser
   black-backed gull Larus fuscus. The gull age-classes groups were defined
   as sets of age-classes that were present in the harbour of Malaga (South
   of Spain) simultaneously during the wintering season. We distinguished
   ten groups of age-classes, which can be subsequently lumped into four
   big age-class groups: (i) immature stage-class, (ii) young breeders,
   (iii) age-classes from 6 to 11 years old, and (iv) age-classes older
   than 11 years old. Our present results supported the `dominance and
   arrival time model'.}},
