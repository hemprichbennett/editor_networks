@article{ ISI:000347395300013,
Author = {Panzeri, Mattia and Menchetti, Mattia and Mori, Emiliano},
Title = {{HABITAT USE AND DIET OF THE EURASIAN SCOPS OWL OTUS SCOPS IN THE
   BREEDING AND WINTERING PERIODS IN CENTRAL ITALY}},
Journal = {{ARDEOLA}},
Year = {{2014}},
Volume = {{61}},
Number = {{2}},
Pages = {{393-399}},
Month = {{DEC}},
Abstract = {{The Eurasian scops owl is one of the least known European nocturnal
   raptors. The species is declining throughout its distributional range,
   thus giving concern for its conservation status. We investigated
   seasonal shifts in its habitat use and diet in central Italy, where
   scops owl is a resident species in Central Tuscany. The owls used
   grasslands and human settlements during the breeding period, while
   woodlands were mainly used during the winter. The main prey species in
   the diet during the warm period were grasshoppers (Orthoptera,
   Tettigoniidae), whereas shrews (Soricomorpha) constituted the main prey
   during the winter. Thus, protecting extensive grassland would not
   suffice to conserve resident populations of Eurasian scops owls and
   preservation of mature deciduous woodlands with trunk cavities is also
   recommended.}},
