@article{ ISI:000272963900024,
Author = {Foote, Jennifer R. and Barber, Colleen A.},
Title = {{Paired Male Song Sparrows Sing More When Their Mate is Incubating}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2009}},
Volume = {{121}},
Number = {{4}},
Pages = {{819-822}},
Month = {{DEC}},
Abstract = {{The fertility announcement hypothesis proposes that avian males should
   sing at a high rate when their mate is fertile to guard their paternity.
   We examined if male Song Sparrows (Melospiza melodia) sing more during
   the incubation period when they are free of constraints of physical mate
   guarding. We assessed song rates of eight males for 1 hr-periods every
   1-2 days during each breeding stage over two consecutive broods within
   one season. Males sang significantly more during periods when their mate
   was incubating than during the pre-fertile, or fertile periods, or while
   feeding young (second brood). Males may be singing at high rates during
   incubation to maintain contact with their mate and/or to advertise for
   extra-pair copulations. Received 18 October 2008. Accepted 12 April
   2009.}},
