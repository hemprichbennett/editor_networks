@article{ ISI:000312285900020,
Author = {Wheeler, Hazel E.},
Title = {{A New Method for Trapping Chimney Swifts and Other Birds That Nest in
   Vertical Hollows}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{124}},
Number = {{4}},
Pages = {{802-807}},
Month = {{DEC}},
Abstract = {{Previous Chimney Swift (Chaetura pelagica) trapping efforts relied on
   chimney-top box traps at large roost sites, which were disruptive and
   inefficient when targeting individual birds. I designed a hoop net for
   use at nest chimneys to trap individual breeding birds. I caught 13
   swifts at 11 different sites in Guelph, Ontario, Canada over two summers
   with no site abandonment. This trap is superior for trapping individuals
   because it: (I) can be deployed quickly without scaling the chimney, (2)
   will work on chimneys of different sizes and shapes, and (3) minimizes
   disturbance to birds. My results indicate this net is an efficient,
   safe, and relatively simple method of trapping individual Chimney Swifts
   at their nest chimneys. Received 17 March 2012. Accepted 22 May 2012.}},
