@article{ ISI:000308786200023,
Author = {Loss, Scott R.},
Title = {{Nesting Density of Hermit Thrushes in a Remnant Invasive Earthworm-free
   Portion of a Wisconsin Hardwood Forest}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{124}},
Number = {{2}},
Pages = {{375-379}},
Month = {{JUN}},
Abstract = {{I observed an exceptionally high density of Hermit Thrush (Catharus
   guttatus) nests (3.1 nests/ha) over two breeding seasons in an isolated
   1.3-ha portion of an earthworm-free study site in the
   Chequamegon-Nicolet National Forest, Wisconsin. This density was much
   greater than the 0.1 to 0.6 nests/ha observed over the rest of the study
   area and exceeds by an order of magnitude most previously reported
   estimates for this species. The mean distance among Hermit Thrush nests
   in earthworm-free sites (215 m; 95\% CI = 180-250 m) was lower than in
   invaded sites (250 m; 95\% CI = 236-264 m); this difference was not
   statistically significant. Nest density did not differ significantly
   between categories. An abundance of suitable nest sites in a favored
   nesting substrate (clubmoss; Lycopodium spp.) could have contributed to
   the exceptionally high density of Hermit Thrush nests observed. High
   Hermit Thrush nest densities may occur in association with forest floor
   conditions that are characteristic of earthworm-free areas. Received 3
   August 2011. Accepted 16 January 2012.}},
