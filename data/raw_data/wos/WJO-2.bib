@article{ ISI:000308786200026,
Author = {Gow, Elizabeth A. and Wiebe, Karen L.},
Title = {{An Unusually Synchronous Double Brooding Attempt by a Northern Flicker
   Pair}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{124}},
Number = {{2}},
Pages = {{389-392}},
Month = {{JUN}},
Abstract = {{We report an unusual case of a monogamous pair of Northern Flickers
   (Colaptes atirauts) initiating a second clutch while there were still
   nestlings in the first nest. The male and female incubated at both nests
   but only the male fed nestlings at the first nest. The second nest
   attempt was abandoned after the first nestlings fledged, possibly
   because the two broods were too synchronous in timing for the male to
   contribute sufficiently to both. Double-brooding has not been previously
   documented for Northern Flickers.}},
