@article{ ISI:000285174000015,
Author = {Dickens, Molly J. and Romero, L. Michael},
Title = {{STRESS RESPONSIVENESS DECREASES WITH AGE IN PRECOCIAL, JUVENILE CHUKAR}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2010}},
Volume = {{122}},
Number = {{4}},
Pages = {{762-766}},
Month = {{DEC}},
Abstract = {{Development of the hypothalamic-pituitary-adrenal (HPA) axis and
   subsequent corticosterone (CURT) release in newly hatched birds is a
   balance between limiting exposure to the detrimental effects of CURT on
   growth and development, and the necessity of mounting an acute stress
   response. We measured the stress responsiveness of juvenile Chukar
   (Alectoris chukar) 20 to 60 days post-hatch prior to molting into full
   adult plumage. The integrated CURT response during 60 min of restraint
   in these individuals decreased with age. Comparisons to mean adult
   integrated CURT values imply the youngest juveniles have greater
   responses than adults while the oldest juveniles have reduced CURT
   responses. This pattern is currently an anomaly within the
   altricial-precocial range and suggests specific tradeoffs, such as
   molting into adult plumage, may affect timing of HPA suppression during
   development. Received 23 November 2009. Accepted 20 April 2010.}},
