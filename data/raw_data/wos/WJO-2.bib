@article{ ISI:000288360100025,
Author = {Long, Ashley M.},
Title = {{Orientation of Sap Wells Excavated by Yellow-bellied Sapsuckers}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2011}},
Volume = {{123}},
Number = {{1}},
Pages = {{164-167}},
Month = {{MAR}},
Abstract = {{Ambient temperature may influence selection of foraging sites by
   organisms that use sap as a primary food source. I examined the spatial
   orientation of sap wells excavated by Yellow-bellied Sapsuckers
   (Sphyrapicus varius) on pine trees (Pious spp.; n = 43) in eastern
   Kansas. Sap wells were oriented toward the southwest ((a) over bar =
   246.04 degrees, s = 65.46 degrees, P = 0.004), unlike in previous
   studies. Benefits of southwesterly sap well orientation may include
   avoidance of high winds while foraging and increased flow of sap on the
   sides of trees warmed by afternoon light. Received 2 November 2009.
   Accepted 25 October 2010.}},
