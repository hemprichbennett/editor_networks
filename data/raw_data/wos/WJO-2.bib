@article{ ISI:000320893300028,
Author = {Zimorski, Sara E. and Perkins, Tandi L. and Selman, Will},
Title = {{Chelonian Species in the Diet of Reintroduced Whooping Cranes (Grus
   americana) in Louisiana}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2013}},
Volume = {{125}},
Number = {{2}},
Pages = {{420-423}},
Month = {{JUN}},
Abstract = {{Migratory and non-migratory Whooping Cranes (Grus americana)
   historically inhabited southwestern Louisiana until they were extirpated
   in 1950. Little is known about the feeding habits or dietary items of
   these cranes except for anecdotal evidence from local residents provided
   to R. P. Allen for his influential 1952 work on Whooping Cranes. Other
   populations of Whooping Cranes have been characterized as opportunistic
   omnivores, consuming small vertebrates, invertebrates, and plant
   material. In 2009, southwestern Louisiana was selected as a
   reintroduction site for Whooping Cranes. We report on four observations
   of reintroduced Whooping Cranes depredating at least two different
