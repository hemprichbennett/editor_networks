@article{ ISI:000278678300005,
Author = {Franzreb, Kathleen E.},
Title = {{RED-COCKADED WOODPECKER MALE/FEMALE FORAGING DIFFERENCES IN YOUNG FOREST
   STANDS}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
