@article{ ISI:000308786100021,
Author = {Collins, Charles T. and Thomas, Betsy Trent},
Title = {{Food Habits of Two Fork-tailed Swifts in Venezuela}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{124}},
Number = {{1}},
Pages = {{152-157}},
Month = {{MAR}},
Abstract = {{The aerial arthropod prey of Neotropical Palm Swifts (Tachornis
   squamata) and Lesser Swallow-tailed Swifts (Panyptila cayennensis) in
   Venezuela included seven Orders and 60 families of insects plus spiders
   and mites. Diptera were the most numerous prey (>50\%) taken by both
   swifts. Prey size ranged from 0.5 to 7.9 mm and averaged 2.43 and 2.77
   mm, respectively. Both prey type and foraging habitat differences of
