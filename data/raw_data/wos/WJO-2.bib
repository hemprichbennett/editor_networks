@article{ ISI:000282115100029,
Author = {Suzuki, Toshitaka N. and Tsuchiya, Yuko},
Title = {{Feeding a Foreign Chick: a Case of a Mixed Brood of Two Tit Species}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2010}},
Volume = {{122}},
Number = {{3}},
Pages = {{618-620}},
Month = {{SEP}},
Abstract = {{We report on a mixed brood of Great (Parus major) and Varied (Poecile
   varius) tits. The brood consisted of one Great Tit and five Varied Tits.
   Incubation and food provisioning to nestlings were by adult Varied Tits,
   but not by adult Great Tits. The foreign Great Tit nestling tended to
   receive more food than the average Varied Tit nestling. Provisioning of
   the foreign Great Tit by Varied Tits lasted even after fledging. We
   discuss the causes and outcomes of this rare phenomenon. Received 5
   December 2009. Accepted 9 March 2010.}},
