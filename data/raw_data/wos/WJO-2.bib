@article{ ISI:000288360100007,
Author = {Brooks, Matthew E. and Stouffer, Philip C.},
Title = {{INTERSPECIFIC VARIATION IN HABITAT PREFERENCES OF GRASSLAND BIRDS
   WINTERING IN SOUTHERN PINE SAVANNAS}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2011}},
Volume = {{123}},
Number = {{1}},
Pages = {{65-75}},
Month = {{MAR}},
Abstract = {{We studied wintering grassland bird communities in De Soto National
   Forest in southern Mississippi, USA to assess differences in bird
   communities and vegetation structure among different stand types. We
   also examined which vegetation structure and plant species predicted
   occurrence of Bachman's Sparrow (Peucaea aestivalis), Hens low's Sparrow
   (Ammodramus henslowii), and Sedge Wren (Cistothorus platensis).
   Bachman's Sparrows occurred only in uplands ((x) over bar = 0.5
   birds/ha) and stands managed for Red-cockaded Woodpeckers (Picoides
   borealis; (x) over bar = 0.9 birds/ha), Hens low's Sparrows occurred
   only in bogs ((x) over bar = 3.8 birds/ha) and stands managed for
   Red-cockaded Woodpeckers ((x) over bar = 2.1 birds/ha), while Sedge
   Wrens occurred in all stand types ((x) over bar = 0.1-0.3 birds/ha).
   There were no significant differences among stand types in total bird
   densities for all three species combined. Dense, spatially uniform
   herbaceous cover and cover of Scleria muhlenbergii, a preferred food
   item in bogs, best predicted Hens low's Sparrow occurrence (39\% s(2)
   explained). Increased woody understory vegetation and decreased tree
   density best predicted Sedge Wren occurrence (17\% s(2) explained).
   Management for Hens low's Sparrows should focus on small-scale
   herbaceous ground-layer restoration in bogs. Bachman's Sparrows will
   respond more to thinning dense upland stands. Sedge Wrens and Bachman's
   Sparrows benefit from Red-cockaded Woodpecker management, whereas Hens
   low's Sparrow use of woodpecker stands is ephemeral. Received 6 March
   2010. Accepted 9 September 2010.}},
