@article{ ISI:000312285900025,
Author = {Gayk, Zach G. and Lindsay, Alec R.},
Title = {{Winter Microhabitat Foraging Preferences of Sympatric Boreal and
   Black-capped chickadees in Michigan's Upper Peninsula}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{124}},
Number = {{4}},
Pages = {{820-824}},
Month = {{DEC}},
Abstract = {{We examined differences in microhabitat use between Boreal (Poecile
   hudsonicus) and Black-capped chickadees (P. atricapillus) where they
   co-occur near Marquette, Michigan, USA. Twenty-four Boreal and 37
   Black-capped chickadees were followed during 60 hrs of field
   observation. Boreal Chickadees foraged only in three conifer species,
   76\% of which were black spruce (Picea mariana), while Black-capped
   Chickadees foraged widely across six coniferous and three deciduous tree
   species. Analysis of foraging data categorized by zones within conifer
   trees indicated high niche overlap (0.676) between Boreal and
   Black-capped chickadees across all foraging zones. Individual
   comparisons on a zone-by-zone basis revealed a significant difference in
   foraging occupancy in the medial portion of the crowns of conifer trees
   (P = 0.0002). Our results indicate exclusive use by Boreal Chickadees of
   dense medial foliage within the top 3 m of conifer crowns. Received 22
   March 2012. Accepted 31 July 2012.}},
