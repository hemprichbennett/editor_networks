@article{ ISI:000298314500009,
Author = {Fu, Yi-Qiang and Dowell, Simon D. and Zhang, Zheng-Wang},
Title = {{BREEDING ECOLOGY OF THE EMEI SHAN LIOCICHLA (LIOCICHLA OMEIENSIS)}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2011}},
Volume = {{123}},
Number = {{4}},
Pages = {{748-754}},
Month = {{DEC}},
Abstract = {{We studied the breeding ecology of the Emei Shan Liocichla (Liocichla
   omeiensis) in two nature reserves (Laojunshan and Wawushan) in Sichuan,
   China from March to August 2009 and April to August 2010. The breeding
   density (mean +/- SE) was between 1.20 +/- 0.46 and 1.55 +/- 0.56
   males/km(2), and the breeding season was from late April to mid-August
   and from mid-May to late August in Laojunshan Nature Reserve in 2009 and
   2010, respectively. This species used the edges of or gaps within
   natural broadleaf forest between elevations of 1,450 and 2,150 m for
   nesting. Vegetation around nests was mainly bamboo (Chimonobambusa
   szechuanensis), small shrubs, and lianes with few large trees. Nests
   were cup-shaped with a mean height of 137.5 +/- 4.6 cm above the ground.
   Nests were mainly in C. szechuanensis. Females laid one egg per clay in
   the morning and the mean (+/- SE) clutch size was 2.9 +/- 0.2 eggs.
   Incubation started after the last egg was laid. Both males and females
   were observed participating in incubation, provisioning, and brooding
   the nestlings. Hatching success, fledging success, and nest success were
   58.6, 70.8, and 27.5\%, respectively. Nest predation and human
   disturbances were the two main factors affecting breeding success of
   Emei Shan Liocichlas. Received 13 January 2011. Accepted 22 May 2011.}},
