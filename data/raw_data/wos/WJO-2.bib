@article{ ISI:000285174000017,
Author = {Jablonski, Kevin E. and McNulty, Stacy A. and Schlesinger, Matthew D.},
Title = {{A Digital Spot-mapping Method for Avian Field Studies}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2010}},
Volume = {{122}},
Number = {{4}},
Pages = {{772-776}},
Month = {{DEC}},
Abstract = {{Avian mapping, also known as spot mapping or territory mapping, is a
   breeding season bird-survey technique that traditionally uses paper maps
   on which locations of birds are recorded. This method is often
   considered the most accurate in yielding a density, but has been
   criticized as being inefficient, time consuming, and inexact. We
   describe a novel digital-mapping method, incorporating a hand-held
   computer and high-accuracy global positioning system receiver (GPS),
   used in an ongoing boreal birds study. Digital mapping surpassed our
   expectations as to efficiency, flexibility, and work flow. We expect
   this method will become increasingly useful in many types of field
   studies, especially as costs decrease (currently similar to\$2,100 for
   the field receiver used). Received 4 January 2010. Accepted 23 June
   2010.}},
