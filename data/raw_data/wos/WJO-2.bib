@article{ ISI:000291776300002,
Author = {Carpenter, John P. and Wang, Yong and Schweitzer, Callie and Hamel, Paul
   B.},
Title = {{AVIAN COMMUNITY AND MICROHABITAT ASSOCIATIONS OF CERULEAN WARBLERS IN
   ALABAMA}},
