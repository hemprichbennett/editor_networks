@article{ ISI:000288360100019,
Author = {Buitron-Jurado, Galo and Galarza, Juan M. and Guarderas, Danny},
Title = {{First Description of Nests and Eggs of Chestnut-headed Crake
   (Anurolimnas castaneiceps) from Ecuador}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2011}},
Volume = {{123}},
Number = {{1}},
Pages = {{142-145}},
Month = {{MAR}},
Abstract = {{We describe the nest and eggs of the Chestnut-headed Crake (Anurolimnas
   castaneiceps) based on observations of two nests found in the border of
   the Lliquino River, Pastaza Province, Ecuador. Nests were found in June
   and December with birds incubating eggs. Both nests were on fallen logs
   covered by vines and epiphytes in natural small gaps. They were open
   cups and built principally with dead leaves. The coloration of the eggs
   was pinkish white with scattered brown spots, similar to other Amazonian
   rails and crakes. The nests were similar in structure to those of wood
   rails (Aramides spp.) suggesting a close relationship between
   Anurolimnas and Aramides. Received 24 May 2010. Accepted 14 October
   2010.}},
