@article{ ISI:000291776300026,
Author = {Boves, Than J. and Buehler, David A. and Boves, N. Emily},
Title = {{Conspecific Egg Destruction by a Female Cerulean Warbler}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2011}},
Volume = {{123}},
Number = {{2}},
Pages = {{401-403}},
Month = {{JUN}},
Abstract = {{Conspecific egg destruction is an adaptive behavior that has typically
   evolved in multi-brooded, polygynous, or colonial avian species, and can
   be difficult to observe. We describe the first case of egg destruction
   in the Parulidae, which consists of mostly single-brooded and socially
   monogamous species. In this case, a female Cerulean Warbler (Dendroica
   cerulea) destroyed 5-day old eggs at a conspecific's nest. This act was
   likely committed to secure a breeding opportunity with a high quality
   male or to decrease local competition for resources. There is also the
