@article{ ISI:000294795400028,
Author = {Triana, Emilia and Sandova, Luis},
Title = {{Nest Reuse by the Scintillant Hummingbird (Selasphorus scintilla)}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
Year = {{2011}},
Volume = {{123}},
Number = {{3}},
Pages = {{635-638}},
Month = {{SEP}},
Abstract = {{Nest reuse behavior in birds is rare because nests are ephemeral
   structures. We describe the first record of multi-season nest reuse by
   the Scintillant Hummingbird (Selasphorus scintilla). The nest was a
   multi-cup of four nests with newer nests placed on top of older nests.
   The nest was under the eave of a roof, which may have reduced nest
   disintegration and facilitated nest reuse. Received 19 November 2010.
   Accepted 19 March 2011.}},
