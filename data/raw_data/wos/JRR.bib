@article{ ISI:000360450500004,
Author = {Barnes, Joseph G. and Haley, Ross D. and Thompson, Daniel B. and Jaeger,
   Jef R.},
Title = {{ATTRIBUTES OF A BREEDING POPULATION OF PEREGRINE FALCONS ASSOCIATED WITH
   RESERVOIRS ON THE COLORADO RIVER}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
