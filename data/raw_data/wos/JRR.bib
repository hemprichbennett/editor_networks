@article{ ISI:000403125300001,
Author = {Newton, Ian},
Title = {{INVITED COMMENTARY: FIFTY YEARS OF RAPTOR RESEARCH}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
