@article{ ISI:000355316700006,
Author = {Piana, Renzo P.},
Title = {{HABITAT ASSOCIATIONS WITHIN A RAPTOR COMMUNITY IN A PROTECTED AREA IN
   NORTHWEST PERU}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
