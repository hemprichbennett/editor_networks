@article{ ISI:000341096200004,
Author = {Bruggeman, Jason E. and Andersen, David E. and Woodford, James E.},
Title = {{FACTORS RELATED TO NORTHERN GOSHAWK LANDSCAPE USE IN THE WESTERN GREAT
   LAKES REGION}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
