@article{ ISI:000327908000003,
Author = {Haley, Katherin L. and Rosenberg, Daniel K.},
Title = {{INFLUENCE OF FOOD LIMITATION ON REPRODUCTIVE PERFORMANCE OF BURROWING
   OWLS}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2013}},
