@article{ ISI:000311868700006,
Author = {Keough, Heather L. and Conover, Michael R.},
Title = {{BREEDING-SITE SELECTION BY FERRUGINOUS HAWKS WITHIN UTAH'S UINTAH BASIN}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
