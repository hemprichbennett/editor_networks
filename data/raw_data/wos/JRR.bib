@article{ ISI:000346408100005,
Author = {Saggese, Miguel D. and Roesler, Ignacio and Marano, Claudia F.},
Title = {{WINTERING OF OSPREYS IN ARGENTINA: INSIGHTS FROM NEW RECORDS BETWEEN
   1993-2008}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
