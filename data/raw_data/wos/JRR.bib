@article{ ISI:000324507200005,
Author = {Johnson, David H. and Swengel, Scott R. and Swengel, Ann B.},
Title = {{SHORT-EARED OWL (ASIO FLAMMEUS) OCCURRENCE AT BUENA VISTA GRASSLAND,
   WISCONSIN, DURING 1955-2011}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
