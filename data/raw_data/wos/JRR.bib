@article{ ISI:000355316700005,
Author = {Keough, Heather L. and Conover, Michael R. and Roberts, Anthony J.},
Title = {{FACTORS INFLUENCING REPRODUCTIVE SUCCESS OF FERRUGINOUS HAWKS IN THE
   UINTAH BASIN, UTAH}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
