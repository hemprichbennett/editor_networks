@article{ ISI:000305313200004,
Author = {Frye, Graham G.},
Title = {{AUTUMN MIGRATION ECOLOGY OF THE NORTHERN SAW-WHET OWL (AEGOLIUS
   ACADICUS) IN NORTHERN MONTANA}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
