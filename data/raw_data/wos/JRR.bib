@article{ ISI:000346408100006,
Author = {Bierregaard, Jr., Richard O. and Ben David, Augustus and Gibson, Lori
   and Kennedy, Robert S. and Poole, Alan F. and Scheibel, Michael S. and
   Victoria, Julie},
Title = {{POST-DDT RECOVERY OF OSPREY (PANDION HALIAETUS) POPULATIONS IN SOUTHERN
   NEW ENGLAND AND LONG ISLAND, NEW YORK, 1970-2013}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2014}},
Volume = {{48}},
Number = {{4}},
Pages = {{361-374}},
Month = {{DEC}},
Abstract = {{The Osprey (Pandion haliaetus) population nesting between New York City
   and Boston, Massachusetts, collapsed from approximately 1000 pairs in
   1940 to 109 in the early 1970s. In the 1970s, within five or six years
   of the cessation of DDT use in the region, the Osprey population began
   recovering. The recovery was asynchronous across the region. Current
   (2013) distribution and numbers differ dramatically from those of the
   pre-DDT period. Colonies on Narragansett and Mount Hope bays in Rhode
   Island, the Connecticut River estuary, and on Gardiners Island, New
   York, failed to recover their former remarkable densities. Osprey
   populations expanded from eastern to western Long Island, New York. In
   Connecticut, Ospreys now occupy the entire coastline and are nesting
   inland. A new concentration of Ospreys has become established in
   southeastern Massachusetts on the Westport River, the islands of
   Martha's Vineyard and Nantucket, and most recently, on Cape Cod. From
   this area, the species' range has gradually expanded northward. The
   population in Massachusetts is now contiguous with the New Hampshire
   population, and pairs are nesting west of the Connecticut River. The
   current population in southern New England easily exceeds 1200 pairs and
   is predominantly (ca. 95\%) nesting on human-made structures either
   erected as nest platforms or co-opted by Ospreys as nest support
   structures.}},
