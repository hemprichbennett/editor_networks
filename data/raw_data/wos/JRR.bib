@article{ ISI:000324507200004,
Author = {Dennhardt, Andrew J. and Wakamiya, Sarah M.},
Title = {{EFFECTIVE DISPERSAL OF PEREGRINE FALCONS (FALCO PEREGRINUS) IN THE
   MIDWEST, USA}},
