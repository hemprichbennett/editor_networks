@article{ ISI:000285441200005,
Author = {Holroyd, Geoffrey L. and Trefry, Helen E. and Duxbury, Jason M.},
Title = {{WINTER DESTINATIONS AND HABITATS OF CANADIAN BURROWING OWLS}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2010}},
Volume = {{44}},
Number = {{4}},
Pages = {{294-299}},
Month = {{DEC}},
Abstract = {{The winter destination of Burrowing Owls (Athene cunicularia) that breed
   in Canada is poorly understood. We flew 166.7 hr in a fixed-wing CESSNA
   172, searching southern Texas and the Gulf Coast lowlands and central
   Mexico for signals from 125 VHF transmitters that were attached to
   Burrowing Owls in Canada in 1997, 1998, and 2000. Nine owls were located
   between Houston, Texas, and Michoacan, Mexico. The diurnal roosts used
   by the owls included vegetation, natural burrows, and a woodpile.
   Land-cover types around roosts were highly variable and less open than
   breeding habitat in Canada, but always included at least 35\% low
   vegetation within I km of roosts.}},
