@article{ ISI:000332750600004,
Author = {Barrowclough, George F. and Groth, Jeff G. and Lai, Jonas E. and Tsang,
   Susan M.},
Title = {{THE PHYLOGENETIC RELATIONSHIPS OF THE ENDEMIC GENERA OF AUSTRALO-PAPUAN
   HAWKS}},
