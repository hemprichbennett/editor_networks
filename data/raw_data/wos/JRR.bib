@article{ ISI:000301965600010,
Author = {Beckett, Sean R. and Proudfoot, Glenn A.},
Title = {{SEX-SPECIFIC MIGRATION TRENDS OF NORTHERN SAW-WHET OWLS IN EASTERN NORTH
   AMERICA}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2012}},
Volume = {{46}},
Number = {{1}},
Pages = {{98-108}},
Month = {{MAR}},
Abstract = {{We used information compiled by the U.S. Geological Survey's Bird
   Banding Laboratory to identify sex-specific trends in annual Northern
   Saw-whet Owl (Aegolius acadicus) movement during autumn migration across
   eastern North America. The study included 40 710 individuals with
   assigned sex, 14\% of which were assigned male and 86\% were assigned
   female. Males were disproportionately handed at higher latitudes, and
   regional differences existed in sex ratios of owls at different banding
   stations, suggesting sex-specific migration patterns exist in this
   species. We used DNA analysis of 592 saw-whet owls caught in eastern New
   York to evaluate the accuracy of the morphometric methods used by
   banders to sex the majority of saw-whet owls reported in the Bird
   Banding Laboratory archives. Eighty-five percent of these owls could be
   sexed morphometrically and 97\% of the morphometric assignments were
   congruent with genetic analysis. However, the current morphometric
   method disproportionately assigned males to the ``unknown{''} class. We
   tested measurement reproducibility between banners and morphometric
   plasticity by comparing measurements of owls banded and subsequently
   recaptured by different banders. Mass changed 0.66 +/- 6.82 g and wing
   chord changed 0.29 mm +/- 3.34 on average between captures. The low
   variation in measurements between captures and the high congruence
   between genetic and morphometric sexing methods support the sex-specific
   migration hypothesis.}},
