@article{ ISI:000298341500003,
Author = {Eisermann, Knut and Howell, Steve N. G.},
Title = {{VOCALIZATIONS OF THE GUATEMALAN PYGMY-OWL (GLAUCIDIUM COBANENSE)}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
