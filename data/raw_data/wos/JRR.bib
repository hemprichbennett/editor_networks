@article{ ISI:000426902000007,
Author = {Doussang, Daniela and Palma, Cristina and Moreno, Lucila and Zambrano,
   Brayan and Pavez, Eduardo and Cerda, Fabiola and Gonzalez-Acuna, Daniel},
