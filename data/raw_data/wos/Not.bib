Type = {{Article}},
Language = {{English}},
Affiliation = {{Bell, B (Reprint Author), Victoria Univ Wellington, Ctr Biodivers \& Restorat Ecol, Wellington, New Zealand.
   Bell, Ben D., Victoria Univ Wellington, Ctr Biodivers \& Restorat Ecol, Wellington, New Zealand.}},
ISSN = {{0029-4470}},
