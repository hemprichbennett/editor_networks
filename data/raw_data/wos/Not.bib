@article{ ISI:000418011800004,
Author = {Beauchamp, A. J.},
Title = {{Breeding and behaviour records of peafowl (Pavo cristatus) at Mansion
   House Historic Reserve, Kawau Island, New Zealand, 1992-2010}},
Journal = {{NOTORNIS}},
Year = {{2013}},
Volume = {{60}},
Number = {{3}},
Pages = {{224-232}},
Abstract = {{Information was collected from a small population of peafowl living in
   Mansion House Historic Reserve, Kawau Island, from 1995 to 2010.
   Peacocks used an exploded lek breeding system, and displayed at areas
   where human and natural foods occurred. Mating took place between late
   Sep and mid Dec. Breeding was successful in the presence of a
   substantial North Island weka (Gallirallus australis greyi) population.
   An average of 1.4 (SE = 0.6, n = 10) young were fledged per successful
   clutch. In the winter of 2004, all peahens disappeared and the
   population thereafter comprised only males. Despite the absence of
   females, peacocks continued to display for 5 years after all peahens
   were lost. The breeding biology of this introduced population appears to
   be similar to that in their native range.}},
