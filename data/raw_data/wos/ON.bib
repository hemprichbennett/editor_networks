@article{ ISI:000385627300013,
Author = {Segura, Luciano N. and Gonzalez, Exequiel and Jauregui, Adrian},
Title = {{UNUSUAL NEST AND NESTLING APPROPRIATION BY THE EARED DOVE (ZENAIDA
   AURICULATA)}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2016}},
Volume = {{27}},
Pages = {{101-104}},
Abstract = {{We report a case of interspecific nest appropriation by the Eared Dove
   (Zenaida auriculata) on a nest of the Creamy-bellied Thrush (Turdus
   amaurochalinus). The usurpation attempt resulted in the death of both
   nestlings, including a parasitic cowbird, despite forceful attempts by
   the thrushes to retake their nest and repeated attempts by the dove to
   feed the nestlings. We believe that the loss of the dove offspring from
   a nest in a neighboring tree could have triggered this behavior, which
   may be a hormonal byproduct caused by the recent loss of their own
   offspring, similar to what has been observed in Emperor Penguins
   (Aptenodytes fosteri).}},
