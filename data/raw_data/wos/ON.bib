@article{ ISI:000269854800009,
Author = {Branco, Joaquim Olinto and Alves Fracasso, Helio Augusto and Barbieri,
   Edison},
Title = {{BREEDING BIOLOGY OF THE KELP GULL (LARUS DOMINICANUS) AT SANTA CATARINA
   COAST, BRAZIL}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2009}},
Volume = {{20}},
Number = {{3}},
Pages = {{409-419}},
Abstract = {{The Kelp gull, Larus dominicanus, is widely distributed in the Southern
   Hemisphere, and its population has expanded in both Hemispheres.
   Breeding colonies in Brazil are found from the coast of Rio de Janeiro
   southwards to Santa Catarina. This paper provides information on the
   breeding biology of L. dominicanus on coastal islands of Santa Catarina.
   Population monitoring, nest and egg counts, and measurements were made
   from July 1996 to December 2006. From March to June of each year, we
   measured adult gull movement to islands, territorial establishment, and
   nest construction. Egg laying started in mid July, and nestlings were
   first seen in August. The average length, weight, and volume of the eggs
   were smaller than in Moleques do Sul Island while the width, weight, and
   volume of eggs were higher than on Tamboretes Island. Accepted 8 July
   2009.}},
