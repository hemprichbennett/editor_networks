@article{ ISI:000441832400001,
Author = {Lopez, Fernando G. and Grande, Juan M. and Berkunsky, Igor and
   Santillan, Miguel A. and Rebollo, Maria E.},
Title = {{FIRST REPORT OF BURROWING PARROT (CYANOLISEUS PATAGONUS) NESTING IN TREE
   CAVITIES}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2018}},
Volume = {{29}},
Pages = {{71-75}},
Month = {{APR 7}},
Abstract = {{The Burrowing Parrot Cyanoliseus patagonus is known to breed in burrows
   mostly on cliffs and ravines in arid or semi-arid regions of Argentina
   and Chile. However, during a tree cavity monitoring project we confirmed
   at least two active nests in tree cavities. Cavity entrances were
   located between 3.1 and 5.3 m above the ground in live calden (Prosopis
   caldenia) trees, Parque Luro, province of La Pampa, Argentina. One nest
   failed while the other one successfully produced three fledglings. The
