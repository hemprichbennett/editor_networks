@article{ ISI:000385625600004,
Author = {Roesler, Ignacio},
Title = {{STATUS OF HOODED GREBE (PODICEPS GALLARDOI) IN CHILE}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
