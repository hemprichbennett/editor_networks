@article{ ISI:000273100500009,
Author = {Hoffmann, Diego and Lopes, Leonardo Esteves and de Vasconcelos, Marcelo
   Ferreira},
Title = {{NATURAL HISTORY NOTES ON THE PALE-THROATED SERRA-FINCH (EMBERNAGRA
   LONGICAUDA) IN EASTERN BRAZIL}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2009}},
Volume = {{20}},
Number = {{4}},
Pages = {{597-607}},
Abstract = {{The Pale-throated Serra-finch (Embernagra longicauda) is restricted to
   the mountains and plateaus of eastern Brazil and almost nothing is known
   about its life history. Here, we present for the first time detailed
   descriptions of its nest, eggs, nestlings, diet, molt pattern, body
   mass, and habitat. The nest is a large cup supported from its bottom and
   constructed mainly with grass culms and leaves. The species is
   omnivorous, its diet is composed predominantly of arthropods and fleshy
   fruits, with a small component of dry seeds. A complete molt occurs
   during March-April, after the reproductive season. The conservation
   status of the Pale-throated Serra-finch may be classified as lower risk
   - least concern. Accepted 27 October 2009.}},
