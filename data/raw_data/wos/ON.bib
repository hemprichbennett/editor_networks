@article{ ISI:000429453600001,
Author = {dos Santos Napoli, Raynara Jenifer and Pesquero, Marcos Antonio and
   Ferreira Mendonca, Cindy Anne and da Silva, Yanara Morgana},
Title = {{MALE AND FEMALE CONTRIBUTIONS TO PARENTAL CARE IN THE RUFOUSTAILED
   JACAMAR (GALBULA RUFICAUDA, GALBULIDAE) IN SOUTHERN GOIAS, BRAZIL}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2018}},
Volume = {{29}},
Pages = {{21-25}},
Month = {{MAR 8}},
Abstract = {{We quantified patterns of male and female parental care of the
   Rufous-tailed Jacamar Galbula ruficauda during incubation and nestling
   feeding, over four breeding seasons in southern Goias, Brazil.
   Reproduction in this species was seasonal, occurring during the
   transition between dry and wet seasons from September to November. Only
   females incubated overnight, but male and female shared incubation
   duties during the day, having similar length of incubation bouts (male =
   45 +/- 16 min vs. female = 47 +/- 14 min; t = -0.56, P = 0.57). The
   nestling diet was composed exclusively of insects - mainly flies,
   butterflies, and dragonflies- and males fed nestlings more often than
   females (male 59\% vs. female 41\%,X-2 = 1783, df = 1, P < 0.0001). The
   results confirm biparental care, with high levels of paternal care,
   especially during incubation, as predicted for tropical birds with a
   monogamous social mating system.}},
