@article{ ISI:000266958200007,
Author = {Castro, Macarena and Suazo, Cristian G. and Quiroga, Eduardo and
   Baessolo, Luisa and Arriagada, Aldo M. and Santos-Pavletic, Gerson D.},
Title = {{DIET SELECTION OF SANDERLINGS (CALIDRIS ALBA) IN ISLA GUAMBLIN NATIONAL
   PARK IN THE CHILEAN FJORDS}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2009}},
Volume = {{20}},
Number = {{2}},
Pages = {{247-253}},
Abstract = {{Knowledge of feeding behaviour of non-breeding Sanderlings is very
   scarce for South Pacific populations. We report data on diet and prey
   selection by Sanderlings wintering in Isla Guamblin National Park in the
   Chilean fjords. We sampled macroinvertebrates, collected feces from
   monospecific roosting sites, and collected bivalves for inspection of
   their siphons. Although the amphipod Orchestoidea tuberculata was most
   abundant, main prey items for Sanderlings were the coleopteran
   Phalerisida maculata and surf clams (Mesodesma donacium) siphons. Of the
   surf clams collected, 44.7\% (n = 85) one or both siphons had been eaten
   by Sanderlings. Sanderlings predated on siphons of surf clams above 2
   cm, suggesting that smaller surf clams are not a profitable prey for
   this species. Our results confirm the importance of combining methods
   for studies of diet and prey selection and reveal the importance of
   collecting bivalves for inspection of their siphons when prey
   identification in the field is difficult. Accepted 23 April 2009.}},
