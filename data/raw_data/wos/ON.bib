@article{ ISI:000266958200005,
Author = {Abalos, Raul and Areta, Juan I.},
Title = {{Natural history and distribution of the Lemon Doradito (Pseudocolopteryx
   cf. citreola) in Argentina}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2009}},
Volume = {{20}},
Number = {{2}},
Pages = {{215-230}},
Abstract = {{Two presumably allopatric populations of Warbling Doradito
   (Pseudocolopteryx flaviventris) differ in bioacoustic and morphometric
   characters, but are otherwise essentially identical in morphology, and
   it is very difficult to separate them based on plumage features alone.
   The song of the population from western Argentina (Chubut, Rio Negro,
   Neuquen, Mendoza, and Salta) and central Chile, here provisionally
   called P. cf. citreola, can be described as ``tick tick tick
   tick-tick-tick-you{''}, while the song of P. f. flaviventris is
   ``u-eet-u, u-eet{''}. Head movements performed during song emission also
   differ between the two forms. The western populations do not respond to
   playback of nominate flaviventris, and vice versa, but both are very
   responsive to their own vocalizations. We found P. cf. citreola in
   different habitats: chilcales (Baccharis salicifolia), tamariscales
   (Tamarix gallica), brushland of pajaro bobo (Tessaria absinthioides)
   with tamarisks, tamarisks and chilcas, and lagoons with reeds (Typha sp.
   and Juncus sp.). The form citreola is migratory, with records from
   October-March in the province of Mendoza. The nest of P. cf. citreola is
   similar to those of other Pseudcolopteryx, but the eggs might be unique
   since they are speckled. Our data demonstrate that, as actually
   delineated, P. flaviventris includes two different biological entities
   (cryptic species).}},
