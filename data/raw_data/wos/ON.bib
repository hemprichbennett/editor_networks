@article{ ISI:000254228400004,
Author = {Ortiz, Diego and Capillonch, Patricia},
Title = {{Seasonality and migratory behavior of the Bran-colored Flycatcher
   (Myiophobus fasciatus) in South America.}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2008}},
Volume = {{19}},
Number = {{1}},
Pages = {{31-41}},
Abstract = {{The Bran-colored Flycatcher (Myiophobus fasciatus) is a migratory
   species divided in two subspecies, auriceps and flammiceps, inhabiting
   in Argentine the chaco and the Atlantic forest, respectively. The race
   auriceps migrates to Bolivia and Peru, and flammiceps to northeastern
   Brazil. Dates of occurrence for auriceps in Argentina are between early
   October and late April. Migrants arrive on breeding territories in
   Argentina in pairs or small groups of 5 to 10 individuals, without
   forming conspicuous flocks. Molt takes place at the end of the summer on
   the breeding territories and individuals migrate in fall with hew flight
   feathers to their wintering grounds. Populations in central and northern
   Argentina are migratory; however, a few individuals have been reported
   overwintering in northeastern Argentina (Littoral).}},
