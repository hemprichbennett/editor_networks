@article{ ISI:000305372000005,
Author = {Carvajal-Rueda, Angelica and Losada-Prado, Sergio},
Title = {{Habitat use of Tolima Dove (Leptotila conoveri) in a degraded area in
   Combeima rivers canyon (Tolima, Colombia)}},
