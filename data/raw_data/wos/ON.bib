@article{ ISI:000392282100006,
Author = {van Dort, John and Juarez-Jovel, Roselvy},
Title = {{A HYBRID AMAZILIA HUMMINGBIRD FROM HONDURAS}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2016}},
Volume = {{27}},
Pages = {{297-301}},
Abstract = {{We present evidence supporting a hybrid origin of a hummingbird caught
   in a mist-net in Reserva Biologica Uyuca, Honduras in 2011. Following a
   defined diagnostic approach, the individual in question showed
   intermediate characters in terms of plumage and measurements between
   Amazilia cyanocephala and A. beryllina, ssp. devillei. The hybrid
   combination of A. cyanocephala with the nominate subspecies of A.
   beryllina has been reported once before from Veracruz, Mexico. This is
   the first time a hybrid of the ssp. devillei - whose taxonomic status is
   uncertain - with A. cyanocephala is described.}},
