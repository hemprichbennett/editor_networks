@article{ ISI:000256158100015,
Author = {Echeverry-Galvis, Maria Angela and Cordoba, Sergio Cordoba},
Title = {{A brief overview on reproduction and molt in Neotropical birds}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2008}},
Volume = {{19}},
Number = {{S}},
Pages = {{197-205}},
Note = {{8th Neotropical Ornithological Congress, Maturin, VENEZUELA, MAY 13-19,
   2007}},
Abstract = {{The published literature considers Neotropical birds as reproductive
   year round, in contrast to what is known. for I temperate birds that
   have restricted reproductive periods according to several environmental
   factors. Molt in temperate zone birds is almost always timed to occur
   after reproduction. However, we have very little information regarding
   the interrelationships of molt and breeding periods for the majority of
