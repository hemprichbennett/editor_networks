@article{ ISI:000254228400005,
Author = {Tarano, Zaida},
Title = {{Group territoriality and plural breeding in the Purple Gallinule
   (Porphyrula martinica) in a seasonally flooded savanna in Venezuela}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
