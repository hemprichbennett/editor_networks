@article{ ISI:000266958200009,
Author = {Boyle, W. Alice},
Title = {{HOW TO KEEP TROPICAL MONTANE FRUGIVOROUS BIRDS IN CAPTIVITY}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2009}},
Volume = {{20}},
Number = {{2}},
Pages = {{265-273}},
Abstract = {{As the study of Neotropical ornithology matures, increasing numbers of
   researchers are shifting from primarily observational studies of birds
   in their natural habitats to experimental studies which often require
   that birds be kept in captivity. Little guidance currently exists to
   help researchers plan captive studies of Neotropical species. I report
   here on experiences bringing wild frugivorous birds into captivity at a
   premontane forest site in Costa Rica. I suggest protocols that will
   increase the likelihood that birds will rapidly learn to consume
   synthetic diet and offer some guidelines that may increase survival
   probability under rustic field conditions. I conclude by considering
   geographic, temporal, and species-level factors that may affect the
   success with which birds adapt to captive conditions to help guide
   species selection in future studies. Accepted 31 March 2009.}},
