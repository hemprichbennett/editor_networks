Author = {CROWE, TM and ESSOP, MF and ALLAN, DG and BROOKE, RK and KOMEN, J},
Title = {{OVERLOOKED UNITS OF COMPARATIVE AND CONSERVATION BIOLOGY - A CASE-STUDY
   OF A SMALL AFRICAN BUSTARD, THE BLACK KORHAAN EUPODOTIS-AFRA}},
Journal = {{IBIS}},
Year = {{1994}},
Volume = {{136}},
Number = {{2}},
Pages = {{166-175}},
Month = {{APR}},
Abstract = {{Two small southern African bustards, the Black-winged Korhaan Eupodotis
   afra afra and the White-winged Korhaan E. a. afraoides, have long been
   treated as conspecific. However, differences in their habitats, plumage,
   morphometrics, displays and vocalizations of males, mitochondrial DNA
   restriction fragment length polymorphisms and, possibly, social systems
   strongly suggest that they are distinct species.}},
