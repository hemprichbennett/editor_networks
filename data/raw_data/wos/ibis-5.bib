@article{ ISI:A1992HA96300009,
Author = {CRICK, HQP},
Title = {{LOAD-LIGHTENING IN COOPERATIVELY BREEDING BIRDS AND THE COST OF
   REPRODUCTION}},
Journal = {{IBIS}},
Year = {{1992}},
Volume = {{134}},
Number = {{1}},
Pages = {{56-61}},
Month = {{JAN}},
Abstract = {{Helpers at the nest of cooperatively breeding birds often
   `lighten-the-load' of the breeders, by allowing them to decrease their
   effort. When the shape of the relationship between survival cost and
   provisioning effort is concave-up then a helper incurs only a small
   increment in mortality, for a given amount of help, but the breeder
   benefits from a larger fall in mortality. A convex-up relationship for
   pre-breeders may explain why some groups (such as larids) do not breed
   cooperatively. Load-lightening could provide significant extra gains to
   the helper's future indirect fitness by increasing the lifespan and
   life-time reproductive success of related breeders.}},
