@article{ ISI:A1995QG44600026,
Author = {MARTIN, CM},
Title = {{RECOVERING ENDANGERED SPECIES AND RESTORING ECOSYSTEMS - CONSERVATION
   PLANNING FOR THE 21ST-CENTURY IN THE UNITED-STATES}},
Journal = {{IBIS}},
Year = {{1995}},
Volume = {{137}},
Number = {{1}},
