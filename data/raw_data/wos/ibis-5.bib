@article{ ISI:A1993LM87000009,
Author = {CRUICKSHANK, AJ and GAUTIER, JP and CHAPPUIS, C},
Title = {{VOCAL MIMICRY IN WILD AFRICAN GRAY PARROTS PSITTACUS-ERITHACUS}},
Journal = {{IBIS}},
Year = {{1993}},
Volume = {{135}},
Number = {{3}},
Pages = {{293-299}},
Month = {{JUL}},
Abstract = {{The ability of captive Grey Parrots Psittacus erithacus to imitate
   heterospecific sounds is well known. Analysis of a sound recording from
   Zaire reveals the first evidence of vocal mimicry in the wild of nine
   species of birds and a bat. Examination of recordings from Gabon and the
   Ivory Coast indicates that vocal mimicry may be widespread in wild
   populations. Possible reasons why this phenomenon has not been detected
   before are discussed.}},
