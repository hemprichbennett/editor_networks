@article{ ISI:A1996UX73400004,
Author = {Harris, MP and Wanless, S and Barton, TR},
Title = {{Site use and fidelity in the common Guillemot Uria aalge}},
Journal = {{IBIS}},
Year = {{1996}},
Volume = {{138}},
Number = {{3}},
Pages = {{399-404}},
Month = {{JUL}},
Abstract = {{The site fidelity of 470 colour-ringed Common Guillemots Uria aalge with
   at least 1 year of breeding experience was followed on the Isle of May
   from 1982 to 1993, On average, 85.7\% of birds recorded breeding at a
   site in any year were present at the same site in the following season,
   Of those which did not retain their site, 35\% had moved to another
   site, 25\% were nonbreeders and 40\% were not seen, Site- fidelity of
   birds which returned to the colony the next year was 91\%, There were no
   significant age or sex effects, but there were significant (and
   unexplained) year and area effects. Most birds which changed sites moved
   less than 2 m, Some birds which obtained a new site improved their
   nesting success, but many others became nonbreeders; it is speculated
   that the former chose to move site, whilst the latter had been evicted.}},
