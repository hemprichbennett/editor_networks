@article{ ISI:A1994PN83200006,
Author = {MARTIN, GR and KATZIR, G},
Title = {{VISUAL-FIELDS IN THE STONE CURLEW BURHINUS-OEDICNEMUS}},
Journal = {{IBIS}},
Year = {{1994}},
Volume = {{136}},
Number = {{4}},
Pages = {{448-453}},
Month = {{OCT}},
Abstract = {{The Stone-curlew Burhinus oedicnemus is a short-billed terrestrial
   wading bird (Burhinidae; Charadriiformes) which forages primarily for
   surface living invertebrates in open, sparsely vegetated habitats during
   twilight and nighttime.  Visual field topography in restrained alert
   birds was investigated using an ophthalmoscopic reflex technique.  The
   visual fields have the following features:  (1) Eye movements of
   significant amplitude appear to be absent.  (2) The retinal binocular
   field is relatively small, with the bill placed near its centre.  It
   extends vertically through 80-degrees in the median sagittal plane with
   a maximum width of 18-degrees occurring 5-degrees above the bill.  (3)
   With the head in a typical posture (eye-bill-tip angle approximately
   15-degrees below the horizontal), the binocular field stretches from
   60-degrees below to 20-degrees above the horizontal.  (4) The blind area
   behind the head is relatively narrow (15-degrees at the horizontal),
   giving the bird near panoramic vision in the horizontal plane, but the
   widest blind area (32-degrees) occurs directly above the head.  (5)
   Monocular retinal fields in the horizontal plane are 182-degrees wide
   and are asymmetric about the optic axis.  (6) There is a blind sector of
   7-12-degrees at the margin of the optical fields, indicating that
   binocular field widths are not maximized.  Interspecific comparisons of
   these visual field features suggest that the foraging of Stone-curlews
   is guided primarily by visual cues.}},
