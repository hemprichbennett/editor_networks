@article{ ISI:A1996VN46300012,
Author = {Choudhury, S and Black, JM and Owen, M},
Title = {{Body size, fitness and compatibility in barnacle geese Branta leucopsis}},
Journal = {{IBIS}},
Year = {{1996}},
Volume = {{138}},
Number = {{4}},
Pages = {{700-709}},
Month = {{OCT}},
Abstract = {{Large-sized Barnacle Geese Branta leucopsis of both sexes had a higher
   probability of breeding successfully in any particular year and produced
   more goslings than did smaller birds, Large females paired at an earlier
   age, suggesting that they were preferred as mates and were likely to
   have entered the breeding population earlier, The relative sizes of the
   pair bond members also affected fitness, Most birds were able to
   maximize their breeding performance by mating with relatively similar
   sized partners; the greater the size disparity of mates, the lower the
   breeding performance. This supports the idea that compatibility of mates
   may be important in determining fitness of the pair. The success of
   different pair types was also affected by environmental conditions, with
   certain size combinations doing better in some years and poorly in other
   years.}},
