@article{ ISI:A1995TF62100001,
Author = {ALSTROM, P and OLSSON, U},
Title = {{A NEW SPECIES OF PHYLLOSCOPUS WARBLER FROM SICHUAN-PROVINCE, CHINA}},
Journal = {{IBIS}},
Year = {{1995}},
Volume = {{137}},
Number = {{4}},
Pages = {{459-468}},
Month = {{OCT}},
Abstract = {{A new species of leaf warbler (Sylviidae: Phylloscopus) is described
   from the mountain Emei Shan, Sichuan Province, China, Morphologically it
   is extremely similar to, yet significantly different from, the sympatric
   Blyth's Leaf Warbler Phylloscopus reguloides and White-tailed Leaf
   Warbler Phylloscopus davisoni. Songs and calls are both very different
   from P. reguloides and P. davisoni, and playback tests confirmed the
   specific status of the new species.}},
