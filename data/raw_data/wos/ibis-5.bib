@article{ ISI:A1997WU99100004,
Author = {Calladine, J and Harris, MP},
Title = {{Intermittent breeding in the Herring Gull Larus argentatus and the
   Lesser Black-backed Gull Larus fuscus}},
Journal = {{IBIS}},
Year = {{1997}},
Volume = {{139}},
Number = {{2}},
Pages = {{259-263}},
Month = {{APR}},
Abstract = {{Thirty-three percent and 37\% of adult Herring Gulls Larus argentatus
   and 34\% and 40\% of adult Lesser Black-backed Gulls Larus fuscus, all
   with some previous breeding experience, failed to breed in 1993 and
   1994, respectively. About half of these intermittent breeders of both
   species failed to breed in both years. Common amongst the intermittent
   breeders were smaller individuals, birds with low survival chances and
   relatively unsuccessful breeders.}},
