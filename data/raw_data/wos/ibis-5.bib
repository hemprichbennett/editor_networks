@article{ ISI:A1995RK86000017,
Author = {HARRISON, JA and MARTINEZ, P},
Title = {{MEASUREMENT AND MAPPING OF AVIAN DIVERSITY IN SOUTHERN AFRICA -
   IMPLICATIONS FOR CONSERVATION PLANNING}},
Journal = {{IBIS}},
Year = {{1995}},
Volume = {{137}},
Number = {{3}},
Pages = {{410-417}},
Month = {{JUL}},
Abstract = {{The potential and problems of using species diversity information,
   derived from the Southern African Bird Atlas Project (SABAP), are
   examined. Atlas data were analysed and the problem of uneven sampling
   effort was ameliorated through an application of an index of diversity.
   Maps of diversity for various suites of species are considered in
