@article{ ISI:A1992JF56600005,
Author = {WINKLER, H and LEISLER, B},
Title = {{ON THE ECOMORPHOLOGY OF MIGRANTS}},
Journal = {{IBIS}},
Year = {{1992}},
Volume = {{134}},
Number = {{1}},
Pages = {{21-28}},
Abstract = {{A multivariate analysis of data from a wide range of avian species
   revealed that migratory distance was related to a high aspect ratio,
   long and pointed wings and well-developed distal wing segments,
   Migration also appeared to set limits on the development of the hind
   limb and on the wing muscles not involved in forward flight. Among
   Sylvia warblers it is shown that habitat use and migration may constrain
   each other.}},
