@article{ ISI:A1994PN83200008,
Author = {BLAKEY, JK},
Title = {{GENETIC-EVIDENCE FOR EXTRA-PAIR FERTILIZATIONS IN A MONOGAMOUS
   PASSERINE, THE GREAT TIT PARUS-MAJOR}},
Journal = {{IBIS}},
Year = {{1994}},
Volume = {{136}},
Number = {{4}},
Pages = {{457-462}},
Month = {{OCT}},
Abstract = {{The incidence of extra-pair paternity in a Great Tit Parus major
   population at Wytham Wood, Oxford, in 1985-1987 was determined using two
   polymorphic allozymes.  In 831 nestlings from 94 broods, 27 genetic
   exclusions were detected in 25 (3\%) nestlings from 16 broods.  Seven
   (44\%) of these broods contained offspring that excluded the putative
   male parent from being the genetic parent.  The distribution of
   exclusion types indicated that excluded offspring were the result of
   fertilizations by extra-pair males and not of egg-dumping.  The true
   frequency of extra-pair paternity was estimated as 14\% of offspring. 
   These results suggest a mixed reproductive strategy for males in which
   they breed monogamously whilst simultaneously seeking extra-pair matings
   with females of other pairs.}},
