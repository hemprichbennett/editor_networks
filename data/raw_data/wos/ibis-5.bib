@article{ ISI:A1995RK86000012,
Author = {FEARE, CJ and GILL, EL and MCKAY, HV and BISHOP, JD},
Title = {{IS THE DISTRIBUTION OF STARLINGS STURNUS-VULGARIS WITHIN ROOSTS
   DETERMINED BY COMPETITION}},
Journal = {{IBIS}},
Year = {{1995}},
Volume = {{137}},
Number = {{3}},
Pages = {{379-382}},
Month = {{JUL}},
Abstract = {{Observations were made in five aviaries to determine the role of
   competition in roost site selection by captive Starlings Sturnus
   vulgaris. Their behaviour in selecting perches in a simple roost
   `'tree'' indicated that (1) a dominance hierarchy exists in birds
   selecting roosting locations, (2) the roost positions occupied are
   determined through competition, (3) higher and inner roost positions are
   preferred over others and (4) dominant Starlings tend to occupy these
