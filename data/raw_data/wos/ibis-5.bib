@article{ ISI:A1995QG44600019,
Author = {WILLIAMS, JM and TASKER, ML and CARTER, IC and WEBB, A},
Title = {{A METHOD OF ASSESSING SEABIRD VULNERABILITY TO SURFACE POLLUTANTS}},
Journal = {{IBIS}},
Year = {{1995}},
Volume = {{137}},
