@article{ ISI:A1997WC68900011,
Author = {KosteleckaMyrcha, A and Zukowski, J and Oksiejczuk, E},
Title = {{Changes in the red blood indices during nestling development of the Tree
   Sparrow Passer montanus in an urban environment}},
Journal = {{IBIS}},
Year = {{1997}},
Volume = {{139}},
Number = {{1}},
Pages = {{92-96}},
Month = {{JAN}},
Abstract = {{The general trends in the red blood indices during nestling development
   of the Tree Sparrow Passer montanus in an urban area polluted by car
   exhaust fumes were the same as for altricial birds living in unpolluted
   habitats, However, the densities of erythrocytes in blood, particularly
   the haemoglobin concentration and its amount per surface unit of
   erythrocytes, were significantly lower in urban area Tree Sparrows as
   compared with those studied in the past in the Kampinoska Old Forest,
   The lower haemoglobin concentration in the urban area is probably caused
   by lead from car exhaust.}},
