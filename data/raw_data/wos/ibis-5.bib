@article{ ISI:A1994MR61200006,
Author = {KEMP, AC and CROWE, TM},
Title = {{MORPHOMETRICS OF FALCONETS AND HUNTING BEHAVIOR OF THE BLACK-THIGHED
   FALCONET MICROHIERAX-FRINGILLARIUS}},
Journal = {{IBIS}},
Year = {{1994}},
Volume = {{136}},
Number = {{1}},
Pages = {{44-49}},
Month = {{JAN}},
Abstract = {{Samples of up to ten study skins for each of five species of
   Microhierax, two species of Polihierax, one species of Spiziapteryx and
   23 species of small Falco were measured for 24 dimensions of the head,
   wings, tail and feet. The logarithmic (log10) values of the means of
   each measurement for each species, and for each sex where dimorphic,
   were compared using cluster and factor analyses. Microhierax falconets
   are most similar to the African Pygmy Falcon Polihierax semitorquatus
   but are morphometrically distinct from all other falconets and small
   Falco species. The five species of Microhierax falconets are
   morphometrically similar, sexually dimorphic and probably replace one
   another ecologically, since all are allopatric or differ in proportions
   and habitat where parapatric (two species). Preliminary observations on
   the hunting behaviour and social organization of the Black-thighed
   Falconet Microhierax fringillarius indicate that the genus has
   morphological and social specializations for aerial capture of large
   forest insects.}},
