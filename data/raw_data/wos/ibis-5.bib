@article{ ISI:A1997XL05200006,
Author = {Thorstrom, R and Hart, J and Watson, RT},
Title = {{New record, ranging behaviour, vocalization and food of the Madagascar
   Red Owl Tyto soumagnei}},
Journal = {{IBIS}},
Year = {{1997}},
Volume = {{139}},
Number = {{3}},
Pages = {{477-481}},
Month = {{JUL}},
Abstract = {{The rare Madagascar Red Owl Tyto soumagnei was captured, radio tagged
   and released for study in October 1994, It ranged within an area of 210
   ha from October to December 1994, and 50\% of the locations were along
   forest edge and newly created tavies, 36\% in rice fields and 14\% in
   cultivated tavies. Nine diurnal roost sites were documented in small
   ravines among secondary vegetation and bananas (Masu sp.), The owl used
   dense canopy-covered roosts that averaged 3.7 m above the ground and
   were extremely close to tavies, averaging 9.8 m. The 17 regurgitated
   pellets collected contained 27 individual vertebrates representing
   native small mammals of the families Tenrecidae and Muridae, The owl's
   vocalization was similar to that of the Barn Owl Tyto alba, This new
   record on Masoala Peninsula at sea level substantially extends the
   species potential range in Madagascar, and our observations of behaviour
   and habitat use contradict previous descriptions of the species as a
   primary forest obligate.}},
