@article{ ISI:A1992JV12700001,
Author = {ALSTROM, P and OLSSON, U and COLSTON, PR},
Title = {{A NEW SPECIES OF PHYLLOSCOPUS WARBLER FROM CENTRAL CHINA}},
Journal = {{IBIS}},
Year = {{1992}},
Volume = {{134}},
Number = {{4}},
Pages = {{329-\&}},
Month = {{OCT}},
Abstract = {{A new species of leaf warbler (Sylviidae: Phylloscopus) is described
   from the mountains of central China. Morphologically it is slightly, yet
   significantly, different from the partly sympatric P. (proregulus)
   chloronotus. However, both song and calls are very different, and
   playback tests elicited no response from either species to the other's
   song. There are also differences in habitat preference and breeding
   biology of the two species.}},
