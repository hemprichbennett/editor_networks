@article{ ISI:A1993MA43900004,
Author = {WATSON, J and LEITCH, AF and RAE, SR},
Title = {{THE DIET OF GOLDEN EAGLES AQUILA-CHRYSAETOS IN SCOTLAND}},
Journal = {{IBIS}},
Year = {{1993}},
Volume = {{135}},
Number = {{4}},
Pages = {{387-393}},
Month = {{OCT}},
Abstract = {{The diet of Golden Eagles Aquila chrysaetos in Scotland is described,
   based on 1793 items identified in pellets. In parts of the range, winter
   diet differed from summer, with more deer (mainly carrion) and fewer
   grouse in winter. The summer diet in mine ecological regions differed
   significantly amongst most regions. Six distinct types of diet were
   recognized, with varying proportions of deer, sheep, lagomorphs (rabbits
   and/or mountain hares), grouse (mainly Red Grouse), seabirds (mainly
   Fulmars) and miscellaneous birds. Differences in diet between regions
   fit current and past land uses and eco-geographical variation across the
   Golden Eagle range in Scotland.}},
