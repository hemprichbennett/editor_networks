@article{ ISI:A1997XL05200002,
Author = {Campos, F and Lekuona, JM},
Title = {{Temporal variations in the feeding habits of the Purple Heron Ardea
   purpurea during the breeding season}},
Journal = {{IBIS}},
Year = {{1997}},
Volume = {{139}},
Number = {{3}},
Pages = {{447-451}},
Month = {{JUL}},
Abstract = {{The foraging strategies of adult Purple Herons Arden purpurea from an
   increasing colony in northern Spain were studied in the feeding areas
   (nonagricultural habitats) during the breeding season (May-July), In
   this area, herons were mainly piscivorous, and there was no seasonal
   variation in prey type. Small fish (<12.5 cm) were more abundant in the
   diet in Tune and medium-sized fish (1.2.5-25 cm) in May and July.
   Foraging strategies varied seasonally: (1) More time was spent by the
   herons in the feeding areas in June than in May or July, whereas
   foraging time was only significantly greater in June than in July, (2)
   The biomass captured by the adults in each foraging session was similar
   in May and June but greater in July. (3) Feeding attempts were more
   numerous (but not more successful)( )in shallow water in Tune and July
   (4) Aggression rates between conspecifics were higher in June than in
   both May and July Probably the hatching of chicks (June was the cause of
   these changes in diet and foraging strategies as well as increased
   intraspecies competition for foraging habitat.}},
