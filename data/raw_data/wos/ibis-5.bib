@article{ ISI:A1992JF56600008,
Author = {BAIRLEIN, F},
Title = {{RECENT PROSPECTS ON TRANS-SAHARAN MIGRATION OF SONGBIRDS}},
Journal = {{IBIS}},
Year = {{1992}},
Volume = {{134}},
Number = {{1}},
Pages = {{41-46}},
Note = {{CONF ON ECOLOGY AND CONSERVATION OF PALAEOARCTIC-AFRICAN MIGRANTS, UNIV
   E ANGLIA, NORWICK, ENGLAND, APR 04-07, 1991}},
Organization = {{BRIT ORNITHOLOGISTS UNION; BRIT TRUST ORNITHOL; INT COUNCIL BIRD
   PRESERVAT; PEOPLES TRUST ENDANGERED SPECIES; ANGLIA SURVIVAL; DAVID
   SHEPHERD CONSERVAT FDN; DULVERTON TRUST; ROYAL SOC; ACAD PRESS}},
Abstract = {{Many palaearctic migrants in tropical Africa have to cross the
   inhospitable land of the Sahara desert. Moreau (1961, 1972) hypothesized
   that these migrants crossed the Sahara in a single non-stop flight.
   Recent field data, however, revealed that some migrants stop-over in
   suitable desert habitats. The majority ot grounded migrants showed a
   high body-mass and fat-loading, indicating sufficient reserves for
   onward flights. Further evidence on resting periods, retrapping rates
   and experiments with caged migrants supports the hypothesis of an
   intermittent migratory strategy, with regular stopovers during the day
   and flight at night, for some desert-crossing passerines.}},
