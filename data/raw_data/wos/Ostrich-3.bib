@article{ ISI:A1993MR37800002,
Author = {LITTLE, RM and GOUS, RM and CROWE, TM},
Title = {{THE DISTRIBUTION AND ABUNDANCE OF GREYWING FRANCOLIN
   FRANCOLINUS-AFRICANUS ON THE STORMBERG PLATEAU, EASTERN CAPE PROVINCE,
   SOUTH-AFRICA, IN RELATION TO DIET AND SUBSTRATA}},
Journal = {{OSTRICH}},
Year = {{1993}},
Volume = {{64}},
Number = {{3}},
Pages = {{105-114}},
Month = {{SEP}},
Abstract = {{Previous research indicates that Greywing Francolin Francolinus
   africanus in conserved grasslands are isolated on areas over igneous
   substrata and are highly qualitatively selective in their diet, but are
   more widespread in grazed grassland.  This study examines whether this
   variation in dispersion and ecology is a result of quantity and/or
   quality of diet, or because of veld management policies. We studied
   quantitative and qualitative aspects of the diet of Greywing Francolin
   populations, and the distribution and abundance of these populations
   over various land-form types and substrata on the Stormberg Plateau,
   eastern Cape Province, South Africa.  Within the montane grassland areas
   of the range of the Greywing Francolin, veld management and the
   resultant structure of the sward determines the number of Greywing
   coveys. Annual variation in summer and autumn arthropod availability may
   influence the number of individuals per covey in Greywing populations.}},
Publisher = {{SOUTH AFRICAN ORNITHOL SOC}},
Address = {{PO BOX 84394, GREENSIDE 2034, SOUTH AFRICA}},
Type = {{Article}},
