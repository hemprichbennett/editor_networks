@article{ ISI:A1991HF06700008,
Author = {SIMMONS, R},
Title = {{COMPARISONS AND FUNCTIONS OF SKY-DANCING DISPLAYS OF CIRCUS HARRIERS -
   UNTANGLING THE MARSH HARRIER COMPLEX}},
Journal = {{OSTRICH}},
Year = {{1991}},
Volume = {{62}},
Number = {{1-2}},
Pages = {{45-51}},
Month = {{JUN}},
Abstract = {{African Marsh Harriers Circus ranivorus perform aerial displays known as
   ``sky-dancing{''} most frequently just before breeding and at other
   times during territorial takeovers. Both males and females display and
   food supplements to seven pairs over two years verified sexual selection
   arguments that birds in better condition display more frequently than
   unfed controls. Typical sky-dancing birds soared to 400 m or more, and
   performed up to 50 U-shaped undulations in a horizontal plane, while
   sky-spiralling birds performed vertical displays, dropping in a series
   of descending U-shaped undulations, whilst twisting about their axis.
   Displaying birds stimulated others to perform and up to eight birds
   displayed in the same aerial arena. I interpret both displays as mainly
   territorial in function but provide evidence that they also have a mate
   attraction component. In form, the display resembles that of the
   Australasian Harrier C. approximans, but not that of the European Marsh
   (C. aeruginosus) nor the Reunion/Malagasy (C. a. maillardi) Harrier,
   providing evidence contrary to the traditional belief that African birds
   are subspecies of aeruginosus.}},
