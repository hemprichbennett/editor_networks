@article{ ISI:A1994QN10600007,
Author = {JOHNSON, DN and MACLEAN, GL},
Title = {{ALTITUDINAL MIGRATION IN NATAL}},
Journal = {{OSTRICH}},
Year = {{1994}},
Volume = {{65}},
Number = {{2}},
Pages = {{86-94}},
Month = {{JUN}},
Note = {{Thematic Symposium of the Southern-African-Ornithological-Society -
   Migration, Dispersal and Nomadism, LANGEBAAN, SOUTH AFRICA, SEP 12-16,
   1993}},
Organization = {{ESKOM; FDN RES DEV; NATL PARKS BOARD; LANGEBAAN MUNICIPAL; BUDGET CAR
   HIRE; SO AFRICAN ORNITHOL SOC}},
Abstract = {{The province of Natal, South Africa, ranges from sea level to an
   elevation of 3 000 m over a distance of only 160 km. Evidence from the
   literature and from ringing shows that at least 76 species of birds are
   altitudinal migrants in Natal, moving to lower elevations in winter
   (from about March to September). Most can be grouped roughly into four
   categories depending on their breeding areas (montane, high- or
   mid-altitude) and the distances over which they migrate (short-distance
   or long-distance): (A) montane to high-altitude, (B) high-altitude to
   mid-altitude, (C) high-altitude to low-altitude and (D) mid-altitude to
   low-altitude; there is some overlap between the categories, some species
   falling into two or even three altitudinal migration patterns. Birds in
   category (A) move to lower altitudes from the Afromontane region of the
   Drakensberg regularly or only when winter weather conditions are very
   severe, but some individuals can be found there at all times (15
   species). Those in category (B), numbering at least 16 species, leave
   the Afromontane and high-altitude regions altogether, but may be found
   at all times of the year from the submontane region (below about 3 000
   m) to the midlands (down to about 1 000 m). Long-distance migrants in
   categories (C) (12 species) and (D) (29 species) include all the
   remaining bird species, many individuals of which regularly reach the
   coastal littoral of Natal, though not all individuals of which
   necessarily migrate. Fifteen species cannot be categorically placed
   without further knowledge of their breeding and non-breeding quarters.
   Only further ringing will show both the proportions of migrants within a
   population and the distances over which they migrate each winter.}},
