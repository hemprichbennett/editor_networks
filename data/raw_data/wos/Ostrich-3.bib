@article{ ISI:A1994QN10600003,
Author = {BERRUTI, A and HARRISON, JA and NAVARRO, RA},
Title = {{SEASONAL MIGRATION OF TERRESTRIAL BIRDS ALONG THE SOUTHERN AND EASTERN
   COASTS OF SOUTHERN AFRICA}},
Journal = {{OSTRICH}},
Year = {{1994}},
Volume = {{65}},
Number = {{2}},
Pages = {{54-65}},
Month = {{JUN}},
Note = {{Thematic Symposium of the Southern-African-Ornithological-Society -
   Migration, Dispersal and Nomadism, LANGEBAAN, SOUTH AFRICA, SEP 12-16,
   1993}},
Organization = {{ESKOM; FDN RES DEV; NATL PARKS BOARD; LANGEBAAN MUNICIPAL; BUDGET CAR
   HIRE; SO AFRICAN ORNITHOL SOC}},
Abstract = {{Several species of terrestrial birds migrate in summer southwestwards
   along the coastal zone of southeastern Africa to breed, followed by a
   reverse migration to wintering grounds in Africa. These migrations were
   investigated using the Southern African Bird Atlas Project data base and
   the reporting rate as an index of relative abundance. Models of
   seasonality in the reporting rate for 18 bird species in each of pix
   coastal areas from the southwestern Cape to northern Natal were fitted
   using Fourier series and generalized linear models. Altitudinal
   movements of each species were investigated in the two northernmost
   zones (Natal). New migration patterns are suggested for the Mangrove
   Kingfisher Halcyon senegaloides and the currently accepted migration
   movements of the Natal Robin Cossypha natalensis, Knysna Warbler
   Bradyterus sylvaticus and Dusky Flycatcher Muscicapa adusta are
   rejected. Known migrations are confirmed in greater detail for Pygmy
   Kingfisher Ispidina picta, Black Sawwing Swallow Psalinoprocne
   holomelas, Starred Robin Pogoncichla stellata and Paradise Flycatcher
   Terpsiphone viridis. Several factors other than migration may explain an
   increase in winter reporting rates for some resident forest species. The
   migration of the Spotted Thrush Zoothera guttata in southern Africa is
   apparently a truncated form of the migration existing in other forest
   bird species.}},
Publisher = {{SOUTH AFRICAN ORNITHOL SOC}},
Address = {{PO BOX 84394, GREENSIDE 2034, SOUTH AFRICA}},
Type = {{Article; Proceedings Paper}},
Language = {{English}},
Affiliation = {{BERRUTI, A (Reprint Author), DURBAN NAT SCI MUSEUM,POB 4085,DURBAN 4000,SOUTH AFRICA.}},
