@article{ ISI:A1993ME55200001,
Author = {SAVALLI, UM},
Title = {{THE TIMING OF BREEDING AND MOLT OF THE YELLOWMANTLED WIDOWBIRD EUPLECTES
   MACROURUS IN WESTERN KENYA}},
Journal = {{OSTRICH}},
Year = {{1993}},
Volume = {{64}},
Number = {{2}},
Pages = {{49-56}},
Month = {{JUN}},
Abstract = {{This paper describes the breeding and moulting seasons of Yellowmantled
   Widowbirds Euplectes macrourus at the Kakamega National Reserve in
   western Kenya. Egg laying began in May, peaked in June, and continued
   into September. The presence of brood patches on females followed a
   similar pattern. Most moulting occurred prior to breeding, in March and
   April. Rainfall was greatest in April and May, with a second peak in
   August to September. The pattern of grass seed abundance closely matched
   rainfall, and breeding appears to start about one month after the start
   of the rains. An exceptionally dry spell in May and June 1990 may have
   resulted in few breeding attempts that year.}},
