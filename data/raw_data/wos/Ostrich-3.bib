@article{ ISI:A1990FG76500004,
Author = {FROST, SK},
Title = {{NOTES ON THE BREEDING-BEHAVIOR OF MARICO AND PALLID FLYCATCHERS IN THE
   CENTRAL TRANSVAAL, SOUTH-AFRICA}},
Journal = {{OSTRICH}},
Year = {{1990}},
Volume = {{61}},
Number = {{3-4}},
Pages = {{111-116}},
Month = {{DEC}},
Abstract = {{The breeding behaviour of Marico and Pallid Flycatchers (Melaenornis
   mariquensis and M. pallidus) was studied at the Nylsvley Nature Reserve,
   South Africa, between September 1980 and January 1981.  The flycatchers
   are morphologically similar but occur in different although sometimes
   adjacent woodlands.  The two species do not differ in their breeding
   seasons, nor in their clutch sizes.  The incubation period, previously
   unrecorded for Pallid Flycatchers, is 14 days and the fledging period 17
   days.  Marico Flycatchers fed their chicks more frequently than Pallid
   Flycatchers and helpers were observed at two Marico Flycatcher nests.}},
