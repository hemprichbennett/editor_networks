@article{ ISI:A1992KY72200005,
Author = {SLOTOW, R and PERRIN, MR},
Title = {{THE IMPORTANCE OF LARGE PREY FOR BLACKSHOULDERED KITE REPRODUCTION}},
Journal = {{OSTRICH}},
Year = {{1992}},
Volume = {{63}},
Number = {{4}},
Pages = {{180-182}},
Month = {{DEC}},
Abstract = {{The diet of Blackshouldered Kites Elanus caeruleus was examined by
   pellet analysis. The proportion of large prey (Angoni vlei rat Otomys
   angoniensis) was higher in the diet of breeding kites than non-breeding
   kites, both overall, and at concurrent time periods. The proportion of
   0. angoniensis in the diet of breeding kites changed over the breeding
   cycle, with a 20\% increase from the nestbuilding stage to egg-laying.
   This increase may be necessary for egglaying to take place. The
   significance of prey size, which has been demonstrated as important in
   the breeding of other small raptors, is discussed.}},
