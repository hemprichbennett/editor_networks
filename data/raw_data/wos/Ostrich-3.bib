@article{ ISI:A1991HF06700001,
Author = {COOPER, J and UNDERHILL, LG},
Title = {{BREEDING, MASS AND PRIMARY MOLT OF EUROPEAN STARLINGS STURNUS-VULGARIS
   AT DASSEN ISLAND, SOUTH-AFRICA}},
Journal = {{OSTRICH}},
Year = {{1991}},
Volume = {{62}},
Number = {{1-2}},
Pages = {{1-7}},
Month = {{JUN}},
Abstract = {{European Starlings Sturnus vulgaris breed from September to November at
   Dassen Island, South Africa. Mean clutch size was 3,8 and mean brood
   size 2,8. An estimated 0,7 juveniles/breeding attempt survived to 2-3
   months of age. Winter (post moult) masses were higher than in summer in
   juveniles, but not in adults. Adult and juvenile starlings undertook a
   complete post-breeding moult. Based on percentage feather mass grown,
   primary moult took 108 d in adults and 106 d in juveniles. On average,
   juveniles commenced moult 19 d later than did adults.}},
