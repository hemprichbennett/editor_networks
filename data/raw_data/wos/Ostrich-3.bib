@article{ ISI:A1993LR33400004,
Author = {KEMP, AC},
Title = {{BREEDING BIOLOGY OF LANNER FALCONS NEAR PRETORIA, SOUTH-AFRICA}},
Journal = {{OSTRICH}},
Year = {{1993}},
Volume = {{64}},
Number = {{1}},
Pages = {{26-31}},
Month = {{MAR}},
Abstract = {{Aspects of the breeding biology of the Lanner Falcon Falco biarmicus
   were recorded from seven territories near Pretoria, South Africa during
   1970 to 1988. Nest were on average 6,9 km apart (n = 9, range 0,2-100
   km) and in most territories were in old crow nests on electricity
   pylons. Birds formed 79\% of the diet (n = 58) but hunting success at
   birds was low (15\%, n = 34) except when pairs hunted in combination
   (50\%, n = 8). Pairs that fledged young raised on average 2,4 per season
   (n = 16), but after 1982 few young were fledged and by 1987 there were
   no longer any falcons breeding in the area.}},
