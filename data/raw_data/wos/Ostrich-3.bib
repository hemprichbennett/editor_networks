@article{ ISI:A1996XR93900001,
Author = {Langrand, O and Goodman, SM},
Title = {{Current distribution and status of Benson's Rockthrush Pseudocossyphus
   bensoni, a Madagascar endemic.}},
Journal = {{OSTRICH}},
Year = {{1996}},
Volume = {{67}},
Number = {{2}},
Pages = {{49-54}},
Month = {{NOV}},
Abstract = {{Benson's Rockthrush Pseudocossyphus bensoni, described in 1971, was
   previously thought to have a limited range in central southwestern
   Madagascar centred around the Isalo Massif. Recent ornithological
   surveys of the high mountain zone of the Reserve Naturelle Integrale
   d'Andringitra, about 170 km east of the Isalo Massif and of the
   Isoky-Vohimena Forest, 60 km west of the Isalo Massif, found substantial
   populations of I? bensoni inhabiting these areas. In this paper we
   provide information on the Andringitra Massif and the Isoky-Vohimena
   Forest populations of this species, as well as summarize other records
   outside previously known areas of its geographical range.}},
