@article{ ISI:000088246100078,
Author = {Cheke, RA and Walsh, JF},
Title = {{Behaviour of Standard-wing Nightjars in Togo}},
Journal = {{OSTRICH}},
Year = {{2000}},
Volume = {{71}},
Number = {{1-2}},
Pages = {{349-350}},
Month = {{MAR}},
Note = {{9th Pan-African Ornithological Congress, ACCRA, GHANA, DEC 01-07, 1996}},
Organization = {{Birdlife Int; Royal Soc Protect Birds; Standard Chartered Bank, Accra;
   Ghana Wildlife Soc; Minist Lands \& Forest; Ghana Natl Petr Corp; Frank
   Champman Mem Fund; Ghana Chamber Mines; Ecobank, Accra; British Council,
   Accra; European Union}},
Abstract = {{Male Standard-wing Nightjars Macrodipteryx longipennis were observed
   interacting on the ground and in the air at ``display arenas{''} in
   Togo. Males walked towards each other in discrete bursts of activity,
   taking it in turns to move, whilst apparently disputing territorial
   boundaries. Males also sang from the ground and from rock perches on
   tracks, flew in circles following each other and a flying male sometimes
   swooped low over another on the ground in an intimidatory manner. These
   actions and others, including the birds' postures and movements on the
   ground and some interactions between the sexes, are described and
   discussed.}},
