@article{ ISI:000073308300003,
Author = {Le Corre, M and Probst, JM},
Title = {{Migrant and vagrant birds of Europa Island (southern Mozambique Channel)}},
Journal = {{OSTRICH}},
Year = {{1997}},
Volume = {{68}},
Number = {{1}},
Pages = {{13-18}},
Month = {{MAR}},
Abstract = {{Thirty non-breeding bird species have been identified on Europa Island.
   Twenty-two species are new records for the island. The most common
   migrants are six species of Palearctic shorebirds, and the Subantarctic
   Skua. Seven other species of Palearctic migrants and two species
   originating from Africa or Madagascar have been recorded more than once
   but in very low numbers. Finally, 15 species among which 8 are land
   birds have been recorded only once on Europa, between March 1993 and
   April 1996. These results are discussed and compared with other data
   from Madagascar and other oceanic islands of the Malagasy Region. Most
   species mentioned on Europa are regular migrants, accidental vagrants or
   breeding species of Madagascar or other oceanic islands. However eight
   species have never been recorded in Madagascar and two of these species
   have never been recorded on any other oceanic island of the area.}},
