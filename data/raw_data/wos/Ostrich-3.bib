@article{ ISI:A1994QN10600002,
Author = {BROOKE, RK},
Title = {{SUBSPECIATION STUDIES AND OUR KNOWLEDGE OF MIGRATION AND OTHER MOVEMENTS
   IN SOUTHERN AFRICAN BIRDS}},
Journal = {{OSTRICH}},
Year = {{1994}},
Volume = {{65}},
Number = {{2}},
Pages = {{49-53}},
Month = {{JUN}},
Note = {{Thematic Symposium of the Southern-African-Ornithological-Society -
   Migration, Dispersal and Nomadism, LANGEBAAN, SOUTH AFRICA, SEP 12-16,
   1993}},
Organization = {{ESKOM; FDN RES DEV; NATL PARKS BOARD; LANGEBAAN MUNICIPAL; BUDGET CAR
   HIRE; SO AFRICAN ORNITHOL SOC}},
Abstract = {{There are three main sources of information on movements and migrations
   of bird populations: observations in the field (including atlassing),
   ringing recoveries and analysis of geographical variation
   (subspeciation) in specimens. In the last case, some individuals move
   into and are collected within the ranges of other subspecies, where
   their different appearance and date of collection reveal their source.
   This paper reviews what can be learnt from critical studies of
   subspeciation in 119 species of southern African birds. In 26 cases
   different populations of nonbreeding migrants from the Palearctic or the
   Southern Ocean can be shown to have different, though usually
   overlapping ranges in southern Africa. Many indigenous breeding species
   show more movement or migration than is commonly appreciated. While
   altitudinal partial migration is well known, and northerly movements of
   40 species are not unexpected, the eastwards movement of arid country
   populations of 37 species in winter, is under-appreciated. Westerly
   movements (six species) are much rarer, while no less than 13 species
   show southerly movements outside the breeding season. Possible reasons
   for these movements are discussed.}},
Publisher = {{SOUTH AFRICAN ORNITHOL SOC}},
Address = {{PO BOX 84394, GREENSIDE 2034, SOUTH AFRICA}},
Type = {{Article; Proceedings Paper}},
Language = {{English}},
Affiliation = {{BROOKE, RK (Reprint Author), UNIV CAPE TOWN,PERCY FITZPATRICK INST AFRICAN ORNITHOL,RONDEBOSCH 7700,SOUTH AFRICA.}},
