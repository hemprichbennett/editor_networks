@article{ ISI:A1994RD22700002,
Author = {TRECA, B},
Title = {{THE DIET OF RUFFS AND BLACKTAILED GODWITS IN SENEGAL}},
Journal = {{OSTRICH}},
Year = {{1994}},
Volume = {{65}},
Number = {{3-4}},
Pages = {{256-263}},
Month = {{DEC}},
Abstract = {{A study of the diet of Blacktailed Godwits Limosa Limosa and Ruffs
   Philomachus pugnax by direct examination of stomach contents emphasizes
   the importance of rice, which accounted for over 80\% of the items
   eaten. Rice was available at planting time in July-August and after the
   harvest in November-December. Thus fat deposition for migration, between
   January and April-May, is based on a rice diet (cultivated or wild
   rice). Very little animal matter was eaten. The choice of feeding ground
   will govern food choice among the available food. Birds which have eaten
   most are those which have found their preferred food.}},
