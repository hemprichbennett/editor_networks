@article{ ISI:000076038700001,
Author = {Cresswell, W and Irwin, M and Jensen, M and Mee, A and Mellanby, R and
   McKean, M and Milne, L},
Title = {{Population estimates and distribution changes of landbirds on Silhouette
   Island, Seychelles}},
