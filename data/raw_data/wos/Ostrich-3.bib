@article{ ISI:A1993LR33400001,
Author = {NUTTALL, RJ},
Title = {{SEASONAL-CHANGES IN THE BIRDLIFE OF A PERIURBAN GRASSLAND COMMUNITY}},
Journal = {{OSTRICH}},
Year = {{1993}},
Volume = {{64}},
Number = {{1}},
Pages = {{1-7}},
Month = {{MAR}},
Abstract = {{Visits to a grassland habitat near Pietermaritzburg, Natal, South Africa
   were undertaken over a ten months (March-December) to document changes
   in avifauna, and to investigate possible reasons any changes. A total of
   72 bird species was recorded. Monthly species totals showed considerable
   fluctuation with species diversity highest in spring-early summer (n=53)
   and lowest in winter (n=14). Terrestrial' insectivores were the best
   represented (in terms of number of species) during the course of the
   study. The influence of rainfall on food availability and on vegetation
   is considered important in determining the structure of this avian
   community. Observations of the assumption (and loss) of nuptial plumage
   in euplectines showed that Longtailed Widows Euplectes progne developed
   breeding plumage earliest, and retained it for the longest period of
   time. Both Whitewinged E. albonotatus and Redcollared Widows E. ardens
   exhibited nuptial plumage for the shortest period. Displays in certain
   Cisticola species (Ayres' C. ayresii and Croaking Cisticola C.
   natalensis) started in early September, while in others (Fantailed C.
   juncidis and Palecrowned Cisticola C. brunnescens), they began at the
   end of October.}},
