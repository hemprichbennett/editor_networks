@article{ ISI:A1995TZ73100003,
Author = {Hartley, RR and Newton, I and Robertson, M},
Title = {{Organochlorine residues and eggshell thinning in the peregrine falcon
   Falco peregrinus minor in Zimbabwe}},
Journal = {{OSTRICH}},
Year = {{1995}},
Volume = {{66}},
Number = {{2-3}},
Pages = {{69-73}},
Month = {{SEP}},
Abstract = {{Several types of organochlorine residues were found in eggs of Peregrine
   Falcons Falco peregrinus minor taken in Zimbabwe in 1990. DDE (range =
   0.2-22 ppm wet weight; geometric mean 1.8 ppm) was found in all eggs (n
   = 15) from 13 sites, including two pairs in captivity. Eggs from two
   sites in the mid-Zambezi Valley, the most recently and heavily sprayed
   areas (5-8 times for tsetse fly Glossina sp. control), had the highest
   levels, one beyond that regarded as critical for successful
   reproduction. Mean shell thickness (Ratcliffe index) of the 1990 eggs
   was 10\% (range = 2-29\%) lower than the presumed pre-DDT mean. HEOD was
   detected in two eggs, HCB in one egg, and PCBs in all eggs, but at
   levels too low to affect breeding performance. Young were fledged from
   each of 12 sampled sites, yielding 1.86 young per site.}},
