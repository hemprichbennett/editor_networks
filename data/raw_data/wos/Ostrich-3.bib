@article{ ISI:A1995UT75400002,
Author = {Gargett, V and Gargett, E and Damania, D},
Title = {{The influence of rainfall on Black Eagle breeding over 31 years in the
   Matobo Hills, Zimbabwe}},
Journal = {{OSTRICH}},
Year = {{1995}},
Volume = {{66}},
Number = {{4}},
Pages = {{114-121}},
Month = {{DEC}},
Abstract = {{The effect of rainfall from 1964 to 1994 on the annual reproductive rate
   and number of resident pairs of Black Eagles Aquila verreauxii in the
   Matobo Hills, Zimbabwe, and on the abundance of their staple prey,
   hyrax, Procavia capensis and Heterohyrax brucei is reviewed. The number
   of resident pairs of Black Eagles increased with increased rainfall,
   when hyrax numbers were estimated to be at a very high level. Subsequent
   poor rainfall years coincided with a decrease in the number of resident
   pairs, a smaller proportion of pairs breeding, a lower reproductive rate
   and a dramatic decline in prey numbers.}},
