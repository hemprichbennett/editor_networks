@article{ ISI:A1992KY72200002,
Author = {TARBOTON, WR},
Title = {{VARIABILITY IN AFRICAN JACANA EGGS AND CLUTCHES}},
Journal = {{OSTRICH}},
Year = {{1992}},
Volume = {{63}},
Number = {{4}},
Pages = {{158-164}},
Month = {{DEC}},
Abstract = {{About half the breeding females in a study population of African Jacanas
   could be recognized by the unique dimensions, colour or markings of
   their eggs. Individual females laid eggs of relatively constant size
   (coefficient of variation (CV) = 1,9-4,0\%), shape (CV = 1,9-4,1\%),
   volume (CV = 4,8-7,1\%), colour and markings within clutches, between
   clutches and between seasons. They laid determinate four-egg clutches,
   the eggs of which were exceptionally small in proportion to female size:
   the African Jacana's clutch:female mass ratio (15,3\%) is lower than
   that reported for any other Charadriiformes. The small egg size is
   considered adaptive, facilitating the rapid re-laying of replacement
   clutches.}},
