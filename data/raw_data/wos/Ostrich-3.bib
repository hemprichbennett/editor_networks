@article{ ISI:A1993ND86100007,
Author = {KOK, OB},
Title = {{DIET OF THE SPOTTED DIKKOP BURHINUS-CAPENSIS}},
Journal = {{OSTRICH}},
Year = {{1993}},
Volume = {{64}},
Number = {{4}},
Pages = {{182-184}},
Month = {{DEC}},
Abstract = {{Analysis of 81 stomach samples of Spotted Dikkops Burhinus capensis
   collected in the central parts of South Africa over a period of eight
   years showed this species to be predominantly insectivorous. Coleoptera,
   followed by Isoptera (exclusively represented by the harvester termite
   Hodotermes mossambicus), made up the bulk of the insect material.
   Coleoptera were the only prey items taken throughout the year, while
   Isoptera were mainly consumed during the winter months and early summer.
   The remainder of the animal matter in the diet was dominated by
   Diplopoda.}},
