@article{ ISI:A1994PB15500001,
Author = {MCCANN, KI and KEMP, AC},
Title = {{HUNTING BEHAVIOR OF A FLEDGLING GREATER KESTREL FALCO-RUPICOLOIDES AND
   ITS MOTHER DURING THE POSTFLEDGING PERIOD}},
Journal = {{OSTRICH}},
Year = {{1994}},
Volume = {{65}},
Number = {{1}},
Pages = {{1-6}},
Month = {{MAR}},
Abstract = {{Changes in hunting behaviour of a Greater Kestrel Falco rupicoloides,
   from fledging to independence, and of its mother during the same period,
   were studied at 4-day intervals during January 1991. During the
   fledgling's transition to independence it increased its hunting time
   from 0-67\% per day, with a corresponding increase in strike success (to
   62\%), experience and therefore self-sufficiency. As hunting experience
   increased, so the fledgling used more difficult hunting methods, such as
   hover hunting, and captured larger and more elusive prey such as
   vertebrates. The adult female maintained a constant hunting success but
   decreased her hunting time from 7847\% per day as the fledgling became
   self-sufficient, resulting in a decrease in food provided to the
   fledgling in the last 12 days of its dependence. The male, providing
   mostly vertebrate prey, also decreased its provisioning rate but only
   over the last six days. The fledgling was estimated to be independent at
   least 26 days after fledging.}},
