@article{ ISI:A1994QN10600018,
Author = {SYROECHKOVSKI, EE and LAPPO, EG},
Title = {{MIGRATION PHENOLOGY OF WADERS (CHARADRII) ON THE TAIMYR PENINSULA,
   NORTHERN RUSSIA}},
Journal = {{OSTRICH}},
