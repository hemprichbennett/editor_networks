@article{ ISI:A1991HR08800001,
Author = {CRAIG, AJFK and HULLEY, PE and WALTER, GH},
Title = {{THE BEHAVIOR OF PALEWINGED STARLINGS AND A COMPARISON WITH OTHER
   ONYCHOGNATHUS SPECIES}},
Journal = {{OSTRICH}},
Year = {{1991}},
Volume = {{62}},
Number = {{3-4}},
Pages = {{97-108}},
Month = {{DEC}},
Abstract = {{Pairs of Palewinged Starlings associate throughout the year and roost at
   the nest sites. Flock sizes are generally small, even during the
   non-breeding season. Allofeeding and allopreening have not been
   recorded. Wing displays emphasize the white region of the primaries. The
   territory appears to be restricted to the nest site. Interspecific
   aggression occurred between Redwinged and Palewinged Starlings, and was
   also directed at other bird species frequenting the cliff. A form of
   ceremonial gathering occurs regularly in Palewinged Starlings. A review
   of the literature suggests that on behavioural grounds the Palewinged
   Starling shows no close affinity to the Redwinged Starling or to any
   other member of the genus.}},
