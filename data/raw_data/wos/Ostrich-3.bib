@article{ ISI:000073308300001,
Author = {Davies, AA and Maclean, GL},
Title = {{Avian response to landscape elements (tesserae) in an upland grassland
   habitat}},
Journal = {{OSTRICH}},
Year = {{1997}},
Volume = {{68}},
Number = {{1}},
Pages = {{1-7}},
Month = {{MAR}},
Abstract = {{Avian response to landscape elements (tesserae) at Camp Andrew, Lion's
   River Farming District, KwaZulu-Natal, South Africa, was measured in
   terms of abundance, occupancy and diversity per tessera. Flight paths
   between the tesserae were also quantified. As expected, occupancy varied
   from highest in the least disturbed tessera chill-grass) to lowest in
   exotic-plant and disturbed tesserae (wattle and mown-grass). Abundance
   and diversity, however, did not show this trend, but varied
   independently of whether the tesserae were natural, disturbed or exotic.
   Continuity diagrams, depicting the movement patterns of birds, show that
   avian response to heterogeneity was influenced by the relative
   suitability of each tessera. The life history traits of a given species
   may explain its unique response to a given tessera. A knowledge of avian
   responses to landscape pattern is important for conservation-management
   strategies.}},
