@article{ ISI:000321051500004,
Author = {Silveira, Luis Fabio and Dantas Santos, Marcos Persio},
Title = {{Bird richness in Serra das Confusoes National Park, Brazil: how many
   species may be found in an undisturbed caatinga?}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2012}},
Volume = {{20}},
Number = {{3}},
Pages = {{188-198}},
Month = {{SEP}},
Abstract = {{The caatinga is an endemic Brazilian Biome and few studies have been
   conducted in its undisturbed areas. Although the state of Piaui is
   mostly composed of caatinga, its avifauna is still largely unknown. Here
   we report the results of ornithological expeditions conducted in Serra
   das Confusoes National Park, Piaui. During 35 days of fieldwork we
   recorded 225 species of birds. The Serra das Confusoes National Park
   maintains the highest richness of bird species in the caatinga, and also
   holds 73\% of the birds endemic to this biome, with four threatened
   species recorded.}},
