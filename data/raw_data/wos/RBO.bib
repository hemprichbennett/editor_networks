@article{ ISI:000389950300008,
Author = {de Lima Pereira, Karla Dayane},
Title = {{Olive-sided Flycatcher, Contopus cooperi, in the Cerrado biome, and a
   review of records in Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2016}},
Volume = {{24}},
Number = {{1}},
Pages = {{46-52}},
Month = {{MAR}},
Abstract = {{The Olive-sided Flycatcher, Contopus cooperi (Nuttall, 1831), is a long
   distance migrant bird from North America. This Tyrannidae bird has
   wintering areas in Central and South America, and such as other long
   distance migrants, has decreased their populations, probably due to the
   use of pesticides, which in turn decrease the abundance of aerial
   insects, their main food supply. Little is known about the wintering
   distribution range of the species, mainly in South America. In Brazil
   its known distribution includes 11 states and 36 municipalities at the
   Amazon and Atlantic Foresst, occurring between October and April. This
   study reviews the distribution area of this species in Brazil, reporting
   for the first time, its occurrence in the Goias state and the Cerrado
   biome.}},
