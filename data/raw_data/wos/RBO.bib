@article{ ISI:000336241200008,
Author = {Dornas, Tulio and Pinheiro, Renato Torres},
Title = {{First record of Merlin Falco columbarius from Tocantins and a review of
   previous Brazilian records}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2014}},
Volume = {{22}},
Number = {{1}},
Pages = {{49-52}},
Month = {{MAR}},
Abstract = {{We describe the first record of Falco columbarius from Tocantins,
   central Brazil, made in the contact zone between the Amazon and Cerrado
   biomes. A review of previous Brazilian records of this species is also
   presented, showing a previously unrecognized wintering area for this
   falcon in South America and that its status as a vagrant should be
   reconsidered. Falco columbarius seems a rare but regular winter visitor
   due to several records in Brazil over the last years.}},
