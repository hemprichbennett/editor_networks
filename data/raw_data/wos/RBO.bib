@article{ ISI:000328204300006,
Author = {Campos e Silva, Joao Vitor},
Title = {{Roost of Leaftossers (genus Sclerurus) in the Brazilian Amazon: hints of
   the low density in fragmented environments}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2013}},
Volume = {{21}},
Number = {{2}},
Pages = {{129-132}},
Month = {{JUN}},
Abstract = {{Leaftossers are characteristic of primary forest, but have a low density
   or are absent from fragmented environments. We found leaftossers to use
   buttress roots of large trees as dormitories. In fragmented forests,
   these large trees are scarce, due to high mortality and logging. I
   suggest that the absence of these large trees may explain the low
   density of Sclerurus populations in anthropogenically altered
   environments.}},
