@article{ ISI:000208353700010,
Author = {Hidasi, Jose and Alves Mendonca, Luiz Gonzaga and Blamires, Daniel},
Title = {{First documented record of Kaempfer's Woodpecker, Celeus obrieni
   (Picidae) for the state of Goias, Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2008}},
Volume = {{16}},
Number = {{4}},
Pages = {{373-375}},
Month = {{DEC}},
Abstract = {{First documented record of Kaempfer's Woodpecker, Celeus obrieni
   (Picidae) for the state of Goias, Brazil. We report on the first record
   of Celeus obrieni for the state of Goias, Central Brazil. One specimen
   was collected in January 1967 at the municipality of Guapo (16 degrees
   49'S, 49 degrees 31'W), and deposited in the Museu de Ornitologia de
   Goiania (FMOG 1230). This record extends the known geographical range of
   C. obrieni ca. 1200 km south of the type locality. The new record
   presented herein underscores the yet inadequate knowledge on the bird
   fauna of the core area of the Cerrado, despite many historical and
   recent inventories carried out in this area.}},
