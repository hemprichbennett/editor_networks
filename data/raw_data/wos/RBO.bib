@article{ ISI:000397400300007,
Author = {Smith, Paul and Garrett, Kimball L.},
Title = {{The juvenile plumage of Quebracho Crested-Tinamou Eudromia formosa
   (Aves: Tinamidae)}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2016}},
Volume = {{24}},
Number = {{4}},
Pages = {{335-337}},
Month = {{DEC}},
Abstract = {{The first description and images of the chick of the range restricted
   Chaco endemic Quebracho Crested-Tinamou Eudromia formosa are provided.
   They are essentially similar to adults, but possess a pale mid-dorsal
   stripe along the body. When crouching in a cryptic defensive posture,
   this stripe combines with the paler lateral stripes on the head and neck
   to effectively break up the outline of the bird. The identity of a
   Eudromia chick in the Los Angeles County museum is also confirmed as E.
   formosa.}},
