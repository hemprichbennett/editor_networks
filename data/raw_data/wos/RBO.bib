@article{ ISI:000397400300013,
Author = {Franca, Pedro F. and Sousa, Jackson C. and Correa, Juliana G. and
   Campos, Carlos E. C.},
Title = {{First record of Anous minutus in the state of Amapa, Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2016}},
Volume = {{24}},
Number = {{4}},
Pages = {{370-371}},
Month = {{DEC}},
Abstract = {{We present the first record of Anous minutus for the state of Amapa,
   Brazil, and only the second for the whole Amazonian region. Within
   Brazil A. minutus is restricted as a breeder to Fernando de Noronha,
   Atol das Rocas, Sao Pedro and Sao Paulo and Martin Vaz/Trindade
   archipelagos, with records of vagrants from the states of Ceara and Sao
   Paulo. We photographed a single bird at the Maraca-Jipioca Ecological
   Station, on the 10 December 2015, west of the Maraca do Sul Island in an
   area known as Ponta Sul. To our knowledge this is the first record from
   Amapa and only the second from northern Brazil and the Amazon region.}},
