@article{ ISI:000305115600007,
Author = {Lino Mota, Joao Vitor and Freire de Carvalho, Anderson Abbehusen and
   Tinoco, Moacir Santos},
Title = {{Restinga's birds distribution and habitat use at the Reserva Imbassai on
   the North Coast of Bahia region}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
