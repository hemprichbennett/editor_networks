@article{ ISI:000321053900008,
Author = {Leal, Sergio and Serapiao, Lucio Claudio H. and Pereira, Glauco Alves},
Title = {{First documented records of Franklin's Gull Leucophaeus pipixcan
   (Wagler, 1831) from north east Brazil and Grey-headed Gull
   Chroicocephalus cirrocephalus (Vieillot, 1817) in the states of Alagoas
   and Pernambuco.}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2013}},
Volume = {{21}},
Number = {{1}},
Pages = {{105-108}},
Month = {{MAR}},
Abstract = {{We report on the first documented record of Franklin's Gull in
   north-eastern Brazil, when one individual was found dead in Maceio,
   state of Alagoas, on 27 July 2010, and other was found and photographed
   very weak on the same beach on 8 May 2011. Since most records of this
   species in South America come from the western part of the continent,
   this individual probably reached northeastern Brazil through the
   Caribbean. About 25 Grey-headed gulls were photographed at Barra de Sao
   Miguel, state of Alagoas, on 5 March 2010, and two individuals were
   observed at the same place on 23 April 2010. In the state of Pernambuco,
   two birds were photographed at Itapissuma on 1 October 2010. These are
   the first records of the Grey-headed Gull for Pernambuco and Alagoas
   states in northeastern Brazil.}},
