@article{ ISI:000305116400008,
Author = {Costa, Lilian Mariana and Freitas, Guilherme H. S. and Pena, Joao Carlos
   C. and Rodrigues, Marcos},
Title = {{The play behavior of a Sharp-shinned Hawk (Accipiter striatus) chasing a
   flock of White-naped Jay (Cyanocorax cyanopogon)}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2012}},
Volume = {{20}},
Number = {{1}},
Pages = {{40-43}},
Month = {{MAR}},
Abstract = {{The play behavior of a Sharp-shinned Hawk (Accipiter striatus) chasing a
   flock of White-naped Jay (Cyanocorax cyanopogon). Play behavior in birds
   is difficult to recognize and interpret, and poor documented in Brazil.
   In this note, we describe an opportunistic record of play behavior for a
   Sharp-shinned Hawk (Accipiter striatus) individual chasing a flock of
   White-naped Jay (Cyanocorax cyanopogon), in cerrado of southeastern
   Brazil. The Sharp-shinned Hawk individual performed silent and slow
   chase flights, without evidence of aggression. The jays intensively
   uttered many vocalization types, including imitative calls of Roadside
   Hawk (Rupornis magnirostris) and Collared Forest-Falcon (Micrastur
   semitorquatus), moving between perches. Despite the existence of other
   potential hypothesis for such observed behavior, like predation or
   defense, we rely on play behavior due the many features of the event,
   such as its repetitiveness and long duration, and the lack of investment
   in attack and speed and no success of the hawk. We hope this report can
   contribute to the awakening of field ornithologists for avian play
   behavior, encouraging the publication of new records.}},
