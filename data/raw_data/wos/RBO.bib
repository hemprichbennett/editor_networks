@article{ ISI:000305115800011,
Author = {Fazio Sanabria, Jose Antonio and Brusco, Giuliano Mueller},
Title = {{Noteworthy records of Charadriiformes on the northern coast of Rio
   Grande do Sul, Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2011}},
Volume = {{19}},
Number = {{4}},
Pages = {{529-534}},
Month = {{DEC}},
Abstract = {{Noteworthy records of Charadriiformes on the northern coast of Rio
   Grande do Sul, Brazil. We report on records of charadriform species not
   previously known or considered rare on the northern coast (29 degrees
   21' S, 49 degrees 44' W; 30 degrees 18' S, 50 degrees 15' W) of the
   state of Rio Grande do Sul in south Brazil. Although long considered
   scarce in the state, Semipalmeted Plover Charadrius semipalmatus
   occurred year-round in 2010, being abundant (up to 680 individuals)
   during spring-summer. Baird's Sandpiper C. bairdii, a species known in
   Brazil from a handful, was observed 11 times in 2010, especially in
   September-November. The other species recorded were: Hudsonian Godwit
   Limosa haemastica, Whimbrel Numenius phaeopus, Semipalmated Sandpiper
   Calidris pusilla, Phalaropus tricolor and Least Tern Sternula
   antillarum.}},
