@article{ ISI:000208353800004,
Author = {Xavier de Lima, Andre Magnani and Roper, James Joseph},
Title = {{The use of playbacks can influence encounters with birds: an experiment}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2009}},
Volume = {{17}},
Number = {{1}},
Pages = {{37-40}},
Month = {{MAR}},
Abstract = {{Playback of recorded bird songs is often used to attract birds for
   sighting as well for captures. The combined use of playbacks and
   captures may in fact change the behavior of birds such that the response
   to playbacks does not reflect responses to natural songs. Here we
   studied whether the use of playbacks changes detectability of the birds.
   Birds previously exposed to playbacks (experienced birds) did not
   approach the sound source as closely as did naive birds in playback
   trials. The response time of experienced birds is also shorter than that
   of naive birds. Limited data suggest that the biased response to
   playbacks is unrelated to whether the bird was captured. Considering the
   common use of playback, we recommend caution in the analysis in
   population studies in which playbacks are used.}},
