@article{ ISI:000208353700007,
Author = {Camilotti, Vagner Luis and Kruegel, Marilise Mendonca and Hartz, Sandra
   Maria},
Title = {{A nesting event of Cinereous Harrier (Circus cinereus) was registered in
   the Fronteira Oeste region, Rio Grande do Sul State}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2008}},
Volume = {{16}},
Number = {{4}},
Pages = {{363-365}},
Month = {{DEC}},
Abstract = {{A nesting event of Cinereous Harrier (Circus cinereus) was registered in
   the Fronteira Oeste region, Rio Grande do Sul State. A single nest
   containing only one white egg was found in a wetland area presenting
   high and dense vegetation within an agricultural landscape. It was
   possible that another nesting event was taking place in another similar
   area 30 km apart of the first one. This is a new nesting site in Rio
   Grande do Sul that may improve the knowledge of the biology and ecology
   of the species.}},
