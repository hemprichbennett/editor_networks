@article{ ISI:000305115600009,
Author = {Jean Desbiez, Arnaud Leonard and Sao Bernardo, Christine Steiner},
Title = {{Density estimates of the Bare-faced Curassow (Crax fasciolata) in the
   Brazilian Pantanal}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
