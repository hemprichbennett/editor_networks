@article{ ISI:000336241200004,
Author = {Silva de Moura, Pedro Tefilo and Franca, Leonardo Fernandes},
Title = {{Refining the distribution of the White Woodpecker (Melanerpes candidus)
   in the Caatinga biome}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2014}},
Volume = {{22}},
Number = {{1}},
Pages = {{32-34}},
Month = {{MAR}},
Abstract = {{The occurrence of Melanerpes candidus (White Woodpecker) in Brazil has
   been documented in all biomes, but so far only marginally in the
   Caatinga. Herein we provide the first records of the White Woodpecker in
   the Depressao Sertaneja Setentrional ecoregion of the Caatinga biome,
   consisting of some of the farthest northeastern records for the species.
   Our results indicate that the White Woodpecker may be distributed
   throughout the Caatinga biome.}},
