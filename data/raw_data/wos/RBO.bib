@article{ ISI:000389952200004,
Author = {Greeney, Harold F. and Goncalves de Lima, Jefferson Luis and Toledo e
   Silva, Thiago},
Title = {{First description of the nest of White-browed Antpitta Hylopezus
   ochroleucus}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2016}},
Volume = {{24}},
Number = {{3}},
Pages = {{213-216}},
Month = {{SEP}},
Abstract = {{The reproductive biology of White-browed Antpitta (Hylopezus
   ochroleucus) is completely unknown. We describe a nest and nestlings
   found at the Parque Natural Municipal do Distrito de Brejinho, Araripe,
   Ceara. Brazil. The nest was a loose, shallow, open cup of sticks built
   into a tangle of branches and vines 0.5 m above the ground. The two
   nestlings were first seen on 5 April 2015, with closed eyes, bright
   orange bills and mouth linings, and dark grayish-pink skin, devoid of
   natal down, with contour feather tracts beginning development (under the
   skin). Six days later they had a dense coating of red-brown, wool-like
   down, their eyes were beginning to open, and secondary feathers were
   emerging. We compare our findings to related species and other members
   of the family Grallariidae.}},
Publisher = {{SOC BRASILEIRA ORNITOLOGIA}},
