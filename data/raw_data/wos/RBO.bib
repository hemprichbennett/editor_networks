@article{ ISI:000349633300008,
Author = {Mafia, Pedro de Oliveira and Jorge Correa, Matheus Rocha and do Rosario
   Cruz, Antonio Jorge and de Azevedo, Cristiano Schetini},
Title = {{Rodent predation by Turdus leucomelas (Passeriformes: Turdidae)}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2014}},
Volume = {{22}},
Number = {{4}},
Pages = {{408-410}},
Month = {{DEC}},
Abstract = {{Pale-breasted Thrush (Turdus leucomelas) is described as an omnivorous
   bird that forage solitarily or in pairs on the soil. This note reports a
   rodent predation event by T. leucomelas. The event was recorded on
   November 14th 2013, in a riparian Forest fragment of Grande River, in
   Igarapava Municipality, Sao Paulo, Brazil. Although the presence of
   small vertebrates on the diet of T. leucomelas is known, this is the
   first record of a mammal being predated by this bird species. This
   record is important because it contributes to a better understand of the
   natural history of Neotropical passerines.}},
