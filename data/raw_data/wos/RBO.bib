@article{ ISI:000332147300005,
Author = {Simon, Jose Eduardo and Magnago, Gustavo Rodrigues},
Title = {{Rediscovery of the Cryptic Forest-Falcon Micrastur mintoni Whittaker,
   2002 (Falconidae) in the Atlantic forest of southeastern Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2013}},
Volume = {{21}},
Number = {{4}},
Pages = {{257-262}},
Month = {{DEC}},
Abstract = {{The population of Micrastur gilvicollis from eastern Brazil has been
   attributed to Micrastur mintoni, a forest raptor described in 2002 based
   primarily on a large series of specimens especially from Amazonian and
   only three historic specimens from the Atlantic rainforest. The last
   records of M. mintoni in Atlantic forest were obtained in 1972 (museum
   specimens) and the literature suggests that it could have been extinct
   from the easternmost part of its geographic distribution. Herein, we
   provide a recent documented record of M. mintoni for the Atlantic
   forest, confirming the rediscovery of the species from northern Espirito
   Santo, southeastern Brazil. Our record is documented by photographs and
   audio recording from Reserva Natural Vale, municipality of Linhares,
   obtained on 29 July 2012. The loss of habitat, the natural rarity and
   furtive behavior of this falcon seems to explain why it remained
   unrecorded for 40 years in the Atlantic forest.}},
