@article{ ISI:000208353600002,
Author = {Lopes, Leonardo Esteves and Malacco, Gustavo Bernardino and de
   Vasconcelos, Marcelo Ferreira and de Alencar Carvalho, Carlos Eduardo
   and Duca, Charles and Fernandes, Alexandre Mendes and D'Angelo Neto,
   Santos and Marini, Miguel Angelo},
Title = {{Birds from Unai and Cabeceira Grande region, northwestern state of Minas
   Gerais, Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
