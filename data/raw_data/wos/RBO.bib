@article{ ISI:000305115800002,
Author = {Alves, Vania Soares and Aroeira Soares, Ana Beatriz and do Couto,
   Gilberto Soares and Draghi, Julia},
Title = {{Pattern of occurrence and distribution of Neotropic Cormorants
   (Phalacrocorax brasilianus) in the Guanabara Bay, Rio de Janeiro, Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2011}},
Volume = {{19}},
Number = {{4}},
Pages = {{469-477}},
Month = {{DEC}},
Abstract = {{Pattern of occurrence and distribution of Neotropic Cormorants
   (Phalacrocorax brasilianus) in the Guanabara Bay, Rio de Janeiro,
   Brazil. Waterbird communities in the Guanabara Bay, state of Rio de
   Janeiro, were studied from August 2005 to August 2007, as part of the
   Environmental Assessment of Guanabara Bay project, coordinated by
   CENPES-PETROBRAS. The study employed census data gathered from boat
   trips in ten different areas (Rivers Iguacu, Estrela, Surui, Iriri,
   Copororoca, Guapi, Guarai, Canal Boca Larga, Cajaibas de Dentro islands
   and Cajaibas de Fora islands), and aboard helicopter. The total wealth
   of birds in Guanabara Bay, also considering the literature, includes 199
   species, 71 of which are waterbirds and 128 are land birds. The
   Neotropic Cormorant (Phalacrocorax brasilianus), recorded in all studied
   areas and during all campaigns, was the most abundant species. On the
   Cajaibas Islands and in the Rivers Estrela, Iriri, Suru and Guarai the
   constancy index was higher than 90\%. On average, the highest
   concentrations were recorded on the Cajaibas Islands and in Estrela
   River, areas where the bird was very common (>50\%). The peak abundance
   was recorded in autumn/winter and spring, with a population decrease
   during the summer. At least during the summer and autumn cormorants
   breed on Tijucas islands (Alfavaca and Pontuda), off Guanabara Bay. The
   temporal distribution pattern of the cormorant in the study area was
   influenced by the occurrence of migratory species like
   Lesser-Yellowlwges (Tringa flavipes) and Semipalmated Plovers
   (Charadrius semipalmatus).}},
