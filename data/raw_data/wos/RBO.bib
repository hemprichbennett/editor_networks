@article{ ISI:000336241200007,
Author = {Mauricio, Giovanni N. and Barreto, Jonathas and Bugoni, Leandro},
Title = {{The Kerguelen Petrel Lugensa brevirostris in the Southwestern Atlantic
   Ocean, with notes on osteology - and plumage-based identification}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2014}},
Volume = {{22}},
Number = {{1}},
Pages = {{42-48}},
Month = {{MAR}},
Abstract = {{Southwestern Atlantic/eastern South American records of the Kerguelen
   Petrel Lugensa brevirostris are scarce, comprised only of specimens
   found dead on the coast. Here we present a review of records known from
   this region, including a novel record based on a specimen stranded at
   Arraial do Cabo, Rio de Janeiro state, Brazil. Additionally, we draw the
