@article{ ISI:000346373800011,
Author = {Ortega-Alvarez, Ruben and Calderon-Parra, Rafael},
Title = {{Hunting the unexpected: Harris's Hawks (Parabuteo unicinctus) preying on
   bats in a Neotropical megacity}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2014}},
Volume = {{22}},
Number = {{3}},
Pages = {{297-299}},
Month = {{SEP}},
Abstract = {{Many wildlife species have modified their behaviors in order to thrive
   within cities. Since the 1980's, Harris's Hawk has become a regular
   resident species in Mexico City, Mexico. Here, we report on what may be
   an urban adaptation - two Harris's Hawks hunting bats in urban, southern
   Mexico City. This represents the first formal record of Harris's Hawk
   preying on bats, either within anthropogenic or natural ecosystems.
