@article{ ISI:000305116400012,
Author = {Pinheiro, Renato Torres and Dornas, Tulio and Leite, Gabriel Augusto and
   Crozariol, Marco Aurelio and Marcelino, Dianes Gomes and Correa, Andre
   Grassi},
Title = {{Kaempfer's Woodpecker Celeus obrieni: new records and conservation
   status in the state of Goias, Central Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2012}},
Volume = {{20}},
Number = {{1}},
Pages = {{59-64}},
Month = {{MAR}},
Abstract = {{Kaempfer's Woodpecker Celeus obrieni: new records and conservation
   status in the state of Goias, Central Brazil. The Kaempfer's Woodpecker
   Celeus obrieni was discovered in the Urucui region, state of Piaui, in
   1926, and remained disappeared for 80 years. Data about its occurrence
   in the state of Goias date as far back as the 1960's, but remained
   unknown until very recently. Based on its historical distribution and
   association with Guadua bamboo, we searched for new records of the
   Kaempfer's Woodpecker in the state of Goias. We covered 24
   municipalities and approximately 20,000 hectares and obtained fourteen
   new records of C. obrieni for the state of Goias. Those new records
   provide a more accurate knowledge on the species' southern distribution
   limits in central Brazil, supporting its status as critically endangered
   due to high levels of destruction in the Cerrado.}},
Publisher = {{SOC BRASILEIRA ORNITOLOGIA}},
Address = {{C/O ROMULO RIBON, MUSEU ZOOLOGIA JOAO MOOJEN, LADEIRA DOS OPERARIOS
   54-204, VICOSA, MG 36570-000, BRAZIL}},
