@article{ ISI:000328204300009,
Author = {de Carvalho, Gustavo Diniz M. and de Vasconcelos, Marcelo Ferreira and
   Peixoto, Gabriela Nunes C. and Lisboa, Jorge Sales and Marques, Leandro
   de Oliveira},
Title = {{155 years after Reinhardt: the second specimen of Bartramia longicauda
   (Charadriiformes: Scolopacidae) from the state of Minas Gerais, Brazil}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2013}},
Volume = {{21}},
Number = {{2}},
Pages = {{141-143}},
Month = {{JUN}},
Abstract = {{The Upland Sandpiper (Bartramia longicauda) is a Nearctic migrant that
   presents few seasonal records in several Brazilian states. In the state
   of Minas Gerais, southeastern Brazil, the only known specimen was
   collected on 26 October 1855 by the Danish naturalist J. T. Reinhardt in
   Capivari, near Andrequice (18 degrees 17' S; 44 degrees 59' W, c. 800
   m). On 10 October 2010, a male of B. longicauda was collected at
   Pampulha Airport (19 degrees 50' S; 43 degrees 57'W, c. 785 m), Belo
   Horizonte. This specimen was prepared as a study skin and deposited in
   the bird collection of the Museu de Ciencias Naturais, Pontificia
   Universidade Catolica de Minas Gerais, under number MCNA 1874. This
   appears to be the second specimen of B. longicauda from the state of
   Minas Gerais and the only deposited in a Brazilian institution. Although
   scarce, the majority of records of this species in Minas Gerais occurred
   in the months of October and November, which suggests a rapid passage
   through the state this time of year. More information must be obtained
   to elucidate the route of this species and the period when it passes
   through Minas Gerais.}},
