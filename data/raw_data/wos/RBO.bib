Author = {Mazar Barnett, Juan and Pugnali, German D. and Pearman Morrison, Mark
   and Bodrati, Alejandro and Moschione, Flavio and Clark, Ricardo and
   Roesler, Ignacio and Monteleone, Diego and Casanas, Hernan and Burgos
   Gallardo, Freddy and Segovia, Jose and Pagano, Luis and Povedano, Hernan
   and Areta, Juan I.},
Title = {{The Andean Swallow (Orochelidon andecola) in Argentina}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2014}},
Volume = {{22}},
Number = {{2}},
Pages = {{172-179}},
Month = {{JUN}},
Abstract = {{During ornithological studies in the provinces of Jujuy, Salta, and San
   Juan, we recorded the Andean Swallow Orochelidon andecola at 40
   localities. These are the first records in Argentina, and also represent
   the southernmost for the species. Some of these localities are up to
   1500 m lower than the previously known elevational limit (now 800 masl),
   and up to 1100 km southwards. This is a relatively poorly known swallow,
   and we present novel natural history data. We found evidence of breeding
   in five localities. We obtained photographs and tape recordings, and
   provide details of a specimen.}},
