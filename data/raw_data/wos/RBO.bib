@article{ ISI:000321053900005,
Author = {Figueroa, Judith},
Title = {{The birds of Lobos de Tierra island, Peru: a review and new records
   (1684-2011).}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2013}},
Volume = {{21}},
Number = {{1}},
Pages = {{58-74}},
Month = {{MAR}},
Abstract = {{The present work is based on a detailed review of bird observations made
   on Lobos de Tierra island (06 degrees 26'S; 80 degrees 51'W) by various
   authors since 1684, as well as interviews with island rangers and other
   researchers, and field assessments carried out in 1999, 2000, 2004 and
   2011. This compilation came to a total of 43 species (11 breeding), Sula
   nebouxii and Pelecanus thagus being the most abundant. The high number
   of records compared with other islands in Peru are related to the
   island's proximity to the coast and its location on the convergence zone
   of the cold waters of the Peruvian Current and the warm waters of the
   Equatorial Countercurrent. Of all the species, 17 are listed in a
   conservation category.}},
