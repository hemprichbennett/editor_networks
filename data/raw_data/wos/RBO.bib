@article{ ISI:000339092200014,
Author = {Clay, Robert and Barnett, Juan Mazar and Esquivel, Estela},
Title = {{First description of the eggs, chick, and nest site of the White-winged
   Nightjar Eleothreptus candicans}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2014}},
Volume = {{22}},
Number = {{2}},
Pages = {{215-218}},
Month = {{JUN}},
Abstract = {{We provide the first description of the nest site, eggs, and chick of
   the globally threatened White-winged Nightjar Eleothreptus candicans,
   based on observations in Aguara Nu, Mbaracayu Forest Nature Reserve,
   Paraguay, made during November-December 1997. Two eggs were laid
   directly on the ground at the edge of a small clearing in campo-sujo
   grassland. Only the female appeared to attend the nest, undertaking a
   distraction display when the nest site was closely approached. Just one
   egg hatched, after a period of at least 16 days.}},
