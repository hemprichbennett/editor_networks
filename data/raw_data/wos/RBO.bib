@article{ ISI:000346373800007,
Author = {Naveda-Rodriguez, Adrian and Lugo, Gabriela},
Title = {{New records and distribution extension of Buteogallus solitarius (Aves:
   Accipitridae) in Venezuela}},
Journal = {{REVISTA BRASILEIRA DE ORNITOLOGIA}},
Year = {{2014}},
Volume = {{22}},
Number = {{3}},
Pages = {{281-284}},
Month = {{SEP}},
Abstract = {{The Solitary Eagle (Buteogallus solitarius) is a poorly known
   Neotropical raptor and many aspects of its natural history remain
   unknown. The species is often misidentified and confused with other
   congeners. In Venezuela, its occurrence, based on voucher specimen and
   photographic records, is restricted to the Cordillera de la Costa
   Central. Here we report on four new confirmed records of the Solitary
   Eagle in Venezuela. These records represent a distribution extension
   (>160 km) from its previously known geographic range within the country.}},
