@article{ ISI:000288237700014,
Author = {Elzanowski, Andrzej and Stidham, Thomas A.},
Title = {{A GALLOANSERINE QUADRATE FROM THE LATE CRETACEOUS LANCE FORMATION OF
   WYOMING}},
