Language = {{English}},
Affiliation = {{Ryder, TB (Reprint Author), Migratory Bird Ctr, Smithsonian Conservat Biol Inst, Natl Zool Pk,POB 37012-MRC5503, Washington, DC 20008 USA.
   Ryder, Thomas B.; Marra, Peter P., Migratory Bird Ctr, Smithsonian Conservat Biol Inst, Washington, DC 20008 USA.
   Fox, James W., British Antarctic Survey, Nat Environm Res Council, Cambridge CB3 0ET, England.}},
DOI = {{10.1525/auk.2011.11091}},
ISSN = {{0004-8038}},
EISSN = {{1938-4254}},
