@article{ ISI:000301470800011,
Author = {Worthy, Trevor H.},
Title = {{A NEW SPECIES OF OLIGO-MIOCENE DARTER (AVES: ANHINGIDAE) FROM AUSTRALIA}},
Journal = {{AUK}},
Year = {{2012}},
Volume = {{129}},
Number = {{1}},
Pages = {{96-104}},
Month = {{JAN}},
Abstract = {{A single fossil tarsometatarsus from the Etadunna Formation, of Late
   Oligocene-Early Miocene age, at the Snake Dam Locality in South
   Australia reveals the first pre-Pliocene record of anhingids in
   Australia. Anhinga walterbolesi sp. nov. provides the oldest record
   globally for the Anhingidae and, with the contemporary presence of
   stem-group phalacrocoracids in the same formation, indicates a probable
   Early Oligocene to Eocene age for the common ancestor of anhingids and
   phalacrocoracids. Received 15 September 2011, accepted 16 December 2011.}},
