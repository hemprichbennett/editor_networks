@article{ ISI:000310659700013,
Author = {Zhang, Zihui and Huang, Yunping and James, Helen F. and Hou, Lianhai},
Title = {{A MARABOU (CICONIIDAE: LEPTOPTILOS) FROM THE MIDDLE PLEISTOCENE OF
   NORTHEASTERN CHINA}},
Journal = {{AUK}},
Year = {{2012}},
Volume = {{129}},
Number = {{4}},
Pages = {{699-706}},
Month = {{OCT}},
Abstract = {{A new species of marabou, Leptoptilos lui sp. nov., is described on the
   basis of a cranium, distal humerus, and proximal phalanx of the major
   digit, from the Middle Pleistocene Jinniushan locality, Liaoning
   Province of northeastern China. Fossil crania of the genus Leptoptilos
   are rare, and that of the new bird is distinct from the crania of its
   extant relatives. The forelimb elements are the largest and most robust
   among living and fossil species of Leptoptilos and are characterized by
   autapomorphic characters. Leptoptilos lui, like the African Marabou
   Stork (L. crumeniferus), probably possessed a considerable flight
   capability and fed mainly on pieces of carrion torn from carcasses by
   other, sympatric large scavengers. The new stork aids in our
   understanding of the paleoecology and paleoenvironment of northeastern
   China in the Middle Pleistocene, helps us characterize the scavenger
   guild that fed on the Middle Pleistocene megafauna, and shows that the
   genus Leptoptilos, now restricted to the Old World tropics, extended
   into higher latitudes in the Middle Pleistocene. Received 7 October
   2011, accepted 11 June 2012.}},
