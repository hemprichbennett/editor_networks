@article{ ISI:000277398000022,
Author = {Krajewski, Carey and Sipiorski, Justin T. and Anderson, Frank E.},
Title = {{COMPLETE MITOCHONDRIAL GENOME SEQUENCES AND THE PHYLOGENY OF CRANES
   (GRUIFORMES: GRUIDAE)}},
Journal = {{AUK}},
