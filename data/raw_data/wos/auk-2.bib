@article{ ISI:000304283600008,
Author = {Veech, Joseph A. and Small, Michael F. and Baccus, John T.},
Title = {{REPRESENTATIVENESS OF LAND COVER COMPOSITION ALONG ROUTES OF THE NORTH
   AMERICAN BREEDING BIRD SURVEY}},
Journal = {{AUK}},
