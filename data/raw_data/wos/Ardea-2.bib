@article{ ISI:000083232300008,
Author = {Bosch, M and Figuerola, J},
Title = {{Detrimental effects of ticks Ornithodoros maritimus on the growth of
   yellow-legged gull Larus michahellis chicks}},
Journal = {{ARDEA}},
Year = {{1999}},
Volume = {{87}},
Number = {{1}},
Pages = {{83-89}},
Abstract = {{The effect of tick Ornithodoros (Alectorobius) maritimus larvae on the
   growth of Yellow-legged Gull Larus michahellis{*} chicks was studied at
   the Medes Islands colony (NE Spain). Within broods, chicks infested by
   more larvae weighed less had a poorer body condition and a shorter wing
   length than their less infested siblings measured at a similar age.
   There were no differences in body measurements before tick infestation.
   Because mass and size of gull chicks are significant predictors of
   probability of hedging, the detrimental effect of O. maritimus larvae on
   these parameters may affect the breeding success of Yellow-legged Gulls,
   and consequently their life-history and population dynamics.}},
