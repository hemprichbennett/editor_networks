@article{ ISI:A1994PN25100005,
Author = {SALVIG, JC and ASBIRK, S and KJELDSEN, JP and RASMUSSEN, PAF},
Title = {{WINTERING WADERS IN THE BIJAGOS-ARCHIPELAGO, GUINEA-BISSAU 1992-1993}},
Journal = {{ARDEA}},
Year = {{1994}},
Volume = {{82}},
Number = {{1}},
Pages = {{137-141}},
Abstract = {{The coast of Guinea-Bissau in West Africa is an area of outstanding
   importance for wintering waders. Earlier surveys during the winters of
   1982/83 and 1986/87 estimated that about 1 million waders use the area.
   In the Bijagos Archipelago, where the present survey was carried out, it
   was estimated that 700 000 waders wintered in 1986/87. The purpose of
   the winter 1992/93 surveys was to count birds in areas which had not
   been previously visited. In all 453 573 waders were counted on
   approximately 64\% of the total area of tidal flats. From this, it was
   estimated that about 710 000 waders were wintering in the Bijagos
   Archipelago, although the species composition in 1992/93 showed
   pronounced differences compared with those of 1982/83 and 1986/87.}},
