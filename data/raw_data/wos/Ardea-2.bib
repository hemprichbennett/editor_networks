@article{ ISI:A1991FB34800006,
Author = {KOK, OB and VANEE, CA and NEL, DG},
Title = {{DAYLENGTH DETERMINES DEPARTURE DATE OF THE SPOTTED FLYCATCHER
   MUSCICAPA-STRIATA FROM ITS WINTER QUARTERS}},
Journal = {{ARDEA}},
Year = {{1991}},
Volume = {{79}},
Number = {{1}},
Pages = {{63-65}},
Abstract = {{The annual arrival and departure dates of the Spotted Flycatcher
   Muscicapa striata at Bloemfontein in the Orange Free State, South
   Africa, were noted for a continuous period of 36 years (1950-1986). 
   According to a stepwise regression analysis, daylength is by far the
   most important environmental factor influencing the date of departure. 
   The influence of the minimum, maximum and mean daily temperatures,
   duration of sunshine, rainfall and wind speed seems to be unimportant in
   this context.}},
