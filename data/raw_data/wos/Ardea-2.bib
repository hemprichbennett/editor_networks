@article{ ISI:A1995TR87700008,
Author = {Erez, A and YomTov, Y},
Title = {{Reproduction of a Hooded Crow Corvus corone population in Israel}},
Journal = {{ARDEA}},
Year = {{1995}},
Volume = {{83}},
Number = {{2}},
Pages = {{405-409}},
Abstract = {{The breeding biology of the Hooded Crow Corvus corone sardonius was
   studied in the Mediterranean coastal plain of Israel. Population density
   was very high, clutch size was larger than in Europe and breeding
   success was similar to that of other populations of crows. Egg size
   declined with laying order. Crows which laid early in the season tended
   to have larger clutches and heavier eggs, and produced more fledglings
   than crows which laid later in the season.}},
