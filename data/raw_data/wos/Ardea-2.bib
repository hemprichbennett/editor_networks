@article{ ISI:000087038600011,
Author = {Hernandez-Carrasquilla, F and Tyrberg, T and von den Driesch, A},
Title = {{A record of Pygmy Cormorant Microcarbo pygmaeus from Medieval Spain}},
Journal = {{ARDEA}},
Year = {{1999}},
Volume = {{87}},
Number = {{2}},
Pages = {{285-288}},
Abstract = {{The discovery of four bones of a Pygmy Cormorant Microcarbo pygmaeus
   during an excavation of a fortress in Central Spain indicates the
   presence of this species in the area during the Late Middle Ages. The
   probability that this bird could be a vagrant is very small. The Tablas
   de Daimiel National Park a wetland close to the site, would have
   fulfilled the breeding requirements of the species although this
   occurrence could have been irregular depending of the annual water
   level.}},
