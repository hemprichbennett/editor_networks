@article{ ISI:A1991FM37700025,
Author = {MOSBECH, A and GLAHDER, C},
Title = {{ASSESSMENT OF THE IMPACT OF HELICOPTER DISTURBANCE ON MOLTING
   PINK-FOOTED GEESE ANSER-BRACHYRHYNCHUS AND BARNACLE GEESE
   BRANTA-LEUCOPSIS IN JAMESON LAND, GREENLAND}},
Journal = {{ARDEA}},
Year = {{1991}},
Volume = {{79}},
Number = {{2}},
Pages = {{232-237}},
Note = {{INTERNATIONAL SYMP ON WESTERN PALEARCTIC GEESE, KLEVE, FED REP GER, FEB,
   1989}},
Organization = {{INT WATERFOWL \& WETLANDS RES BUR}},
Abstract = {{Jameson Land, at the southern border of the high arctic zone in
   Northeast Greenland holds in the summer c. 5000 moulting Pink-footed
   Geese Anser brachyrhynchus and 5000 moulting Barnacle Geese Branta
   leucopsis and breeding populations of about 500 pairs of each species. 
   Oil exploration activities in 1987 and 1988 have involved massive
   helicopter traffic.  To assess the impact of this disturbance, the
   behavioural responses of the moulting geese to helicopter disturbance
   have been studied and related to time budget of undisturbed geese. 
   Disturbance reactions were shown by the geese when helicopters
   approached to 10 km (large ones, Bell 212) or 5 km (small ones, Bell
   206).  Time budgets of Pink-footed Geese were strongly affected by the
   disturbance.  They were seen to graze and rest less often and to swim
   more often, and probably did not get enough food.  The effect on the
   time budget of Barnacle Geese was not so strong.}},
