@article{ ISI:A1991FB34800005,
Author = {HILGERLOH, G},
Title = {{SPRING MIGRATION OF PASSERINE TRANS-SAHARAN MIGRANTS ACROSS THE STRAITS
   OF GIBRALTAR}},
Journal = {{ARDEA}},
Year = {{1991}},
Volume = {{79}},
Number = {{1}},
Pages = {{57-62}},
Abstract = {{Radar observations of passerine trans-Saharan migrants in 52 nights
   during spring migration (April, 15 till May, 31 in 1981, 1984 and 1986)
   were conducted in the Straits of Gibraltar.  The directions of migration
   were closely correlated to the prevailing wind directions - easterly and
   westerly side winds.  Average track was to the N (6-degrees), in weak
   winds average track was toward 15-degrees.  From the observations in
   Gibraltar and other European and African locations, it was deduced that
   the observed songbirds do not fly in a rhumb line from their wintering
   quarters to the breeding grounds.}},
