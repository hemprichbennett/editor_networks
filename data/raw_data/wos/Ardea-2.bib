@article{ ISI:A1991FM37700038,
Author = {OBRIAIN, M and HEALY, B},
Title = {{WINTER DISTRIBUTION OF LIGHT-BELLIED BRENT GEESE BRANTA-BERNICLA-HROTA
   IN IRELAND}},
Journal = {{ARDEA}},
Year = {{1991}},
Volume = {{79}},
Number = {{2}},
Pages = {{316-326}},
Note = {{INTERNATIONAL SYMP ON WESTERN PALEARCTIC GEESE, KLEVE, FED REP GER, FEB,
   1989}},
Organization = {{INT WATERFOWL \& WETLANDS RES BUR}},
Abstract = {{The movements and redistribution of Light-bellied Brent Geese was
   investigated on the Irish wintering grounds using censuses and
   observations of leg-banded birds.  Over 75\% of the population stage at
   Strangford Lough in autumn before large-scale dispersal to sites around
   the coast.  Although dispersal continues throughout the winter and
   spring, the distances travelled by the geese are much shorter and
   generally within defined regions.  They are not weather-induced but
   arise from changes in food availability at different sites.  The
   relative importance of traditional intertidal and inland feeding areas
   to the population is assessed.}},
