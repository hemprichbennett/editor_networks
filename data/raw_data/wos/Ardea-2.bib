Affiliation = {{Hotker, H (Reprint Author), Univ Kiel, Forsch \& Technol Zentrum Westkuste, D-25761 Busum, Germany.
   Univ Kiel, Forsch \& Technol Zentrum Westkuste, D-25761 Busum, Germany.}},
ISSN = {{0373-2266}},
