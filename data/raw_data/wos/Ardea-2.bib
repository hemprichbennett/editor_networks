@article{ ISI:A1992JG83600005,
Author = {ELLEGREN, H},
Title = {{ESTIMATED EFFECTS OF AGE AND SEX ON THE FAT-FREE BODY-MASS OF AUTUMN
   MIGRATING BLUETHROATS LUSCINIA-SVECICA-SVECICA}},
Journal = {{ARDEA}},
Year = {{1992}},
Volume = {{80}},
Number = {{2}},
Pages = {{255-259}},
Abstract = {{If constant and size-specific values of the fat-free body mass are used
   for estimating the fat content of migrating birds, it is required that
   the fat-free body mass does not vary considerable between age and sex
   groups. To test this requirement, body masses of autumn migrating
   Bluethroats from different age and sex groups were analysed when wing
   length and fat index had been compensated for. Age has significant
   effect on body mass among males (adults being 0.16 g heavier) while no
   other age or sex effects were observed. The impact of the observed
   situation on flight range estimates is discussed.}},
