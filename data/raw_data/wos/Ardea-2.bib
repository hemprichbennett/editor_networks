@article{ ISI:A1991FM37700029,
Author = {VANEERDEN, MR and ZIJLSTRA, M and LOONEN, MJJE},
Title = {{INDIVIDUAL PATTERNS OF STAGING DURING AUTUMN MIGRATION IN RELATION TO
   BODY CONDITION IN GREYLAG GEESE ANSER-ANSER IN THE NETHERLANDS}},
Journal = {{ARDEA}},
Year = {{1991}},
Volume = {{79}},
Number = {{2}},
Pages = {{260-264}},
Abstract = {{In autumn, the length of stay of individual Greylag Geese at the
   stopover site in Flevoland is related to the rate of increase in their
   body condition (Fig. 2). The condition was estimated by classifying the
   roundness of the abdominal profile (Fig. 1). At departure from our study
   area, the abdominal profile for all geese is approximately the same
   (Table 1), independent of the duration of the stay. Geese start their
   migration (to Spain) after reaching a certain threshold body condition.}},
