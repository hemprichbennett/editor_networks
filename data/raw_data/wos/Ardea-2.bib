@article{ ISI:A1991FM37700022,
Author = {MOOIJ, JH},
Title = {{HUNTING - A QUESTIONABLE METHOD OF REGULATING GOOSE DAMAGE}},
Journal = {{ARDEA}},
Year = {{1991}},
Volume = {{79}},
Number = {{2}},
Pages = {{219-223}},
Note = {{INTERNATIONAL SYMP ON WESTERN PALEARCTIC GEESE, KLEVE, FED REP GER, FEB,
   1989}},
Organization = {{INT WATERFOWL \& WETLANDS RES BUR}},
Abstract = {{Arguments are given that there is no direct relation between goose
   damage and the general number of wintering geese or the maximum number
   of geese in an area.  The best way to reduce the negative effects of
   goose feeding in an area is to create goose-feeding reserves that are
   big enough to feed the number of wintering geese without overgrazing
   endangered areas.  Outside these reserves the geese can be scared away
   from endangered crops.  Shooting as a scaring method is rather
   questionable, because of the uncontrollable negative side-effects on
   goose populations as well as on their environment, which are:  lead
   burden for wetlands and geese, uncontrolled shooting of migrating
   species and needless widespread disturbance of feeding geese.}},
