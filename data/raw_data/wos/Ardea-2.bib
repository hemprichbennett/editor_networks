@article{ ISI:A1991FM37700044,
Author = {FOX, AD and GITAY, H},
Title = {{BREEDING SUCCESS IN GREENLAND BARNACLE GEESE BRANTA-LEUCOPSIS WINTERING
   ON ISLAY, SCOTLAND}},
Journal = {{ARDEA}},
Year = {{1991}},
Volume = {{79}},
Number = {{2}},
Pages = {{359-363}},
Note = {{INTERNATIONAL SYMP ON WESTERN PALEARCTIC GEESE, KLEVE, FED REP GER, FEB,
   1989}},
Organization = {{INT WATERFOWL \& WETLANDS RES BUR}},
Abstract = {{This article attempts to explain the annual fluctuations in breeding
   success amongst Greenland Barnacle Geese Branta leucopsis, reflected in
   the proportion of juveniles in the population wintering on Islay,
   Scotland.  In spite of considerable changes in population size, the
   numbers of successfully breeding pairs is directly related to the size
   of the breeding population.  This suggests that density-dependent
   factors are not yet operating on recruitment.  Breeding success is
   strongly correlated with meterorological conditions.  Multiple
   regression analysis shows that up to 60\% of the variance can be
   explained by weather conditions in early spring in Scotland, in Iceland
   during staging and in Greenland on arrival.}},
