@article{ ISI:A1991FM37700037,
Author = {BURGERS, J and SMIT, JJ and VANDERVOET, H},
Title = {{ORIGINS AND SYSTEMATICS OF 2 TYPES OF THE BEAN GOOSE ANSER-FABALIS
   (LATHAM, 1787) WINTERING IN THE NETHERLANDS}},
