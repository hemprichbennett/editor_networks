@article{ ISI:A1991FM37700015,
Author = {PATTERSON, IJ},
Title = {{CONFLICT BETWEEN GEESE AND AGRICULTURE - DOES GOOSE GRAZING CAUSE DAMAGE
   TO CROPS}},
Journal = {{ARDEA}},
Year = {{1991}},
Volume = {{79}},
Number = {{2}},
Pages = {{178-186}},
Note = {{INTERNATIONAL SYMP ON WESTERN PALEARCTIC GEESE, KLEVE, FED REP GER, FEB,
   1989}},
Organization = {{INT WATERFOWL \& WETLANDS RES BUR}},
Abstract = {{Increasing goose populations are being blamed by farmers for causing
   loss of yield from grass and cereal crops.  For effective assessment of
   techniques to manage and control goose grazing on crops, it is essential
   to know how much loss is associated with a given amount of grazing. 
   Losses can be measured by sampling inside and outside exclosures and
   goose grazing levels can be estimated from dropping densities or from
   the time the birds spend on a field.  Clipping experiments suggest that
   the amount of green material removed is closely related to loss of
   yield; the biomass removed by geese can be estimated from dropping
   intervals and ingestion rates.  There is considerable evidence that
   goose grazing is associated with significant loss of yield, but there is
   great variability in the degree of loss suffered at any given level of
   grazing.  It is argued that management and control of goose grazing must
   take this unpredictability into account.}},
