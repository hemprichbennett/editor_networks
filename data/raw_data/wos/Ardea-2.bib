@article{ ISI:A1992HJ17200020,
Author = {MIGOT, P},
Title = {{DEMOGRAPHIC-CHANGES IN FRENCH HERRING GULL LARUS-ARGENTATUS POPULATIONS
   - A MODELING APPROACH AND HYPOTHESES CONCERNING THE REGULATION OF
   NUMBERS}},
Journal = {{ARDEA}},
Year = {{1992}},
Volume = {{80}},
Number = {{1}},
Pages = {{161-169}},
Note = {{INTERNATIONAL WORKSHOP ON POPULATION DYNAMICS OF LARI IN RELATION TO
   FOOD RESOURCES, SCHIERMONNIKOOG, NETHERLANDS, SEP 23-27, 1989}},
Abstract = {{This work presents an interpretation of the domographic changes in the
   French Herring Gull Larus argentatus population recorded since 1850. 
   Data obtained by a Leslie matrix model, together with population
   parameters resulting from field work and subsequent simulations, suggest
   that at present a large proportion of potentially mature birds does not
   breed.  I discuss the parameter `proportion of breeders' (with
   `immigration' versus `emigration' as intrinsic and extrinsic elements of
   recruitment, respectively), together with its suggested role in the
   population regulation of the gulls.}},
