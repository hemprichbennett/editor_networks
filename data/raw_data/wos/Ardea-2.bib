@article{ ISI:000169431700008,
Author = {Lubjuhn, T and Brun, J and Gerken, T and Epplen, JT},
Title = {{Inconsistent pattern of extra-pair paternity in first and second broods
   of the Great Tit Parus major}},
Journal = {{ARDEA}},
Year = {{2001}},
Volume = {{89}},
Number = {{1, SI}},
Pages = {{69-73}},
Note = {{International Conference on Bird Ringing 100 Years, HELGOLAND, GERMANY,
   SEP 29-OCT 03, 1999}},
Organization = {{European Union Bird Ringing}},
Abstract = {{In this study we compare extra-pair paternity rates in first and second
   broods of 25 individual breeding pairs of the Great Tit Parus major.
   24\% of the first broods contained at least one nestling due to
   copulations outside the pair bond in contrast to 60\% for second broods.
   The average proportion of extra-pair young per brood equalled 3.9 +/-
   7.3\% for first broods and 24.4 +/- 28.7\% for second broods. Extra-pair
   paternity was not consistently observed in first and second broods, i.e.
   > 50\% of the breeding pairs had extra-pair young in one but not the
   other brood. Some possible explanations for this inconsistent pattern
   are discussed.}},
