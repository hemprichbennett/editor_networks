@article{ ISI:A1997XZ41200006,
Author = {Pulliainen, E and Saari, L},
Title = {{Attendance at the nest of polyandrous Dotterel Charadrius morinellus in
   Finland}},
Journal = {{ARDEA}},
Year = {{1997}},
Volume = {{85}},
Number = {{1}},
Pages = {{67-71}},
Abstract = {{The behaviour of a polyandrous female Dotterel was studied in 1970 at
   Varriotunturi, eastern Finnish Lapland (67 degrees 44'N, 29 degrees
   37'E). Five days after the completion of her first clutch, a second one
   was started in which both parents shared the incubation duties. However,
   during the first week the female participated only a little, and was
   often seen in the area with an unringed bird. The nest incubated by both
   the male and the female remained unoccupied for only 3.8\% of the
   visits, a successful nest incubated only by a male for 10.1\%, and an
   unsuccessful nest incubated almost only by a male for 23.0\% of the
   visits. In only four of 32 nests at Varrio females took some part in
   incubation.}},
