@article{ ISI:000296708500001,
Author = {Ozguden-Akkoc, Cansel Guzin and Erdost, Hatice and Ilhan, Tuncay and
   Gunes, Nazmiye},
Title = {{The effects of systemic administration of amylin on the pancreas of
   laying hens}},
Journal = {{AVIAN BIOLOGY RESEARCH}},
Year = {{2011}},
Volume = {{4}},
Number = {{1}},
Pages = {{1-5}},
Abstract = {{The aim of present study was to investigate the effects of systemic
   administration of amylin in laying hens and also to examine the
   localisation of amylin in the pancreas of laying hens at three different
   ages. It involved a variety of histological, immunohistochemical, and
   biochemical techniques. Thirty Super Nick laying hens of 10 weeks of age
   were used. The control group had no treatment. On alternate days the
   experimental group was injected subcutaneously in the loose skin at the
   nape of neck with recombinant rat amylin prepared in water at a dose of
   75 mu g kg(-1). The morphology of pancreas was studied at 14, 16, and 18
   weeks of age. Our immunohistochemical results conclusively showed that
   amylin was expressed in beta cells and yet to our knowledge, this is the
   first study to report the expression of amylin in serous acini of 14 and
