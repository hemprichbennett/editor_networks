@article{ ISI:000305321800007,
Author = {Kjelland, Michael E. and Blue-McLendon, Alice and Kraemer, Duane},
Title = {{Determining air cell location and embryo development in opaque shelled
   eggs}},
Journal = {{AVIAN BIOLOGY RESEARCH}},
Year = {{2012}},
Volume = {{5}},
Number = {{2}},
Pages = {{99-102}},
Abstract = {{A novel method for determining air cell location, embryo development,
   and eggshell defects in opaque thick-shelled eggs was tested. Results
   demonstrated that a camcorder with IR ``Night Shot{''} function
   performed significantly better (P < 0.05) than unaided visual
   determination of air cell location in Emu eggs. In addition, the Mini
   DVC camcorder unit was as accurate as the heavier, non-portable, and
   more expensive traditional IR candling units. Commercial ratite
   producers, waterfowl biologists, and others working with opaque
   thick-shelled eggs may benefit from this research.}},
