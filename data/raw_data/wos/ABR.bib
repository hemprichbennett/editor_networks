@article{ ISI:000333947300002,
Author = {Yan, L. and Gao, J. S. and Rui, L. and Lu, Y. N. and Wan, Z. Y. and Yu,
   X. X. and Zhang, W. X. and Li, H. T. and Shao, Q. and Zhang, J. and Li,
   Z. D.},
Title = {{A review of strategies for producing chimeric birds}},
Journal = {{AVIAN BIOLOGY RESEARCH}},
Year = {{2014}},
Volume = {{7}},
Number = {{1, SI}},
Pages = {{3-9}},
Abstract = {{Avian chimeras have been shown to be very useful tools for studying
   embryonic development. Blastodermal cells form stage X embryos and
   primordial germ cells (PGCs) are extensively used in chimera production.
   Intraspecies and interspecies germline chimeras have been successfully
   produced by transferring blastodermal cells or PGCs into recipient
   blastoderm at stage X or into the early embryonic blood. The long-term
   culture system established using pluripotent cells from birds provides
   an alternative method for preserving the endangered species source and
   facilitates the manipulation of avian transgenesis. A high rate of donor
   cell-derived progeny production has always been pursued by germline
   chimera studies. However, the rate of progeny production was unstable.
   Different methods have been used in an attempt to improve the efficiency
   of germline chimera production. Several methods have been shown to
   greatly enhance the efficiency of donor cell-derived progeny production.
   In this review, we focus on the strategies used for germline chimera
   production in birds and discuss the factors and measures affecting the
   efficiency of obtaining donor cell-derived progeny.}},
