@article{ ISI:000279708700001,
Author = {Takekawa, John Y. and Prosser, Diann J. and Newman, Scott H. and Bin
   Muzaffar, Sabir and Hill, Nichola J. and Yan, Baoping and Xiao,
   Xiangming and Lei, Fumin and Li, Tianxian and Schwarzbach, Steven E. and
   Howell, Judd A.},
Title = {{Victims and vectors: highly pathogenic avian influenza H5N1 and the
   ecology of wild birds}},
Journal = {{AVIAN BIOLOGY RESEARCH}},
Year = {{2010}},
Volume = {{3}},
Number = {{2}},
Pages = {{51-73}},
Month = {{JUN}},
Abstract = {{The emergence of highly pathogenic avian influenza (HPAI) viruses has
   raised concerns about the role of wild birds in the spread and
   persistence of the disease. In 2005, an outbreak of the highly
   pathogenic subtype H5N1 killed more than 6,000 wild waterbirds at
   Qinghai Lake, China. Outbreaks have continued to periodically occur in
   wild birds at Qinghai Lake and elsewhere in Central China and Mongolia.
   This region has few poultry but is a major migration and breeding area
   for waterbirds in the Central Asian Flyway, although relatively little
   is known about migratory movements of different species and connectivity
   of their wetland habitats. The scientific debate has focused on the role
   of waterbirds in the epidemiology, maintenance and spread of HPAI H5N1:
   to what extent are they victims affected by the disease, or vectors that
   have a role in disease transmission? In this review, we summarise the
   current knowledge of wild bird involvement in the ecology of HPAI H5N1.
   Specifically, we present details on: (1) origin of HPAI H5N1; (2)
   waterbirds as LPAI reservoirs and evolution into HPAI; (3) the role of
   waterbirds in virus spread and persistence; (4) key biogeographic
   regions of outbreak; and (5) applying an ecological research perspective
   to studying AIVs in wild waterbirds and their ecosystems.}},
