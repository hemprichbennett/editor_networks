@article{ ISI:A1991FH49400004,
Author = {DIAZ, JA and ASENSIO, B},
Title = {{EFFECTS OF GROUP-SIZE AND DISTANCE TO PROTECTIVE COVER ON THE VIGILANCE
   BEHAVIOR OF BLACK-BILLED MAGPIES PICA-PICA}},
Journal = {{BIRD STUDY}},
Year = {{1991}},
Volume = {{38}},
Number = {{1}},
Pages = {{38-41}},
Month = {{MAR}},
Abstract = {{We studied the influence of group size and distance to protective cover
   on scanning behaviour of Black-billed Magpies, considering their effects
   on scanning rate and scan duration.  The percentage of time spent
   vigilant increased with increasing group size and with increasing
   distance to the nearest tree, but the way in which vigilance changed was
   different in both cases.  Birds in larger groups scanned less, whereas
   birds near protective cover had shorter scans.}},
