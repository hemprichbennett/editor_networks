@article{ ISI:A1991FU97000007,
Author = {GIROUX, JF},
Title = {{ROOST FIDELITY OF PINK-FOOTED GEESE ANSER-BRACHYRHYNCHUS IN NORTH-EAST
   SCOTLAND}},
Journal = {{BIRD STUDY}},
Year = {{1991}},
Volume = {{38}},
Number = {{2}},
Pages = {{112-117}},
Month = {{JUL}},
Abstract = {{The use of wintering roosts in north-east Scotland by Pink-footed Geese
   was studied by following 10 birds fitted with radio transmitters. 
   Pinkfeet changed their roosting sites approximately every 10 nights
   between December and April.  On average, each bird visited 3.4 different
   roosts returning to the same sites on many occasions.  Some shifts
   followed disturbances at the roosts resulting from shooting, trapping
   and other human activities.  Others took place during periods of harsh
   weather or while geese were field feeding at night during a full moon. 
   A switch from inland roosts to a nearby estuary coincided with the end
   of the shooting season along the estuary.}},
