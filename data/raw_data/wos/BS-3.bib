@article{ ISI:A1994PN23500002,
Author = {STONE, CJ and WEBB, A and TASKER, ML},
Title = {{THE DISTRIBUTION OF MANX SHEARWATERS PUFFINUS-PUFFINUS IN NORTH-WEST
   EUROPEAN WATERS}},
Journal = {{BIRD STUDY}},
Year = {{1994}},
Volume = {{41}},
Number = {{3}},
Pages = {{170-180}},
Month = {{NOV}},
Abstract = {{Seabirds in north-west European waters were surveyed offshore throughout
   the year from 1979 to 1993.  During the breeding season the distribution
   of Manx Shearwaters centred around the main colonies of Rhum, Skomer and
   Skokholm.  Densities were low in the North Sea, where birds were mainly
   restricted to waters off the north-east coasts of England and Scotland,
   in particular the Moray Firth, the Firth of Forth and Flamborough Head. 
   Manx Shearwaters can forage at long distances from the colonies, but
   highest densities were observed in the Irish Sea, south-west Wales,
   south-west Ireland and the inshore waters of West Scotland during the
   chick-rearing period when parent birds visit the colony more frequently.
   The area of the Irish Sea front was important, particularly in the
   autumn, and feeding occurred here in preference to other areas of the
   Irish Sea.  From November to February Manx Shearwaters were virtually
   absent from north-west European waters.}},
