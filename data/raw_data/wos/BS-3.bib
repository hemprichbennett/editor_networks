@article{ ISI:A1996VB63600003,
Author = {Summers, RW and Taylor, WG},
Title = {{Use by tits of nest boxes of different designs in pinewoods}},
Journal = {{BIRD STUDY}},
Year = {{1996}},
Volume = {{43}},
Number = {{2}},
Pages = {{138-141}},
Month = {{JUL}},
Abstract = {{Two experiments tested the preferences by tits in pinewoods for
   different nest box designs. Great, Blue and Coal Tits preferred deep
   boxes (approximate to 19 cm from base to entrance lip) over shallow ones
   (approximate to 9 cm). Crested Tits preferred boxes that had been filled
   with sawdust and wood-shaving which they removed before nesting. The
   excavation of a fresh nest cavity each year perhaps ensures that nests
   are free of fleas for Crested Tits.}},
