@article{ ISI:A1994NC31900001,
Author = {AREVALO, JE and GOSLER, AG},
Title = {{THE BEHAVIOR OF TREECREEPERS CERTHIA-FAMILIARIS IN MIXED-SPECIES FLOCKS
   IN WINTER}},
Journal = {{BIRD STUDY}},
Year = {{1994}},
Volume = {{41}},
Number = {{1}},
Pages = {{1-6}},
Month = {{MAR}},
Abstract = {{The flocking behaviour of Treecreepers within mixed-species flocks was
   studied from December to March during 1989190 and 1990191. Treecreepers
   tended to flock more in mid-winter and during 1990191 when mean
   temperature was lower. Tree trunks were the most common foraging sites.
   Internal branches > 5 cm diameter were also commonly used although less
   so as flock-size increased. The niche-breadth of Treecreeper foraging
   sites narrowed as flock-size increased. In each year there was a
   significant positive relationship between flock-size and the
   pecking-rate of Treecreepers. Pecking-rate was not related to ambient
   temperature and did not differ between tree species. We suggest that the
   main benefit to Treecreepers of joining mixed-species flocks is the
   avoidance of predation.}},
