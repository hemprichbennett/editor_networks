@article{ ISI:A1993MJ19400010,
Author = {SOLER, M and SOLER, JJ},
Title = {{DOES THE RISK OF NEST PREDATION AFFECT CLUTCH SIZE IN THE JACKDAW CORVUS
   MONEDULA}},
Journal = {{BIRD STUDY}},
Year = {{1993}},
Volume = {{40}},
Number = {{3}},
Pages = {{232-235}},
Month = {{NOV}},
Abstract = {{The nest predation hypothesis predicts that clutch size should be
   inversely related to the risk of nest predation. In this paper we
   analyse data on nest predation, clutch size and fledgling production in
   128 Jackdaw nests in order to test if clutches are larger in the safer
   sites. The predation rate was positively correlated with the minimum
   nest-entrance dimensions (predation is greater in nests with large
   entrances). Clutch size was not related to the risk of nest predation,
   and our data do not therefore support the nest predation hypothesis.}},
