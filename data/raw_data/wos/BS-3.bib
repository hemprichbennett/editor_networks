@article{ ISI:A1991FU97000010,
Author = {SUTHERLAND, WJ and ALLPORT, G},
Title = {{THE DISTRIBUTION AND ECOLOGY OF NATURALIZED EGYPTIAN GEESE
   ALOPOCHEN-AEGYPTIACUS IN BRITAIN}},
Journal = {{BIRD STUDY}},
Year = {{1991}},
Volume = {{38}},
Number = {{2}},
Pages = {{128-134}},
Month = {{JUL}},
Abstract = {{Egyptian Geese Alopochen aegyptiacus were introduced to Britain from
   Africa in the seventeen century and were widely distributed by the
   eighteenth and nineteenth century.  Their distribution in Britain is now
   mainly restricted to Norfolk.  In the breeding season 144 pairs were
   located and the population was estimated to be about 400 birds. 
   Breeding birds were restricted to sites with open water, short grass and
   suitable nesting sites (either islands, holes in old trees or amongst
   epicormic shoots on old trees).  There is a moult migration to Holkham
   Hall which has a moulting flock of about 150 birds.  During the winter
   they are widely dispersed within river valleys where they feed on short
   grass and cereals.  The population size seems to have changed little
   over the last one and a half centuries, but there is some evidence of a
   localized increase and spread of range over recent years.  The
   productivity of fledged young is low.}},
