@article{ ISI:A1994NC31900002,
Author = {KING, JR and GRIFFITHS, R},
Title = {{SEXUAL DIMORPHISM OF PLUMAGE AND MORPHOLOGY IN THE COAL TIT PARUS-ATER}},
Journal = {{BIRD STUDY}},
Year = {{1994}},
Volume = {{41}},
Number = {{1}},
Pages = {{7-14}},
Month = {{MAR}},
Abstract = {{Sexual dimorphism of 2 plumage features and some morphological
   measurements was assessed in a sample of 140 museum skins and over 300
   wild-trapped Coal Tits Parus ater britannicus. All the characters
   examined displayed highly significant sexual dimorphism, and have
   allowed the presentation of a recommended methodology which should allow
   over 90\% of all British Coal Tits to be sexed correctly by
   fieldworkers. The validity of year-round sexing using characteristics of
   the throat feathering is proven by comparison with the results of a
   molecular DNA sexing technique.}},
