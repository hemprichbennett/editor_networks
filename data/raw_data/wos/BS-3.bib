@article{ ISI:000072629400004,
Author = {Redpath, S and Madders, M and Donnelly, E and Anderson, B and Thirgood,
   S and Martin, A and McLeod, D},
Title = {{Nest site selection by Hen Harriers in Scotland}},
Journal = {{BIRD STUDY}},
Year = {{1998}},
Volume = {{45}},
Number = {{1}},
Pages = {{51-61}},
Month = {{MAR}},
Abstract = {{The aim of this study was to examine nest site selection by Hen Harriers
   Circus cyaneus at two spatial scales in Scotland and to assess whether
   breeding success was influenced by choice of nest site. At the landscape
   scale, toe compared availability and utilization of habitats in 610
   km(2) of Argyll, and at the local scale, we compared vegetation and
   topography at 52 harrier nests and random points within three averts of
   heather moorland covering a total area of 462 km(2). At both scales,
   harriers showed a clear preference for nesting in heather. Within
   heather moorland, harriers nested in taller heather (average height
   46cm) and nearer streams than expected by chalice. More nests were on
   northwest-facing slopes than expected by chance. We found no evidence
   that breeding success was influenced by habitat or topography. Heather
   moorland is declining in the uplands due to overgrazing and
   afforestation. The association of harriers with heather suggests that
   their future may become increasingly dependent on moorland, where
   heather is maintained for grouse.}},
