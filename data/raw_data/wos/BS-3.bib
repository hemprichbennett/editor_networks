@article{ ISI:A1996UF90900011,
Author = {Kollinsky, C and Landmann, A},
Title = {{Altitudinal distribution of male black redstarts: Are there
   age-dependent patterns?}},
Journal = {{BIRD STUDY}},
Year = {{1996}},
Volume = {{43}},
Number = {{1}},
Pages = {{103-107}},
Month = {{MAR}},
Abstract = {{We censused territory densities and age-specific distribution patterns
   of Black Redstarts Phoenicurus ochruros along an altitudinal transect
   running from 600 to 2000 m asl in the Central Alps of Tyrol, Austria,
   and found no support for the hypothesis that young males are forced to
   settle at higher altitudes. Adult and young males were evenly
   distributed along the altitudinal gradient and we found no support for
   the assumption that higher altitudes are less favourable for this
   species as population densities generally increased with altitude.
   Moreover, altitudinal segregation of territorial males by age would not
   fit with our knowledge of population dynamics and social strategies of
   the species. Reported differences in the altitudinal distribution of
   adult vs. young males(17,18) are thus regarded as artefacts rather than
   as the outcome of direct competitive interactions.}},
