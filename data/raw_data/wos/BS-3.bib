@article{ ISI:000087033300013,
Author = {Newton, I and Dale, L and Little, B},
Title = {{Trends in organochlorine and mercurial compounds in the eggs of British
   Merlins Falco columbarius}},
Journal = {{BIRD STUDY}},
Year = {{1999}},
Volume = {{46}},
Number = {{3}},
Pages = {{356-362}},
Month = {{NOV}},
Abstract = {{Over the period 1967-97, eggs from 630 Merlin clutches, obtained in
   various parts of Britain, were analysed for residues of DDE (from DDT),
   HEOD (from aldrin and dieldrin), PCBs (industrial polychlorinated
   biphenyls) and Hg (mercury). The organochlorine pesticides (DDE and
   HEOD) had previously been held responsible for an earlier decline in the
   numbers of Merlins in Britain, along with some other birds of prey.
   During the study period, these residues declined markedly PCB levels
   showed declines in some regions but not in others, while mercury levels
   (measured only from 1978) declined to the mid-1980s and then increased
   again. The decline in pesticide residues, and associated improvement in
   shell indices, coincided with a marked increase between 1983-84 and
   1993-94 in the numbers of Merlins found breeding in Britain.}},
