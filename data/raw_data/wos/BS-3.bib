@article{ ISI:000081841900004,
Author = {Griffin, LR},
Title = {{Colonization patterns at Rook Corvus frugilegus colonies: implications
   for survey strategies}},
Journal = {{BIRD STUDY}},
Year = {{1999}},
Volume = {{46}},
Number = {{2}},
Pages = {{170-173}},
Month = {{JUL}},
Abstract = {{The increase in Rook Corvus frugilegus L. nest numbers at 18 colonies in
   County Durham, UK was monitored during the spring period of colony
   build-up. Nests were mapped to avoid miscounts and to track the stages
   of nest completion. Colonies were asynchronous in reaching maximum
   numbers, although all followed the same general pattern. Despite large
   variation in initial colony size on 1 April, a constant proportional
   increase up to the modal date of maximum counts on 22 April was
   recorded. The implications for survey strategy are discussed.}},
