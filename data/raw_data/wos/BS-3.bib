@article{ ISI:000079518500008,
Author = {Valkama, J and Korpimaki, E},
Title = {{Nestbox characteristics, habitat quality and reproductive success of
   Eurasian Kestrels}},
Journal = {{BIRD STUDY}},
Year = {{1999}},
Volume = {{46}},
Number = {{1}},
Pages = {{81-88}},
Month = {{MAR}},
Abstract = {{We studied nestbox selection, habitat quality and reproductive success
   of Eurasian Kestrels Falco tinnunculus during 1985-95 in western
   Finland. Nestboxes were termed small, intermediate or large depending on
   the basal area, and exposed or sheltered depending on their orientation
   to prevailing weather conditions. Egg-laying started earlier in the
   intermediate nestboxes than in small boxes, indicating that intermediate
   nestboxes were selected first by arriving Kestrels. There were no
   differences in clutch size or number of fledglings produced with respect
   to nestbox size or orientation. The occupancy rate did not differ
   markedly between different-sized nestboxes, but it was lower in exposed
   nestboxes than in sheltered ones. We found that occupied nestboxes were
   further from forest edges, roads and inhabited houses, and had more
   grassy ditches (previously identified as good habitat for Microtus
   voles, the main prey of Kestrels) in their vicinity than unoccupied
   boxes. There were no obvious differences in body size and body condition
   of parent Kestrels with respect to nestbox size and habitat qualify.
   These results suggest that both nestbox size and orientation were
   important nest-site selection criteria, but breeding success was largely
   determined by laying date, habitat characteristics around the nest, and
   cyclic food abundance.}},
