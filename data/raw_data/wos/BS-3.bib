@article{ ISI:A1996VU70600009,
Author = {Bell, CP},
Title = {{The relationship between geographic variation in clutch size and
   migration pattern in the yellow wagtail}},
Journal = {{BIRD STUDY}},
Year = {{1996}},
Volume = {{43}},
Number = {{3}},
Pages = {{333-341}},
Month = {{NOV}},
Abstract = {{Clutch size in the Yellow Wagtail increases by about one egg per 19
   degrees of latitude throughout Europe, but at a given latitude larger
   clutches are laid in western than in eastern Europe. In eastern Asia
   there is evidence of a reverse latitudinal dine, in which clutch size is
   smaller at higher latitudes, and considerably smaller than in Europe at
   equivalent latitudes. These trends suggest that smaller clutches are
   laid by populations with access to extensive wintering areas in east
   Africa and south-east Asia, whereas larger clutches are laid by
   populations with more restricted wintering areas in west Africa and
   India. Higher winter mortality in the latter populations may reduce
   breeding density, leading to less competition for resources and allowing
   larger clutches to be produced.}},
