@article{ ISI:A1997XJ74100007,
Author = {Wright, PM},
Title = {{Distribution, site occupancy and breeding success of the Merlin Falco
   columbarius on Barden moor and fell, north Yorkshire}},
Journal = {{BIRD STUDY}},
Year = {{1997}},
Volume = {{44}},
Number = {{2}},
Pages = {{182-193}},
Month = {{JUL}},
Abstract = {{This study examines the distribution, site occupancy and breeding
   success of Merlins on Barden Moor and Barden Fell, thus repeating a
   study undertaken by Rowan over a number of seasons around 1918. Since
   the start of this study in 1983, the number of breeding pairs found on
   the Moor has increased from four to five. This compares with a maximum
   of four pairs recorded in Rowan's day. The number of pairs on the Fell
   has fluctuated between two and three. The mean density of nesting pairs
   was equivalent to 12 pairs per 100 km2. A study of 51 breeding
   occurrences on the Moor and 31 on the Fell over 12 years yielded a mean
   clutch size of 4.24, a mean laying date of 10 May, 85\% of nests with at
   least one fledged chick and mean productivity figures of 2.9 young per
   pair and 3.4 young per successful pair.}},
