@article{ ISI:000181691000008,
Author = {Madders, M},
Title = {{Hen Harrier Circus cyaneus foraging activity in relation to habitat and
   prey}},
Journal = {{BIRD STUDY}},
Year = {{2003}},
Volume = {{50}},
Number = {{1}},
Pages = {{55-60}},
Month = {{MAR}},
Abstract = {{Capsule In west Scotland, the birds foraged in accordance with the
   distribution of prey and vegetation associated with early-growth conifer
   forests.
   Aims To investigate foraging Hen Harrier activity in relation to
   vegetation cover and structure and the abundance of potential prey.
   Methods Forty 1 x 1-km sample observation areas were monitored at four
   sites in west Scotland.
   Results Foraging time in the sample squares was dependent on the
   proximity of harrier nests. Harriers preferred areas with relatively
   high densities of Field Voles Microtus agrestis and Meadow Pipits Anthus
   pratensis, associated with tall ground vegetation and shrub layers. They
   also preferred areas planted with trees but preference declined with
   increasing tree foliage 2-5 m tall and bare ground.
   Conclusion Conifer forests can provide favourable conditions for
   foraging harriers during the early growth stages. Further work is
   required to determine the value of other types of woodland.}},
