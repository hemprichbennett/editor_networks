@article{ ISI:000169694200001,
Author = {Nilsson, L and Kahlert, J and Persson, H},
Title = {{Moult and moult migration of Greylag Geese Anser anser from a population
   in Scania, south Sweden}},
Journal = {{BIRD STUDY}},
Year = {{2001}},
Volume = {{48}},
Number = {{2}},
Pages = {{129-138}},
Month = {{JUL}},
Abstract = {{The moult migration of Greylag Geese Anser anser from Scania,
   southernmost Sweden, was studied based on resightings of neck-banded
   individuals marked in the breeding areas. Most geese seen in moulting
   areas were recorded in the marking area both before and after moult. Lip
   to 1992, 72\% of all neck-banded Greylag Geese recorded front a moulting
   area were reported from Oostvaardeusplassen in the Netherlands, 600 km
   from the marking area. After 1992, there was a change in the moult
   migration and the majority of the reports of moulting Greylag Geese from
   Scania were from the Danish island of Saltholm a distance of 50 km away.
   Owing to the increase in the Greylag Goose population, the moulting sire
   at Oostvardersplassen probably reached its carrying capacity and the
   geese had to move their moult site. The moult migration of the study
   population is discussed in relation to the food availability of the two
   main moulting sites.}},
