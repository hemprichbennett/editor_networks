@article{ ISI:000085882200009,
Author = {Balachandran, S and Hussain, SA and Underhill, LG},
Title = {{Primary moult, biometrics, mass and age composition of Grey Plovers
   Pluvialis squatarola in southeastern India}},
Journal = {{BIRD STUDY}},
Year = {{2000}},
Volume = {{47}},
Number = {{1}},
Pages = {{82-90}},
Month = {{MAR}},
Abstract = {{Little is known about the biology of waders wintering in southern Asia;
   this paper deals with the Grey Plover Pluvialis squatarola, a species
   extensively studied only in western Europe. Adult Grey Plovers wintering
   in southeastern India underwent primary moult in autumn; the duration
   was estimated to be 127 days, with mean starting date 1 September and
   mean completion date 5 January. Some first-year Grey Plovers initiated
   primary moult in late winter and spring, and completed this moult the
   following spring. The average mass of adults on arrival in September was
   200 g, fluctuated close to 220 g from October to February, and increased
   to 280 g near the end of May. The mass variation did not show the
   January peak observed in western Europe. Breeding productivity, measured
   as the percentage of first-year birds in winter catches, varied between
   5\% and 70\% over six years, and showed a positive correlation with that
   of Dark-bellied Brent Geese Branta b. bernicla in western Europe and
   Curlew Sandpipers Calidris ferruginea in South Africa.}},
