@article{ ISI:000181691000003,
Author = {Stoate, C and Szczur, J and Aebischer, NJ},
Title = {{Winter use of wild bird cover crops by passerines on farmland in
   northeast England}},
Journal = {{BIRD STUDY}},
Year = {{2003}},
Volume = {{50}},
Number = {{1}},
Pages = {{15-21}},
Month = {{MAR}},
Abstract = {{Capsule Declining farmland species were more abundant in these crops
   which can be matched to the birds' requirements.
   Aims To assess the use of seed-bearing crop by a range of bird species
   in winter.
   Methods The study was carried out over three winters at a site in County
   Durham (England). Eleven bird species were represented, five of which
   are nationally targeted for conservation action.
   Results Bird abundance was significantly higher where wild bird cover
   crops were present. Kale Brassica napus and Quinoa Chenopodium quinoa
   were the most used crop species, although cereals and linseed were also
   used.
   Conclusion Simple crop mixtures can be designed to meet the needs of
   farmland bird communities. Our results suggest that seed production may
   be limited within government agri-environment schemes by restrictions on
   fertilizer use. The agronomy of seed-bearing crops for birds requires
   further investigation.}},
