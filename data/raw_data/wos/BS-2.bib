@article{ ISI:000085882200002,
Author = {Fojt, E and Triplet, P and Robert, JC and Stillman, RA},
Title = {{Comparison of the breeding habitats of Little Ringed Plover Charadrius
   dubius and Kentish Plover Charadrius alexandrinus on a shingle bed}},
Journal = {{BIRD STUDY}},
Year = {{2000}},
Volume = {{47}},
Number = {{1}},
Pages = {{8-12}},
Month = {{MAR}},
Abstract = {{The breeding habitat preferences of Kentish Plover and Little Ringed
   Plover were studied on a shingle bed. The location of each nest was
   measured as the distance to the sea wall, nearest pond and nearest path.
   The habitat surrounding nests was measured at radii of 0.15 m and 15 m
   as the proportional cover of shingle, gravel, sand, live and dead
   vegetation. When all variables were considered simultaneously, the
   differences between the habitats used by the species were related to the
   cover of shingle within 15 m of the nest and the distance to the sea
   wall. Nest sites located close to the sea wall and with high shingle
   cover almost always belonged to Kentish Plover, while those more distant
   and with low shingle cover almost always belonged to Little Ringed
   Plover. Plovers used areas which had been stable for longer and hence
   were colonized by vegetation. This study suggests that management which
   maintains a high proportion of exposed substrate on shingle beds will
   increase their suitability for Kentish Plover.}},
