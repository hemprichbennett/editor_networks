@article{ ISI:000087033300009,
Author = {Newton, I},
Title = {{Age ratios in a Bullfinch Pyrrhula pyrrhula population over six years}},
Journal = {{BIRD STUDY}},
Year = {{1999}},
Volume = {{46}},
Number = {{3}},
Pages = {{330-335}},
Month = {{NOV}},
Abstract = {{Each year over five years (1962-67), the ratio of first-year to adult
   Bullfinches netted in Wytham Woods, near Oxford, increased from July or
   August to October, then declined to a fairly constant level from January
   onwards. The first-year to adult ratio in October used as a measure of
   annual productivity, varied from 2.8 to 5.5 in different years. A high
   first-year to adult ratio in October was followed by a relatively high
   ratio throughout the next year and beyond (excluding the new juveniles
   that appeared among netted birds from July).}},
