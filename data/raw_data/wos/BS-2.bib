@article{ ISI:000175070000004,
Author = {Henderson, IG and Wilson, AM and Steele, D and Vickery, JA},
Title = {{Population estimates, trends and habitat associations of breeding
   Lapwing Vanellus vanellus, Curlew Numenius arquata and Snipe Gallinago
   gallinago in Northern Ireland in 1999}},
Journal = {{BIRD STUDY}},
Year = {{2002}},
Volume = {{49}},
Number = {{1}},
Pages = {{17-25}},
Month = {{MAR}},
Abstract = {{Capsule Declines in the breeding populations of Snipe, Lapwing and
   Curlew were recorded between April and June 1999 and compared with
   previous estimates in 1987.
   Aims To compare populations of non-coastal breeding waders between 1987
   and 1999.
   Methods In 106 2 km x 2 km square tetrads, observers recorded the number
   of breeding pairs of waders and habitat details on 1 :10 000 scale maps.
   Results A significant decline of c. 60\% for Lapwinq and Curlew, and a
   non-significant decline of 30\% for Snipe was recorded over 12 years.
   Concentrations of these species were found in County Tyrone, but
   Counties Antrim, Down and Armagh supported few breeding pairs of any
   species. Very few pairs of any species were recorded on improved
   grassland despite its widespread availability.
   Conclusion A successful conservation strategy for these species must
   address the wider countryside and not just key sites. Intensive pastoral
   forming in upland and lowland areas and activities such as drainage and
   peat extraction will further reduce the suitability of open habitats for
   these wader species.}},
