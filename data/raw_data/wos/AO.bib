@article{ ISI:000226177400006,
Author = {Ponton, F and Elzanowski, A and Castanet, J and Chinsamy, A and de
   Margerie, E and de Ricqles, A and Cubo, J},
Title = {{Variation of the outer circumferential layer in the limb bones of birds}},
Journal = {{ACTA ORNITHOLOGICA}},
Year = {{2004}},
Volume = {{39}},
Number = {{2}},
Pages = {{137-140}},
Month = {{WIN}},
Abstract = {{The core of the limb bone cortex of mammals and birds is made of rapidly
   deposited, fibro-lamellar bone tissue (also present in non-avian
   theropods), which is usually surrounded by an avascular outer
   circumferential layer (OCL) of slowly deposited parallel-fibered bone.
   We present the first comparative allometric study of the relative OCL
   thickness (expressed as a fraction of the diaphyseal radius) in modem
   birds. Body size explains 79\% of the OCL variation in thickness, which
   is inversely correlated with size, that is, shows negative allometry
   (slope-0.799). This may explain the apparent absence of OCL in the
   ratites. Since the OCL is deposited at the end of growth, we propose
   that its relative thickness probably correlates with the amount of slow,
   residual growth, which our results suggest to be on the average larger
   in small birds.}},
