@article{ ISI:000262817800014,
Author = {Mayr, Gerald},
Title = {{The Madagascan ``Cuckoo-roller{''} (Aves: Leptosomidae) is not a roller
   - notes on the phylogenetic affinities and evolutionary history of a
   ``living fossil{''}}},
Journal = {{ACTA ORNITHOLOGICA}},
Year = {{2008}},
Volume = {{43}},
Number = {{2}},
Pages = {{226-230}},
Month = {{WIN}},
Abstract = {{The phylogenetic affinities and evolutionary history of the Madagascan
   Leptosomidae (Courol or ``Cuckoo-roller{''}) are reviewed to rectify
   erroneous accounts in the recent literature. These birds are not closely
   related to rollers, and multiple molecular and morphological data sets
   congruently support their position outside the clade including
   Coraciiformes sensu stricto (rollers and ground rollers), Piciformes
   (woodpeckers and allies), and Alcediniformes (kingfishers and allies).
   The recent discovery that Plesiocathartes, from the Eocene of Europe and
   North America, is a stern lineage representative of the Leptosomidae
   further shows that Pan-Leptosomidae were widely distributed across the
   Northern Hemisphere in the early Paleogene. The Courol is among the few
   avian taxa which qualify as ``living fossils{''}, and its persistence on
   Madagascar may have been facilitated by the absence of ecological
   factors that led to extinction of Pan-Leptosomidae elsewhere.}},
