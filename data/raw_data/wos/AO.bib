@article{ ISI:000258628500003,
Author = {Artemyev, Alexander V.},
Title = {{Factors responsible for the long-term dynamics of the Pied Flycatcher
   Ficedula hypoleuca populations in the taiga of Karelia, Russia}},
Journal = {{ACTA ORNITHOLOGICA}},
Year = {{2008}},
Volume = {{43}},
Number = {{1}},
Pages = {{10-16}},
Month = {{SUM}},
Note = {{Meeting on Hole Using - Adaptational and Constraints, Bialowieza,
   POLAND, SEP, 2007}},
Organization = {{Univ Podlasie; Wroclaw Univ; Warsaw Univ Life Sci \& Museum; Polish Acad
   Sci, Inst Zool}},
Abstract = {{A Pied Flycatcher population breeding in nest-boxes in a Karelia forest
   was studied from 1981 to 2007. Breeding density varied between 47.3 and
   94.6 pairs/km(2). The density of the breeding population was influenced
   by three demographic factors: maximum summer population density in the
   previous season, male return rate and immigration rate. Three relatively
   independent processes play a significant part in the population dynamics
   of the study area: 1) initial abundance and reproductive intensity, 2)
   Survival rate between breeding seasons, and 3) redistribution of birds
   across their range. The weather in spring, when the birds arrive and
   settle, was found to be equally important. The population density, nest
   site fidelity of adults and yearling,.;, and immigration rate were
   related to temperature patterns in May. It may be assumed that the
   spring weather influences the birds' survival and their distribution
   across the northern part of their range, as well as the participation of
   one-year old birds in breeding.}},
