@article{ ISI:000230449200003,
Author = {Polak, M},
Title = {{Temporal pattern of vocal activity of the Water Rail Rallus aquaticus
   and the Little Crake Porzana parva in the breeding season}},
Journal = {{ACTA ORNITHOLOGICA}},
Year = {{2005}},
Volume = {{40}},
Number = {{1}},
Pages = {{21-26}},
Month = {{SUM}},
Abstract = {{In the Water Rail the patterns of the two main types of vocalization
   were quite different: the announcement call was uttered throughout the
   breeding season with two peaks, one in April and the other in early
   June, but the courtship call occurred only in the pre-laying period. The
   vocalization period of the Little Crake is short but intensive, peaking
   before the start of incubation. In general, Water Rails produced more
   vocalizations in the evening than in the morning. The vocal activity of
   the Little Crake peaked at dawn. In both species a peak in vocal output
   occurred shortly before sunrise and before sunset.}},
