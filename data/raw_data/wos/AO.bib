@article{ ISI:000252759000011,
Author = {Rodriguez, Beneharo and Rodriguez, Airam},
Title = {{Breeding biology of Grey Wagtail Motacilla cinerea canariensis on
   Tenerife, Canary Islands}},
Journal = {{ACTA ORNITHOLOGICA}},
Year = {{2007}},
Volume = {{42}},
Number = {{2}},
Pages = {{195-199}},
Month = {{WIN}},
Abstract = {{The paper reports on the breeding biology of the endemic subspecies of
   the Grey Wagtail in a human transformed area (banana plantations) on
   Tenerife (Canary Islands). Clutch size (3.96 +/- 0.6) was lower than in
   continental zones, probably because of the climatic stability of the
   islands. Furthermore, the distance from the nest to the nearest water
   source and the length of the breeding season (late February-June) were
   greater than in other European populations. Some breeding traits fit the
   predictions of the so-called insular ``syndrome{''}. Possible mechanisms
   underpinning these differences in breeding traits are discussed.}},
Publisher = {{MUSEUM \& INST ZOOLOGY}},
Address = {{WILCZA 64, PL-00-679 WARSAW, POLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Rodriguez, B (Reprint Author), C-La Malecita S-N,38480 Buenavista Norte,S-C, Tenerife, Canary Isl, Spain.
