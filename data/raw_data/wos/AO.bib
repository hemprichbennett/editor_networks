@article{ ISI:000262817800012,
Author = {Brambilla, Mattia and Quaglierini, Alessio and Reginato, Fabrizio and
   Vitulano, Severino and Guidali, Franca},
Title = {{Syntopic taxa in the Sylvia cantillans species complex}},
Journal = {{ACTA ORNITHOLOGICA}},
Year = {{2008}},
Volume = {{43}},
Number = {{2}},
Pages = {{217-220}},
Month = {{WIN}},
Abstract = {{Distributional relationships among closely related taxa can provide key
   information about the levels of their reproductive isolation or
   compatibility, and thus the stage of speciation process. Here, we
   present new information or) the sites where two taxa traditionally
   considered as subspecies (if the Subalpine Warbler Sylvia cantillans, S.
   c. cantillans and S. (c.) moltonii, breed sympatrically in mainland
   Italy, We analyse geographic distribution and behaviour as well as other
   characteristics shown by these warblers in the areas of contact. The
   distributional pattern shown by these taxa, which is partly sympatric
   with syntopic breeding, and the apparent lack of interbreeding, strongly
   Suggest that they behave as different species, confirming previous
   findings about genetic divergence and differential song perception.}},
