@article{ ISI:000394196800008,
Author = {Rodrigues, Pedro and Lopes, Ricardo Jorge and Resendes, Roberto and
   Ramos, Jaime Albino and Cunha, Regina Tristao},
Title = {{Genetic diversity of the Azores Blackbirds Turdus merula reveals
   multiple founder events}},
Journal = {{ACTA ORNITHOLOGICA}},
Year = {{2016}},
Volume = {{51}},
Number = {{2}},
Pages = {{221-234}},
Month = {{WIN}},
Abstract = {{We assessed the genetic diversity and phylogeography of the Azores
   Blackbird Turdus merula, based on sequences of two mitochondrial genes
   (Cytochrome b and NADH Dehydrogenase subunit 2) and one nuclear gene
   (Aconitase 1 intron 9) from 45 individuals and an outgroup of 15 birds
   from Madeira, continental west Europe and north Africa. Our results
   revealed the lack of genetic structure on these islands and the presence
   of, at least, two different lineage groups that may indicate two
   different founder events of the Azores by Blackbirds.}},
