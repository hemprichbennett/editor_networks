@article{ ISI:000295355300005,
Author = {Filippi-Codaccioni, Ondine and Moussus, Jean-Pierre and Urcun, Jean-Paul
   and Jiguet, Frederic},
Title = {{Advanced autumn migration of the Common Crane Grus grus over Western
   Pyrenean passes}},
Journal = {{ACTA ORNITHOLOGICA}},
