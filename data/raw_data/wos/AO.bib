@article{ ISI:000322148900009,
Author = {Petrzelkova, Adela and Klvana, Petr and Albrecht, Tomas and Horak, David},
Title = {{Conspecific brood parasitism and host clutch size in Common Pochards
   Aythya ferina}},
Journal = {{ACTA ORNITHOLOGICA}},
