@article{ ISI:000292478300018,
Author = {Karagicheva, Julia and Rakhimberdiev, Eldar and Dobrynin, Dmitry and
   Saveliev, Anatoly and Rozenfeld, Sofia and Pokrovskaya, Olga and Stahl,
   Julia and Prop, Jouke and Litvin, Konstantin},
Title = {{Individual inter-annual nest-site relocation behaviour drives dynamics
   of a recently established Barnacle Goose Branta leucopsis colony in
   sub-arctic Russia}},
Journal = {{IBIS}},
Year = {{2011}},
Volume = {{153}},
Number = {{3}},
Pages = {{622-626}},
Month = {{JUL}},
Abstract = {{Avian breeding colonies are generally in a continuous state of flux,
   some parts growing whilst others shrink as individuals move within the
   colony on the search for better nest-sites. We examined the role of
   experience in breeding patch choice by individually marked Barnacle
   Geese Branta leucopsis in a recently established colony in sub-arctic
   Russia. Individuals failing to reproduce successfully tended to shift
   nest location further the following season than did successful pairs,
   and they did so towards the most dense nest aggregations within the
   colony, where reproductive success was higher. We suggest that
   individual decisions on nest-site choice shape the spatial dynamics of
   this colony.}},
