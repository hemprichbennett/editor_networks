@article{ ISI:000288507100020,
Author = {Donaghy, Anita M. and Green, Rhys E. and O'Halloran, John},
Title = {{Timing of autumn migration of young Corncrakes Crex crex}},
Journal = {{IBIS}},
Year = {{2011}},
Volume = {{153}},
Number = {{2}},
Pages = {{425-428}},
Month = {{APR}},
Abstract = {{Knowledge of the timing of migration of young Corncrakes is required for
   effective implementation of conservation measures. Forty Corncrake
   chicks were radiotracked during the period between independence and
   departure from the natal area to determine the age and time at which
   fledged juveniles depart on migration. Departure occurred between early
   August and mid-September at a mean age of 44 days, around the age at
   which growth of the primary feathers ceases. Chicks from later broods
   are exposed to greater risk of being killed during mowing, but provision
   of late mowing options in conservation schemes could enhance their
   survival.}},
