@article{ ISI:000285008400021,
Author = {Hatch, Jeremy J. and Nisbet, Ian C. T.},
Title = {{Costs of incubation in Common Terns Sterna hirundo: replication and
   extension of a clutch-enlargement experiment}},
Journal = {{IBIS}},
Year = {{2011}},
Volume = {{153}},
Number = {{1}},
Pages = {{185-189}},
Month = {{JAN}},
Abstract = {{Manipulation of clutch size during incubation has been used in a number
   of studies to investigate life-history costs of incubation. We increased
   or decreased clutches of Common Terns Sterna hirundo during incubation
   and measured subsequent chick growth and productivity. Our results
   provide little support for the hypothesis that costs of incubation in
   Common Terns are substantially affected by the number of eggs incubated.}},
