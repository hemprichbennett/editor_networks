@article{ ISI:000288507100004,
Author = {Zou, Fasheng and Chen, Guizhu and Yang, Qiongfang and Fellowes, John R.},
Title = {{Composition of mixed-species flocks and shifts in foraging location of
   flocking species on Hainan Island, China}},
