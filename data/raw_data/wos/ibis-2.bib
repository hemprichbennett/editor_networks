@article{ ISI:000310738900020,
Author = {Yohannes, Elizabeth and Lee, Raymond W. and Valcu, Mihai and Kempenaers,
   Bart},
Title = {{Feather deuterium as an indicator of age-class in the Pectoral Sandpiper
   Calidris melanotos}},
Journal = {{IBIS}},
Year = {{2012}},
Volume = {{154}},
Number = {{4}},
Pages = {{868-873}},
Month = {{OCT}},
Abstract = {{We measured deuterium isotope ratios (delta D-f) in primary feathers to
   distinguish first-year from older Pectoral Sandpipers Calidris melanotos
   captured in Barrow, Alaska, during the breeding season. delta D-f showed
   a distinct bimodal distribution, and model-based clustering placed the
   delta D-f values into two non-overlapping groups. More negative delta
   D-f corresponded to Arctic areas, probably identifying first-year birds
   with Arctic-grown juvenile feathers retained from the previous year. The
   more positive values corresponded to lower latitudes, possibly
   identifying older birds that grew their feathers at non-Arctic
   latitudes.}},
