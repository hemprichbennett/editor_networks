@article{ ISI:000292478300010,
Author = {Hagen, Erin N. and Hale, Marie L. and Maloney, Richard F. and Steeves,
   Tammy E.},
Title = {{Conservation genetic management of a critically endangered New Zealand
   endemic bird: minimizing inbreeding in the Black Stilt Himantopus
   novaezelandiae}},
Journal = {{IBIS}},
Year = {{2011}},
Volume = {{153}},
Number = {{3}},
Pages = {{556-561}},
Month = {{JUL}},
Abstract = {{For threatened species with small captive populations, it is advisable
   to incorporate conservation management strategies that minimize
   inbreeding in an effort to avoid inbreeding depression. Using multilocus
   microsatellite genotype data, we found a significant negative
   relationship between genetic relatedness (inbreeding) and reproductive
   success (fitness) in a captive population of the critically endangered
   Black Stilt or Kaki Himantopus novaezelandiae. In an effort to avoid
   inbreeding depression in this iconic New Zealand endemic, we recommend
   re-pairing closely related captive birds with less related individuals
   and pairing new captive birds with distantly related individuals.}},
