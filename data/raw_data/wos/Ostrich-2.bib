@article{ ISI:000088246100032,
Author = {Davenport, LB and Urban, EK},
Title = {{Time budgets for captive Wattled Cranes Bugeranus carunculatus}},
Journal = {{OSTRICH}},
Year = {{2000}},
Volume = {{71}},
