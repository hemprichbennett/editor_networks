@article{ ISI:000171793700015,
Author = {Whittington-Jones, CA and Hulley, PE and Craig, AJFK},
Title = {{Red-Billed Queleas Quelea quelea in the Eastern Cape, South Africa: are
   they settling down?}},
Journal = {{OSTRICH}},
Year = {{2001}},
Number = {{15}},
Pages = {{89-91}},
Month = {{JUL}},
Note = {{10th Pan-African Ornithological Congress, MAKERERE UNIV, KAMPALA,
   UGANDA, SEP 01-08, 2000}},
Organization = {{BirdLife Int; Barclays; RSPB; Wetlands Int; MacArthur Fdn; ABC;
   NatureUganda; Agence Intergovn Francophonie; NABU; WWF}},
Abstract = {{Over the past 20 years, Red-billed Quelea have been recorded more widely
   and more frequently in the Eastern Cape province. Since 1996 we have
   censused seed-eaters and ringed more than 4600 Red-billed Quelea at
   three study sites. Of these, 81 have been recaptured and 8 recovered
   dead,all within 26 km of the ringing site, except for one bird 415 km to
   the north. In South Africa over the same period, over 43 000 Red-billed
   Quelea have been ringed, of which 73 have been recovered, 56 > 100 km
   from the ringing site, including records from Zimbabwe and the
   Democratic Republic of Congo. Our data suggest local rather than
   long-distance movements in the Eastern Cape population, and we postulate
   that changes in agricultural practices could enable Red-billed Queleas
   to adopt a less nomadic life-style in this region.}},
