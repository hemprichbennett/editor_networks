@article{ ISI:000171793700026,
Author = {Burke, A and Hudson, V and Wilkins, H and McCann, K and Krige, F and
   Foden, W and Rodwell, L},
Title = {{South African Wattled Crane supplementation programme June 1999-June
   2000, with notes on the non-breeding flock in KwaZulu-Natal}},
Journal = {{OSTRICH}},
Year = {{2001}},
Number = {{15}},
Pages = {{143-146}},
Month = {{JUL}},
Note = {{10th Pan-African Ornithological Congress, MAKERERE UNIV, KAMPALA,
   UGANDA, SEP 01-08, 2000}},
Organization = {{BirdLife Int; Barclays; RSPB; Wetlands Int; MacArthur Fdn; ABC;
   NatureUganda; Agence Intergovn Francophonie; NABU; WWF}},
Abstract = {{Three costume-reared juvenile Wattled Cranes were released to a
   non-breeding flock of Wattled Cranes in KwaZulu-Natal, South Africa in
   February and April 2000. The birds were released singly and as a cohort
   of 2 birds. All 3 remain alive at the time of this writing. The adult
   flock initially exhibited extreme aggression towards the single bird,
   while the 2 birds experienced only low intensity threats. The single
   bird and the female of the cohort fully integrated with the wild flock 5
   and 11 days post release. The male of the cohort did not join with any
   cranes. Integrated birds dispersed 30 km (n = 2) and the lone bird
   dispersed 5 km (n = 1) from the release site. All birds exhibited
   improper roosting behaviour. In future releases, young birds will be
   trained to roost in open water prior to release. Results suggest that
   bird age, physical and behavioural development at release, and specific
   time of release are factors wic influence integration of young,
   inexperienced birds into a wild flock.}},
