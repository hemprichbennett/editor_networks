@article{ ISI:000171793700022,
Author = {Muheebwa-Muhoozi, J},
Title = {{The status of the Grey Crowned Crane, Balearica regulorum in Uganda,
   with special reference to breeding}},
Journal = {{OSTRICH}},
Year = {{2001}},
Number = {{15}},
Pages = {{122-125}},
Month = {{JUL}},
Note = {{10th Pan-African Ornithological Congress, MAKERERE UNIV, KAMPALA,
   UGANDA, SEP 01-08, 2000}},
Organization = {{BirdLife Int; Barclays; RSPB; Wetlands Int; MacArthur Fdn; ABC;
   NatureUganda; Agence Intergovn Francophonie; NABU; WWF}},
Abstract = {{Grey Crowned Cranes (henceforth ``cranes{''}) depend upon seasonal
   swamps for breeding, and many of these have been converted to
   agriculture in recent decades, a process which is continuing. Evidence
   from four different parts of Uganda where cranes are still common
   suggests that breeding success is not good but that intentional
   harassment is not a major factor. This is partly because of traditional
   fear of the consequences of harming cranes directly; however, cranes
   inadvertently disturbed whilst breeding tend to abandon the attempt.
   Working with local communities, through Wildlife Clubs in Primary
   Schools, is a promising way of promoting crane conservation. It may be
   possible for people to avoid places where cranes are nesting for the few
   weeks they require each year.}},
