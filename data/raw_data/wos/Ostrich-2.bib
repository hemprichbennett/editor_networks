@article{ ISI:000088246100073,
Author = {Janoo, A},
Title = {{Rooting the Dodo Raphus cucullatus Linnaeus 1758 and the Solitaire
   Pezophaps solitaria Gmelin 1789 within the Ornithurae: a cladistic
   reappraisal}},
Journal = {{OSTRICH}},
Year = {{2000}},
