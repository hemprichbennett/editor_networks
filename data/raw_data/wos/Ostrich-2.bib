@article{ ISI:000171793700035,
Author = {Bober, SO and Herremans, M and Louette, M and Peterhans, JCK and Bates,
   JM},
Title = {{Geographical and altitudinal distribution of birds endemic to the
   Albertine Rift}},
Journal = {{OSTRICH}},
Year = {{2001}},
Number = {{15}},
