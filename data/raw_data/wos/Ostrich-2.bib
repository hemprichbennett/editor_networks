@article{ ISI:000168962900013,
Author = {Nuttall, DB and Parker, V},
Title = {{An overview of the history and distribution of the Olive-headed Weaver,
   Ploceus olivaceiceps, with preliminary observations of a sub-population
   near Panda, Mozambique}},
Journal = {{OSTRICH}},
Year = {{2001}},
Volume = {{72}},
Number = {{1-2}},
Pages = {{101-104}},
Month = {{MAR}},
Abstract = {{A comprehensive overview of historical information regarding the
   Olive-headed Weaver, Ploceus olivaceiceps, reveals a highly fragmented
   distribution ranging from northern Tanzania to central and southern
   Malawi, easternmost Zambia and southern, central and northern
   Mozambique. More recent information based on a small number of published
   and unpublished records both confirms and expands upon the historical
   distribution though in general a paucity of information exists. An
   overview of distribution for the species is provided as well as
   preliminary observations on the biology and threat status of a
   sub-population near Panda in southern Mozambique. Finally, possible
   threats to the species are identified and it is recommended that the
   species be considered `near-threatened' with respect to IUCN criteria.}},
