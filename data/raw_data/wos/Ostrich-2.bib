@article{ ISI:000171793700007,
Author = {Sande, E and Dranzoa, C and Wegge, R},
Title = {{Population density of the Nahan's Francolin Francolinus nahani in
   Budongo Forest Reserve, Uganda}},
Journal = {{OSTRICH}},
Year = {{2001}},
Number = {{15}},
Pages = {{33-37}},
Month = {{JUL}},
Note = {{10th Pan-African Ornithological Congress, MAKERERE UNIV, KAMPALA,
   UGANDA, SEP 01-08, 2000}},
Organization = {{BirdLife Int; Barclays; RSPB; Wetlands Int; MacArthur Fdn; ABC;
   NatureUganda; Agence Intergovn Francophonie; NABU; WWF}},
Abstract = {{Nahan's Francolin, Francolinus nahani, is a globally threatened species
   classified as Endangered and is a regional endemic only recorded in a
   few tropical forests in Uganda and the Democratic Republic of Congo. We
   carried out population surveys of this species in Budongo Forest
   Reserve, Uganda from January 1998 to December 1999 using a playback
   technique. By means of Distance sampling analyses and with a sampling
   effort of 3470 points, the overall Nahan's Francolin density was 54.02
   +/-5.59 individuals per km(2) (66.27 +/- 12.77 individuals per km(2) in
   the intact forest N15, 47.13 +/-9.13 in N3, the forest selectively
   logged in 1947-52 and 76.31 +/- 11.79 in W21,which was selectively
   logged twice in 1963-64 and 1996-97). The densities between the three
   compartments N15, N3 and W21 did not differ significantly and the
   population of the forest as a whole was estimated as 23000 +/- 2000. The
   group size, which ranged from 1 to 6 individuals, was significantly
   higher in the intact forest, and that selectively logged fifty years
   ago, than in the one selectively logged twice and recently.}},
