@article{ ISI:000237527400007,
Author = {Ndithia, H and Perrin, MR},
Title = {{Diet and foraging behaviour of the Rosy-faced Lovebird Agapornis
   roseicollis in Namibia}},
Journal = {{OSTRICH}},
Year = {{2006}},
Volume = {{77}},
Number = {{1-2}},
Pages = {{45-51}},
Month = {{APR}},
Abstract = {{The natural diet of the Rosy-faced Lovebird in the wild was investigated
   from January-August 2004, at three Namibian locations: Claratal,
   Hohewarte and Haris. Foraging patterns, habitat use, daily feeding
   activities and preferred dietary species were investigated. The
   lovebirds fed on food items from at least 19 species, which included
   seeds, green and dry leaves, dry leaf stems, plant fruits and cattle and
   horse dung; they also ingested soil. Seeds of Anthephora schinzii were
   the most preferred food item. Ground-level flock foraging was dominant,
   with a mean size of 7.3 +/- 8.9. The feeding lovebird flocks associated
   with flocks of doves, pigeons and weavers. The lovebirds were
   crepuscular and most (53\%) foraging observations were recorded in open
   grassland with scattered shrubs of up to 10m.}},
