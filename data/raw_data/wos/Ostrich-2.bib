@article{ ISI:000224348000009,
Author = {Simmons, RE and Kemper, J},
Title = {{Cave breeding by African Penguins near the northern extreme of their
   range: Sylvia Hill, Namibia}},
Journal = {{OSTRICH}},
Year = {{2003}},
Volume = {{74}},
Number = {{3-4}},
Pages = {{217-221}},
Month = {{SEP}},
Abstract = {{Only three mainland sites are known among the total 27 breeding colonies
   of the African Penguin, Spheniscus demersus: two in South Africa and one
   in Namibia. The latter is a unique cave site near the northern extreme
   of the species' range on the edge of the Namib Desert. To determine the
   colony size, long-term viability and any difference in breeding ecology
   relative to more southern sites we combined data from previous visits to
   the site with four visits in 2001. We found that about 240 to 300 birds
   use the Sylvia Hill cave, and about 90 nests are active with a laying
   peak in January. Eggs are laid on top of guano mounds not in burrows as
   is usual for this species. Smaller clutches (1.68 eggs/nest) but larger
   broods ((x) over bar = 1.31 chicks/nest) were apparent in this colony
   than those farther south. Larger mean clutches farther south suggest a
