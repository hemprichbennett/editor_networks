@article{ ISI:000165891100007,
Author = {Kopij, G and Kok, OB and Nuttall, RJ},
Title = {{Breeding cycle of the Southern Bald Ibis, Geronticus calvus}},
Journal = {{OSTRICH}},
Year = {{2000}},
Volume = {{71}},
Number = {{3-4}},
Pages = {{393-399}},
Month = {{SEP}},
Abstract = {{The breeding cycle of the Southern Bald Ibis, Geronticus calvus, was
   studied at a colony In the eastern Free State. Birds were paired before
   breeding commenced and nest building began regularly two weeks before
   the first eggs were laid. Nesting material was collected chiefly by the
   male. Incubation started with the first egg and lasted 26-32 days. Both
   sexes incubated, the female usually in the morning, the mole around
   noon, and both sexes in the afternoon. Hatching is asynchronous with
   intervals between eggs ranging from one id four days. Both parents
   participate in rearing the young; the female broods more often, while
   the mole brings the bulk of the food for the nestlings that were fed 3-4
   times a day. Strong competition for food between nestlings war recorded.
   Nestlings remain in nests for at least 35 days.}},
