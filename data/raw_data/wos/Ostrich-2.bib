@article{ ISI:000088246100066,
Author = {Beibro, KHY and Ellenberg, H},
Title = {{A contribution to the study of the avifauna of the Bossematie Forest
   (Abengorou/Cote d'Ivoire). The future of Bossematie Forest: current
   status}},
