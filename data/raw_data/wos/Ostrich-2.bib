@article{ ISI:000171793700012,
Author = {Jackson, C},
Title = {{Ageing Afrotropical birds in the hand: a proposed system}},
Journal = {{OSTRICH}},
Year = {{2001}},
Number = {{15}},
Pages = {{62-65}},
Month = {{JUL}},
Note = {{10th Pan-African Ornithological Congress, MAKERERE UNIV, KAMPALA,
   UGANDA, SEP 01-08, 2000}},
Organization = {{BirdLife Int; Barclays; RSPB; Wetlands Int; MacArthur Fdn; ABC;
   NatureUganda; Agence Intergovn Francophonie; NABU; WWF}},
Abstract = {{A number of aspects of avian biology are known to vary according to the
   age and sex of an individual. In studies of bird populations using
   ringing as a technique, accurate age determination is therefore a vital
   component. The European EURING age code system provides a neat system by
   which to categorise the age of an individual of Palaearctic species. As
   this is based on a set breeding season, it cannot be applied to tropical
   Africa where the breeding season varies between species, between,
   localities, and sometimes between years. Existing ageing systems used in
   Africa differ between regions and even ringers,and often there are no
   clear or easily applied definitions for the different age classes. An
   age code system is proposed that. allows Afrotropical birds in the hand
   to be placed into clearly defined age classes. The age data thus
   collected may be easily compared and analysed as well as shared between
   observers.}},
