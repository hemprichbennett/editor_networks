@article{ ISI:000165891100009,
Author = {Jackson, HD},
Title = {{Food of the nightjars in Zimbabwe}},
Journal = {{OSTRICH}},
Year = {{2000}},
Volume = {{71}},
Number = {{3-4}},
Pages = {{404-407}},
Month = {{SEP}},
Abstract = {{Analysis of the stomach contents of 282 nightjars (Caprimulgidae)
   collected in Zimbabwe showed that the seven species concerned feed
   almost exclusively on nocturnal flying insects, predominantly on beetles
   (Coleoptera, 52.7\%), especially lamellicorn beetles (Scarabaeidae,
   67.5\% of all beetles), and to a lesser extent on moths (Lepidoptera,
   12.7\%), grasshoppers (Orthoptera, 7.2\%), bugs (Hemiptera, 3.5\%) and,
   when available, termites (Isoptera, 20.4\%). There is some seasonal
   variation in diet, and prey selection varies between species. Deliberate
   ingestion of grit to aid digestion is unlikely.}},
