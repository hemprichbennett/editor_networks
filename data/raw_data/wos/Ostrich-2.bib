@article{ ISI:000176219000005,
Author = {Goodman, SM and Weigt, LA},
Title = {{The generic and species relationships of the reputed endemic Malagasy
   genus Pseudocossyphus (family Turdidae)}},
Journal = {{OSTRICH}},
Year = {{2002}},
Volume = {{73}},
Number = {{1-2}},
Pages = {{26-35}},
Month = {{MAR}},
Abstract = {{The generic and specific relationships of the Malagasy rock-thrushes,
   variously placed In the genus Monticola or as an endemic genus
   Pseudocossyphus, are examined using morphological and molecular
   characters. On the basis of mitochondrial DNA cytochrome b sequence data
   It was concluded that all Malagasy rock-thrushes fall between Asian
   Monticola and a species group of Monticolo occurring In numerous Old
   World areas. It is recommended that all Malagasy species be placed In
   the genus Monticola. Relationships between various named forms of
   Malagasy rock-thrushes Indicate: M. imerinus Is a distinct species, M.
   bensoni Is a synonym of M. sharpei, the subspecies M. sharpei
   erythronotus should be elevated to a full species, and that no
   geographic forms of M. sharpei (including interioris and salomonseni)
   should be recognized on the basis of current evidence. These taxonomic
   conclusions have important implications for conservation projects
   currently underway on Madagascar.}},
