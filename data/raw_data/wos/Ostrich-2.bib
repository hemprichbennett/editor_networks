@article{ ISI:000171793700021,
Author = {Manu, S},
Title = {{Possible factors influencing the decline of Nigeria's rarest endemic
   bird, the Ibadan Malimbe Malimbus ibadanensis}},
Journal = {{OSTRICH}},
Year = {{2001}},
Number = {{15}},
Pages = {{119-121}},
Month = {{JUL}},
Note = {{10th Pan-African Ornithological Congress, MAKERERE UNIV, KAMPALA,
   UGANDA, SEP 01-08, 2000}},
Organization = {{BirdLife Int; Barclays; RSPB; Wetlands Int; MacArthur Fdn; ABC;
   NatureUganda; Agence Intergovn Francophonie; NABU; WWF}},
Abstract = {{Past information and the current status of the critically endangered
   Ibadan Malimbe Malimbus ibadanensis is described. Twenty-one sites were
   surveyed for the Ibadan Malimbe over an eight month period in 2000.
   Ibadan Malimbes were found at two sites:IITA at Ibadan and Moniya about
   8 km to the north. Ibadan Malimbes were found to occur in mixed species
   flocks of other malimbe species and a wide range of other forest
   flocking species. There were no superficial differences in habitat
   between occupied sites, unoccupied sites and historically occupied sites
   suggesting that habitat change is not the main factor associated with
   the species' decline. Further work is needed urgently to identify the
   causes of the decline.}},
