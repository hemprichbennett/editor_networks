@article{ ISI:000088246100015,
Author = {Adjakpa, JB},
Title = {{The breeding biology of Abdim's Stork Ciconia abdimii in the far north
   of Benin}},
Journal = {{OSTRICH}},
Year = {{2000}},
Volume = {{71}},
Number = {{1-2}},
Pages = {{61-63}},
Month = {{MAR}},
Note = {{9th Pan-African Ornithological Congress, ACCRA, GHANA, DEC 01-07, 1996}},
Organization = {{Birdlife Int; Royal Soc Protect Birds; Standard Chartered Bank, Accra;
   Ghana Wildlife Soc; Minist Lands \& Forest; Ghana Natl Petr Corp; Frank
   Champman Mem Fund; Ghana Chamber Mines; Ecobank, Accra; British Council,
   Accra; European Union}},
Abstract = {{Little is known about the biology of Abdim's Stork Ciconia abdimii, in
   Benin. I studied the nesting of this intra-African migrant in the wet
   season of 1996. Breeding pairs arrived at the end of March 1996. Seven
   nest sites were found, involving 92 pairs; the largest colony was of 51
   pairs. The storks used five different large tree species for nesting.
   Egg-laying began in early April and lasted until 2 July. Most clutches
   were of 3-4 eggs. Incubation lasted 28-29 days. A total of 196 young
   storks fledged (all of which were ringed), representing a success rate
   of 76.5\% per egg laid and 86.0\% per egg hatched. The last storks left
   the colonies on 9 September 1996, 164 days after the first ones arrived
   on 30 March. The species is threatened in Benin by human persecution and
   by widespread pesticide use: it urgently requires official protected
   status.}},
