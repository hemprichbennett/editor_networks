@article{ ISI:000088246100010,
Author = {Smith, I and Bouwman, H},
Title = {{Levels of organochlorine pesticides in raptors from the North-West
   Province, South Africa}},
Journal = {{OSTRICH}},
Year = {{2000}},
Volume = {{71}},
Number = {{1-2}},
Pages = {{36-39}},
Month = {{MAR}},
Note = {{9th Pan-African Ornithological Congress, ACCRA, GHANA, DEC 01-07, 1996}},
Organization = {{Birdlife Int; Royal Soc Protect Birds; Standard Chartered Bank, Accra;
   Ghana Wildlife Soc; Minist Lands \& Forest; Ghana Natl Petr Corp; Frank
   Champman Mem Fund; Ghana Chamber Mines; Ecobank, Accra; British Council,
   Accra; European Union}},
Abstract = {{The levels of organochlorine pesticides in raptors from southern Africa
   remain poorly known. We found generally low levels of DDE, dieldrin and
   endosulfan in the blood of 20 raptors from the North-West Province,
   South Africa. Despite the low levels, these pesticides were present in
   all the birds, raising the issue of combined effects. The continuing use
   of organochlorines, despite their ban in many developed countries,
   remains. Trends in contamination and use elsewhere should therefore be
   monitored on a more regular basis, while the sampling of blood, rather
   than eggs seems to provide adequate information. The effects of other
   pesticides should also be investigated.}},
